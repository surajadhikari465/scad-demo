                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @RegionTraitID INT = (SELECT TraitID FROM Trait WHERE TraitGroupId = 5 AND TraitCode = 'ABB')
DECLARE @BusinessUnitID INT = (SELECT TraitID FROM Trait WHERE TraitGroupId = 5 AND TraitCode = 'BU')
DECLARE @StoreAbbreviationID INT = (SELECT TraitID FROM Trait WHERE TraitGroupId = 5 AND TraitCode = 'SAB')
DECLARE @CurrencyTraitId INT = (SELECT TraitID FROM Trait WHERE TraitCode = 'CUR')

SELECT localeID,
	   localeName,
       localeTypeID,
       NULL AS storeId,
	   NULL AS MetroId,
       NULL AS RegionId,
       NULL AS ChainId,
       localeOpenDate,
       localeCloseDate,
	   Null AS RegionCode,
	   NULL AS BusinessUnitId,
       NULL AS StoreAbbreviation,
       NULL AS CurrencyCode,
      0 AS Hospitality

  FROM [iCON].[dbo].[Locale] l
  WHERE LocaleTypeID=1

UNION

SELECT l.localeID,
	   l.localeName,
       l.localeTypeID,
       NULL AS storeId,
	   NULL AS MetroId,
       NULL AS RegionId,
       c.localeID AS ChainId,
       l.localeOpenDate,
       l.localeCloseDate,
	   lt.traitValue AS RegionCode,
	   NULL AS BusinessUnitId,
       NULL AS StoreAbbreviation,
       NULL AS CurrencyCode,
       0 AS Hospitality

  FROM [iCON].[dbo].[Locale] l
  left join LocaleTrait lt ON lt.localeID = l.localeID and lt.traitID = @RegionTraitID
  Inner Join Locale c on l.parentLocaleID = c.localeID
  WHERE l.LocaleTypeID=2

UNION

SELECT l.localeID,
	   l.localeName,
       l.localeTypeID,
       NULL AS storeId,
	   NULL AS MetroId,
       rg.localeID AS RegionId,
       c.localeID AS ChainId,
       l.localeOpenDate,
       l.localeCloseDate,
	   NULL AS RegionCode,
	   NULL AS BusinessUnitId,
       NULL AS StoreAbbreviation,
       NULL AS CurrencyCode,
       0 AS Hospitality

  FROM [iCON].[dbo].[Locale] l
   Inner Join Locale rg on l.parentLocaleID = rg.localeID
   Inner Join Locale c on rg.parentLocaleID = c.localeID
  WHERE l.LocaleTypeID=3
 
UNION

SELECT l.localeID,
	   l.localeName,
       l.localeTypeID,
       NULL AS storeId,
	   mt.localeID AS MetroId,
       rg.localeID AS RegionId,
       c.localeID AS ChainId,
       l.localeOpenDate,
       l.localeCloseDate,
	   NULL AS RegionCode,
	   Bu.traitValue AS BusinessUnitId,
       s.traitValue AS StoreAbbreviation,
       cur.traitValue AS CurrencyCode,
       0 AS Hospitality

  FROM [iCON].[dbo].[Locale] l
  	Inner Join Locale mt on l.parentLocaleID = mt.localeID
    Inner Join Locale rg on mt.parentLocaleID = rg.localeID
	Inner Join Locale c on rg.parentLocaleID = c.localeID
	Inner join LocaleTrait Bu ON Bu.localeID = l.localeID and Bu.traitID = @BusinessUnitID
	Inner join LocaleTrait s ON s.localeID = l.localeID and s.traitID = @StoreAbbreviationID
	left join LocaleTrait cur ON cur.localeID = l.localeID and cur.traitID = @CurrencyTraitId

  WHERE l.LocaleTypeID=4
  
UNION

SELECT l.localeID,
	   l.localeName,
       l.localeTypeID,
       s.localeID AS storeId,
	   mt.localeID AS MetroId,
       rg.localeID AS RegionId,
       c.localeID AS ChainId,
       l.localeOpenDate,
       l.localeCloseDate,
	   NULL AS RegionCode,
	   NULL AS BusinessUnitId,
       NULL AS StoreAbbreviation,
       NULL AS CurrencyCode,
       1 AS Hospitality

FROM [iCON].[dbo].[Locale] l
	Inner Join Locale s on l.parentLocaleID =  s.localeID
  	Inner Join Locale mt on s.parentLocaleID = mt.localeID
    Inner Join Locale rg on mt.parentLocaleID = rg.localeID
	Inner Join Locale c on rg.parentLocaleID = c.localeID
  
  WHERE l.LocaleTypeID=5</property>
