<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx2="http://schemas.wfm.com/Enterprise/RetailMgmt/ListType/V1" xmlns:WFM.UID="java://UID" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:pfx3="http://schemas.wfm.com/Mammoth/MessageHeaders.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:ns2="http://schemas.wfm.com/Enterprise/RetailMgmt/CommonRefTypes/V1" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns13="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:ns12="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema">
    <xsd:import namespace="http://schemas.wfm.com/Enterprise/RetailMgmt/ListType/V1" schemaLocation="/SharedResources/Schemas/WholeFoodsMarket_v3.0.0.1000/Common/ListType.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MessageHeaders.xsd" schemaLocation="/SharedResources/Schemas/JMS Message/MessageHeaders.xsd"/>
    <pd:name>Processes/SendToESB.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="InstructionData">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="InstructionMembers" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="InstructionCode" type="xsd:string"/>
                                <xsd:element name="InstructionDescription" type="xsd:string"/>
                                <xsd:element name="InstructionId" type="xsd:string"/>
                                <xsd:element name="Id" type="xsd:string"/>
                                <xsd:element name="Sequence" type="xsd:string"/>
                                <xsd:element name="Description" type="xsd:string"/>
                                <xsd:element name="ActionCode" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>68</pd:startX>
    <pd:startY>177</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>1398</pd:endX>
    <pd:endY>183</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1552590882893</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>593</pd:x>
        <pd:y>29</pd:y>
        <pd:width>320</pd:width>
        <pd:height>288</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>errorCount</pd:indexSlot>
            <pd:errorCondition>$_globalVariables/pfx:GlobalVariables/Application/DbErrorRetryCount &lt; $errorCount</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>JMS Queue Sender</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JMS Queue Sender</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JMS Queue Sender</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-52378</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>716</pd:x>
            <pd:y>240</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/Application/DbRetryDelayInMilliseconds"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>835</pd:x>
            <pd:y>239</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns12:ActivityInput>
                    <message>
                        <xsl:value-of select="concat(&quot;Error Message: &quot;, $_error/ns:ErrorReport/Msg, &quot;. StackTrace: &quot;, $_error/ns:ErrorReport/StackTrace)"/>
                    </message>
                    <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns12:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="JMS Queue Sender">
            <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
            <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
            <pd:x>748</pd:x>
            <pd:y>114</pd:y>
            <config>
                <PermittedMessageType>Text</PermittedMessageType>
                <SessionAttributes>
                    <transacted>false</transacted>
                    <acknowledgeMode>1</acknowledgeMode>
                    <maxSessions>1</maxSessions>
                    <destination>%%Destinations/InstructionListQueue%%</destination>
                </SessionAttributes>
                <ConfigurableHeaders>
                    <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                    <JMSExpiration>0</JMSExpiration>
                    <JMSPriority>4</JMSPriority>
                </ConfigurableHeaders>
                <ConnectionReference>/SharedResources/Connections/ESB.sharedjmscon</ConnectionReference>
                <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
            </config>
            <pd:inputBindings>
                <ns1:ActivityInput>
                    <OtherProperties>
                        <TransactionID>
                            <xsl:value-of select="$Create-Message-Header/pfx3:MessageHeaders/pfx3:TransactionID"/>
                        </TransactionID>
                        <xsl:if test="$Create-Message-Header/pfx3:MessageHeaders/pfx3:TransactionType">
                            <TransactionType>
                                <xsl:value-of select="$Create-Message-Header/pfx3:MessageHeaders/pfx3:TransactionType"/>
                            </TransactionType>
                        </xsl:if>
                        <xsl:if test="$Create-Message-Header/pfx3:MessageHeaders/pfx3:Source">
                            <Source>
                                <xsl:value-of select="$Create-Message-Header/pfx3:MessageHeaders/pfx3:Source"/>
                            </Source>
                        </xsl:if>
                        <xsl:if test="$Create-Message-Header/pfx3:MessageHeaders/pfx3:nonReceivingSysName">
                            <nonReceivingSysName>
                                <xsl:value-of select="$Create-Message-Header/pfx3:MessageHeaders/pfx3:nonReceivingSysName"/>
                            </nonReceivingSysName>
                        </xsl:if>
                    </OtherProperties>
                    <Body>
                        <xsl:value-of select="$Render-XML/xmlString"/>
                    </Body>
                </ns1:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>227</pd:x>
        <pd:y>180</pd:y>
        <config>
            <element ref="pfx2:lists"/>
        </config>
        <pd:inputBindings>
            <pfx2:lists>
                <pfx2:list>
                    <xsl:if test="$Start/InstructionData/InstructionMembers[1]/InstructionId">
                        <pfx2:id>
                            <xsl:value-of select="$Start/InstructionData/InstructionMembers[1]/InstructionId"/>
                        </pfx2:id>
                    </xsl:if>
                    <pfx2:type>
                        <pfx2:code>
                            <xsl:value-of select="$Start/InstructionData/InstructionMembers[1]/InstructionCode"/>
                        </pfx2:code>
                        <xsl:if test="$Start/InstructionData/InstructionMembers[1]/InstructionDescription">
                            <pfx2:description>
                                <xsl:value-of select="$Start/InstructionData/InstructionMembers[1]/InstructionDescription"/>
                            </pfx2:description>
                        </xsl:if>
                    </pfx2:type>
                    <pfx2:values>
                        <xsl:for-each select="$Start/InstructionData/InstructionMembers">
                            <pfx2:value>
                                <xsl:attribute name="Sequence">
                                    <xsl:value-of select="Sequence"/>
                                </xsl:attribute>
                                <xsl:attribute name="ns2:Action">
                                    <xsl:value-of select="ActionCode"/>
                                </xsl:attribute>
                                <pfx2:id>
                                    <xsl:value-of select="Id"/>
                                </pfx2:id>
                                <pfx2:name>
                                    <xsl:value-of select="Description"/>
                                </pfx2:name>
                                <pfx2:description>
                                    <xsl:value-of select="Description"/>
                                </pfx2:description>
                            </pfx2:value>
                        </xsl:for-each>
                    </pfx2:values>
                </pfx2:list>
            </pfx2:lists>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>335</pd:x>
        <pd:y>180</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>true</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx2:lists"/>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Map-Data/pfx2:lists"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render Message Headers JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>1047</pd:x>
        <pd:y>180</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx3:MessageHeaders"/>
        </config>
        <pd:inputBindings>
            <ns13:ActivityInputClass>
                <xsl:copy-of select="$Create-Message-Header/pfx3:MessageHeaders"/>
            </ns13:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Archive Instruction Message">
        <pd:type>com.tibco.plugin.jdbc.JDBCCallActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCCallActivity</pd:resourceType>
        <pd:x>1220</pd:x>
        <pd:y>192</pd:y>
        <config>
            <timeout>10</timeout>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/KitBuilder.sharedjdbc</jdbcSharedConfig>
            <schema>app</schema>
            <catalog/>
            <ProcedureName>MessageHistoryInsert;1</ProcedureName>
            <parameterTypes>
                <parameter>
                    <colName>@RETURN_VALUE</colName>
                    <colType>5</colType>
                    <dataType>4</dataType>
                    <typeName>int</typeName>
                </parameter>
                <parameter>
                    <colName>@MessageTypeId</colName>
                    <colType>1</colType>
                    <dataType>4</dataType>
                    <typeName>int</typeName>
                </parameter>
                <parameter>
                    <colName>@Message</colName>
                    <colType>1</colType>
                    <dataType>12</dataType>
                    <typeName>varchar</typeName>
                </parameter>
                <parameter>
                    <colName>@MessageHeader</colName>
                    <colType>1</colType>
                    <dataType>-9</dataType>
                    <typeName>nvarchar</typeName>
                </parameter>
            </parameterTypes>
        </config>
        <pd:inputBindings>
            <inputs>
                <inputSet>
                    <MessageTypeId>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/Application/MessageTypeId"/>
                    </MessageTypeId>
                    <Message>
                        <xsl:value-of select="tib:render-xml($Start/InstructionData, true(), true())"/>
                    </Message>
                    <MessageHeader>
                        <xsl:value-of select="$Render-Message-Headers-JSON/ns13:ActivityOutputClass/jsonString"/>
                    </MessageHeader>
                </inputSet>
            </inputs>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Create Message Header">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>474</pd:x>
        <pd:y>187</pd:y>
        <config>
            <element ref="pfx3:MessageHeaders"/>
        </config>
        <pd:inputBindings>
            <pfx3:MessageHeaders>
                <pfx3:TransactionID>
                    <xsl:value-of select="WFM.UID:UID()"/>
                </pfx3:TransactionID>
                <pfx3:TransactionType>
                    <xsl:value-of select="&quot;Instruction&quot;"/>
                </pfx3:TransactionType>
                <pfx3:Source>
                    <xsl:value-of select="&quot;KitBuilder&quot;"/>
                </pfx3:Source>
                <pfx3:nonReceivingSysName>
                    <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/Application/NonReceivingSystems"/>
                </pfx3:nonReceivingSysName>
            </pfx3:MessageHeaders>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Render Message Headers JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render Message Headers JSON</pd:from>
        <pd:to>Archive Instruction Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Archive Instruction Message</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>Create Message Header</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Create Message Header</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>