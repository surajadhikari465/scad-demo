                <statement>UPDATE dbo.KitLocale
SET StatusId =(select StatusID from Status where statuscode='PF'),
	LastUpdatedDateUtc = GETUTCDATE()
WHERE KitLocaleId = ?
</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>KitLocaleId</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <KitLocaleId>
