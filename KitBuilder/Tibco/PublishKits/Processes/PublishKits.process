           ,[MachineName]
           ,[InsertDateUtc]
           ,[LogDateUtc]
           ,[Thread]
           ,[Message]
           ,[CallSite]
           ,[Exception]
           ,[StackTrace])
     VALUES
(
?,
?,
?,
?,
?,
?,
?,
?,
?,
?,
?,
?)</statement>
