        <pd:activity name="Update KitQueue Table With ESB Error">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>865</pd:x>
            <pd:y>484</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>UPDATE dbo.KitQueue
SET Status ='F',
	MessageTimestampUtc = GETUTCDATE()
WHERE kitLocaleId = ?
       AND StoreId = ?
</statement>
                <jdbcSharedConfig>/SharedResources/Connections/KitBuilder.sharedjdbc</jdbcSharedConfig>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>kitLocaleId</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>StoreId</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <kitLocaleId>
                        <xsl:choose>
                            <xsl:when test="exists($GroupEvents/pfx5:Events/pfx5:EventType[1]/pfx5:kitLocaleId)">
                                <xsl:value-of select="$GroupEvents/pfx5:Events/pfx5:EventType[1]/pfx5:kitLocaleId"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </kitLocaleId>
                    <StoreId>
                        <xsl:value-of select="$CurrentStoreId/pfx5:StoreId"/>
                    </StoreId>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="CatchSendingToEsbException">
