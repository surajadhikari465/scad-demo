  </connectionStrings>
  <appSettings>
    <add key="ServerUrl" value="ssl://cerd1616.wfm.pvt:7233" />
    <add key="JmsUsername" value="kitUser" />
    <add key="JmsPassword" value="kitUser" />
    <add key="JndiUsername" value="jndiKitUser" />
    <add key="JndiPassword" value="jndiKitUser" />
    <add key="ConnectionFactoryName" value="ItemQueueConnectionFactory" />
    <add key="SslPassword" value="esb" />
