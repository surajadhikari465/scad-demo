                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @BrandHierarchyClassID INT = (SELECT TOP 1 hierarchyID 
									  FROM Hierarchy 
									  WHERE hierarchyName = 'Brands')

SELECT i.ItemID AS ItemID,
	   i.ScanCode AS ScanCode,
	   i.Desc_Product AS Desc_Product,
       iak.ImageUrl AS ImageUrl,
	   i.Desc_CustomerFriendly AS Desc_CustomerFriendly,
	   iak.Desc_Kitchen AS Desc_Kitchen,
	   hc.HierarchyClassName AS HierarchyClassName ,
	   GetUTCDate() AS [InsertDateUtc],
	   GetUTCDate() AS [LastUpdatedDateUtc]
FROM ItemAttributes_Kit iak
INNER JOIN items i ON i.ItemID = iak.ItemId 
INNER JOIN HierarchyClass hc ON hc.hierarchyID = @BrandHierarchyClassID 
		   AND hc.HierarchyClassID =  i.BrandHCID
WHERE iak.HospitalityItem = 1</property>
