  <connectionStrings>
    <add  xdt:Locator="Match(name)" xdt:Transform="Replace"  name="KitBuliderDb" providerName="System.Data.SqlClient" connectionString="Server=cldshr-mssql-prd;Database=KitBuilder;User Id=KitBuilderWebApi;Password=${KitBuilderWebApi.Password.Prod};Application Name=KBItemListener" />
  </connectionStrings>

  

</configuration>