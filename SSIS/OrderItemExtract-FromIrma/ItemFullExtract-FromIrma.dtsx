        private void OpenConnection()
        {
            LookupContext lookupContext = CreateLookupContext();
            factory = (ConnectionFactory)lookupContext.Lookup("OrderQueueConnectionFactory");
            factory.SetTargetHostName(Dts.Variables["esbTargetHostName"].Value.ToString());

            connection = factory.CreateConnection(Dts.Variables["esbJmsUsername"].Value.ToString(), Dts.Variables["esbJmsPassword"].Value.ToString());
            session = connection.CreateSession(false, SessionMode.ClientAcknowledge);
            CreateDestination(Dts.Variables["esbQueueName"].Value.ToString());
            producer = session.CreateProducer(destination);
            producer.DeliveryMode = DeliveryMode.PERSISTENT;
        }

        private X509Certificate GetEsbCert()
        {
            var store = new X509Store(StoreName.Root, StoreLocation.LocalMachine);
            store.Open(OpenFlags.ReadOnly);
            var cert = store.Certificates.Find(X509FindType.FindBySubjectDistinguishedName, Dts.Variables["esbCertificateName"].Value.ToString(), true)[0];
            store.Close();
            return cert;
        }

        private LookupContext CreateLookupContext()
        {
            EMSSSLFileStoreInfo storeInfo = new EMSSSLFileStoreInfo();
            storeInfo.SetSSLPassword(Dts.Variables["esbSslPassword"].Value.ToString().ToCharArray());
            storeInfo.SetSSLClientIdentity(GetEsbCert());

            LookupContext lc = new LookupContext(new Hashtable
                {
                    { LookupContext.PROVIDER_URL, Dts.Variables["esbServerUrl"].Value.ToString()},
                    { LookupContext.SECURITY_PRINCIPAL, Dts.Variables["esbSecurityPrincipal"].Value.ToString() },
                    { LookupContext.SECURITY_CREDENTIALS, Dts.Variables["esbSecurityCredentials"].Value.ToString() },
                    { LookupContext.SSL_STORE_INFO, storeInfo },
                    { LookupContext.SSL_STORE_TYPE, EMSSSLStoreType.EMSSSL_STORE_TYPE_FILE },
                    { LookupContext.SSL_TARGET_HOST_NAME, Dts.Variables["esbTargetHostName"].Value.ToString() },
                    { LookupContext.SECURITY_PROTOCOL, "ssl" },
                    { LookupContext.SSL_TRACE, false }
                });

            return lc;
        }
        protected virtual void CreateDestination(string queueName)
        {
            destination = session.CreateQueue(queueName);
        }

        public void Send(Dictionary<string, string> messageProperties = null)
        {
            foreach (string filePath in Directory.EnumerateFiles(Dts.Variables["FileRootPath"].Value.ToString()))
            {

                byte[] bytes = File.ReadAllBytes(filePath);
                BytesMessage bytesMessage = session.CreateBytesMessage();
                bytesMessage.WriteBytes(bytes);

                char[] delim = { '_' };
                string businessUnit = "";
                StringBuilder transactionId = new StringBuilder();

                if (filePath.Contains(FileNames.item_subscription.ToString()))
                {
                    string[] strArr = filePath.Split(delim);
                    businessUnit = strArr[3];
                    transactionId.Append(businessUnit).Append('_').Append(Dts.Variables["RunAsDateString"].Value.ToString())
                                  .Append(".F").Append(((int)FileNames.item_subscription).ToString());
                    messageProperties = new Dictionary<string, string>
                    {
                        { TransactionTypeKey, ItemSubscriptionTransactionTypeValue },
                        { TransactionId, transactionId.ToString() },
                        { Source, Dts.Variables["ExtractSource"].Value.ToString() },
                        { MessageType, Dts.Variables["esbMessageType"].Value.ToString() }
                    };
                }
                else if (filePath.Contains(FileNames.item_vendor_lane.ToString()))
                {
                    string[] strArr = filePath.Split(delim);
                    businessUnit = strArr[4];
                    transactionId.Append(businessUnit).Append('_').Append(Dts.Variables["RunAsDateString"].Value.ToString())
                                  .Append(".F").Append(((int)FileNames.item_vendor_lane).ToString());

                    messageProperties = new Dictionary<string, string>
                    {
                        { TransactionTypeKey, ItemVendorTransactionTypeValue },
                        { TransactionId, transactionId.ToString() },
                        { Source, Dts.Variables["ExtractSource"].Value.ToString() },
                        { MessageType, Dts.Variables["esbMessageType"].Value.ToString() }
                    };
                }

                Send(bytesMessage, messageProperties);
            }
        }

        private void Send(BytesMessage textMessage, Dictionary<string, string> messageProperties = null)
        {
            if (messageProperties != null)
            {
                foreach (var property in messageProperties)
                {
                    textMessage.SetStringProperty(property.Key, property.Value);
                }
            }

            producer.Send(textMessage);
        }
