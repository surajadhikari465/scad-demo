		public void Main()
		{
            System.Collections.ArrayList arr = new System.Collections.ArrayList();
    
            foreach(string region in Dts.Variables["$Project::RegionCode"].Value.ToString().Split('|'))
            {
                switch (Dts.Variables["$Project::Environment"].Value.ToString())
                {
                    case "DEV":
                        switch (region)
                        {
                            case "FL":
                            case "MA":
                            case "MW":
                            case "NA":
                            case "RM":
                            case "SO":
                                arr.Add("IDD-" + region + @"\" + region + "D");
                                break;
                            default:
                                arr.Add("IDT-" + region + @"\" + region + "T");
                                break;
                        }
                        break;
                    case "QA":
                        arr.Add("IDQ-" + region + @"\" + region + "Q");
                        break;
                    default:
                        arr.Add("IDP-" + region + @"\" + region + "P");
