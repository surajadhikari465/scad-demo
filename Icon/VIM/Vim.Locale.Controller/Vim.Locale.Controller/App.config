<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="MaxNumberOfRowsToMark" value="100" />
    <add key="FirstAttemptWaitTimeInMinute" value="5" />
    <add key="SecondAttemptWaitTimeInMinute" value="60" />
    <add key="ThirdAttemptWaitTimeInMinute" value="720" />
    <add key="BaseAddress" value="http://cewd4506:8081/" />
    <add key="SendEmails" value="false" />
    <add key="EmailHost" value="smtp.wholefoods.com" />
    <add key="EmailPort" value="25" />
    <add key="EmailUsername" value="" />
    <add key="EmailPassword" value="" />
    <add key="Sender" value="Icon-Dev@wholefoods.com" />
    <add key="Recipients" value="irma.developers@wholefoods.com" />
  </appSettings>
  <connectionStrings>
    <add name="Icon" providerName="System.Data.SqlClient" connectionString="data source=cewd1815\SQLSHARED2012D;initial catalog=IconDev;Integrated Security=True;MultipleActiveResultSets=True" />
  </connectionStrings>
  <nlog autoReload="true" throwExceptions="false" internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">
    <targets async="true">
      <target name="dbLogVimLocaleController" type="Database" connectionString="data source=cewd1815\SQLSHARED2012D;initial catalog=IconDev;Integrated Security=True;MultipleActiveResultSets=True" commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName)                          VALUES            (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="13" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
    <target name="json-file" p4:type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3" xmlns:p4="http://www.w3.org/2001/XMLSchema-instance"><layout p4:type="JsonLayout"><attribute name="date" layout="${longdate}" /><attribute name="level" layout="${level:upperCase=true}" /><attribute name="message" layout="${message}" /><attribute name="exception" layout="${exception:format=ToString}}" /></layout></target></targets>
    <rules>
      <logger name="*" minlevel="Info" writeTo="dbLogVimLocaleController,json-file" final="true" />
    </rules>
  </nlog>
</configuration>
