      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"
declare @ShelfLifeAttributeID as int, @UnwrappedTareWeightID as int, @WrappedTareWeightID as int, @UseByEABID as int, @SendtoCFSID as int, @ForceTareID as int
select @ShelfLifeAttributeID = AttributeID from dbo.Attributes where AttributeDesc = 'Shelf Life'
select @UnwrappedTareWeightID = AttributeID from dbo.Attributes where AttributeDesc = 'Unwrapped Tare Weight'
select @WrappedTareWeightID = AttributeID from dbo.Attributes where AttributeDesc = 'Wrapped Tare Weight'
select @UseByEABID = AttributeID from dbo.Attributes where AttributeDesc = 'Use By EAB'
select @SendtoCFSID = AttributeID from dbo.Attributes where AttributeDesc = 'CFS Send to Scale'
select @ForceTareID = AttributeID from dbo.Attributes where AttributeDesc = 'Force Tare'

delete ile
from dbo.ItemAttributes_Locale_" + @[$Project::RegionCode] + "_Ext ile
join dbo.Locales_" + @[$Project::RegionCode] + " l on ile.Region = l.Region
					 and ile.LocaleID = l.LocaleID 
join ##tmpItemScaleLoad  tis on ile.Region = tis.Region
							and ile.ItemID = tis.ItemID
							and l.BusinessUnitID = tis.BusinessUnitID
							and ile.AttributeID in (@ShelfLifeAttributeID, @UnwrappedTareWeightID, @WrappedTareWeightID, @UseByEABID, @SendtoCFSID, @ForceTareID)

insert into dbo.ItemAttributes_Locale_" + @[$Project::RegionCode] + "_Ext
           ([Region]
           ,[ItemID]
           ,[LocaleID]
