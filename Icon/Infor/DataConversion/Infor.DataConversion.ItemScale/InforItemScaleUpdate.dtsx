                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  srm.Region_Code as RegionCode, s.businessUnit_Id, InforItemId, ii.Scale_Identifier as ScaleItem, isc.ShelfLife_Length, wta.Description as WrappedTareWeight,
		uta.Description as UnwrappedTareWeight, sub.Description as UsedBy, isc.ForceTare, icf.SendToScale
from ValidatedScanCode vsc
join ItemIdentifier ii on vsc.ScanCode = ii.Identifier
join Item i on i.Item_key = ii.Item_key
join StoreItem si on si.Item_Key = i.Item_Key
join Store s on s.Store_No = si.Store_No
join StoreRegionMapping srm on srm.Store_No = s.Store_no
left join (select max(ItemScale_ID) as ItemScale_ID, Item_Key from ItemScale group by Item_Key) isci on isci.Item_Key = i.Item_Key
left join ItemScale isc on isc.ItemScale_ID = isci.ItemScale_ID
left join Scale_Tare wta on isc.Scale_Tare_ID = wta.Scale_Tare_ID
left join Scale_Tare uta on isc.Scale_Alternate_Tare_ID = uta.Scale_Tare_ID
left join Scale_EatBy sub on sub.Scale_EatBy_ID = isc.Scale_EatBy_ID
left join  ItemCustomerFacingScale icf on icf.Item_key = i.Item_key
where i.Deleted_Item = 0 and i.Remove_Item = 0
and ii.Deleted_Identifier = 0 and ii.Remove_Identifier = 0
and ii.Scale_Identifier = 1
and (s.Mega_Store = 1 or s.WFM_Store = 1)
and s.Internal = 1
and srm.Region_Code = ?</property>
