                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  srm.Region_Code as RegionCode, InforItemId, ScanCode, s.businessUnit_Id, ii.Default_Identifier, ISNULL(OrderedByInfor, 0) as OrderedByInfor, ior.Package_Desc2 as AltSize, iu.Unit_Name as AltUnit
from ValidatedScanCode vsc
join ItemIdentifier ii on vsc.ScanCode = ii.Identifier
join Item i on i.Item_key = ii.Item_key
join StoreItem si on si.Item_Key = i.Item_Key
join Store s on s.Store_No = si.Store_No
join StoreRegionMapping srm on srm.Store_No = s.Store_no
left join StoreItemExtended sie on sie.Item_key = i.Item_key
		and sie.Store_No = s.Store_No
left join ItemOverride ior on i.Item_key = ior.Item_key
left join ItemUnit iu on iu.Unit_Id = ior.Package_Unit_ID
where i.Deleted_Item = 0 and i.Remove_Item = 0
and ii.Deleted_Identifier = 0 and ii.Remove_Identifier = 0
and (s.Mega_Store = 1 or s.WFM_Store = 1)
and s.Internal = 1
and srm.Region_Code = ?</property>
