<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb"/>
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
  <appSettings>
    <!-- Windows Background Service Settings -->
    <add key="ServiceDisplayName" value="Infor HierarchyClass Listener"/>
    <add key="ServiceName" value="InforHierarchyClassListener"/>
    <add key="ServiceDescription" value="Processes Infor HierarchyClass updates to Icon."/>
    
    <!-- ESB Settings -->
    <add key="ServerUrl" value="ssl://cerd1616.wfm.pvt:7233"/>
    <add key="JmsUsername" value="iconUser"/>
    <add key="JmsPassword" value="iconUser"/>
    <add key="JndiUsername" value="jndiIconUser"/>
    <add key="JndiPassword" value="jndiIconUser"/>
    <add key="ConnectionFactoryName" value="ItemQueueConnectionFactory"/>
    <add key="SslPassword" value="esb"/>
    <add key="QueueName" value="WFMESB.ProductDev.HierarchyMgmt.Hierarchy.ItemMgmtSys.Queue.V2"/>
    <add key="SessionMode" value="ClientAcknowledge"/>
    <add key="CertificateName" value="CN=CA-ROOT-I, O=Whole Foods Market"/>
    <add key="CertificateStoreName" value="Root"/>
    <add key="CertificateStoreLocation" value="LocalMachine"/>
    <add key="TargetHostName" value="cerd1616.wfm.pvt"/>
    <add key="ReconnectDelay" value="30000"/>
    <add key="SendEmails" value="true"/>
    <add key="EmailHost" value="smtp.wholefoods.com"/>
    <add key="EmailPort" value="25"/>
    <add key="EmailUsername" value=""/>
    <add key="EmailPassword" value=""/>
    <add key="Sender" value="Icon-Dev@wholefoods.com"/>
    <add key="Recipients" value="irma.developers@wholefoods.com"/>
    <add key="NumberOfListenerThreads" value="1"/>
    
    <!-- Application Settings-->
    <add key="Regions" value="FL,MA,MW,NC,NE,PN,RM,SO,SP,SW"/>
  </appSettings>
  <connectionStrings>
    <add name="IconContext" connectionString="metadata=res://*/IconModel.csdl|res://*/IconModel.ssdl|res://*/IconModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=cewd1815\SQLSHARED2012D;initial catalog=IconDev;Integrated Security=True;multipleactiveresultsets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient"/>
    <add name="Icon" connectionString="data source=cewd1815\SQLSHARED2012D;initial catalog=IconDev;Integrated Security=True;multipleactiveresultsets=True;" providerName="System.Data.EntityClient"/>
  </connectionStrings>
  <nlog>
    <targets>
      <target name="dbLog"
              type="Database"
              connectionString="data source=cewd1815\SQLSHARED2012D;initial catalog=IconDev;Integrated Security=True;MultipleActiveResultSets=True"
              commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName)
                         VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="16" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="dbLog" final="true" />
    </rules>
  </nlog>
</configuration>
