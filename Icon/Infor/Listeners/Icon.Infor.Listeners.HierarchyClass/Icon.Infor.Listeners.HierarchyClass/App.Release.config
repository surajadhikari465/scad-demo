<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailHost" value="smtp.wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailPort" value="25" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailUsername" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailPassword" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Sender" value="Icon@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Recipients" value="irma.developers@wholefoods.com,GlobalDataIntegrity.InforErrors@wholefoods.com,WFMITIncidentAlert.INFORSupport@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Regions" value="FL,MA,MW,NA,NC,NE,PN,RM,SO,SP,SW" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <!--HierarchyClassServices key has names of services. We are using reflection to create instance of services-->
    <add key="HierarchyClassServices" value="AddOrUpdateHierarchyClasses,DeleteHierarchyClasses" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <esbConnections>
    <connections>
      <esbConnection name="ConfirmBOD"
                     serverUrl="ssl://cerp1616.wfm.pvt:37293,ssl://cerp1617.wfm.pvt:37293,ssl://odrp1609.wfm.pvt:37293,ssl://odrp1610.wfm.pvt:37293"
                     jmsUsername="iconUser"
                     jmsPassword="${iCon.Password}"
                     jndiUsername="jndiIconUser"
                     jndiPassword="${jndiIcon.Password}"
                     connectionFactoryName="ItemQueueConnectionFactory"
                     sslPassword="esb"
                     queueName="WFMESB.Technical.ErrorBOD.Error.Queue.V2"
                     sessionMode="ClientAcknowledge"
                     certificateName="CN=CA-ROOT-I, O=Whole Foods Market"
                     certificateStoreName="Root"
                     certificateStoreLocation="LocalMachine"
                     targetHostName="*.wfm.pvt" xdt:Transform="Replace" xdt:Locator="Match(name)" />
      <esbConnection name="Infor"
                     serverUrl="ssl://cerp1616.wfm.pvt:37293,ssl://cerp1617.wfm.pvt:37293,ssl://odrp1609.wfm.pvt:37293,ssl://odrp1610.wfm.pvt:37293"
                     jmsUsername="iconUser"
                     jmsPassword="${iCon.Password}"
                     jndiUsername="jndiIconUser"
                     jndiPassword="${jndiIcon.Password}"
                     connectionFactoryName="ItemQueueConnectionFactory"
                     sslPassword="esb"
                     queueName="WFMESB.ProductDev.HierarchyMgmt.Hierarchy.ItemMgmtSys.Queue.V2"
                     sessionMode="ClientAcknowledge"
                     certificateName="CN=CA-ROOT-I, O=Whole Foods Market"
                     certificateStoreName="Root"
                     certificateStoreLocation="LocalMachine"
                     targetHostName="*.wfm.pvt"
                     reconnectDelay="30000" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
    </connections>
  </esbConnections>
  <connectionStrings>
    <add name="IconContext" connectionString="metadata=res://*/IconModel.csdl|res://*/IconModel.ssdl|res://*/IconModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=idp-iCon\Shared3P;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="Icon" providerName="System.Data.SqlClient" connectionString="data source=idp-iCon\Shared3P;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="Mammoth" connectionString="data source=MAMMOTH-DB01-PRD\MAMMOTH;initial catalog=Mammoth;Integrated Security=True;MultipleActiveResultSets=True;" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets xdt:Transform="Replace">
      <target name="dbLog"
              xsi:type="Database"
              connectionString="data source=idp-iCon\Shared3P;initial catalog=iCon;Integrated Security=True;MultipleActiveResultSets=True"
              commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName)
                         VALUES            (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="16" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
    </targets>
  </nlog>
</configuration>