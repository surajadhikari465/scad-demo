<?xml version="1.0" encoding="utf-8"?>
<!--For more information on using transformations see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <configSections>
        <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
        <section name="esbConnections" type="Icon.Esb.ConfigReader.EsbConnectionConfigReader, Icon.Esb" />
        <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
        <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <entityFramework>
        <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
            <parameters>
                <parameter value="mssqllocaldb" />
            </parameters>
        </defaultConnectionFactory>
        <providers>
            <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
        </providers>
    </entityFramework>
    <connectionStrings>
        <add name="MammothContext" xdt:Transform="Replace" xdt:Locator="Match(name)" providerName="System.Data.SqlClient" connectionString="Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI;MultipleActiveResultSets=True;App=EntityFramework" />
    </connectionStrings>
    <esbConnections>
        <connections>
            <esbConnection
                xdt:Transform="Replace"
                xdt:Locator="Match(name)"
            name="MammothR10PriceEsbProducer"
            serverUrl="ssl://PROD-ESB-EMS-1.wfm.pvt:37293,ssl://PROD-ESB-EMS-2.wfm.pvt:37293,ssl://PROD-ESB-EMS-3.wfm.pvt:37293,ssl://PROD-ESB-EMS-4.wfm.pvt:37293"
            jmsUsername="mammothUser"
            jmsPassword="${mammothUser.Password}"
            jndiUsername="jndiMammothUser"
            jndiPassword="${jndiMammothUser.Password}"
            sslPassword="esb"
            connectionFactoryName="ItemQueueConnectionFactory"
            queueName="WFMESB.Commerce.Retail.PriceMgmt.Price.JustInTime.Queue.V2"
            sessionMode="AutoAcknowledge"
            certificateName="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=Austin, S=TX, C=US"
            certificateStoreName="Root"
            certificateStoreLocation="LocalMachine"
            targetHostName="PROD-ESB-EMS-1.wfm.pvt"
            reconnectDelay="30000"
		    />
        </connections>
    </esbConnections>
    <appSettings>
        <add key="Sender" value="Mammoth-PRD@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
        <add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="SQSListenerQueueUrl" value="https://sqs.us-east-1.amazonaws.com/968126238180/MammothGpmMammothR10Queue" xdt:Transform="Replace" xdt:Locator="Match(key)" />
        <add key="DvsGpmSourceBucket" value="gpmsourcebucket-701054487906" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="AwsRegion" value="us-east-1" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    </appSettings>
    <nlog autoReload="true" throwExceptions="false" internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">
        <targets xdt:Transform="Replace" async="true">
            <target name="dbLogMammoth" type="Database" connectionString="Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI" commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
                <parameter name="@AppId" layout="13" />
                <parameter name="@Level" layout="${level}" />
                <parameter name="@Logger" layout="${logger}" />
                <parameter name="@UserName" layout="${windows-identity}" />
                <parameter name="@MachineName" layout="${machinename}" />
                <parameter name="@LogDate" layout="${date}" />
                <parameter name="@Thread" layout="${threadid}" />
                <parameter name="@Message" layout="${message}" />
                <parameter name="@CallSite" layout="${callsite}" />
                <parameter name="@Exception" layout="${exception:format=tostring}" />
                <parameter name="@StackTrace" layout="${stacktrace}" />
            </target>
            <target name="json-file" type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
                <layout type="JsonLayout">
                    <attribute name="date" layout="${longdate}" />
                    <attribute name="level" layout="${level:upperCase=true}" />
                    <attribute name="message" layout="${message}" />
                    <attribute name="exception" layout="${exception:format=ToString}" />
                </layout>
            </target>
        </targets>
        <rules>
            <logger name="*" minlevel="Info" writeTo="dbLogMammoth, json-file" final="true" />
        </rules>
    </nlog>
</configuration>