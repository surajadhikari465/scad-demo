			<s3FacadeConfiguration xdt:Transform="Replace" xdt:Locator="Match(name)" name="ActivePriceListenerS3Config" awsAccessKey="${activeprice-jobscheduleruser.AccessKey}" awsSecretKey="${activeprice-jobscheduleruserSecretKey}" awsRegion="us-east-1" />
			<s3FacadeConfiguration xdt:Transform="Replace" xdt:Locator="Match(name)" name="ExpiringTPRListenerS3Config" awsAccessKey="${expiringtpr-jobscheduleruser.AccessKey}" awsSecretKey="${expiringtpr-jobscheduleruserSecretKey}" awsRegion="us-east-1" />
		</s3FacadeConfigurations>
		<sqsFacadeConfigurations>
			<sqsFacadeConfiguration xdt:Transform="Replace" xdt:Locator="Match(name)" name="ActivePriceListenerSQSConfig" awsAccessKey="${activeprice-jobscheduleruser.AccessKey}" awsSecretKey="${activeprice-jobscheduleruserSecretKey}" awsRegion="us-east-1" />
			<sqsFacadeConfiguration xdt:Transform="Replace" xdt:Locator="Match(name)" name="ExpiringTPRListenerSQSConfig" awsAccessKey="${expiringtpr-jobscheduleruser.AccessKey}" awsSecretKey="${expiringtpr-jobscheduleruserSecretKey}" awsRegion="us-east-1" />
		</sqsFacadeConfigurations>
		<sqsExtendedClientListenerConfigurations>
