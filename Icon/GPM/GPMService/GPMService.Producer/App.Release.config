			<sqsExtendedClientListenerConfiguration xdt:Transform="Replace" xdt:Locator="Match(name)" name="ExpiringTPRListenerSQSExtendedClientConfig" sqsListenerApplicationName="ExpiringTPRListener" sqsListenerQueueUrl="https://sqs.us-east-1.amazonaws.com/679325814203/JobScheduler-ExpiringTprServiceIncoming" />
		</sqsExtendedClientListenerConfigurations>
	</wfmAwsConfigurations>
	<appSettings>
		<add key="Sender" value="Mammoth-PRD@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<!-- GPM Bridge Settings -->
		<add key="SQSListenerQueueUrl" value="https://sqs.us-east-1.amazonaws.com/968126238180/MammothGpmIngressQueue" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="GpmConfirmBODBucket" value="mammothgpm-bridge-confirmbod-968126238180" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="GpmProcessBODBucket" value="mammothgpm-bridge-processbod-968126238180" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="GpmJustInTimeBucket" value="mammoth-gpm-just-in-time-prices-968126238180" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="GpmJustInTimeTopicArn" value="arn:aws:sns:us-east-1:968126238180:MammothGPMProcessTopic" xdt:Transform="Replace" xdt:Locator="Match(key)" />
        <add key="DvsGpmSourceBucket" value="gpmsourcebucket-701054487906" xdt:Transform="Replace" xdt:Locator="Match(key)" />
		<add key="AwsRegion" value="us-east-1" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	</appSettings>
	<nlog autoReload="true" throwExceptions="false" internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">
		<targets xdt:Transform="Replace" async="true">
			<target name="dbLogMammoth" type="Database" connectionString="Data Source=Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI" commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
				<parameter name="@AppId" layout="9" />
				<parameter name="@Level" layout="${level}" />
				<parameter name="@Logger" layout="${logger}" />
				<parameter name="@UserName" layout="${windows-identity}" />
				<parameter name="@MachineName" layout="${machinename}" />
				<parameter name="@LogDate" layout="${date}" />
				<parameter name="@Thread" layout="${threadid}" />
				<parameter name="@Message" layout="${message}" />
				<parameter name="@CallSite" layout="${callsite}" />
				<parameter name="@Exception" layout="${exception:format=tostring}" />
				<parameter name="@StackTrace" layout="${stacktrace}" />
			</target>
			<target name="json-file" type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
				<layout type="JsonLayout">
					<attribute name="date" layout="${longdate}" />
					<attribute name="level" layout="${level:upperCase=true}" />
					<attribute name="message" layout="${message}" />
					<attribute name="exception" layout="${exception:format=ToString}" />
				</layout>
			</target>
		</targets>
		<rules>
			<logger name="*" minlevel="Info" writeTo="dbLogMammoth, json-file" final="true" />
		</rules>
	</nlog>
</configuration>