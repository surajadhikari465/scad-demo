<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="IconContext" connectionString="metadata=res://*/IconModel.csdl|res://*/IconModel.ssdl|res://*/IconModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=Icon-db-prd;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="NonReceivingSystemsAll" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NonReceivingSystemsItemLocale" value="AMZ,ESL1Plum,Mammoth,Slaw,Spice,KB,Infor,ML,Pricer" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NonReceivingSystemsPrice" value="AMZ,ESL1Plum,Mammoth,Slaw,Spice,KB,Infor,ML" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NonReceivingSystemsProduct" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NonReceivingSystemsHierarchy" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Sender" value="Icon@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="RunInterval" value="30000" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="SendPreviousItemLinkGroup" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <!-- Message Generation Settings -->
    <add xdt:Transform="Replace" xdt:Locator="Match(key)" key="EnableNationalHierarchy" value="true" />
    <add xdt:Transform="Replace" xdt:Locator="Match(key)" key="EnableHospitalityHospitalityItem" value="true" />
    <add xdt:Transform="Replace" xdt:Locator="Match(key)" key="EnableHospitalityKitchenItem" value="true" />
    <add xdt:Transform="Replace" xdt:Locator="Match(key)" key="EnableHospitalityKitchenDescription" value="true" />
    <add xdt:Transform="Replace" xdt:Locator="Match(key)" key="EnableHospitalityImageUrl" value="true" />
    <add xdt:Transform="Replace" xdt:Locator="Match(key)" key="EnableHospitalityVenues" value="true" />
    <!-- ActiveMQ Settings (Prod) -->
    <add key="ActiveMqServerUrl" value="failover:(ssl://b-956057af-2031-4b35-b35c-52344b82aae7-1.mq.us-east-1.amazonaws.com:61617,ssl://b-896b2334-60cf-4b48-90a6-fe91ced0b6b4-1.mq.us-east-1.amazonaws.com:61617,ssl://b-bf60d142-270f-4008-8ac9-7c0156fd5174-1.mq.us-east-1.amazonaws.com:61617)?randomize=true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqJmsUsername" value="wfmdatavayuactivemqv2producer" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqJmsPassword" value="${WfmDataVayuActiveMqV2Producer.Password}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqItemQueueName" value="WFMActiveMQ.Commerce.Retail.ItemMgmt.Item.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqHierarchyQueueName" value="WFMActiveMQ.Commerce.Retail.HierarchyMgmt.Hierarchy.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqItemLocaleQueueName" value="WFMActiveMQ.Commerce.Retail.ItemLocaleMgmt.ItemLocale.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	  <add key="ActiveMqLocaleQueueName" value="WFMActiveMQ.Commerce.Retail.LocaleMgmt.Locale.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqProductSelectionGroupQueueName" value="WFMActiveMQ.Commerce.Retail.ItemMgmt.SelectionGroup.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqSessionMode" value="AutoAcknowledge" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ActiveMqReconnectDelay" value="30000" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets xdt:Transform="Replace" async="true">
      <target name="dbLogIconApiController" xsi:type="Database" connectionString="data source=Icon-db-prd;initial catalog=iCon;Integrated Security=True;MultipleActiveResultSets=True" commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName) VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="5" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
      <target name="json-file" xsi:type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
        <layout xsi:type="JsonLayout">
          <attribute name="date" layout="${longdate}" />
          <attribute name="level" layout="${level:upperCase=true}" />
          <attribute name="message" layout="${message}" />
          <attribute name="exception" layout="${exception:format=ToString}" />
        </layout>
      </target>
    </targets>
  </nlog>
</configuration>