<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="IconContext" connectionString="metadata=res://*/IconModel.csdl|res://*/IconModel.ssdl|res://*/IconModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=idp-iCon\Shared3P;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="NonReceivingSystemsItemLocale" value="ESL1Plum,Mammoth,Slaw,Spice" xdt:Transform="Replace" xdt:Locator="Match(key)" />

    <add key="Sender" value="Icon@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ServerUrl" value="ssl://cerp1616.wfm.pvt:37293,ssl://cerp1617.wfm.pvt:37293,ssl://odrp1609.wfm.pvt:37293,ssl://odrp1610.wfm.pvt:37293" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JmsUsername" value="iconUser" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="JmsPassword" value="${iconUser.Password}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JndiUsername" value="jndiIconUser" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JndiPassword" value="${jndiIconUser.Password}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="TargetHostName" value="*.wfm.pvt" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ConnectionFactoryName" value="ItemQueueConnectionFactory"/>
    <add key="LocaleQueueName" value="WFMESB.Commerce.Retail.LocaleMgmt.Locale.Queue.V2"/>
    <add key="HierarchyQueueName" value="WFMESB.Commerce.Retail.HierarchyMgmt.Hierarchy.Queue.V2"/>
    <add key="ItemQueueName" value="WFMESB.Commerce.Retail.ItemMgmt.Item.Queue.V2"/>
    <add key="ProductSelectionGroupQueueName" value="WFMESB.Commerce.Retail.ItemMgmt.SelectionGroup.Queue.V2"/>
    <add key="SessionMode" value="AutoAcknowledge"/>
    <add key="CertificateName" value="E=Thomas.Autry@wholefoods.com, CN=*.wfm.pvt, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=Austin, S=TX, C=US"/>
    <add key="CertificateStoreName" value="Root"/>
    <add key="CertificateStoreLocation" value="LocalMachine"/>
    <add key="TargetHostName" value="*.wfm.pvt"/>
    <add key="SslPassword" value="esb"/>
    
    <add key="SendPreviousItemLinkGroup" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EnableNationalHierarchy" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets xdt:Transform="Replace">
      <target name="dbLogIconApiController"
              xsi:type="Database"
              connectionString="data source=idp-iCon\Shared3P;initial catalog=iCon;Integrated Security=True;MultipleActiveResultSets=True"
              commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName) VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="5" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
    </targets>
  </nlog>
</configuration>