@model Icon.Web.Mvc.Models.ConfigurationSearchViewModel

@{
    ViewBag.Title = "Configuration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@{Html.RenderPartial("_HeaderPartial", "Configuration Management");}

<div id="searchOptions" class="page-section">
    @{Html.RenderPartial("_ConfigurationSearchOptionsPartial");}
</div>

<div id="searchResults" class="page-section"></div>

@section scripts
{
    @Scripts.Render("~/Scripts/common-search.js")
    @Scripts.Render("~/Scripts/configuration-search.js")
    @Scripts.Render("~/Scripts/apply-autocomplete.js")
}