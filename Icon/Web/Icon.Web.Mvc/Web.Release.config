                     jmsPassword="${ewicIconUser.Password}"
                     jndiUsername="jndiEwicIconUser"
                     jndiPassword="${jndiEwicIconUser.Password}"
                     connectionFactoryName="EwicQueueConnectionFactory"
                     sslPassword=""
