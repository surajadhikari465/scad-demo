<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="Audits" type="Audit.AuditConfigSection, MammothAudit"/>
    <section name="Upload" type="Audit.UploadConfigSection, MammothAudit"/>
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>

  <Audits>
    <add name="GPMPrice" proc="dbo.AuditGPMPrice" excludeRegions="MA,MW,NA,NC,NE,PN,RM,SP,SW,UK" isActive="false" fileName="Mammoth_Price_{Region}_{Date}.txt" delimiter="|" isGlobal="false" groupSize="250000" profileName="localTemp" dbConnectionName="Mammoth" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemLocale" proc="dbo.AuditItemLocale" excludeRegions="MA,MW,NA,NC,NE,PN,RM,SP,SW,UK" isActive="false" fileName="Mammoth_ItemLocale_{Region}_{Date}.txt" delimiter="|" isGlobal="false" groupSize="250000" profileName ="localTemp" dbConnectionName="Mammoth" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="GlobalItem" proc="dbo.AuditGlobalItem" isActive="false" fileName="Mammoth_Item_{Date}.txt" delimiter="|" isGlobal="true" groupSize="250000" profileName="localTemp" dbConnectionName="Mammoth" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="GPMMessageSequence" proc="dbo.AuditGPMMessageSequence" isActive="false" fileName="Mammoth_Sequence_Number_{Date}.txt" delimiter="|" isGlobal="true" groupSize="250000" profileName="localTemp" dbConnectionName="Mammoth" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="IRMAItemAuth" proc="dbo.AuditAuthorizedItems" excludeRegions="MA,MW,NA,NC,NE,PN,RM,SP,SW,UK" isActive="false" fileName="IRMA_Item_Auths_{Region}_{Date}.txt" delimiter="|" isGlobal="false" groupSize="250000" profileName="localTemp" dbConnectionName="IRMA_{Region}" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="IconLocale" proc="app.AuditLocationHierarchy" isActive="false" fileName="Icon_Location_Hierarchy_{Date}.txt" delimiter="|" isGlobal="true" groupSize="250000" profileName="localTemp" dbConnectionName="Icon" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </Audits>
  
  <Upload>
    <add profileName="localTemp" isS3Bucket="false" accessKey="" secretKey="" bucketRegion="" bucketName="14447-gpm-dev" destinationDir="C:\Temp\MammothAudit\Debug"  xdt:Transform="Replace" xdt:Locator="Match(profileName)" />
  </Upload>

  <connectionStrings>
    <add name="Icon" connectionString="data source=icon-db01-dev;initial catalog=Icon;integrated security=True;MultipleActiveResultSets=True" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
</configuration>