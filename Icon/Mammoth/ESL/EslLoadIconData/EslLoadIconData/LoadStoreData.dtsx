                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @businessUnitTraitID int =
	(select traitID from Trait where traitCode = 'BU')
select 
	bu.traitValue AS BusinessUnitID,
	pa.addressLine1 AS Address1,
	pa.addressLine2 AS Address2,
	pa.addressLine3 AS Address3,
	CAST(c.cityName AS NVARCHAR(100)) AS City,
	CAST(t.territoryName AS NVARCHAR(100)) AS Territory,
	t.territoryCode AS TerritoryAbbrev,
	pc.postalCode AS PostalCode,
	ctry.countryName AS Country,
	ctry.countryCode AS CountryAbbrev,
	tz.timezoneName AS Timezone,
                GETDATE() AS AddedDate
from LocaleTrait bu
join LocaleAddress la 
	on bu.localeID = la.localeID
join PhysicalAddress pa
	on la.addressID = pa.addressID
join City c 
	on pa.cityID = c.cityID
join Territory t 
	on pa.territoryID = t.territoryID
join PostalCode pc
	on pa.postalCodeID = pc.postalCodeID
join Country ctry
	on pa.countryID = ctry.countryID
join Timezone tz 
	on pa.timezoneID = tz.timezoneID
where bu.traitID = @businessUnitTraitID</property>
