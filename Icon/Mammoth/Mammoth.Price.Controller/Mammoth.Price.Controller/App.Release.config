<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="BaseAddress" value="http://mammoth/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Sender" value="Mammoth-Prd@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />

    <add key="RunIntervalInMilliseconds" value="30000" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="RegionsToProcess" value="FL,MA,MW,NA,NC,NE,PN,RM,SO,SP,SW" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="MaxNumberOfRowsToMark" value="30" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SendEmails" value="true" xdt:Transform="Replace"  xdt:Locator="Match(key)" />
  </appSettings>
  <connectionStrings>
    <add name="ItemCatalog_FL" providerName="System.Data.EntityClient" connectionString="data source=idp-fl\flp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_MA" providerName="System.Data.EntityClient" connectionString="data source=idp-ma\map;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_MW" providerName="System.Data.EntityClient" connectionString="data source=idp-mw\mwp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_NA" providerName="System.Data.EntityClient" connectionString="data source=idp-na\nap;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_NC" providerName="System.Data.EntityClient" connectionString="data source=idp-nc\ncp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_NE" providerName="System.Data.EntityClient" connectionString="data source=idp-ne\nep;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_PN" providerName="System.Data.EntityClient" connectionString="data source=idp-pn\pnp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_RM" providerName="System.Data.EntityClient" connectionString="data source=IRMA-RM-DB-PRD;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_SO" providerName="System.Data.EntityClient" connectionString="data source=idp-so\sop;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_SP" providerName="System.Data.EntityClient" connectionString="data source=idp-sp\spp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_SW" providerName="System.Data.EntityClient" connectionString="data source=idp-sw\swp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="ItemCatalog_UK" providerName="System.Data.EntityClient" connectionString="data source=idp-uk\ukp;initial catalog=ItemCatalog;Integrated Security=SSPI" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <nlog>
    <targets xdt:Transform="Replace">
      <target
              name="dbLogMammoth"
              type="Database"
              connectionString="Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI"
              commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
        <parameter name="@AppId" layout="3" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Logger" layout="${logger}" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@MachineName" layout="${machinename}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Thread" layout="${threadid}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@CallSite" layout="${callsite}" />
        <parameter name="@Exception" layout="${exception:format=tostring}" />
        <parameter name="@StackTrace" layout="${stacktrace}" />
      </target>
    </targets>
  </nlog>
</configuration>