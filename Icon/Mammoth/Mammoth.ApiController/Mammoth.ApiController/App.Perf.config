<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations  
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="MammothContext" providerName="System.Data.SqlClient" connectionString="Data Source=QA-01-MAMMOTH02\MAMMOTH02;Initial Catalog=Mammoth;Integrated Security=SSPI;MultipleActiveResultSets=True;App=EntityFramework" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="Sender" value="Mammoth-Perf@wholefoods.com"  xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SendEmails" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="RunInterval" value="30000"  xdt:Transform="Replace" xdt:Locator="Match(key)"/>
	<!-- Active MQ Settings -->
	<add key="ActiveMqServerUrl" value="failover:(ssl://b-dbe17350-b1ae-40c5-909b-7670ddfb52a7-1.mq.us-west-2.amazonaws.com:61617,ssl://b-c24c6cbd-bc7a-4ff2-82f6-315673768772-1.mq.us-west-2.amazonaws.com:61617,ssl://b-4231a649-45d2-4cfb-b5d0-ef8e1d373e00-1.mq.us-west-2.amazonaws.com:61617)?randomize=true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqJmsUsername" value="wfmdatavayuactivemqv2producer" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqJmsPassword" value="03SSSVeKoWnBWZiA" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqItemLocaleQueueName" value="WFMActiveMQ.Commerce.Retail.ItemLocaleMgmt.ItemLocale.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqLegacyPriceQueueName" value="WFMActiveMQ.Commerce.Retail.LegacyPriceMgmt.LegacyPrice.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqSessionMode" value="AutoAcknowledge" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqReconnectDelay" value="30000" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <nlog>
    <targets xdt:Transform="Replace">
      <target
              name="dbLogMammoth"
              type="Database"
              connectionString="Data Source=QA-01-MAMMOTH02\MAMMOTH02;Initial Catalog=Mammoth;Integrated Security=SSPI"
              commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
        <parameter name="@AppId" layout="4" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Logger" layout="${logger}" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@MachineName" layout="${machinename}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Thread" layout="${threadid}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@CallSite" layout="${callsite}" />
        <parameter name="@Exception" layout="${exception:format=tostring}" />
        <parameter name="@StackTrace" layout="${stacktrace}" />
    </target>
		<target name="json-file" type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
				<layout type="JsonLayout">
						<attribute name="date" layout="${longdate}" />
						<attribute name="level" layout="${level:upperCase=true}" />
						<attribute name="message" layout="${message}" />
						<attribute name="exception" layout="${exception:format=ToString}" />
				</layout>
		</target>
    </targets>
    <rules xdt:Transform="Replace">
      <logger name="*" minlevel="Info" writeTo="dbLogMammoth, json-file" final="true" />
    </rules>
  </nlog>
</configuration>