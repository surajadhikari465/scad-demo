<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="MammothContext" providerName="System.Data.SqlClient" connectionString="Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI;MultipleActiveResultSets=True;App=EntityFramework" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="ServerUrl" value="ssl://PROD-ESB-EMS-1.wfm.pvt:37293,ssl://PROD-ESB-EMS-2.wfm.pvt:37293,ssl://PROD-ESB-EMS-3.wfm.pvt:37293,ssl://PROD-ESB-EMS-4.wfm.pvt:37293" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="TargetHostName" value="PROD-ESB-EMS-1.wfm.pvt" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Sender" value="Mammoth-PRD@wholefoods.com"  xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="JmsUsername" value="mammothUser"  xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JmsPassword" value="${mammothUser.Password}"  xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JndiUsername" value="jndiMammothUser"  xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JndiPassword" value="${jndiMammothUser.Password}"  xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="CertificateName" value="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=Austin, S=TX, C=US" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
	<!-- Active MQ Settings -->
	<add key="ActiveMqServerUrl" value="failover:(ssl://b-7a5c30e5-f37f-4f8e-b0af-af5cc42ea4b1-1.mq.us-east-1.amazonaws.com:61617,ssl://b-7a5c30e5-f37f-4f8e-b0af-af5cc42ea4b1-2.mq.us-east-1.amazonaws.com:61617)?randomize=true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqJmsUsername" value="wfmdatavayuactivemqproducer" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqJmsPassword" value="${WfmDataVayuActiveMqProducer.Password}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqItemLocaleQueueName" value="WFMActiveMQ.Commerce.Retail.ItemLocaleMgmt.ItemLocale.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqLegacyPriceQueueName" value="WFMActiveMQ.Commerce.Retail.LegacyPriceMgmt.LegacyPrice.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqSessionMode" value="AutoAcknowledge" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqReconnectDelay" value="30000" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <nlog>
    <targets xdt:Transform="Replace">
      <target
              name="dbLogMammoth"
              type="Database"
              connectionString="Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI"
              commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
        <parameter name="@AppId" layout="4" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Logger" layout="${logger}" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@MachineName" layout="${machinename}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Thread" layout="${threadid}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@CallSite" layout="${callsite}" />
        <parameter name="@Exception" layout="${exception:format=tostring}" />
        <parameter name="@StackTrace" layout="${stacktrace}" />
      </target>
		  <target name="json-file" type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
		  		<layout type="JsonLayout">
		  				<attribute name="date" layout="${longdate}" />
		  				<attribute name="level" layout="${level:upperCase=true}" />
		  				<attribute name="message" layout="${message}" />
		  				<attribute name="exception" layout="${exception:format=ToString}" />
		  		</layout>
		  </target>
    </targets>
    <rules xdt:Transform="Replace">
      <logger name="*" minlevel="Info" writeTo="dbLogMammoth, json-file" final="true" />
    </rules>
  </nlog>
</configuration>