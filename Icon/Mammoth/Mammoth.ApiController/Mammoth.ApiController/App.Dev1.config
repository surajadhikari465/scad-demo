  </connectionStrings>
  <appSettings>
    <add key="ServerUrl" value="ssl://DEV-ESB-EMS-1.wfm.pvt:7233" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="TargetHostName" value="DEV-ESB-EMS-1.wfm.pvt" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Sender" value="Mammoth-Dev1@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SendEmails" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Recipients" value="irma.developers@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="CertificateName" value="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=&quot;Austin &quot;, S=TX, C=US" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<!-- Active MQ Settings -->
	<add key="ActiveMqServerUrl" value="failover:(ssl://b-b1a575b2-58e7-4bd5-b17a-03dbdd11e562-1.mq.us-west-2.amazonaws.com:61617,ssl://b-b1a575b2-58e7-4bd5-b17a-03dbdd11e562-2.mq.us-west-2.amazonaws.com:61617)?randomize=true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqJmsUsername" value="wfmdatavayuactivemqproducer" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqJmsPassword" value="4dsV8hY1zm3HBOtq" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqItemLocaleQueueName" value="WFMActiveMQ.Commerce.Retail.ItemLocaleMgmt.ItemLocale.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqLegacyPriceQueueName" value="WFMActiveMQ.Commerce.Retail.LegacyPriceMgmt.LegacyPrice.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
	<add key="ActiveMqSessionMode" value="AutoAcknowledge" xdt:Transform="Replace" xdt:Locator="Match(key)" />
