<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    
    <TargetDatabaseName>Mammoth_Dev</TargetDatabaseName>
    <DeployScriptFileName>Mammoth.Db.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=MAMMOTH-DB01-DEV\MAMMOTH;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>

    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DoNotAlterReplicatedObjects>True</DoNotAlterReplicatedObjects>
    <DropConstraintsNotInSource>True</DropConstraintsNotInSource>
    <DropDmlTriggersNotInSource>False</DropDmlTriggersNotInSource>
    <DropExtendedPropertiesNotInSource>False</DropExtendedPropertiesNotInSource>
    <DropIndexesNotInSource>False</DropIndexesNotInSource>
    <DropStatisticsNotInSource>False</DropStatisticsNotInSource>
    <IgnoreIdentitySeed>True</IgnoreIdentitySeed>
    <IgnoreNotForReplication>True</IgnoreNotForReplication>
    <IgnorePartitionSchemes>True</IgnorePartitionSchemes>
    <IgnoreUserSettingsObjects>True</IgnoreUserSettingsObjects>

    <IncludeCompositeObjects>False</IncludeCompositeObjects>
    <IncludeTransactionalScripts>True</IncludeTransactionalScripts>

    <ScriptDatabaseCollation>True</ScriptDatabaseCollation>
    <ScriptDatabaseOptions>False</ScriptDatabaseOptions>

  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="Staging">
      <Value>MammothStaging.Db</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>