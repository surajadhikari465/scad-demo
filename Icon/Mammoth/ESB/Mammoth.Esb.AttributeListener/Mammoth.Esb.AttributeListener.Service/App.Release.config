<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="Mammoth" connectionString="data source=MAMMOTH-DB01-PRD\MAMMOTH;initial catalog=Mammoth;integrated security=True;MultipleActiveResultSets=True" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="EnablePrimeAffinityMessages" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Sender" value="Mammoth-PRD@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <esbConnections>
    <connections xdt:Transform="Replace">
      <esbConnection
        name="SB1"
        serverUrl="ssl://odrp1631.wfm.pvt:37233,ssl://odrp1632.wfm.pvt:37233,ssl://cerp1640.wfm.pvt:37233,ssl://cerp1641.wfm.pvt:37233"
        jmsUsername="sb1_iconUser"
        jmsPassword="${SB1.icon_User.Password}"
        sslPassword="esb"
        queueName="WFMSB1.Mammoth.Attributes.Queue.V1"
        sessionMode="ClientAcknowledge"
        certificateName="E=&quot;cerp1640.wfm.pvt,cerp1641.wfm.pvt,odrp1631.wfm.pvt,odrp1632.wfm.pvt&quot;, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=Austin, S=TX, C=US"
        certificateStoreName="Root"
        certificateStoreLocation="LocalMachine"
        targetHostName="cerp1640.wfm.pvt" />
      <esbConnection
        name="SB1Error"
        serverUrl="ssl://odrp1631.wfm.pvt:37233,ssl://odrp1632.wfm.pvt:37233,ssl://cerp1640.wfm.pvt:37233,ssl://cerp1641.wfm.pvt:37233"
        jmsUsername="sb1_mammothPriceUser"
        jmsPassword="${SB1.MammothPriceUser.Password}"
        sslPassword="esb"
        queueName="WFMSB1.Mammoth.Retail.Errors.Queue.V1"
        sessionMode="ClientAcknowledge"
        certificateName="E=&quot;cerp1640.wfm.pvt,cerp1641.wfm.pvt,odrp1631.wfm.pvt,odrp1632.wfm.pvt&quot;, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=Austin, S=TX, C=US"
        certificateStoreName="Root"
        certificateStoreLocation="LocalMachine"
        targetHostName="cerp1640.wfm.pvt" />
    </connections>
  </esbConnections>
  <nlog>
    <targets xdt:Transform="Replace">
      <target
              name="dbLogMammoth"
              type="Database"
              connectionString="Data Source=MAMMOTH-DB01-PRD\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI"
              commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
        <parameter name="@AppId" layout="26" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Logger" layout="${logger}" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@MachineName" layout="${machinename}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Thread" layout="${threadid}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@CallSite" layout="${callsite}" />
        <parameter name="@Exception" layout="${exception:format=tostring}" />
        <parameter name="@StackTrace" layout="${stacktrace}" />
      </target>
    </targets>
    <rules xdt:Transform="Replace">
      <logger name="*" minlevel="Info" writeTo="dbLogMammoth" final="true" />
    </rules>
  </nlog>
</configuration>