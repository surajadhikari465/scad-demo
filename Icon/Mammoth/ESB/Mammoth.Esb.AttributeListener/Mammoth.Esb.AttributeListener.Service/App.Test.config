<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="Mammoth" connectionString="data source=MAMMOTH-DB01-DEV\MAMMOTH;initial catalog=Mammoth;integrated security=True;MultipleActiveResultSets=True" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="EnablePrimeAffinityMessages" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Sender" value="Mammoth-Test@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <esbConnections>
    <connections xdt:Transform="Replace">
      <esbConnection
          name="SB1"
          serverUrl="ssl://cerd1669.wfm.pvt:17233,ssl://cerd1670.wfm.pvt:17233"
          jmsUsername="sb1_iconUser"
          jmsPassword="sb1_iconUser"
          sslPassword="esb"
          queueName="WFMSB1.Mammoth.Attributes.Queue.V1"
          sessionMode="ClientAcknowledge"
          certificateName="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=&quot;Austin &quot;, S=TX, C=US"
          certificateStoreName="Root"
          certificateStoreLocation="LocalMachine"
          targetHostName="cerd1669.wfm.pvt" />
      <esbConnection
          name="SB1Error"
          serverUrl="ssl://cerd1669.wfm.pvt:17253,ssl://cerd1670.wfm.pvt:17253"
          jmsUsername="sb1_mammothPriceUser"
          jmsPassword="sb1_mammothPriceUser"
          sslPassword="esb"
          queueName="WFMSB1.Mammoth.Retail.Errors.Queue.V1"
          sessionMode="ClientAcknowledge"
          certificateName="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=&quot;Austin &quot;, S=TX, C=US"
          certificateStoreName="Root"
          certificateStoreLocation="LocalMachine"
          targetHostName="cerd1669.wfm.pvt" />
    </connections>
  </esbConnections>
  <nlog>
    <targets xdt:Transform="Replace">
      <target
              name="dbLogMammoth"
              type="Database"
              connectionString="Data Source=MAMMOTH-DB01-DEV\MAMMOTH;Initial Catalog=Mammoth;Integrated Security=SSPI"
              commandText="INSERT INTO app.AppLog(AppId, Level, Logger, UserName, MachineName, LogDate, Thread, Message, CallSite, Exception, StackTrace) VALUES (@AppId, @Level, @Logger, @UserName, @MachineName, @LogDate, @Thread, @Message, @CallSite, @Exception, @StackTrace);">
        <parameter name="@AppId" layout="26" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Logger" layout="${logger}" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@MachineName" layout="${machinename}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Thread" layout="${threadid}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@CallSite" layout="${callsite}" />
        <parameter name="@Exception" layout="${exception:format=tostring}" />
        <parameter name="@StackTrace" layout="${stacktrace}" />
      </target>
    </targets>
    <rules xdt:Transform="Replace">
      <logger name="*" minlevel="Info" writeTo="dbLogMammoth" final="true" />
    </rules>
  </nlog>
</configuration>