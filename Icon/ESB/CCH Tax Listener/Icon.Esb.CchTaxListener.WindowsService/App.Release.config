    <add key="JmsUsername" value="iconUser" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JmsPassword" value="${iCon.Password}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JndiUsername" value="jndiIconUser" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="JndiPassword" value="${jndiIcon.Password}" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ServerUrl" value="ssl://PROD-ESB-EMS-1.wfm.pvt:37293,ssl://PROD-ESB-EMS-2.wfm.pvt:37293,ssl://PROD-ESB-EMS-3.wfm.pvt:37293,ssl://PROD-ESB-EMS-4.wfm.pvt:37293" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ConnectionFactoryName" value="ItemQueueConnectionFactory" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="SslPassword" value="esb" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="QueueName" value="WFMESB.Enterprise.TaxMgmt.Item.Queue.V2" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="SessionMode" value="ExplicitClientAcknowledge" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="CertificateName" value="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=Austin, S=TX, C=US" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="CertificateStoreName" value="Root" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="CertificateStoreLocation" value="LocalMachine" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="TargetHostName" value="PROD-ESB-EMS-1.wfm.pvt" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="ReconnectDelay" value="30000" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="IconToIrmaTaxHierarchyUpdateEnabledRegionsList" value="SP,NC,MA,NE,MW,RM,SO,NA,SW,PN,FL" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailHost" value="smtp.wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailPort" value="25" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailUsername" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EmailPassword" value="" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Sender" value="Icon@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="Recipients" value="SCAD.Alerts.Prd@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="NumberOfListenerThreads" value="1" />
    <add key="Phase2" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="UpdateMammoth" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <connectionStrings>
    <add name="IconContext" connectionString="metadata=res://*/IconModel.csdl|res://*/IconModel.ssdl|res://*/IconModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=Icon-db-prd;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="Icon" providerName="System.Data.SqlClient" connectionString="data source=Icon-db-prd;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="Mammoth" connectionString="data source=MAMMOTH-DB01-PRD\MAMMOTH;initial catalog=Mammoth;Integrated Security=True;MultipleActiveResultSets=True;" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets xdt:Transform="Replace" async="true">
      <target name="dbLogIconEsb" xsi:type="Database" connectionString="data source=Icon-db-prd;initial catalog=iCon;Integrated Security=True;MultipleActiveResultSets=True" commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName)                          VALUES            (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="3" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
      <target name="json-file" xsi:type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
        <layout xsi:type="JsonLayout">
          <attribute name="date" layout="${longdate}" />
          <attribute name="level" layout="${level:upperCase=true}" />
          <attribute name="message" layout="${message}" />
          <attribute name="exception" layout="${exception:format=ToString}" />
        </layout>
      </target>
    </targets>
  </nlog>
</configuration>