	<!-- ================== -->
	<xs:import namespace="http://schemas.wfm.com/Enterprise/StatusMgmt/CommonRefTypes/V1" schemaLocation="CommonRefTypes.xsd"/>
	<xs:import namespace="http://schemas.wfm.com/Enterprise/StatusMgmt/Status/V2" schemaLocation="Status.xsd"/>
