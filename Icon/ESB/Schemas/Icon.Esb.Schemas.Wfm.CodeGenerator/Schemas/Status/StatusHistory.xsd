	<!-- ================== -->
	<!-- Global Elements             -->
	<!-- ================== -->
	<xs:element name="statusHistory" type="StatusHistoryType"/>
	<xs:element name="statusHistories" type="StatusHistoriesType"/>
	<xs:complexType name="StatusHistoryType">
		<xs:sequence>
			<xs:element name="effectiveDate" type="crf:WfmDateTimeType" minOccurs="0" maxOccurs="1"/>
