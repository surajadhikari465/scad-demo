<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.wfm.com/Enterprise/StatusMgmt/StatusHistory/V1" xmlns:crf="http://schemas.wfm.com/Enterprise/RetailMgmt/CommonRefTypes/V1" xmlns:srf="http://schemas.wfm.com/Enterprise/StatusMgmt/CommonRefTypes/V1" xmlns:stt="http://schemas.wfm.com/Enterprise/StatusMgmt/Status/V2" targetNamespace="http://schemas.wfm.com/Enterprise/StatusMgmt/StatusHistory/V1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<!-- ================== -->
