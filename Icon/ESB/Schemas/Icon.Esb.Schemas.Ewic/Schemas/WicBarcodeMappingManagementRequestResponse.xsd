			<xs:attribute name="MajorVersion" type="xs:integer" use="required" fixed="1"/>
			<xs:attribute name="MinorVersion" type="xs:integer" use="optional" default="0"/>
			<xs:attribute name="FixVersion" type="xs:integer" use="optional" default="0"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="WicBarcodeMappingWithAction" mixed="false">
		<xs:complexContent>
			<xs:extension base="ewic:BarcodeMapping1">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
