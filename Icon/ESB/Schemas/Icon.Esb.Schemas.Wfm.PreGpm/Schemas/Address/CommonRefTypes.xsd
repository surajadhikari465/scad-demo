	<!-- U.S. ZIP Code Type -->
	<xs:simpleType name="ZipCodeType">
		<xs:annotation>
			<xs:documentation>ZIP Code - 5 digits plus optional 4 or 7 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmStrType">
			<xs:pattern value="[0-9]{5}(([0-9]{4})|([0-9]{7}))?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- U.S. ZIP Code 5 Digits -->
	<xs:simpleType name="ZipCode5">
		<xs:annotation>
			<xs:documentation>ZIP Code - 5 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmStrType">
			<xs:pattern value="[0-9]{5}"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- U.S. ZIP Code Type -->
	<xs:simpleType name="ZipCode4">
		<xs:annotation>
			<xs:documentation>ZIP Code - 4 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmStrType">
			<xs:pattern value="[0-9]{4}"/>
		</xs:restriction>
