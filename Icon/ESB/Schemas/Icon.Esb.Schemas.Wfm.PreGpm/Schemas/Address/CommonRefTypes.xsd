	</xs:simpleType>
	<xs:simpleType name="LatType">
		<xs:annotation>
			<xs:documentation>Latitude measurements range from –90° to +90° (unbounded decimals allowed)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmStrType">
			<xs:pattern value="[-+]?([1-8]?\d(\.\d+)?|90(\.0+)?)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LongType">
		<xs:annotation>
			<xs:documentation>Longitude measurements range from –180° to +180° (unbounded decimals allowed)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmStrType">
			<xs:pattern value="[-+]?((180(\.0+))?|((1[0-7]\d)|([1-9]?\d))(\.\d+)?)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="urlType">
		<xs:restriction base="crf:WfmStrType">
			<!--<xs:pattern value=""/>-->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PrimaryBoolType">
		<xs:annotation>
			<xs:documentation>Flag to distinguish if address element is the primary/main entity.  Example, many email usages, but one of the emails is used as the primary.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmBoolType"/>
	</xs:simpleType>
	<xs:simpleType name="PrivacyBoolType">
		<xs:annotation>
			<xs:documentation>Flag to distinguish if address element is private; meaning it is not to be used/contacted.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="crf:WfmBoolType"/>
	</xs:simpleType>
