JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode
	AND ial.BusinessUnitId = id.BusinessUnitId
WHERE id.DeletedItem = 1
	AND (ial.Authorized = 1
		OR ial.IrmaItemKey IS NOT NULL
		OR ial.DefaultScanCode = 1)
union
