    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{EAF30041-D462-4CA7-ADE7-68EEEC432ECB}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT i.ScanCode&#xA;&#x9;,ial.*&#xA;FROM Items i&#xA;JOIN ItemAttributes_Locale_&quot; + @[User::Region] + &quot; ial ON i.ItemID = ial.ItemID&#xA;JOIN Locales_&quot; + @[User::Region] + &quot; l ON ial.BusinessUnitID = l.BusinessUnitID&#xA;&#x9;AND l.LocaleCloseDate IS NULL&#xA;WHERE (ial.Authorized = 1&#xA;&#x9;OR ial.DefaultScanCode = 1&#xA;&#x9;OR ial.IrmaItemKey IS NOT NULL)&#xA;&#x9;AND NOT EXISTS (&#xA;&#x9;&#x9;SELECT 1&#xA;&#x9;&#x9;FROM stage.IrmaData id&#xA;&#x9;&#x9;WHERE id.DeletedItem = 0&#xA;&#x9;&#x9;&#x9;AND id.DeletedIdentifier = 0&#xA;&#x9;&#x9;&#x9;AND id.ScanCode = i.ScanCode&#xA;&#x9;&#x9;&#x9;AND id.BusinessUnitId = ial.BusinessUnitID&#xA;&#x9;&#x9;)&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="PublishMammothRecordsNotInIrma">
      <DTS:VariableValue
        DTS:DataType="8">SELECT i.ScanCode
	,ial.*
FROM Items i
JOIN ItemAttributes_Locale_FL ial ON i.ItemID = ial.ItemID
JOIN Locales_FL l ON ial.BusinessUnitID = l.BusinessUnitID
	AND l.LocaleCloseDate IS NULL
WHERE (ial.Authorized = 1
	OR ial.DefaultScanCode = 1
	OR ial.IrmaItemKey IS NOT NULL)
	AND NOT EXISTS (
		SELECT 1
		FROM stage.IrmaData id
		WHERE id.DeletedItem = 0
			AND id.DeletedIdentifier = 0
			AND id.ScanCode = i.ScanCode
			AND id.BusinessUnitId = ial.BusinessUnitID
		)</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{D1F5987F-3751-4699-B72E-908A4CFF62B6}"
