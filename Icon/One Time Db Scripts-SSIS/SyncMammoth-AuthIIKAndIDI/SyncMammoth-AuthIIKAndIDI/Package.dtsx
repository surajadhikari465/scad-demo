      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select * &#xA;from stage.IrmaData id&#xA;where id.DeletedItem = 0 &#xA;&#x9;and id.DeletedIdentifier = 0&#xA;&#x9;and not exists&#xA;&#x9;(&#xA;&#x9;&#x9;select 1&#xA;&#x9;&#x9;from Items i&#xA;&#x9;&#x9;join ItemAttributes_Locale_&quot; + @[User::Region] + &quot; ial on i.ItemID = ial.ItemID&#xA;&#x9;&#x9;where i.ScanCode = id.ScanCode&#xA;&#x9;&#x9;&#x9;and id.BusinessUnitId = ial.BusinessUnitID&#xA;&#x9;)&quot;"
