	ial.BusinessUnitID,
	ial.ItemAttributeLocaleID,
	ial.Authorized MammothAuthorized,
	ial.DefaultScanCode MammothDefaultScanCode,
	ial.IrmaItemKey MammothIrmaItemKey,
	id.IrmaItemKey,
	id.DefaultIdentifier,
	id.Authorized,
	id.RemoveItem,
	id.DeletedItem,
	id.RemoveIdentifier,
	id.DeletedIdentifier,
	CASE 
		WHEN ial.Authorized = 1 THEN 'Item is deleted in IRMA but is authorized in Mammoth'
		WHEN ial.IrmaItemKey IS NOT NULL THEN 'Item is deleted in IRMA but has an IRMA Item Key in Mammoth'
		WHEN ial.DefaultScanCode = 1 THEN 'Item is deleted in IRMA but has a Default Scan Code set to true'
		ELSE 'Invalid error'
	END AS Error
