                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">declare @Region NVARCHAR(2) = (SELECT TOP 1 RegionCode FROM Region)
declare @ExcludedStoreNo varchar(250) = (
	SELECT acv.Value
	FROM 
		AppConfigValue acv
		INNER JOIN AppConfigEnv ace ON acv.EnvironmentID = ace.EnvironmentID 
		INNER JOIN AppConfigApp aca ON acv.ApplicationID = aca.ApplicationID 
		INNER JOIN AppConfigKey ack ON acv.KeyID = ack.KeyID 
	WHERE 
		aca.Name = 'IRMA Client'
		AND ack.Name = 'LabAndClosedStoreNo'
		and SUBSTRING(ace.Name, 1, 1) = SUBSTRING((SELECT top(1) Environment FROM Version), 1, 1))

declare @ActiveStores as table (Store_No int Primary Key)

INSERT INTO @ActiveStores
SELECT Store_No
FROM Store s
WHERE s.Store_No not in (select Key_Value from dbo.fn_Parse_List(@ExcludedStoreNo, '|'))
	AND (s.WFM_Store = 1 OR s.Mega_Store = 1 )
	AND (Internal = 1 AND BusinessUnit_ID IS NOT NULL)

select
                @Region Region,
	CAST(vsc.ScanCode AS NVARCHAR(13)) AS ScanCode,
	s.BusinessUnit_ID AS BusinessUnitId,
	i.Item_Key AS IrmaItemKey,
	CAST(ii.Default_Identifier AS BIT) AS DefaultIdentifier,
	si.Authorized AS Authorized,
	CAST(i.Remove_Item AS BIT) AS RemoveItem,
	CAST(i.Deleted_Item AS BIT) AS DeletedItem,
	CAST(ii.Remove_Identifier AS BIT) AS RemoveIdentifier,
	CAST(ii.Deleted_Identifier AS BIT) AS DeletedIdentifier
from Item i
join ItemIdentifier ii on i.Item_Key = ii.Item_Key
join StoreItem si on ii.Item_Key = si.Item_Key
join Store s on si.Store_No = s.Store_No
join @ActiveStores acts on s.Store_No = acts.Store_No
join ValidatedScanCode vsc on ii.Identifier = vsc.ScanCode
where (i.Deleted_Item = 0 and ii.Deleted_Identifier = 0)
    or (i.Deleted_Item = 1 and ii.Deleted_Identifier = 1
        and ii.Identifier not in (select ii2.Identifier 
									from ItemIdentifier ii2
									join Item i2 on ii2.Item_Key = i2.Item_Key
									where ii2.Deleted_Identifier = 0
									and i2.Deleted_Item = 0))</property>
