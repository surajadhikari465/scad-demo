      DTS:Expression="&quot;UPDATE ial&#xA;SET Authorized = 0&#xA;&#x9;,IrmaItemKey = NULL&#xA;&#x9;,DefaultScanCode = 0&#xA;&#x9;,ModifiedDate = SYSDATETIME()&#xA;OUTPUT inserted.ItemID&#xA;&#x9;,inserted.BusinessUnitID&#xA;&#x9;,CASE &#xA;&#x9;&#x9;WHEN deleted.Authorized = 1 THEN 'Authorized is true but item is deleted in IRMA'&#xA;&#x9;&#x9;WHEN deleted.IrmaItemKey IS NOT NULL THEN 'IrmaItemKey is not null but item is deleted in IRMA'&#xA;&#x9;&#x9;WHEN deleted.DefaultScanCode = 1 THEN 'DefaultScanCode is true but item is deleted in IRMA'&#xA;&#x9;&#x9;ELSE 'Invalid Error'&#xA;&#x9;END&#xA;INTO stage.IrmaDataUpdatedRecords_&quot; + @[User::Region] + &quot;&#xA;FROM ItemAttributes_Locale_&quot; + @[User::Region] + &quot; ial&#xA;JOIN Items i ON ial.ItemId = i.ItemId&#xA;JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode&#xA;&#x9;AND ial.BusinessUnitId = id.BusinessUnitId&#xA;WHERE id.DeletedItem = 1&#xA;&#x9;AND (&#xA;&#x9;&#x9;ial.Authorized = 1&#xA;&#x9;&#x9;OR ial.IrmaItemKey IS NOT NULL&#xA;&#x9;&#x9;OR ial.DefaultScanCode = 1&#xA;&#x9;&#x9;)&#xA;&#xA;UPDATE ial&#xA;SET Authorized = 0&#xA;&#x9;,IrmaItemKey = NULL&#xA;&#x9;,DefaultScanCode = 0&#xA;&#x9;,ModifiedDate = SYSDATETIME()&#xA;OUTPUT inserted.ItemID&#xA;&#x9;,inserted.BusinessUnitID&#xA;&#x9;,CASE &#xA;&#x9;&#x9;WHEN deleted.Authorized = 1 THEN 'Authorized is true but item does not exist in IRMA'&#xA;&#x9;&#x9;WHEN deleted.IrmaItemKey IS NOT NULL THEN 'IrmaItemKey is not null but item does not exist in IRMA'&#xA;&#x9;&#x9;WHEN deleted.DefaultScanCode = 1 THEN 'DefaultScanCode is true but item does not exist in IRMA'&#xA;&#x9;&#x9;ELSE 'Invalid Error'&#xA;&#x9;END&#xA;INTO stage.IrmaDataUpdatedRecords_&quot; + @[User::Region] + &quot;&#xA;FROM ItemAttributes_Locale_&quot; + @[User::Region] + &quot; ial&#xA;JOIN Locales_&quot; + @[User::Region] + &quot; l ON ial.BusinessUnitId = l.BusinessUnitId&#xA;JOIN Items i ON ial.ItemId = i.ItemId&#xA;LEFT JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode&#xA;&#x9;AND ial.BusinessUnitId = id.BusinessUnitId&#xA;WHERE (&#xA;&#x9;&#x9;ial.Authorized = 1&#xA;&#x9;&#x9;OR ial.DefaultScanCode = 1&#xA;&#x9;&#x9;OR ial.IrmaItemKey IS NOT NULL&#xA;&#x9;&#x9;)&#xA;&#x9;AND id.ScanCode IS NULL&#xA;&#xA;UPDATE ial&#xA;SET Authorized = id.Authorized&#xA;&#x9;,IrmaItemKey = id.IrmaItemKey&#xA;&#x9;,DefaultScanCode = id.DefaultIdentifier&#xA;&#x9;,ModifiedDate = SYSDATETIME()&#xA;OUTPUT inserted.ItemID&#xA;&#x9;,inserted.BusinessUnitID&#xA;&#x9;,CASE &#xA;&#x9;&#x9;WHEN deleted.Authorized &lt;&gt; inserted.Authorized THEN 'Authorized does not match'&#xA;&#x9;&#x9;WHEN deleted.IrmaItemKey &lt;&gt; inserted.IrmaItemKey THEN 'IrmaItemKey does not match'&#xA;&#x9;&#x9;WHEN deleted.DefaultScanCode &lt;&gt; inserted.DefaultScanCode THEN 'DefaultScanCode does not match'&#xA;&#x9;&#x9;ELSE 'Invalid Error'&#xA;&#x9;END&#xA;INTO stage.IrmaDataUpdatedRecords_&quot; + @[User::Region] + &quot;&#xA;FROM ItemAttributes_Locale_&quot; + @[User::Region] + &quot; ial&#xA;JOIN Items i ON ial.ItemId = i.ItemId&#xA;JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode&#xA;&#x9;AND ial.BusinessUnitId = id.BusinessUnitId&#xA;WHERE id.DeletedItem = 0&#xA;&#x9;AND id.DeletedIdentifier = 0&#xA;&#x9;AND (&#xA;&#x9;&#x9;ISNULL(ial.IrmaItemKey, - 1) &lt;&gt; id.IrmaItemKey&#xA;&#x9;&#x9;OR ial.Authorized &lt;&gt; id.Authorized&#xA;&#x9;&#x9;OR ial.DefaultScanCode &lt;&gt; id.DefaultIdentifier&#xA;&#x9;&#x9;)&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SyncMammothSql">
      <DTS:VariableValue
        DTS:DataType="8">UPDATE ial
SET Authorized = 0
	,IrmaItemKey = NULL
	,DefaultScanCode = 0
	,ModifiedDate = SYSDATETIME()
OUTPUT inserted.ItemID
	,inserted.BusinessUnitID
	,CASE 
		WHEN deleted.Authorized = 1 THEN 'Authorized is true but item is deleted in IRMA'
		WHEN deleted.IrmaItemKey IS NOT NULL THEN 'IrmaItemKey is not null but item is deleted in IRMA'
		WHEN deleted.DefaultScanCode = 1 THEN 'DefaultScanCode is true but item is deleted in IRMA'
		ELSE 'Invalid Error'
	END
INTO stage.IrmaDataUpdatedRecords_FL
FROM ItemAttributes_Locale_FL ial
JOIN Items i ON ial.ItemId = i.ItemId
JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode
	AND ial.BusinessUnitId = id.BusinessUnitId
WHERE id.DeletedItem = 1
	AND (
		ial.Authorized = 1
		OR ial.IrmaItemKey IS NOT NULL
		OR ial.DefaultScanCode = 1
		)

UPDATE ial
SET Authorized = 0
	,IrmaItemKey = NULL
	,DefaultScanCode = 0
	,ModifiedDate = SYSDATETIME()
OUTPUT inserted.ItemID
	,inserted.BusinessUnitID
	,CASE 
		WHEN deleted.Authorized = 1 THEN 'Authorized is true but item does not exist in IRMA'
		WHEN deleted.IrmaItemKey IS NOT NULL THEN 'IrmaItemKey is not null but item does not exist in IRMA'
		WHEN deleted.DefaultScanCode = 1 THEN 'DefaultScanCode is true but item does not exist in IRMA'
		ELSE 'Invalid Error'
	END
INTO stage.IrmaDataUpdatedRecords_FL
FROM ItemAttributes_Locale_FL ial
JOIN Locales_FL l ON ial.BusinessUnitId = l.BusinessUnitId
JOIN Items i ON ial.ItemId = i.ItemId
LEFT JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode
	AND ial.BusinessUnitId = id.BusinessUnitId
WHERE (
		ial.Authorized = 1
		OR ial.DefaultScanCode = 1
		OR ial.IrmaItemKey IS NOT NULL
		)
	AND id.ScanCode IS NULL

UPDATE ial
SET Authorized = id.Authorized
	,IrmaItemKey = id.IrmaItemKey
	,DefaultScanCode = id.DefaultIdentifier
	,ModifiedDate = SYSDATETIME()
OUTPUT inserted.ItemID
	,inserted.BusinessUnitID
	,CASE 
		WHEN deleted.Authorized &lt;&gt; inserted.Authorized THEN 'Authorized does not match'
		WHEN deleted.IrmaItemKey &lt;&gt; inserted.IrmaItemKey THEN 'IrmaItemKey does not match'
		WHEN deleted.DefaultScanCode &lt;&gt; inserted.DefaultScanCode THEN 'DefaultScanCode does not match'
		ELSE 'Invalid Error'
	END
INTO stage.IrmaDataUpdatedRecords_FL
FROM ItemAttributes_Locale_FL ial
JOIN Items i ON ial.ItemId = i.ItemId
JOIN stage.IrmaData id ON i.ScanCode = id.ScanCode
	AND ial.BusinessUnitId = id.BusinessUnitId
WHERE id.DeletedItem = 0
	AND id.DeletedIdentifier = 0
	AND (
		ISNULL(ial.IrmaItemKey, - 1) &lt;&gt; id.IrmaItemKey
		OR ial.Authorized &lt;&gt; id.Authorized
		OR ial.DefaultScanCode &lt;&gt; id.DefaultIdentifier
		)</DTS:VariableValue>
    </DTS:Variable>
