        public void Main()
        {
            string environment = Dts.Variables["Environment"].Value.ToString();
            string region = Dts.Variables["Region"].Value.ToString();
            string irmaDatabase;
            string irmaServer;

            if (environment.Equals("Dev") || environment.Equals("Dev0") || environment.Equals("Test") || environment.Equals("Test0"))
            {
                irmaDatabase = "ItemCatalog_Test";
            }
            else
            {
                irmaDatabase = "ItemCatalog";
            }
            if (environment.Equals("Dev") || environment.Equals("Dev0") || environment.Equals("Test") || environment.Equals("Test0") || environment.Equals("Test1"))
            {
                irmaServer = irmaTest0Servers[region];
            }
            else if (environment.Equals("Test1"))
            {
                irmaServer = irmaTest1Servers[region];
            }
            else if (environment.Equals("QA") || environment.Equals("QA0"))
            {
                irmaServer = irmaQa0Servers[region];
            }
            else if (environment.Equals("QA1"))
            {
                irmaServer = irmaQa1Servers[region];
            }
            else if (environment.Equals("Prod"))
            {
                irmaServer = irmaProdServers[region];
            }
            else
            {
                throw new ArgumentException("Unexpected value for Environment argument. Possible expected values are either Test0, Test1, QA0, QA1, or Prod.");
            }

            Dts.Variables["IrmaServer"].Value = irmaServer;
            Dts.Variables["IrmaDatabase"].Value = irmaDatabase;

            Dts.TaskResult = (int)ScriptResults.Success;
        }

