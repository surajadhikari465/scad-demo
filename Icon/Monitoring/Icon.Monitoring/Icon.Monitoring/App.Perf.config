  <nlog autoReload="true" throwExceptions="false" internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log" xdt:Transform="Replace">
    <targets async="true">
      <target name="dbLogMonitor" type="Database" connectionString="data source=icon-db-qa-perf;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True" commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName) VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="14" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
    <target name="json-file" p6:type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3" xmlns:p6="http://www.w3.org/2001/XMLSchema-instance"><layout p6:type="JsonLayout"><attribute name="date" layout="${longdate}" /><attribute name="level" layout="${level:upperCase=true}" /><attribute name="message" layout="${message}" /><attribute name="exception" layout="${exception:format=ToString}}" /></layout></target></targets>
    <rules>
      <logger name="Icon.Monitoring.*" minlevel="Debug" writeTo="dbLogMonitor,json-file" final="true" />
    </rules>
  </nlog>
