<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="Icon" connectionString="data source=icon-db01-dev;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True" xdt:Locator="Match(name)" xdt:Transform="Replace" />
    <add name="Mammoth" connectionString="Data Source=MAMMOTH-DB01-DEV\MAMMOTH;Initial Catalog=MammothDev;integrated security=True;MultipleActiveResultSets=True"  xdt:Locator="Match(name)" xdt:Transform="Replace" />
    <!-- ** Conn strings for IRMA DBs are the same for Dev and Test, so we don't need to specify here, as default app.config should have Test IRMA DB connections. -->
  </connectionStrings>
  <appSettings>
    <add key="Environment" value="Dev" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="Sender" value="Icon-Dev@wholefoods.com" xdt:Locator="Match(key)" xdt:Transform="Replace"/>

    <add key="EnableVimLocaleConJobMonitor" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace"/>

    <!-- Prime Affinity Controller Monitor Settings -->
    <add key="PrimeAffinityControllerMonitorEnabled_FL" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_MA" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_MW" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_NA" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_NC" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_NE" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_PN" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_RM" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_SO" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_SP" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityControllerMonitorEnabled_SW" value="false" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_FL" value="07:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_MA" value="07:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_MW" value="07:20:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_NA" value="07:40:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_NC" value="07:40:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_NE" value="07:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_PN" value="08:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_RM" value="09:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_SO" value="07:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_SP" value="09:10:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="PrimeAffinityPsgCompletionUtcTime_SW" value="07:40:00" xdt:Locator="Match(key)" xdt:Transform="Replace" />
  </appSettings>
  <nlog autoReload="true" throwExceptions="false" internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log" xdt:Transform="Replace">
    <targets>
      <target name="dbLogMonitor" type="Database" connectionString="data source=icon-db01-dev;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True" commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName) VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="14" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
    </targets>
    <rules>
      <logger name="Icon.Monitoring.*" minlevel="Debug" writeTo="dbLogMonitor" final="true" />
    </rules>
  </nlog>
</configuration>