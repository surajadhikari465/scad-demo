@model Icon.Dashboard.Mvc.Models.ApplicationTypeEnum?

@{
    string displayName = Model.ToString();
    switch (Model.GetValueOrDefault(Icon.Dashboard.Mvc.Models.ApplicationTypeEnum.WindowsService))
    {
        case Icon.Dashboard.Mvc.Models.ApplicationTypeEnum.WindowsService:
            displayName = "Windows Service";
            break;
        case Icon.Dashboard.Mvc.Models.ApplicationTypeEnum.ScheduledTask:
            displayName = "Scheduled Task";
            break;
        case Icon.Dashboard.Mvc.Models.ApplicationTypeEnum.Unknown:
        default:
            break;
    }
}

@Html.DisplayTextFor(model => displayName)