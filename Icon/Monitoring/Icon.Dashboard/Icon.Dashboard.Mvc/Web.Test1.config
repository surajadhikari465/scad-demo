<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="hostingEnvironment" value="Tst1" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="securityGroupsForReadOnly" value="IRMA.Applications,IRMA.Support" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="securityGroupsForEditing" value="IRMA.Applications,IRMA.Support" xdt:Locator="Match(key)" xdt:Transform="Replace" />
  </appSettings>
  <connectionStrings>
    <add name="IconDatabaseContext"
         connectionString="metadata=res://*/IconDatabaseModelForDashboard.csdl|res://*/IconDatabaseModelForDashboard.ssdl|res://*/IconDatabaseModelForDashboard.msl;
           provider=System.Data.SqlClient;
           provider connection string=&quot;
            data source=icon-db01-tst01;
            initial catalog=Icon;
            integrated security=True;
            MultipleActiveResultSets=True;
            App=EntityFramework&quot;"
         providerName="System.Data.EntityClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)"/>
    <add name="MammothDatabaseContext"
         connectionString="
         metadata=res://*/MammothDatabaseModelForDashboard.csdl|res://*/MammothDatabaseModelForDashboard.ssdl|res://*/MammothDatabaseModelForDashboard.msl;
         provider=System.Data.SqlClient;
         provider connection string=&quot;
           data source=mammoth-db01-tst01;
           initial catalog=Mammoth;
           integrated security=True;
           MultipleActiveResultSets=True;
           App=EntityFramework&quot;"
         providerName="System.Data.EntityClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)" />
  </connectionStrings>
  <system.web>
    <customErrors mode="Off" xdt:Transform="Insert" />
  </system.web>
</configuration>