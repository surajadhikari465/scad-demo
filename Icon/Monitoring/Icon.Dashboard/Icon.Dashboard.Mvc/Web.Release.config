<?xml version="1.0"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="activeEnvironment" value="Prd" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="securityGroupForReadOnly" value="IRMA.Applications,IRMA.Developers,IRMA.Support" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="securityGroupForEditing" value="" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="pathToXmlDataFile" value="DashboardApplicationsPrd.xml" xdt:Locator="Match(key)" xdt:Transform="Replace" />
  </appSettings>
  <connectionStrings>
    <add name="IconDatabaseContext"
         connectionString="metadata=res://*/IconDatabaseModelForDashboard.csdl|res://*/IconDatabaseModelForDashboard.ssdl|res://*/IconDatabaseModelForDashboard.msl;
           provider=System.Data.SqlClient;
           provider connection string=&quot;
            data source=idp-iCon\Shared3P;
            initial catalog=Icon;
            integrated security=True;
            MultipleActiveResultSets=True;
            App=EntityFramework&quot;"
         providerName="System.Data.EntityClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)"/>
    <add name="MammothDatabaseContext"
         connectionString="
         metadata=res://*/MammothDatabaseModelForDashboard.csdl|res://*/MammothDatabaseModelForDashboard.ssdl|res://*/MammothDatabaseModelForDashboard.msl;
         provider=System.Data.SqlClient;
         provider connection string=&quot;
           data source=MAMMOTH-DB01-PRD\MAMMOTH;
           initial catalog=Mammoth;
           integrated security=True;
           MultipleActiveResultSets=True;
           App=EntityFramework&quot;"
         providerName="System.Data.EntityClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)" />
  </connectionStrings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="Off" xdt:Transform="Insert" />
  </system.web>
</configuration>
