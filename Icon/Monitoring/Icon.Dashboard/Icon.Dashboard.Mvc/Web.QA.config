<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="hostingEnvironment" value="QA" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="securityGroupsForReadOnly" value="IRMA.Applications,IRMA.Support" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="securityGroupsForEditing" value="IRMA.Developers" xdt:Locator="Match(key)" xdt:Transform="Replace" />
  </appSettings>
  <connectionStrings>    
    <add name="IconDatabaseContext" 
         connectionString="
          metadata=res://*/IconDatabaseModelForDashboard.csdl|res://*/IconDatabaseModelForDashboard.ssdl|res://*/IconDatabaseModelForDashboard.msl;
          provider=System.Data.SqlClient;
          provider connection string=&quot;
            data source=idq-iCon\SQLShared3Q;
            initial catalog=Icon;
            integrated security=True;
            MultipleActiveResultSets=True;
            App=EntityFramework&quot;" 
         providerName="System.Data.EntityClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)"/>
    <add name="MammothDatabaseContext"
         connectionString="
         metadata=res://*/MammothDatabaseModelForDashboard.csdl|res://*/MammothDatabaseModelForDashboard.ssdl|res://*/MammothDatabaseModelForDashboard.msl;
         provider=System.Data.SqlClient;
         provider connection string=&quot;
           data source=MAMMOTH-DB01-QA\MAMMOTH;
           initial catalog=Mammoth;
           integrated security=True;
           MultipleActiveResultSets=True;
           App=EntityFramework&quot;"
         providerName="System.Data.EntityClient"
         xdt:Transform="Replace"
         xdt:Locator="Match(name)" />
  </connectionStrings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="RemoteOnly" xdt:Transform="Insert" />    
  </system.web>
</configuration>