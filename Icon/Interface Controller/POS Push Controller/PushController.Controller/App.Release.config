  <appSettings>
    <!-- Regions to Process -->
    <add key="RegionsToProcess" value="SP,NC,MA,NE,MW,RM,SO,NA,SW,PN,FL" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <!-- email client configuration -->
    <add key="Sender" value="Icon@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="SendEmails" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <!-- email configuration for the Retail UOM changes. -->
    <add key="PriceUomChangeRecipients" value="WFMOnePOSRTIDeployment@wholefoods.com,Sean.Doran@wholefoods.com" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <!-- Infor Settings -->
    <add key="UseItemTypeInsteadOfNonMerchTrait" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform" xmlns="http://www.nlog-project.org/schemas/NLog.xsd">
    <targets xdt:Transform="Replace" async="true">
      <target name="dbLogIconPushController" xsi:type="Database" connectionString="data source=Icon-db-prd;initial catalog=iCon;Integrated Security=True;MultipleActiveResultSets=True" commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName) VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="6" />
        <parameter name="@UserName" layout="${windows-identity}" />
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@Level" layout="${level}" />
        <parameter name="@Source" layout="${logger}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@MachineName" layout="${machinename}" />
      </target>
      <target name="json-file" xsi:type="File" fileName="application-logs.json" archiveAboveSize="20971520" maxArchiveFiles="3">
        <layout xsi:type="JsonLayout">
          <attribute name="date" layout="${longdate}" />
          <attribute name="level" layout="${level:upperCase=true}" />
          <attribute name="message" layout="${message}" />
          <attribute name="exception" layout="${exception:format=ToString}}" />
        </layout>
      </target>
    </targets>
  </nlog>
