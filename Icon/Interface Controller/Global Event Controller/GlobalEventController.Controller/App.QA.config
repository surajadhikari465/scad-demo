              xsi:type="Database"
              connectionString="data source=idq-icon\SQLSHARED3Q;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True"
              commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName)
