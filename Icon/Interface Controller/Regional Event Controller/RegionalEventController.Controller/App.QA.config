    <targets xdt:Transform="Replace">
      <target name="dbLogIconRegionalController"
              xsi:type="Database"
              connectionString="data source=Icon-db-qa;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True"
              commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName)
