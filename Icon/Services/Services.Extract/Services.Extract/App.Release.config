  <esbConnections>
    <connections>
      <esbConnection xdt:Transform="Replace" xdt:Locator="Match(name)" name="Sb1EmsConnection" serverUrl="ssl://odrp1631.wfm.pvt:37233, ssl://odrp1632.wfm.pvt:37233, ssl://cerp1640.wfm.pvt:37233, ssl://cerp1641.wfm.pvt:37233" jmsUsername="sb1_auditUser" jmsPassword="{sb1_auditUser.Password}" jndiUsername="" jndiPassword="" sslPassword="esb" queueName="WFMSB1.SCAD.Audit.ExtractService.Queue.V1" sessionMode="AutoAcknowledge" certificateName="E=uday.bhaskar@wholefoods.com, CN=Uday Bhaskar, OU=InfraESBAdmins@wholefoods.com, O=Whole Foods Market, L=&quot;Austin &quot;, S=TX, C=US" certificateStoreName="Root" certificateStoreLocation="LocalMachine" targetHostName="odrp1631.wfm.pvt" reconnectDelay="" />
    </connections>
  </esbConnections>


  <S3Credentials>
    <add xdt:Transform="Replace" xdt:Locator="Match(profileName)" profileName="s3Icon" accessKey="${AccessKey.Production}" secretKey="${SecretKey.Production}" bucketRegion="https://s3.us-east-1.amazonaws.com" bucketName="14447-gpm-dev"  />
    <add xdt:Transform="Replace" xdt:Locator="Match(profileName)" profileName="s3DART" accessKey="${AccessKey.DART.Production}" secretKey="${SecretKey.DART.Production}" bucketRegion="https://s3.us-east-1.amazonaws.com" bucketName="14447-scmpa-dart-prd"  />
  </S3Credentials>

  <nlog>
    <targets xdt:Transform="Replace" >
      <target name="console" type="console" />
      <target name="dbLog" type="Database"
              connectionString="data source=Icon-db-prd;initial catalog=Icon;Integrated Security=True;MultipleActiveResultSets=True"
              commandText="INSERT INTO app.AppLog(AppId, UserName, LogDate, Level, Logger, Message, MachineName) VALUES (@AppId, @UserName, @LogDate, @Level, @Source, @Message, @MachineName);">
        <parameter name="@AppId" layout="21"/>
        <parameter name="@UserName" layout="${windows-identity}"/>
        <parameter name="@LogDate" layout="${date}"/>
        <parameter name="@Level" layout="${level}"/>
        <parameter name="@Source" layout="${logger}"/>
        <parameter name="@Message" layout="${message}"/>
        <parameter name="@MachineName" layout="${machinename}"/>
      </target>
      <target name="file" type="File"
              layout="${longdate} ${logger} ${message}${exception:format=ToString}"
              fileName="${basedir}/logs/logfile.txt"
              maxArchiveFiles="4"
              archiveAboveSize="1024000"
              archiveEvery="Day" />
    </targets>
  </nlog>

  </configuration>