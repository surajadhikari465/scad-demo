<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>Mammoth</name>
    <resourceType>ae.shared.JDBCSharedResource</resourceType>
    <config>
        <driver>com.microsoft.sqlserver.jdbc.SQLServerDriver</driver>
        <maxConnections>10</maxConnections>
        <loginTimeout>0</loginTimeout>
        <connectionType>JDBC</connectionType>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
        <location>%%MammothConnectionString%%</location>
        <user>%%Connections/Mammoth/UserName%%  </user>
        <password>%%Connections/Mammoth/Password%%</password>
    </config>
</BWSharedResource>