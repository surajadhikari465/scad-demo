<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://schemas.wfm.com/Mammoth/IrmaDbPrice.xsd" xmlns:ns3="http://services.wfm.com/Enterprise/DeadLetterException/V1" xmlns:ns0="http://www.tibco.com/schemas/ActivePriceQueuePublisher/SharedResources/Schemas/Application/Schema.xsd" xmlns:pfx="http://www.tibco.com/CommonLE/Client/Schema/ClientInterface.xsd">
    <pd:name>Processes/JobSchedulerService.process</pd:name>
    <pd:startName>Timer</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Timer">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>63</pd:x>
        <pd:y>103</pd:y>
        <config>
            <StartTime>1503935337000</StartTime>
            <Frequency>false</Frequency>
            <TimeInterval>%%PublisherTimerInMilliseconds%%</TimeInterval>
            <FrequencyIndex>Millisecond</FrequencyIndex>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>654</pd:endX>
    <pd:endY>254</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1503935270277</pd:targetNamespace>
    <pd:activity name="JDBC Query">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>181</pd:x>
        <pd:y>101</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/ICON.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT JobScheduleId ,
		JobName,
	  	 Region,
	 	 DestinationQueueName,
	 	 StartDateTimeUtc,
	  	 LastRunDateTimeUtc,
	  	 IntervalInSeconds,
	   	 Enabled,
	  	 XmlObject
	   FROM [app].[JobSchedule]
	   WHERE ISNULL( DATEDIFF( second, LastRunDateTimeUtc,SYSUTCDATETIME())
							 ,IntervalInSeconds) &gt;= IntervalInSeconds
            AND Enabled = 1
	   AND JobName = ?</statement>
            <wizardData/>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>JobScheduleId</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>JobName</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>Region</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>DestinationQueueName</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>StartDateTimeUtc</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>datetime2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>LastRunDateTimeUtc</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>datetime2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>IntervalInSeconds</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>Enabled</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-7</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>bit</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>XmlObject</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>JobName</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <JobName>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Application/JobName"/>
                </JobName>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Iterate">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>403</pd:x>
        <pd:y>21</pd:y>
        <pd:width>395</pd:width>
        <pd:height>158</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$JDBC-Query/resultSet/Record</pd:over>
            <pd:iterationElementSlot>JobSchedule</pd:iterationElementSlot>
            <pd:indexSlot>JobScheduleIndex</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>SendToPublisherQueue</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JDBC Update</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SendToPublisherQueue</pd:from>
            <pd:to>JDBC Update</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="SendToPublisherQueue">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>496</pd:x>
            <pd:y>102</pd:y>
            <config>
                <processName>/Processes/ActivePricePushQueue.process</processName>
            </config>
            <pd:inputBindings>
                <ns0:JobSchedule>
                    <ns0:JobName>
                        <xsl:value-of select="$JobSchedule/Record/JobName"/>
                    </ns0:JobName>
                    <ns0:Region>
                        <xsl:value-of select="$JobSchedule/Record/Region"/>
                    </ns0:Region>
                    <ns0:DestinationQueueName>
                        <xsl:value-of select="$JobSchedule/Record/DestinationQueueName"/>
                    </ns0:DestinationQueueName>
                    <ns0:StartDateTimeUtc>
                        <xsl:value-of select="$JobSchedule/Record/StartDateTimeUtc"/>
                    </ns0:StartDateTimeUtc>
                    <ns0:LastRunDateTimeUtc>
                        <xsl:value-of select="$JobSchedule/Record/LastRunDateTimeUtc"/>
                    </ns0:LastRunDateTimeUtc>
                    <ns0:IntervalInSeconds>
                        <xsl:value-of select="$JobSchedule/Record/IntervalInSeconds"/>
                    </ns0:IntervalInSeconds>
                    <ns0:Enabled>
                        <xsl:value-of select="$JobSchedule/Record/Enabled"/>
                    </ns0:Enabled>
                    <ns0:XmlObject>
                        <xsl:value-of select="$JobSchedule/Record/XmlObject"/>
                    </ns0:XmlObject>
                    <ns0:JobScheduleId>
                        <xsl:value-of select="$JobSchedule/Record/JobScheduleId"/>
                    </ns0:JobScheduleId>
                </ns0:JobSchedule>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="JDBC Update">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>662</pd:x>
            <pd:y>105</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>	UPDATE  [app].[JobSchedule]
	SET LastRunDateTimeUtc = SYSUTCDATETIME()
	WHERE JobScheduleID = ?</statement>
                <jdbcSharedConfig>/Connections/ICON.sharedjdbc</jdbcSharedConfig>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>JobScheduledID1</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <JobScheduledID1>
                        <xsl:value-of select="$JobSchedule/Record/JobScheduleId"/>
                    </JobScheduledID1>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>94</pd:x>
        <pd:y>249</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Dead Letter Queue Exception">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>392</pd:x>
        <pd:y>256</pd:y>
        <config>
            <processName>/Processes/Common/JMS/PublishOntoDeadLetterQ.process</processName>
        </config>
        <pd:inputBindings>
            <DeadLetterInput>
                <ns3:request>
                    <ns3:applicationID>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Application/ApplicationID"/>
                    </ns3:applicationID>
                    <ns3:transactionID>
                        <xsl:value-of select="&quot;ActivePriceQueuePublisher&quot;"/>
                    </ns3:transactionID>
                    <ns3:correlationID>
                        <xsl:value-of select="&quot;ActivePriceQueuePublisher&quot;"/>
                    </ns3:correlationID>
                    <ns3:failedSystemName>
                        <xsl:value-of select="&quot;ActivePriceQueuePublisher&quot;"/>
                    </ns3:failedSystemName>
                    <ns3:timeInterval>
                        <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/DLQ/TimeInterval"/>
                    </ns3:timeInterval>
                    <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                        <ns3:errorCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </ns3:errorCode>
                    </xsl:if>
                    <ns3:errorMessage>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns3:errorMessage>
                    <xsl:if test="$_error/ns1:ErrorReport/StackTrace">
                        <ns3:stackTrace>
                            <xsl:value-of select="$_error/ns1:ErrorReport/StackTrace"/>
                        </ns3:stackTrace>
                    </xsl:if>
                    <ns3:currentTimestamp>
                        <xsl:value-of select="current-dateTime()"/>
                    </ns3:currentTimestamp>
                    <ns3:exception>
                        <ns3:data>
                            <ns3:toRecipient>
                                <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Application/Notification/To"/>
                            </ns3:toRecipient>
                            <ns3:ccRecipient>
                                <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Application/Notification/cC"/>
                            </ns3:ccRecipient>
                        </ns3:data>
                    </ns3:exception>
                </ns3:request>
                <OtherProperties/>
            </DeadLetterInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Timer</pd:from>
        <pd:to>JDBC Query</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Query</pd:from>
        <pd:to>Iterate</pd:to>
        <pd:xpathDescription>count &gt; 0</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($JDBC-Query/resultSet/Record)&gt;0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Dead Letter Queue Exception</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Query</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Iterate</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Dead Letter Queue Exception</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>