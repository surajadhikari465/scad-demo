    <pd:activity name="Get Job Schedules">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>248</pd:x>
        <pd:y>99</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>1000</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>SELECT
	JobScheduleId ,
	JobName,
	Region,
	DestinationQueueName,
	StartDateTimeUtc,
	LastScheduledDateTimeUtc,
	LastRunEndDateTimeUtc,
	-- DatePart returns number of minutes off of UTC time. This is setup for Central time
	CASE 
		-- 5 hour difference when in Daylight Savings
		WHEN DATEPART(TZoffset, SYSDATETIMEOFFSET()) = -300 THEN DATEADD(hour, -1, NextScheduledDateTimeUtc)
		ELSE NextScheduledDateTimeUtc
	END AS NextScheduledDateTimeUtc,
	IntervalInSeconds,
	Enabled,
	Status,
	XmlObject
FROM [app].[JobSchedule]
WHERE StartDateTimeUtc &lt;=SYSUTCDATETIME()
	AND NextScheduledDateTimeUtc &lt;=SYSUTCDATETIME()
	AND Enabled = 1
	AND Status = 'ready'
	AND ISNULL(DestinationQueueName, '') &lt;&gt; ''
</statement>
            <wizardData/>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>JobScheduleId</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>JobName</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>Region</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>DestinationQueueName</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>StartDateTimeUtc</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>datetime2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>LastScheduledDateTimeUtc</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>datetime2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>LastRunEndDateTimeUtc</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>datetime2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>NextScheduledDateTimeUtc</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>datetime2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>IntervalInSeconds</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>Enabled</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-7</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>bit</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>Status</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>varchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>XmlObject</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <Prepared_Param_DataType/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput/>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Iterate">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>493</pd:x>
        <pd:y>18</pd:y>
        <pd:width>395</pd:width>
        <pd:height>158</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Get-Job-Schedules/resultSet/Record</pd:over>
            <pd:iterationElementSlot>JobSchedule</pd:iterationElementSlot>
            <pd:indexSlot>JobScheduleIndex</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Start Jobs</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Update Last Run DateTime</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Start Jobs</pd:from>
            <pd:to>Update Last Run DateTime</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Start Jobs">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>610</pd:x>
            <pd:y>106</pd:y>
            <config>
                <processName>/Processes/StartJob.process</processName>
            </config>
            <pd:inputBindings>
                <ns5:JobSchedule>
                    <ns5:JobName>
                        <xsl:value-of select="$JobSchedule/Record/JobName"/>
                    </ns5:JobName>
                    <ns5:Region>
                        <xsl:value-of select="$JobSchedule/Record/Region"/>
                    </ns5:Region>
                    <ns5:DestinationQueueName>
                        <xsl:value-of select="$JobSchedule/Record/DestinationQueueName"/>
                    </ns5:DestinationQueueName>
                    <ns5:StartDateTimeUtc>
                        <xsl:value-of select="$JobSchedule/Record/StartDateTimeUtc"/>
                    </ns5:StartDateTimeUtc>
                    <ns5:LastScheduledDateTimeUtc>
                        <xsl:value-of select="$JobSchedule/Record/LastScheduledDateTimeUtc"/>
                    </ns5:LastScheduledDateTimeUtc>
                    <ns5:LastRunEndDateTimeUtc>
                        <xsl:value-of select="$JobSchedule/Record/LastRunEndDateTimeUtc"/>
                    </ns5:LastRunEndDateTimeUtc>
                    <ns5:NextScheduledDateTimeUtc>
                        <xsl:value-of select="$JobSchedule/Record/NextScheduledDateTimeUtc"/>
                    </ns5:NextScheduledDateTimeUtc>
                    <ns5:IntervalInSeconds>
                        <xsl:value-of select="$JobSchedule/Record/IntervalInSeconds"/>
                    </ns5:IntervalInSeconds>
                    <ns5:Enabled>
                        <xsl:value-of select="$JobSchedule/Record/Enabled"/>
                    </ns5:Enabled>
                    <ns5:Status>
                        <xsl:value-of select="$JobSchedule/Record/Status"/>
                    </ns5:Status>
                    <ns5:XmlObject>
                        <xsl:value-of select="$JobSchedule/Record/XmlObject"/>
                    </ns5:XmlObject>
                    <ns5:JobScheduleId>
                        <xsl:value-of select="$JobSchedule/Record/JobScheduleId"/>
                    </ns5:JobScheduleId>
                </ns5:JobSchedule>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Update Last Run DateTime">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>752</pd:x>
            <pd:y>114</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>	UPDATE  [app].[JobSchedule]
	SET LastScheduledDateTimeUtc = NextScheduledDateTimeUtc
		,NextScheduledDateTimeUtc = DATEADD(ss, CEILING(CAST(DATEDIFF(ss, NextScheduledDateTimeUtc, SYSUTCDATETIME()) as FLOAT) / IntervalInSeconds) * IntervalInSeconds, NextScheduledDateTimeUtc)
	WHERE JobScheduleID = ?</statement>
                <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>JobScheduledID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <JobScheduledID>
                        <xsl:value-of select="$JobSchedule/Record/JobScheduleId"/>
                    </JobScheduledID>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>62</pd:x>
        <pd:y>252</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Send Error Message">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>413</pd:x>
        <pd:y>255</pd:y>
        <config>
            <processName>/SharedResources/Processes/PublishErrorMessage.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns4:ErrorMessage>
                <ns4:Application>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns4:Application>
                <ns4:MessageID>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns4:MessageID>
                <ns4:MessageProperties>
                    <ns4:NameValuePair>
                        <xsl:attribute name="name">
                            <xsl:value-of select="&quot;Run Time&quot;"/>
                        </xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="current-dateTime()"/>
                        </xsl:attribute>
                    </ns4:NameValuePair>
                </ns4:MessageProperties>
                <ns4:Message>
                    <xsl:value-of select="$JobSchedule/Record/JobName"/>
                </ns4:Message>
                <ns4:ErrorCode>
                    <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                </ns4:ErrorCode>
                <ns4:ErrorDetails>
                    <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                </ns4:ErrorDetails>
                <xsl:choose>
                    <xsl:when test="contains($_error/ns1:ErrorReport/Msg, &quot;Connection is closed&quot;)">
                        <ns4:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns4:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns1:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Invalid object name&quot;)">
                        <ns4:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns4:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns1:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Cannot open database&quot;)">
                        <ns4:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns4:ErrorSeverity>
                    </xsl:when>
                    <xsl:otherwise>
                        <ns4:ErrorSeverity>
                            <xsl:value-of select="&quot;Error&quot;"/>
                        </ns4:ErrorSeverity>
                    </xsl:otherwise>
                </xsl:choose>
            </ns4:ErrorMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>236</pd:x>
        <pd:y>253</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <pfx2:AppLog>
                <pfx2:AppId>
                    <xsl:value-of select="$_globalVariables/ns2:GlobalVariables/Application/ApplicationID"/>
                </pfx2:AppId>
                <pfx2:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </pfx2:Level>
                <pfx2:Logger>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </pfx2:Logger>
                <pfx2:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </pfx2:LogDate>
                <pfx2:Message>
                    <xsl:value-of select="&quot;Error occurred in JobScheduler&quot;"/>
                </pfx2:Message>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <pfx2:Exception>
                        <xsl:value-of select="concat($_error/ns1:ErrorReport/MsgCode, $_error/ns1:ErrorReport/Msg, $_error/ns1:ErrorReport/StackTrace)"/>
                    </pfx2:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/StackTrace">
                    <pfx2:StackTrace>
                        <xsl:value-of select="$_error/ns1:ErrorReport/StackTrace"/>
                    </pfx2:StackTrace>
                </xsl:if>
            </pfx2:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Timer</pd:from>
        <pd:to>Get Job Schedules</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Job Schedules</pd:from>
        <pd:to>Iterate</pd:to>
        <pd:xpathDescription>count &gt; 0</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($Get-Job-Schedules/resultSet/Record)&gt;0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Send Error Message</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Job Schedules</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Iterate</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>Send Error Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>