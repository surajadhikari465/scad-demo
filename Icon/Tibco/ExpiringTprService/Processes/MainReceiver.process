<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns12="http://schemas.wfm.com/Mammoth/ErrorMessage.xsd" xmlns:pfx="http://schemas.wfm.com/Mammoth/JobSchedule.xsd">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/JobSchedule.xsd" schemaLocation="/SharedResources/Schemas/Job Scheduler/JobSchedule.xsd"/>
    <pd:name>Processes/MainReceiver.process</pd:name>
    <pd:startName>Inactive Tpr Price Starter Queue Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Inactive Tpr Price Starter Queue Receiver">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>81</pd:x>
        <pd:y>64</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>23</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/InactiveTprPriceStarterQueue%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/SharedResources/Connections/SB1.sharedjmscon</ConnectionReference>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>780</pd:endX>
    <pd:endY>147</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1515621798527</pd:targetNamespace>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>194</pd:x>
        <pd:y>50</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx:JobSchedule"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Inactive-Tpr-Price-Starter-Queue-Receiver/ns:ActivityOutput/Body"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Call Inactive Tpr Service">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>513</pd:x>
        <pd:y>60</pd:y>
        <config>
            <processName>/Processes/RunInactiveTprService.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Parse-XML/pfx:JobSchedule"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log Start">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>374</pd:x>
        <pd:y>53</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns2:AppLog>
                <ns2:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns2:AppId>
                <ns2:Level>
                    <xsl:value-of select="&quot;Info&quot;"/>
                </ns2:Level>
                <ns2:Logger>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns3:ProcessContext/ProcessId)"/>
                </ns2:Logger>
                <ns2:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns2:LogDate>
                <ns2:Message>
                    <xsl:value-of select="concat(&quot;Region: &quot;, $Parse-XML/pfx:JobSchedule/pfx:Region, &quot;. Starting Expiring TPR service.&quot;)"/>
                </ns2:Message>
            </ns2:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>375</pd:x>
        <pd:y>148</pd:y>
        <config>
            <ConfirmEvent>Inactive Tpr Price Starter Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="LogNoExecution">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>524</pd:x>
        <pd:y>148</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns2:AppLog>
                <ns2:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns2:AppId>
                <ns2:Level>
                    <xsl:value-of select="&quot;Info&quot;"/>
                </ns2:Level>
                <ns2:Logger>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns3:ProcessContext/ProcessId)"/>
                </ns2:Logger>
                <ns2:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns2:LogDate>
                <ns2:Message>
                    <xsl:value-of select="concat(&quot;Region:&quot;, $Parse-XML/pfx:JobSchedule/pfx:Region, &quot;. Tpr Service Already running So Service will not run.&quot;)"/>
                </ns2:Message>
            </ns2:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm-1">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>731</pd:x>
        <pd:y>59</pd:y>
        <config>
            <ConfirmEvent>Inactive Tpr Price Starter Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>64</pd:x>
        <pd:y>249</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>216</pd:x>
        <pd:y>248</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns2:AppLog>
                <ns2:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns2:AppId>
                <ns2:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </ns2:Level>
                <ns2:Logger>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns3:ProcessContext/ProcessId)"/>
                </ns2:Logger>
                <ns2:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns2:MachineName>
                <ns2:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns2:LogDate>
                <ns2:Message>
                    <xsl:value-of select="concat('Region: ',$Parse-XML/pfx:JobSchedule/pfx:Region, '. Expiring TPR Service error occurred: ', $_error/ns3:ErrorReport/Msg)"/>
                </ns2:Message>
                <xsl:if test="$_error/ns3:ErrorReport/MsgCode">
                    <ns2:Exception>
                        <xsl:value-of select="$_error/ns3:ErrorReport/MsgCode"/>
                    </ns2:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns3:ErrorReport/StackTrace">
                    <ns2:StackTrace>
                        <xsl:value-of select="$_error/ns3:ErrorReport/StackTrace"/>
                    </ns2:StackTrace>
                </xsl:if>
            </ns2:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm-Error">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>518</pd:x>
        <pd:y>248</pd:y>
        <config>
            <ConfirmEvent>Inactive Tpr Price Starter Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Publish Error Message">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>384</pd:x>
        <pd:y>257</pd:y>
        <config>
            <processName>/SharedResources/Processes/PublishErrorMessage.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns12:ErrorMessage>
                <ns12:Application>
                    <xsl:value-of select="$_processContext/ns3:ProcessContext/ProjectName"/>
                </ns12:Application>
                <ns12:MessageID>
                    <xsl:value-of select="$Parse-XML/pfx:JobSchedule/pfx:JobScheduleId"/>
                </ns12:MessageID>
                <ns12:MessageProperties>
                    <ns12:NameValuePair>
                        <xsl:attribute name="name">
                            <xsl:value-of select="&quot;Region&quot;"/>
                        </xsl:attribute>
                        <xsl:attribute name="value">
                            <xsl:value-of select="$Parse-XML/pfx:JobSchedule/pfx:Region"/>
                        </xsl:attribute>
                    </ns12:NameValuePair>
                </ns12:MessageProperties>
                <ns12:Message>
                    <xsl:value-of select="$Inactive-Tpr-Price-Starter-Queue-Receiver/ns:ActivityOutput/Body"/>
                </ns12:Message>
                <ns12:ErrorCode>
                    <xsl:value-of select="$_error/ns3:ErrorReport/MsgCode"/>
                </ns12:ErrorCode>
                <ns12:ErrorDetails>
                    <xsl:value-of select="$_error/ns3:ErrorReport/Msg"/>
                </ns12:ErrorDetails>
                <xsl:choose xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
                    <xsl:when test="contains($_error/ns3:ErrorReport/Msg, &quot;Connection is closed&quot;)">
                        <ns12:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns12:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns3:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Invalid object name&quot;)">
                        <ns12:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns12:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns3:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Cannot open database&quot;)">
                        <ns12:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns12:ErrorSeverity>
                    </xsl:when>
                    <xsl:otherwise>
                        <ns12:ErrorSeverity>
                            <xsl:value-of select="&quot;Error&quot;"/>
                        </ns12:ErrorSeverity>
                    </xsl:otherwise>
                </xsl:choose>
            </ns12:ErrorMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Update-Job-Status-1">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>642</pd:x>
        <pd:y>248</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
            <statement>update [app].[JobSchedule]
set status = 'ready',
      LastRunEndDateTimeUtc = GETUTCDATE()
where JobScheduleId  = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>JobScheduleId</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <JobScheduleId>
                    <xsl:value-of select="$Parse-XML/pfx:JobSchedule/pfx:JobScheduleId"/>
                </JobScheduleId>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log End">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>633</pd:x>
        <pd:y>54</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns2:AppLog>
                <ns2:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns2:AppId>
                <ns2:Level>
                    <xsl:value-of select="'Info'"/>
                </ns2:Level>
                <ns2:Logger>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns3:ProcessContext/ProcessId)"/>
                </ns2:Logger>
                <ns2:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns2:LogDate>
                <ns2:Message>
                    <xsl:value-of select="concat(&quot;Region: &quot;, $Parse-XML/pfx:JobSchedule/pfx:Region, &quot;. Ending Expiring TPR service successfully.&quot;)"/>
                </ns2:Message>
            </ns2:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Inactive Tpr Price Starter Queue Receiver</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Log Start</pd:to>
        <pd:xpathDescription>Status=Ready</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Start</pd:from>
        <pd:to>Call Inactive Tpr Service</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Confirm</pd:to>
        <pd:xpathDescription>Status=Running</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-10092289</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Parse-XML/pfx:JobSchedule/pfx:Status="running"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm</pd:from>
        <pd:to>LogNoExecution</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogNoExecution</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Call Inactive Tpr Service</pd:from>
        <pd:to>Log End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>Publish Error Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Publish Error Message</pd:from>
        <pd:to>Confirm-Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm-Error</pd:from>
        <pd:to>Update-Job-Status-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Update-Job-Status-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log End</pd:from>
        <pd:to>Confirm-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>