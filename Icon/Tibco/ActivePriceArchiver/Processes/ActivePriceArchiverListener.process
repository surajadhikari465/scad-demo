<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns5="http://schemas.wfm.com/Mammoth/ErrorMessage.xsd" xmlns:ns6="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:ns7="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://www.tibco.com/CommonLE/Client/Schema/ClientInterface.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:ns1="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd" xmlns:ns4="http://services.wfm.com/Enterprise/DeadLetterException/V1" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx2="http://schemas.wfm.com/Mammoth/MammothPrices.xsd">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd" schemaLocation="/SharedResources/Schemas/Application/MammothDbPrice.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" schemaLocation="/SharedResources/Schemas/Mammoth/MammothPrices.xsd"/>
    <pd:name>Processes/ActivePriceArchiverListener.process</pd:name>
    <pd:startName>Archive Active Price Reciever</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Archive Active Price Reciever">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>135</pd:x>
        <pd:y>97</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>23</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/ArchiveJustInTimePriceQueue%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/SharedResources/Connections/SB1.sharedjmscon</ConnectionReference>
            <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>884</pd:endX>
    <pd:endY>97</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1502126172637</pd:targetNamespace>
    <pd:activity name="ArchivePrices">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>387</pd:x>
        <pd:y>91</pd:y>
        <config>
            <processName>/Processes/ArchivePrices.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$ParseActivePriceXML/pfx2:MammothPrices"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>213</pd:x>
        <pd:y>246</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>387</pd:x>
        <pd:y>248</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
        </config>
        <pd:inputBindings>
            <ns6:AppLog>
                <ns6:AppId>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Application/ApplicationID"/>
                </ns6:AppId>
                <ns6:Level>
                    <xsl:value-of select="'Error'"/>
                </ns6:Level>
                <ns6:Logger>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProjectName"/>
                </ns6:Logger>
                <ns6:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns6:LogDate>
                <ns6:Message>
                    <xsl:value-of select="concat('{ArchivePriceError:{MessageID:',$Archive-Active-Price-Reciever/ns:ActivityOutput/OtherProperties/TransactionID,', MsgCode:',$_error/ns2:ErrorReport/MsgCode, ',Msg:',$_error/ns2:ErrorReport/Msg, ', StackTrace:',$_error/ns2:ErrorReport/StackTrace,'}}')"/>
                </ns6:Message>
            </ns6:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Publish Error Message">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>567</pd:x>
        <pd:y>253</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/SharedResources/Processes/PublishErrorMessage.process</processName>
        </config>
        <pd:inputBindings>
            <ns5:ErrorMessage>
                <ns5:Application>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProjectName"/>
                </ns5:Application>
                <ns5:MessageID>
                    <xsl:value-of select="$Archive-Active-Price-Reciever/ns:ActivityOutput/OtherProperties/TransactionID"/>
                </ns5:MessageID>
                <ns5:MessageProperties>
                    <xsl:for-each select="$Archive-Active-Price-Reciever/ns:ActivityOutput/OtherProperties/node()">
                        <ns5:NameValuePair>
                            <xsl:attribute name="name">
                                <xsl:value-of select="local-name(current())"/>
                            </xsl:attribute>
                            <xsl:attribute name="value">
                                <xsl:value-of select="current()"/>
                            </xsl:attribute>
                        </ns5:NameValuePair>
                    </xsl:for-each>
                </ns5:MessageProperties>
                <ns5:Message>
                    <xsl:value-of select="$Archive-Active-Price-Reciever/ns:ActivityOutput/Body"/>
                </ns5:Message>
                <ns5:ErrorCode>
                    <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                </ns5:ErrorCode>
                <ns5:ErrorDetails>
                    <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                </ns5:ErrorDetails>
                <xsl:choose>
                    <xsl:when test="contains($_error/ns2:ErrorReport/Msg, &quot;Connection is closed&quot;)">
                        <ns5:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns5:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns2:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Invalid object name&quot;)">
                        <ns5:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns5:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns2:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Cannot open database&quot;)">
                        <ns5:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </ns5:ErrorSeverity>
                    </xsl:when>
                    <xsl:otherwise>
                        <ns5:ErrorSeverity>
                            <xsl:value-of select="'Error'"/>
                        </ns5:ErrorSeverity>
                    </xsl:otherwise>
                </xsl:choose>
            </ns5:ErrorMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ConfirmFailure">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>747</pd:x>
        <pd:y>247</pd:y>
        <config>
            <ConfirmEvent>Archive Active Price Reciever</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="ConfirmSuccess">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>747</pd:x>
        <pd:y>96</pd:y>
        <config>
            <ConfirmEvent>Archive Active Price Reciever</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="ParseActivePriceXML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>259</pd:x>
        <pd:y>90</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx2:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Archive-Active-Price-Reciever/ns:ActivityOutput/Body"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log Message">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>617</pd:x>
        <pd:y>96</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
        </config>
        <pd:inputBindings>
            <ns6:AppLog>
                <ns6:AppId>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Application/ApplicationID"/>
                </ns6:AppId>
                <ns6:Level>
                    <xsl:value-of select="'Info'"/>
                </ns6:Level>
                <ns6:Logger>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProjectName"/>
                </ns6:Logger>
                <ns6:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns6:LogDate>
                <ns6:Message>
                    <xsl:value-of select="concat('{Archived Prices:{TransactionID:', $Archive-Active-Price-Reciever/ns:ActivityOutput/OtherProperties/TransactionID, ', Prices:', tib:concat-sequence($Map-Log-Data/ArchivedPrices/ArchivedPrice), '}')"/>
                </ns6:Message>
            </ns6:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Log Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>496</pd:x>
        <pd:y>93</pd:y>
        <config>
            <element>
                <xsd:element name="ArchivedPrices">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="ArchivedPrice" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <ArchivedPrices>
                <xsl:for-each select="$ParseActivePriceXML/pfx2:MammothPrices/pfx2:MammothPrice">
                    <ArchivedPrice>
                        <xsl:value-of select="concat('{Region:', current()/pfx2:Region, ', PriceID:', current()/pfx2:PriceID, '}')"/>
                    </ArchivedPrice>
                </xsl:for-each>
            </ArchivedPrices>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>ArchivePrices</pd:from>
        <pd:to>Map Log Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>Publish Error Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ConfirmSuccess</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Publish Error Message</pd:from>
        <pd:to>ConfirmFailure</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ConfirmFailure</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Archive Active Price Reciever</pd:from>
        <pd:to>ParseActivePriceXML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ParseActivePriceXML</pd:from>
        <pd:to>ArchivePrices</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Log Data</pd:from>
        <pd:to>Log Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Message</pd:from>
        <pd:to>ConfirmSuccess</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>