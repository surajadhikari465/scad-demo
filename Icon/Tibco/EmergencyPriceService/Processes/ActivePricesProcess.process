<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://www.tibco.com/schemas/MammothActivePriceService/SharedResources/Schemas/Schema.xsd" xmlns:WFM.UID="java://UID" xmlns:pfx2="http://www.tibco.com/schemas/ActivePriceQueuePublisher/SharedResources/Schemas/Application/Schema.xsd" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pfx3="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" xmlns:pfx4="http://schemas.wfm.com/Mammoth/JobSchedule.xsd" xmlns:pfx5="http://schemas.wfm.com/Mammoth/ErrorMessage.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:ns12="http://www.tibco.com/namespaces/tnt/plugins/jms">
    <xsd:import namespace="http://www.tibco.com/schemas/ActivePriceQueuePublisher/SharedResources/Schemas/Application/Schema.xsd" schemaLocation="/SharedResources/Schemas/JobSchedule.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/JobSchedule.xsd" schemaLocation="/SharedResources/Schemas/Job Scheduler/JobSchedule.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" schemaLocation="/SharedResources/Schemas/Mammoth/MammothPrices.xsd"/>
    <pd:name>Processes/ActivePricesProcess.process</pd:name>
    <pd:startName>Timer</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Timer">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>47</pd:x>
        <pd:y>47</pd:y>
        <config>
            <StartTime>%%Application/StartTime%%</StartTime>
            <Frequency>false</Frequency>
            <TimeInterval>%%Application/TimeInterval%%</TimeInterval>
            <FrequencyIndex>Millisecond</FrequencyIndex>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>1128</pd:endX>
    <pd:endY>450</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <EmergencyPrices>
            <xsd:element name="DoExist" type="xsd:boolean"/>
        </EmergencyPrices>
        <SendErrorCount>
            <xsd:element name="Count" type="xsd:int"/>
        </SendErrorCount>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1499373839783</pd:targetNamespace>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>232</pd:x>
        <pd:y>454</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns3:AppLog>
                <ns3:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns3:AppId>
                <ns3:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </ns3:Level>
                <ns3:Logger>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns4:ProcessContext/ProcessId)"/>
                </ns3:Logger>
                <ns3:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns3:MachineName>
                <ns3:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns3:LogDate>
                <ns3:Message>
                    <xsl:value-of select="'Error occurred in the Mammoth Emergency Price Service'"/>
                </ns3:Message>
                <ns3:Exception>
                    <xsl:value-of select="$_error/ns4:ErrorReport/FullClass"/>
                </ns3:Exception>
                <ns3:StackTrace>
                    <xsl:value-of select="$_error/ns4:ErrorReport/StackTrace"/>
                </ns3:StackTrace>
            </ns3:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="CatchAll">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>53</pd:x>
        <pd:y>451</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log Start">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>134</pd:x>
        <pd:y>51</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns3:AppLog>
                <ns3:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns3:AppId>
                <ns3:Level>
                    <xsl:value-of select="&quot;Info&quot;"/>
                </ns3:Level>
                <ns3:Logger>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                </ns3:Logger>
                <ns3:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns3:LogDate>
                <ns3:Message>
                    <xsl:value-of select="'Starting Mammoth Emergency Price Service'"/>
                </ns3:Message>
            </ns3:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log End">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>1127</pd:x>
        <pd:y>218</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
        </config>
        <pd:inputBindings>
            <ns3:AppLog>
                <ns3:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns3:AppId>
                <ns3:Level>
                    <xsl:value-of select="'Info'"/>
                </ns3:Level>
                <ns3:Logger>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                </ns3:Logger>
                <ns3:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns3:LogDate>
                <ns3:Message>
                    <xsl:value-of select="'Ending Mammoth Emergency Price Service successfully.'"/>
                </ns3:Message>
            </ns3:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>352</pd:x>
        <pd:y>67</pd:y>
        <pd:width>708</pd:width>
        <pd:height>318</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>while</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:activityOutputName/>
            <pd:whileCondition>$EmergencyPrices/DoExist = "true"</pd:whileCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Get Emergency Prices</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Assign Emergency Price Exist as False</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Assign Emergency Prices Exist 2</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Get Emergency Prices</pd:from>
            <pd:to>Send Emergency Prices</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send Emergency Prices</pd:from>
            <pd:to>Clear out Send Error Count</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Check If Emergency Prices Exist In Loop</pd:from>
            <pd:to>Assign Emergency Prices Exist 2</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send Emergency Prices</pd:from>
            <pd:to>Increment Send Error Count</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-52429</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Insert Prices into Emergency Queue</pd:from>
            <pd:to>Check If Emergency Prices Exist In Loop</pd:to>
            <pd:xpathDescription>If Send Error Count Is Less Than Retry</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$SendErrorCount/Count &lt; $_globalVariables/ns1:GlobalVariables/Application/MaxSendToEsbRetryCount</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Increment Send Error Count</pd:from>
            <pd:to>Log Send Emergency Price Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Clear out Send Error Count</pd:from>
            <pd:to>Check If Emergency Prices Exist In Loop</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Insert Prices into Emergency Queue</pd:from>
            <pd:to>Log Send Emergency Prices Exceeded Max Retry</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log Send Emergency Price Error</pd:from>
            <pd:to>Insert Prices into Emergency Queue</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log Send Emergency Prices Exceeded Max Retry</pd:from>
            <pd:to>Assign Emergency Price Exist as False</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Assign Emergency Prices Exist 2">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>947</pd:x>
            <pd:y>158</pd:y>
            <config>
                <variableName>EmergencyPrices</variableName>
            </config>
            <pd:inputBindings>
                <DoExist>
                    <xsl:value-of select="$Check-If-Emergency-Prices-Exist-In-Loop/EmergencyPricesExist"/>
                </DoExist>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Get Emergency Prices">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>449</pd:x>
            <pd:y>152</pd:y>
            <config>
                <processName>/Processes/GetEmergencyPrices.process</processName>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Send Emergency Prices">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>569</pd:x>
            <pd:y>151</pd:y>
            <config>
                <processName>/Processes/SendEmergencyPrices.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Get-Emergency-Prices/pfx3:MammothPrices"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Check If Emergency Prices Exist In Loop">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>807</pd:x>
            <pd:y>163</pd:y>
            <config>
                <processName>/Processes/CheckIfEmergencyPricesExist.process</processName>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Insert Prices into Emergency Queue">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>696</pd:x>
            <pd:y>298</pd:y>
            <config>
                <processName>/Processes/InsertPricesIntoEmergencyQueue.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Get-Emergency-Prices/pfx3:MammothPrices"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Increment Send Error Count">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>456</pd:x>
            <pd:y>300</pd:y>
            <config>
                <variableName>SendErrorCount</variableName>
            </config>
            <pd:inputBindings>
                <xsl:choose>
                    <xsl:when test="$SendErrorCount/Count > 0">
                        <Count>
                            <xsl:value-of select="$SendErrorCount/Count + 1"/>
                        </Count>
                    </xsl:when>
                    <xsl:otherwise>
                        <Count>
                            <xsl:value-of select="1"/>
                        </Count>
                    </xsl:otherwise>
                </xsl:choose>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Clear out Send Error Count">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>675</pd:x>
            <pd:y>154</pd:y>
            <config>
                <variableName>SendErrorCount</variableName>
            </config>
            <pd:inputBindings>
                <Count>
                    <xsl:value-of select="0"/>
                </Count>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Log Send Emergency Price Error">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>571</pd:x>
            <pd:y>300</pd:y>
            <config>
                <spawn>true</spawn>
                <processName>/SharedResources/Processes/LogPublisher.process</processName>
            </config>
            <pd:inputBindings>
                <ns3:AppLog>
                    <ns3:AppId>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                    </ns3:AppId>
                    <ns3:Level>
                        <xsl:value-of select="'Error'"/>
                    </ns3:Level>
                    <ns3:Logger>
                        <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                    </ns3:Logger>
                    <ns3:LogDate>
                        <xsl:value-of select="current-dateTime()"/>
                    </ns3:LogDate>
                    <ns3:Message>
                        <xsl:value-of select="'Error occurred when attempting to send prices to ESB. Will attempt to requeue emergency prices.'"/>
                    </ns3:Message>
                    <xsl:if test="$_error/ns4:ErrorReport/MsgCode">
                        <ns3:Exception>
                            <xsl:value-of select="$_error/ns4:ErrorReport/MsgCode"/>
                        </ns3:Exception>
                    </xsl:if>
                    <ns3:StackTrace>
                        <xsl:value-of select="$_error/ns4:ErrorReport/ProcessStack"/>
                    </ns3:StackTrace>
                </ns3:AppLog>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Log Send Emergency Prices Exceeded Max Retry">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>827</pd:x>
            <pd:y>307</pd:y>
            <config>
                <spawn>true</spawn>
                <processName>/SharedResources/Processes/LogPublisher.process</processName>
            </config>
            <pd:inputBindings>
                <ns3:AppLog>
                    <ns3:AppId>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                    </ns3:AppId>
                    <ns3:Level>
                        <xsl:value-of select="'Error'"/>
                    </ns3:Level>
                    <ns3:Logger>
                        <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                    </ns3:Logger>
                    <ns3:LogDate>
                        <xsl:value-of select="current-dateTime()"/>
                    </ns3:LogDate>
                    <ns3:Message>
                        <xsl:value-of select="'Exceeded max number of send errors. Will retry sending emergency prices on next run.'"/>
                    </ns3:Message>
                </ns3:AppLog>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Assign Emergency Price Exist as False">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>967</pd:x>
            <pd:y>306</pd:y>
            <config>
                <variableName>EmergencyPrices</variableName>
            </config>
            <pd:inputBindings>
                <DoExist>
                    <xsl:value-of select="0"/>
                </DoExist>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Assign Emergency Prices Exist">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>257</pd:x>
        <pd:y>224</pd:y>
        <config>
            <variableName>EmergencyPrices</variableName>
        </config>
        <pd:inputBindings>
            <DoExist>
                <xsl:value-of select="$Check-If-Emergency-Prices-Exist/EmergencyPricesExist"/>
            </DoExist>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Check If Emergency Prices Exist">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>256</pd:x>
        <pd:y>60</pd:y>
        <config>
            <processName>/Processes/CheckIfEmergencyPricesExist.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>CatchAll</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign Emergency Prices Exist</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log End</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Log End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Start</pd:from>
        <pd:to>Check If Emergency Prices Exist</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Check If Emergency Prices Exist</pd:from>
        <pd:to>Assign Emergency Prices Exist</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Timer</pd:from>
        <pd:to>Log Start</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>