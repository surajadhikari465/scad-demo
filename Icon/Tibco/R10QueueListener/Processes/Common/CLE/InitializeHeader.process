<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx5="http://www.tibco.com/Customer/CommonLE/namespace/Public/Common/CustomerRequestHeader.xsd" xmlns:pfx4="http://www.macys.com/CommonLE/namespace/Public/Common/RequestHeader.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.realmed.com/CommonLE/namespace/Public/Common/RequestHeader.xsd" xmlns:pfx2="http://www.nscorp.com/CommonLE/namespace/Public/Common/RequestHeader.xsd" xmlns:pfx="http://www.tibco.com/CommonLE2/namespace/Public/Common/RequestHeader.xsd" xmlns:pfx6="http://www.tibco.com/CommonLE/Client/Schema/ClientInterface.xsd">
    <xsd:import namespace="http://www.tibco.com/Customer/CommonLE/namespace/Public/Common/CustomerRequestHeader.xsd" schemaLocation="/SharedResources/Schemas/CLE2007/Common/CustomerRequestHeader.xsd"/>
    <xsd:import namespace="http://www.tibco.com/CommonLE2/namespace/Public/Common/RequestHeader.xsd" schemaLocation="/SharedResources/Schemas/Public/Common/RequestHeader.xsd"/>
    <xsd:import namespace="http://www.tibco.com/CommonLE/Client/Schema/ClientInterface.xsd" schemaLocation="/SharedResources/Schemas/CLE/ClientInterface.xsd"/>
    <pd:name>Processes/Common/CLE/InitializeHeader.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx6:Header"/>
    <pd:startX>73</pd:startX>
    <pd:startY>63</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>331</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1222435153953</pd:targetNamespace>
    <pd:activity name="Set Default Header">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>191</pd:x>
        <pd:y>62</pd:y>
        <config>
            <variableConfig>/SharedResources/Variables/Header.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Start/pfx6:Header"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Set Default Header</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Set Default Header</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:label>
        <pd:name>Label</pd:name>
        <pd:description>This process sets the default values for the header information used in both 
Log and Exception Requests. If you want the default values to be used in the 
header, you can leave the header values empty when you call SendLog and
SendException API. If you specify the header values in the requests, the 
default header values are overwritten. 

Your application should call this process during startup if you want the
default header values to be used throughout your application lifecycle.</pd:description>
        <pd:x>230</pd:x>
        <pd:y>213</pd:y>
        <pd:width>433</pd:width>
        <pd:height>145</pd:height>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-16724992</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
</pd:ProcessDefinition>