    <pd:activity name="GetMammothPrice">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>320</pd:x>
        <pd:y>59</pd:y>
        <config>
            <timeout>120</timeout>
            <commit>false</commit>
            <maxRows>1000</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT top 200
	PriceID, 
	CAST(p.Region AS NVARCHAR(2)) AS Region, 
	CAST(GpmID AS NVARCHAR(255)) AS GpmID, 
	p.ItemID + 1 AS ItemID, 
	p.BusinessUnitID, 
	StartDate, 
	NULL as EndDate, 
	Price, 
	PriceType, 
	PriceTypeAttribute, 
	SellableUOM, 
	CurrencyCode, 
	Multiple, 
	NewTagExpiration, 
	InsertDateUtc, 
	ModifiedDateUtc,
	it.itemTypeCode as itemTypeCode,
	Items.ScanCode as ScanCode,
	lc.StoreName as StoreName
FROM gpm.Price_FL p
INNER JOIN Items on Items.ItemID = p.ItemID
INNER JOIN ItemTypes it on Items.ItemTypeID = it.itemTypeID
INNER JOIN Locale lc on p.BusinessUnitID = lc.BusinessUnitID
WHERE it.itemTypeCode NOT IN ( 'CPN', 'NRT')
	AND PriceID = 16376
	AND NOT EXISTS
	(
		SELECT 1 
		FROM gpm.ActivePriceSentArchive a
		WHERE a.PriceID = p.PriceID
	)
	ORDER BY p.BusinessUnitID desc</statement>
            <processInSubsets>true</processInSubsets>
            <wizardData/>
            <oraObjects/>
            <oraTables/>
