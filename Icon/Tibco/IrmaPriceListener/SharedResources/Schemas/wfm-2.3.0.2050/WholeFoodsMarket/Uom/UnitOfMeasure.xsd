	<!-- ================== -->
	<xs:import namespace="http://schemas.wfm.com/Enterprise/RetailMgmt/CommonRefTypes/V1" schemaLocation="../Common/CommonRefTypes.xsd"/>
	<xs:import namespace="http://schemas.wfm.com/Enterprise/UnitOfMeasureMgmt/CommonRefType/V2" schemaLocation="CommonRefTypes.xsd"/>
	<!-- ================== -->
	<!-- Global Elements             -->
	<!-- ================== -->
	<xs:element name="uom" type="UomType"/>
	<xs:complexType name="UomType">
		<xs:sequence>
			<xs:element name="code" type="urf:WfmUomCodeEnumType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>abbreviated unit of measure</xs:documentation>
