    <pd:activity name="Data Issue Query">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>874</pd:x>
        <pd:y>281</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>1000</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>SELECT 
   CASE 
		WHEN vsc.inforitemId IS NULL
			THEN + 'The item ' + CAST(mp.ItemId as VARCHAR) + ' does not exist or is deleted or is not marked as validated in IRMA ** BusinessUnit_ID: ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' | ItemId: ' + CAST(mp.ItemId as VARCHAR) + ' | PriceType: ' + CAST(mp.PriceType as VARCHAR) + ' | StartDate: ' + CAST(CAST(mp.StartDate as DATE) as VARCHAR) + '**;   '
		WHEN s.BusinessUnit_ID IS NULL
			THEN 'The store ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' dose not exist in IRMA ** BusinessUnit_ID: ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' | ItemId: ' + CAST(mp.ItemId as VARCHAR) + ' | PriceType: ' + CAST(mp.PriceType as VARCHAR) + ' | StartDate: ' + CAST(CAST(mp.StartDate as DATE) as VARCHAR) + '**;   '
		WHEN iu.Unit_Abbreviation IS NULL
			THEN  'The unit ' + mp.SellableUOM + ' dose not exist in IRMA ** BusinessUnit_ID: ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' | ItemId: ' + CAST(mp.ItemId as VARCHAR) + ' | PriceType: ' + CAST(mp.PriceType as VARCHAR) + ' | StartDate: ' + CAST(CAST(mp.StartDate as DATE) as VARCHAR) + '**;   '
		WHEN ii.Default_Identifier = 0
			THEN  'The item ' + ii.Identifier + ' is not a default identifier in IRMA ** BusinessUnit_ID: ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' | ItemId: ' + CAST(mp.ItemId as VARCHAR) + ' | PriceType: ' + CAST(mp.PriceType as VARCHAR) + ' | StartDate: ' + CAST(CAST(mp.StartDate as DATE) as VARCHAR) + '**;   '
		WHEN (s2.BusinessUnit_ID IS NULL)
			THEN 'The business unit ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' is not marked as a store in IRMA ** BusinessUnit_ID: ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' | ItemId: ' + CAST(mp.ItemId as VARCHAR) + ' | PriceType: ' + CAST(mp.PriceType as VARCHAR) + ' | StartDate: ' + CAST(CAST(mp.StartDate as DATE) as VARCHAR) + '**;   '
		WHEN p.Item_Key is NULL AND ip.Item_Key IS NOT NULL
		    THEN 'The item ' + CAST(mp.ItemId as VARCHAR) + ' for the business unit ' + CAST(mp.BusinessUnit_ID as VARCHAR) +  ' does not have a Price record in IRMA ** BusinessUnit_ID: ' + CAST(mp.BusinessUnit_ID as VARCHAR) + ' | ItemId: ' + CAST(mp.ItemId as VARCHAR) + ' | PriceType: ' + CAST(mp.PriceType as VARCHAR) + ' | StartDate: ' + CAST(CAST(mp.StartDate as DATE) as VARCHAR) + '**;   '
	END AS ErrorReason
FROM infor.StagingMammothPrice mp  
LEFT JOIN dbo.ValidatedScanCode vsc ON mp.ItemId = vsc.inforitemId
LEFT JOIN dbo.ItemIdentifier ii ON vsc.scanCode = ii.identifier 
								AND ii.Deleted_Identifier = 0
								AND ii.Remove_Identifier = 0
LEFT JOIN dbo.Item i ON i.Item_Key = ii.Item_Key
					 AND i.Deleted_Item = 0
                     AND i.Remove_Item = 0
LEFT JOIN dbo.Store s ON mp.BusinessUnit_ID = s.BusinessUnit_ID
LEFT JOIN dbo.Store s2 ON mp.BusinessUnit_ID = s2.BusinessUnit_ID 
                      AND (s2.WFM_Store = 1 OR s2.Mega_Store = 1)
LEFT JOIN dbo.ItemUnit iu ON mp.SellableUOM = iu.Unit_Abbreviation
LEFT JOIN infor.StagingIrmaPrice ip ON  ip.TransactionId = mp.TransactionId
									AND ip.Item_Key = ii.Item_Key
									AND ip.Store_No = s2.Store_No
LEFT JOIN dbo.Price p ON ip.Item_key = p.Item_Key
					 AND ip.Store_No = p.Store_No					
 WHERE mp.TransactionId = ?
  AND (vsc.inforitemId IS NULL
   OR s.BusinessUnit_ID IS NULL
   OR iu.Unit_Abbreviation IS NULL
   OR ip.TransactionId IS NULL)</statement>
            <jdbcSharedConfig>/SharedResources/Connections/IRMA.sharedjdbc</jdbcSharedConfig>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>TransactionId</parameterName>
                    <dataType>LONGNVARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <wizardData/>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>ErrorReason</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <TransactionId>
                    <xsl:value-of select="$transactionId/UID"/>
                </TransactionId>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
