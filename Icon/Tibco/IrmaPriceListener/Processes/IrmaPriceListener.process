    <pd:activity name="Publish Error Message">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>907</pd:x>
        <pd:y>548</pd:y>
        <config>
            <processName>/SharedResources/Processes/PublishErrorMessage.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <pfx12:ErrorMessage>
                <pfx12:Application>
                    <xsl:value-of select="$_processContext/pfx4:ProcessContext/ProjectName"/>
                </pfx12:Application>
                <pfx12:MessageID>
                    <xsl:value-of select="$IrmaPriceMessageReceiver/ns:ActivityOutput/OtherProperties/TransactionID"/>
                </pfx12:MessageID>
                <pfx12:MessageProperties>
                    <xsl:for-each select="$IrmaPriceMessageReceiver/ns:ActivityOutput/OtherProperties/node()">
                        <pfx12:NameValuePair>
                            <xsl:attribute name="name">
                                <xsl:value-of select="local-name(current())"/>
                            </xsl:attribute>
                            <xsl:attribute name="value">
                                <xsl:value-of select="current()"/>
                            </xsl:attribute>
                        </pfx12:NameValuePair>
                    </xsl:for-each>
                </pfx12:MessageProperties>
                <pfx12:Message>
                    <xsl:value-of select="$IrmaPriceMessageReceiver/ns:ActivityOutput/Body"/>
                </pfx12:Message>
                <pfx12:ErrorCode>
                    <xsl:value-of select="$_error/pfx4:ErrorReport/MsgCode"/>
                </pfx12:ErrorCode>
                <pfx12:ErrorDetails>
                    <xsl:value-of select="$_error/pfx4:ErrorReport/Msg"/>
                </pfx12:ErrorDetails>
                <xsl:choose>
                    <xsl:when test="contains($_error/pfx4:ErrorReport/Msg, &quot;Connection is closed&quot;)">
                        <pfx12:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </pfx12:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/pfx4:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Invalid object name&quot;)">
                        <pfx12:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </pfx12:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/pfx4:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Cannot open database&quot;)">
                        <pfx12:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </pfx12:ErrorSeverity>
                    </xsl:when>
                    <xsl:otherwise>
                        <pfx12:ErrorSeverity>
                            <xsl:value-of select="&quot;Error&quot;"/>
                        </pfx12:ErrorSeverity>
                    </xsl:otherwise>
                </xsl:choose>
            </pfx12:ErrorMessage>
        </pd:inputBindings>
    </pd:activity>
