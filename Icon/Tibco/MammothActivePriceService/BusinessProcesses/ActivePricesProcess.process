                </config>
                <pd:inputBindings>
                    <pfx3:MammothPrices>
                        <xsl:variable name="var" select="($Counter - 1)*$_globalVariables/ns1:GlobalVariables/Mammoth/Application/EMSPriceBatchsize "/>
                        <xsl:variable name="var2" select="($Counter)*  $_globalVariables/ns1:GlobalVariables/Mammoth/Application/EMSPriceBatchsize"/>
                        <xsl:for-each select="$GetActivePrices/resultSet/Record[position() > $var and position()&lt;= $var2]">
