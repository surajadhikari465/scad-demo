                                </pfx3:StartDate>
                                <xsl:if test="EndDate">
                                    <pfx3:EndDate>
                                        <xsl:value-of select="EndDate"/>
                                    </pfx3:EndDate>
                                </xsl:if>
                                <pfx3:PriceType>
                                    <xsl:value-of select="PriceType"/>
                                </pfx3:PriceType>
                                <pfx3:PriceTypeAttribute>
                                    <xsl:value-of select="PriceTypeAttribute"/>
                                </pfx3:PriceTypeAttribute>
                                <pfx3:SellableUom>
                                    <xsl:value-of select="SellableUOM"/>
                                </pfx3:SellableUom>
                                <pfx3:CurrencyCode>
                                    <xsl:value-of select="CurrencyCode"/>
                                </pfx3:CurrencyCode>
                                <xsl:if test="NewTagExpiration">
                                    <pfx3:NewTagExpiration>
                                        <xsl:value-of select="NewTagExpiration"/>
                                    </pfx3:NewTagExpiration>
                                </xsl:if>
                                <pfx3:Action>
                                    <xsl:value-of select="&quot;AddOrUpdate&quot;"/>
                                </pfx3:Action>
                                <pfx3:ItemTypeCode>
                                    <xsl:value-of select="itemTypeCode"/>
                                </pfx3:ItemTypeCode>
                                <pfx3:StoreName>
                                    <xsl:value-of select="StoreName"/>
                                </pfx3:StoreName>
                                <pfx3:ScanCode>
                                    <xsl:value-of select="ScanCode"/>
                                </pfx3:ScanCode>
                            </pfx3:MammothPrice>
                        </xsl:for-each>
                    </pfx3:MammothPrices>
