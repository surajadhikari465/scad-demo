        <pd:activity name="ArchiveSentPrices">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:description>Inserts the prices that were sent to the ESB into a table so that there is a record of what was sent to the ESB. The GetActivePrices query uses that table to filter out any prices that have already been sent.</pd:description>
            <pd:x>764</pd:x>
            <pd:y>363</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>INSERT INTO gpm.ActivePriceSentArchive(Region, PriceID)
VALUES (?, ?)
</statement>
                <batchUpdate>true</batchUpdate>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>Region</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>PriceID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <jdbcSharedConfig>/SharedResources/Connections/JDBC/Mammoth.sharedjdbc</jdbcSharedConfig>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:for-each select="$GetActivePrices/resultSet/Record">
                        <Record>
                            <Region>
                                <xsl:value-of select="Region"/>
                            </Region>
                            <PriceID>
                                <xsl:value-of select="PriceID"/>
                            </PriceID>
                        </Record>
                    </xsl:for-each>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:group name="GroupAndSendActivePrices">
