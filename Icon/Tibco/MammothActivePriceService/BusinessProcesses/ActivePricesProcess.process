            <pd:groupVariables/>
            <config>
                <pd:groupType>while</pd:groupType>
                <pd:serializable>false</pd:serializable>
                <pd:indexSlot>Counter</pd:indexSlot>
                <pd:outSlot>Prices</pd:outSlot>
                <pd:activityOutputName>Map Data</pd:activityOutputName>
                <pd:whileCondition>$Counter&lt;=ceiling((count($GetActivePrices/resultSet/Record) div $_globalVariables/ns1:GlobalVariables/Mammoth/Application/EMSPriceBatchsize))</pd:whileCondition>
