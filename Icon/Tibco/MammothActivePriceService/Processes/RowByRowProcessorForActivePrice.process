<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns12="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:pfx="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" xmlns:WFM.UID="java://UID">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" schemaLocation="/SharedResources/Schemas/Mammoth/MammothPrices.xsd"/>
    <pd:name>Processes/RowByRowProcessorForActivePrice.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx:MammothPrices"/>
                    <xsd:element name="RegionCode" type="xsd:string"/>
                    <xsd:element name="MessageId" type="xsd:string"/>
                    <xsd:element name="ProcessStartDateTime" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>28</pd:startX>
    <pd:startY>201</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>813</pd:endX>
    <pd:endY>201</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1509551253976</pd:targetNamespace>
    <pd:activity name="InfoLogPublisher">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>140</pd:x>
        <pd:y>199</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns12:AppLog>
                <ns12:AppId>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Application/ApplicationID"/>
                </ns12:AppId>
                <ns12:Level>
                    <xsl:value-of select="&quot;Info&quot;"/>
                </ns12:Level>
                <ns12:Logger>
                    <xsl:value-of select="(BW:getCurrentProcessName($_processContext/ns2:ProcessContext/ProcessId))"/>
                </ns12:Logger>
                <ns12:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns12:MachineName>
                <ns12:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns12:LogDate>
                <ns12:Message>
                    <xsl:value-of select="concat(&quot;Process Start time:&quot;,$Start/Input/ProcessStartDateTime ,&quot; Started row by row processing for Message ID:&quot;,$Start/Input/MessageId,&quot; for Region:&quot;,$Start/Input/RegionCode)"/>
                </ns12:Message>
            </ns12:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Iterate">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>277</pd:x>
        <pd:y>49</pd:y>
        <pd:width>292</pd:width>
        <pd:height>277</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Start/Input/pfx:MammothPrices/pfx:MammothPrice</pd:over>
            <pd:iterationElementSlot>CurrentPrice</pd:iterationElementSlot>
            <pd:indexSlot>ActivePrice</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Map Data</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ErrorLogPublisher</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send PriceToTopic</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Catch</pd:from>
            <pd:to>ErrorLogPublisher</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Map Data</pd:from>
            <pd:to>Send PriceToTopic</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Map Data">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>354</pd:x>
            <pd:y>120</pd:y>
            <config>
                <element ref="pfx:MammothPrices"/>
            </config>
            <pd:inputBindings>
                <pfx:MammothPrices>
                    <xsl:copy-of select="$CurrentPrice/pfx:MammothPrice"/>
                </pfx:MammothPrices>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Catch">
            <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
            <pd:resourceType>ae.activities.catch</pd:resourceType>
            <pd:x>324</pd:x>
            <pd:y>247</pd:y>
            <pd:handler>true</pd:handler>
            <config>
                <catchAll>true</catchAll>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="ErrorLogPublisher">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>461</pd:x>
            <pd:y>250</pd:y>
            <config>
                <processName>/SharedResources/Processes/LogPublisher.process</processName>
                <spawn>true</spawn>
            </config>
            <pd:inputBindings>
                <ns12:AppLog>
                    <ns12:AppId>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Application/ApplicationID"/>
                    </ns12:AppId>
                    <ns12:Level>
                        <xsl:value-of select="&quot;Error&quot;"/>
                    </ns12:Level>
                    <ns12:Logger>
                        <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns2:ProcessContext/ProcessId)"/>
                    </ns12:Logger>
                    <ns12:MachineName>
                        <xsl:value-of select="BW:getHostName()"/>
                    </ns12:MachineName>
                    <ns12:LogDate>
                        <xsl:value-of select="current-dateTime()"/>
                    </ns12:LogDate>
                    <ns12:Message>
                        <xsl:value-of select="concat(&quot;Error while processing record with Item ID:&quot;,  $CurrentPrice/pfx:MammothPrice/pfx:ItemId,&quot; Business Unit ID:&quot;,$CurrentPrice/pfx:MammothPrice/pfx:BusinessUnit ,&quot; Error is:&quot;, $_error/ns2:ErrorReport/Msg)"/>
                    </ns12:Message>
                    <xsl:if test="$_error/ns2:ErrorReport/MsgCode">
                        <ns12:Exception>
                            <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                        </ns12:Exception>
                    </xsl:if>
                    <xsl:if test="$_error/ns2:ErrorReport/ProcessStack">
                        <ns12:StackTrace>
                            <xsl:value-of select="$_error/ns2:ErrorReport/ProcessStack"/>
                        </ns12:StackTrace>
                    </xsl:if>
                </ns12:AppLog>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Send PriceToTopic">
            <pd:type>com.tibco.plugin.jms.JMSTopicPublishActivity</pd:type>
            <pd:resourceType>ae.activities.JMSTopicPublishActivity</pd:resourceType>
            <pd:x>467</pd:x>
            <pd:y>119</pd:y>
            <config>
                <PermittedMessageType>XML Text</PermittedMessageType>
                <SessionAttributes>
                    <transacted>false</transacted>
                    <acknowledgeMode>1</acknowledgeMode>
                    <maxSessions>1</maxSessions>
                    <destination>%%Destinations/PriceMammothTopic%%</destination>
                </SessionAttributes>
                <ConfigurableHeaders>
                    <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                    <JMSExpiration>0</JMSExpiration>
                    <JMSPriority>4</JMSPriority>
                </ConfigurableHeaders>
                <ConnectionReference>/SharedResources/Connections/SB1.sharedjmscon</ConnectionReference>
                <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
                <InDataxsdString ref="pfx:MammothPrices"/>
            </config>
            <pd:inputBindings>
                <ns1:ActivityInput>
                    <OtherProperties>
                        <TransactionID>
                            <xsl:value-of select="WFM.UID:UID()"/>
                        </TransactionID>
                        <xsl:if test="$Start/Input/ProcessStartDateTime">
                            <CorrelationID>
                                <xsl:value-of select="$Start/Input/ProcessStartDateTime"/>
                            </CorrelationID>
                        </xsl:if>
                        <TransactionType>
                            <xsl:value-of select="&quot;Price&quot;"/>
                        </TransactionType>
                        <Source>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Source"/>
                        </Source>
                        <xsl:if test="$Start/Input/RegionCode">
                            <RegionCode>
                                <xsl:value-of select="$Start/Input/RegionCode"/>
                            </RegionCode>
                        </xsl:if>
                    </OtherProperties>
                    <Body>
                        <xsl:copy-of select="$Map-Data/pfx:MammothPrices"/>
                    </Body>
                </ns1:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>InfoLogPublisher</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>InfoLogPublisher</pd:from>
        <pd:to>Iterate</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Iterate</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>