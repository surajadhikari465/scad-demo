<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="www.tibco.com/plugin/Sleep" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns3="http://schemas.wfm.com/Mammoth/MessageHeaders.xsd" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:pfx5="http://www.tibco.com/ns/no_namespace_schema_location/SharedResources/Schemas/Infor/PriceSchemas/derivative/ProcessPriceChangePatchMaster.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://schema.infor.com/InforOAGIS/2" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd" xmlns:pfx2="http://schemas.wfm.com/Mammoth/Error.xsd" xmlns:ns12="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx8="http://services.wfm.com/Enterprise/DeadLetterException/V1" xmlns:pfx="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pfx6="http://www.tibco.com/ns/no_namespace_schema_location/SharedResources/Schemas/Infor/PriceSchemas/derivative/PriceChangeMaster.xsd">
    <xsd:import namespace="http://services.wfm.com/Enterprise/DeadLetterException/V1" schemaLocation="/SharedResources/Schemas/Public/Exception/DeadLetterException.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/Error.xsd" schemaLocation="/SharedResources/Schemas/Application/Error.xsd"/>
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/SharedResources/Schemas/Infor/PriceSchemas/derivative/ProcessPriceChangePatchMaster.xsd" schemaLocation="/SharedResources/Schemas/Infor/PriceSchemas/derivative/ProcessPriceChangePatchMaster.xsd"/>
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/SharedResources/Schemas/Infor/PriceSchemas/derivative/PriceChangeMaster.xsd" schemaLocation="/SharedResources/Schemas/Infor/PriceSchemas/derivative/PriceChangeMaster.xsd"/>
    <pd:name>Processes/ProcessBODRequestION.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Body" type="xsd:string"/>
                    <xsd:element name="MessageID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PatchFamilyID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SequenceID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ResetFlag" type="xsd:boolean" minOccurs="0"/>
                    <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Source" type="xsd:string" minOccurs="0"/>
                    <xsd:element ref="pfx2:Error"/>
                    <xsd:element name="LastProcessedGpmSequenceId" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>36</pd:startX>
    <pd:startY>360</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>801</pd:endX>
    <pd:endY>374</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1499349598177</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>444</pd:x>
        <pd:y>678</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>808</pd:x>
        <pd:y>682</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <pfx:ActivityInput>
                <message>
                    <xsl:value-of select="concat( &quot;Error trying to send BOD Request to JMS Queue: &#xD;&#xA; &quot; &#xA;,tib:render-xml($_error_Send-to-Process-BOD-Queue/ActivityErrors, false() , true()) )"/>
                </message>
                <msgCode>
                    <xsl:value-of select="&quot;BOD-EXCEPTION-QSEND-ERROR&quot;"/>
                </msgCode>
            </pfx:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Loop">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>180</pd:x>
        <pd:y>98</pd:y>
        <pd:width>552</pd:width>
        <pd:height>535</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>sequenceIndex</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$sequenceIndex = $Start/Input/SequenceID - $Start/Input/LastProcessedGpmSequenceId</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Group</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ArchiveProcessBODMessage</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Group</pd:from>
            <pd:to>ArchiveProcessBODMessage</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:group name="Group">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>237</pd:x>
            <pd:y>130</pd:y>
            <pd:width>339</pd:width>
            <pd:height>476</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:groupVariables/>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>errorLoop</pd:groupType>
                <pd:serializable>false</pd:serializable>
                <pd:indexSlot>retryCountIndex</pd:indexSlot>
                <pd:errorCondition>$retryCountIndex = $_globalVariables/ns:GlobalVariables/Application/MaxSendMessageRetryCount</pd:errorCondition>
                <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>Map PriceChangeMaster</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Send to Process BOD Queue</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Sleep</pd:from>
                <pd:to>Generate Error</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Send to Process BOD Queue</pd:from>
                <pd:to>Sleep</pd:to>
                <pd:xpathDescription>Error</pd:xpathDescription>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-52429</pd:lineColor>
                <pd:conditionType>error</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Map PriceChangeMaster</pd:from>
                <pd:to>Send to Process BOD Queue</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:activity name="Generate Error">
                <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
                <pd:resourceType>ae.activities.throw</pd:resourceType>
                <pd:x>497</pd:x>
                <pd:y>507</pd:y>
                <config>
                    <faultName/>
                </config>
                <pd:inputBindings>
                    <ns12:ActivityInput>
                        <message>
                            <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                        </message>
                        <xsl:if test="$_error/ns2:ErrorReport/MsgCode">
                            <messageCode>
                                <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                            </messageCode>
                        </xsl:if>
                    </ns12:ActivityInput>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="Sleep">
                <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
                <pd:resourceType>ae.activities.sleep</pd:resourceType>
                <pd:x>377</pd:x>
                <pd:y>510</pd:y>
                <config/>
                <pd:inputBindings>
                    <ns1:SleepInputSchema>
                        <IntervalInMillisec>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Application/SendMessageRetrySleepInMilliseconds"/>
                        </IntervalInMillisec>
                    </ns1:SleepInputSchema>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="Send to Process BOD Queue">
                <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
                <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
                <pd:x>379</pd:x>
                <pd:y>382</pd:y>
                <config>
                    <PermittedMessageType>XML Text</PermittedMessageType>
                    <SessionAttributes>
                        <transacted>false</transacted>
                        <acknowledgeMode>1</acknowledgeMode>
                        <maxSessions>1</maxSessions>
                        <destination>%%Application/BODRequestQueue%%</destination>
                    </SessionAttributes>
                    <ConfigurableHeaders>
                        <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                        <JMSExpiration/>
                        <JMSPriority>0</JMSPriority>
                    </ConfigurableHeaders>
                    <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
                    <ConnectionReference>/SharedResources/Connections/ESB.sharedjmscon</ConnectionReference>
                    <InDataxsdString ref="pfx6:PriceChangeMaster"/>
                </config>
                <pd:inputBindings>
                    <ns0:ActivityInput>
                        <OtherProperties>
                            <xsl:if test="$Start/Input/MessageID">
                                <TransactionID>
                                    <xsl:value-of select="$Start/Input/MessageID"/>
                                </TransactionID>
                            </xsl:if>
                            <xsl:if test="$Start/Input/PatchFamilyID">
                                <CorrelationID>
                                    <xsl:value-of select="$Start/Input/PatchFamilyID"/>
                                </CorrelationID>
                            </xsl:if>
                            <xsl:if test="$Start/Input/SequenceID">
                                <SequenceID>
                                    <xsl:value-of select="$Start/Input/SequenceID"/>
                                </SequenceID>
                            </xsl:if>
                            <xsl:if test="$Start/Input/ResetFlag">
                                <ResetFlag>
                                    <xsl:value-of select="$Start/Input/ResetFlag"/>
                                </ResetFlag>
                            </xsl:if>
                            <xsl:if test="$Start/Input/TransactionType">
                                <TransactionType>
                                    <xsl:value-of select="$Start/Input/TransactionType"/>
                                </TransactionType>
                            </xsl:if>
                            <xsl:if test="$Start/Input/Source">
                                <Source>
                                    <xsl:value-of select="$Start/Input/Source"/>
                                </Source>
                            </xsl:if>
                        </OtherProperties>
                        <Body>
                            <xsl:copy-of select="$Map-PriceChangeMaster/PriceChangeMaster"/>
                        </Body>
                    </ns0:ActivityInput>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="Map PriceChangeMaster">
                <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
                <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
                <pd:x>385</pd:x>
                <pd:y>204</pd:y>
                <config>
                    <element ref="pfx6:PriceChangeMaster"/>
                </config>
                <pd:inputBindings>
                    <PriceChangeMaster>
                        <xsl:attribute name="isCheckPoint">
                            <xsl:value-of select="false()"/>
                        </xsl:attribute>
                        <BusinessKey>
                            <xsl:attribute name="variationID">
                                <xsl:value-of select="0"/>
                            </xsl:attribute>
                            <xsl:value-of select="&quot;00000000-0000-4000-8000-000000000000&quot;"/>
                        </BusinessKey>
                        <PriceChangeHeader>
                            <PatchFamilyID>
                                <xsl:value-of select="$Start/Input/PatchFamilyID"/>
                            </PatchFamilyID>
                            <PatchNum>
                                <xsl:value-of select="$Start/Input/LastProcessedGpmSequenceId + $sequenceIndex"/>
                            </PatchNum>
                            <TimeStamp>
                                <xsl:value-of select="current-dateTime()"/>
                            </TimeStamp>
                        </PriceChangeHeader>
                    </PriceChangeMaster>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
        <pd:activity name="ArchiveProcessBODMessage">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>643</pd:x>
            <pd:y>375</pd:y>
            <config>
                <spawn>true</spawn>
                <processName>/Processes/SaveToMammothMessageArchiveTable.process</processName>
            </config>
            <pd:inputBindings>
                <Input>
                    <Body>
                        <xsl:value-of select="tib:render-xml($Map-PriceChangeMaster/PriceChangeMaster,1)"/>
                    </Body>
                    <ns3:MessageHeaders>
                        <xsl:if test="$Start/Input/MessageID">
                            <ns3:TransactionID>
                                <xsl:value-of select="$Start/Input/MessageID"/>
                            </ns3:TransactionID>
                        </xsl:if>
                        <xsl:if test="$Start/Input/PatchFamilyID">
                            <ns3:CorrelationID>
                                <xsl:value-of select="$Start/Input/PatchFamilyID"/>
                            </ns3:CorrelationID>
                        </xsl:if>
                        <xsl:if test="$Start/Input/SequenceID">
                            <ns3:SequenceID>
                                <xsl:value-of select="$Start/Input/SequenceID"/>
                            </ns3:SequenceID>
                        </xsl:if>
                        <xsl:if test="$Start/Input/TransactionType">
                            <ns3:TransactionType>
                                <xsl:value-of select="$Start/Input/TransactionType"/>
                            </ns3:TransactionType>
                        </xsl:if>
                        <xsl:if test="$Start/Input/Source">
                            <ns3:Source>
                                <xsl:value-of select="$Start/Input/Source"/>
                            </ns3:Source>
                        </xsl:if>
                        <xsl:if test="$Start/Input/ResetFlag">
                            <ns3:ResetFlag>
                                <xsl:value-of select="$Start/Input/ResetFlag"/>
                            </ns3:ResetFlag>
                        </xsl:if>
                    </ns3:MessageHeaders>
                    <MessageID>
                        <xsl:value-of select="$Start/Input/MessageID"/>
                    </MessageID>
                    <MessageTypeName>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Application/MessageTypeNames/ProcessBod"/>
                    </MessageTypeName>
                </Input>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Loop</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Loop</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>