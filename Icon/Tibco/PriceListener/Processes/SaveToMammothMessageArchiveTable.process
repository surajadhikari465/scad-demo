<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:ns3="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx2="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pfx="http://schemas.wfm.com/Mammoth/MessageHeaders.xsd">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MessageHeaders.xsd" schemaLocation="/SharedResources/Schemas/JMS Message/MessageHeaders.xsd"/>
    <pd:name>Processes/SaveToMammothMessageArchiveTable.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Body" type="xsd:string"/>
                    <xsd:element ref="pfx:MessageHeaders" minOccurs="0"/>
                    <xsd:element name="MessageID" type="xsd:string"/>
                    <xsd:element name="MessageTypeName" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>149</pd:startX>
    <pd:startY>224</pd:startY>
    <pd:returnBindings>
        <Input/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType/>
    <pd:endX>854</pd:endX>
    <pd:endY>224</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1519069399673</pd:targetNamespace>
    <pd:activity name="JsonSerializeHeaders">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>350</pd:x>
        <pd:y>220</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx:MessageHeaders"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <xsl:copy-of select="$Start/Input/pfx:MessageHeaders"/>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ArchiveXml">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>545</pd:x>
        <pd:y>225</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
            <statement>INSERT INTO [esb].[MessageArchive] (MessageID, MessageTypeID, MessageStatusID, MessageHeadersJson, MessageBody)
SELECT 
?,
(SELECT MessageTypeId FROM esb.MessageType WHERE MessageTypeName = ?) as MessageTypeID,
(SELECT MessageStatusId FROM esb.MessageStatus WHERE MessageStatusName = 'Sent') as MessageStatusID,
?,
?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>MessageID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageTypeName</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageHeadersJson</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageBody</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <MessageID>
                    <xsl:value-of select="$Start/Input/MessageID"/>
                </MessageID>
                <MessageTypeName>
                    <xsl:value-of select="$Start/Input/MessageTypeName"/>
                </MessageTypeName>
                <MessageHeadersJson>
                    <xsl:value-of select="$JsonSerializeHeaders/ns:ActivityOutputClass/jsonString"/>
                </MessageHeadersJson>
                <MessageBody>
                    <xsl:value-of select="$Start/Input/Body"/>
                </MessageBody>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>185</pd:x>
        <pd:y>343</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="LogError">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>407</pd:x>
        <pd:y>342</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns3:AppLog>
                <ns3:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns3:AppId>
                <ns3:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </ns3:Level>
                <ns3:Logger>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProjectName"/>
                </ns3:Logger>
                <ns3:UserName>
                    <xsl:value-of select="$_processContext/ns2:ProcessContext/ProjectName"/>
                </ns3:UserName>
                <ns3:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns3:MachineName>
                <ns3:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns3:LogDate>
                <ns3:Thread>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns2:ProcessContext/ProcessId)"/>
                </ns3:Thread>
                <xsl:if test="$_error/ns2:ErrorReport/Msg">
                    <ns3:Message>
                        <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                    </ns3:Message>
                </xsl:if>
                <xsl:if test="$_error/ns2:ErrorReport/ProcessStack">
                    <ns3:CallSite>
                        <xsl:value-of select="$_error/ns2:ErrorReport/ProcessStack"/>
                    </ns3:CallSite>
                </xsl:if>
                <xsl:if test="$_error/ns2:ErrorReport/FullClass">
                    <ns3:Exception>
                        <xsl:value-of select="$_error/ns2:ErrorReport/FullClass"/>
                    </ns3:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns2:ErrorReport/StackTrace">
                    <ns3:StackTrace>
                        <xsl:value-of select="$_error/ns2:ErrorReport/StackTrace"/>
                    </ns3:StackTrace>
                </xsl:if>
            </ns3:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>579</pd:x>
        <pd:y>345</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <pfx2:ActivityInput>
                <message>
                    <xsl:value-of select="$_error/ns2:ErrorReport/StackTrace"/>
                </message>
                <xsl:if test="$_error/ns2:ErrorReport/MsgCode">
                    <msgCode>
                        <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                    </msgCode>
                </xsl:if>
            </pfx2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>JsonSerializeHeaders</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JsonSerializeHeaders</pd:from>
        <pd:to>ArchiveXml</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ArchiveXml</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>LogError</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogError</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>