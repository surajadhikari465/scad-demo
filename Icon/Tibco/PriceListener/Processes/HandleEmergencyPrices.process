<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:WFM.DateTimeCompare="java://com.tibco.wfm.DateTimeComparison" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" schemaLocation="/SharedResources/Schemas/Mammoth/MammothPrices.xsd"/>
    <pd:name>Processes/HandleEmergencyPrices.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx:MammothPrices"/>
                    <xsd:element name="TransactionID" type="xsd:string"/>
                    <xsd:element name="CorrelationID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SequenceID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ResetFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Source" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="nonReceivingSystems" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>96</pd:startX>
    <pd:startY>104</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>666</pd:endX>
    <pd:endY>114</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1503353308380</pd:targetNamespace>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>299</pd:x>
        <pd:y>284</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <pfx:MammothPrices>
                <xsl:copy-of select="$Map-Data/pfx:MammothPrices/pfx:MammothPrice"/>
            </pfx:MammothPrices>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Mammoth Topic Publisher">
        <pd:type>com.tibco.plugin.jms.JMSTopicPublishActivity</pd:type>
        <pd:resourceType>ae.activities.JMSTopicPublishActivity</pd:resourceType>
        <pd:x>530</pd:x>
        <pd:y>282</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/PriceMammothTopic%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/SharedResources/Connections/SB1.sharedjmscon</ConnectionReference>
            <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <OtherProperties>
                    <TransactionID>
                        <xsl:value-of select="$Start/root/TransactionID"/>
                    </TransactionID>
                    <xsl:if test="$Start/root/CorrelationID">
                        <CorrelationID>
                            <xsl:value-of select="$Start/root/CorrelationID"/>
                        </CorrelationID>
                    </xsl:if>
                    <xsl:if test="$Start/root/SequenceID">
                        <SequenceID>
                            <xsl:value-of select="$Start/root/SequenceID"/>
                        </SequenceID>
                    </xsl:if>
                    <xsl:if test="$Start/root/ResetFlag">
                        <ResetFlag>
                            <xsl:value-of select="$Start/root/ResetFlag"/>
                        </ResetFlag>
                    </xsl:if>
                    <xsl:if test="$Start/root/TransactionType">
                        <TransactionType>
                            <xsl:value-of select="$Start/root/TransactionType"/>
                        </TransactionType>
                    </xsl:if>
                    <Source>
                        <xsl:value-of select="'Infor'"/>
                    </Source>
                    <xsl:if test="$Start/root/nonReceivingSystems">
                        <nonReceivingSysName>
                            <xsl:value-of select="$Start/root/nonReceivingSystems"/>
                        </nonReceivingSysName>
                    </xsl:if>
                    <xsl:if test="$Map-Data/pfx:MammothPrices/pfx:MammothPrice[1]/pfx:Region">
                        <RegionCode>
                            <xsl:value-of select="$Map-Data/pfx:MammothPrices/pfx:MammothPrice[1]/pfx:Region"/>
                        </RegionCode>
                    </xsl:if>
                </OtherProperties>
                <Body>
                    <xsl:value-of select="$Render-XML/xmlString"/>
                </Body>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>281</pd:x>
        <pd:y>104</pd:y>
        <config>
            <element ref="pfx:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <pfx:MammothPrices>
                <xsl:for-each select="$Start/root/pfx:MammothPrices/pfx:MammothPrice[&#xA;tib:compare-date(tib:parse-date(&quot;yyyy-MM-dd&quot;, pfx:StartDate), tib:parse-date(&quot;yyyy-MM-dd&quot;, current-date())) = 0&#xA;or ((upper-case(pfx:Action) = &quot;UPDATE&quot; or upper-case(pfx:Action) = &quot;DELETE&quot;)&#xA;&#x9;and upper-case(pfx:PriceType) = &quot;TPR&quot;&#xA; &#x9;and (tib:compare-date(tib:parse-date(&quot;yyyy-MM-dd&quot;, pfx:StartDate), tib:parse-date(&quot;yyyy-MM-dd&quot;, current-date())) = -1 ))]">
                    <pfx:MammothPrice>
                        <pfx:Region>
                            <xsl:value-of select="pfx:Region"/>
                        </pfx:Region>
                        <pfx:BusinessUnit>
                            <xsl:value-of select="pfx:BusinessUnit"/>
                        </pfx:BusinessUnit>
                        <pfx:ItemId>
                            <xsl:value-of select="pfx:ItemId"/>
                        </pfx:ItemId>
                        <pfx:GpmId>
                            <xsl:value-of select="pfx:GpmId"/>
                        </pfx:GpmId>
                        <pfx:Multiple>
                            <xsl:value-of select="pfx:Multiple"/>
                        </pfx:Multiple>
                        <pfx:Price>
                            <xsl:value-of select="pfx:Price"/>
                        </pfx:Price>
                        <pfx:StartDate>
                            <xsl:value-of select="pfx:StartDate"/>
                        </pfx:StartDate>
                        <xsl:if test="pfx:EndDate">
                            <pfx:EndDate>
                                <xsl:value-of select="pfx:EndDate"/>
                            </pfx:EndDate>
                        </xsl:if>
                        <pfx:PriceType>
                            <xsl:value-of select="pfx:PriceType"/>
                        </pfx:PriceType>
                        <pfx:PriceTypeAttribute>
                            <xsl:value-of select="pfx:PriceTypeAttribute"/>
                        </pfx:PriceTypeAttribute>
                        <pfx:SellableUom>
                            <xsl:value-of select="pfx:SellableUom"/>
                        </pfx:SellableUom>
                        <pfx:CurrencyCode>
                            <xsl:value-of select="pfx:CurrencyCode"/>
                        </pfx:CurrencyCode>
                        <xsl:if test="current()/pfx:TagExpirationDate">
                            <pfx:TagExpirationDate>
                                <xsl:value-of select="current()/pfx:TagExpirationDate"/>
                            </pfx:TagExpirationDate>
                        </xsl:if>
                        <pfx:Action>
                            <xsl:value-of select="pfx:Action"/>
                        </pfx:Action>
                        <pfx:ItemTypeCode>
                            <xsl:value-of select="pfx:ItemTypeCode"/>
                        </pfx:ItemTypeCode>
                        <pfx:StoreName>
                            <xsl:value-of select="pfx:StoreName"/>
                        </pfx:StoreName>
                        <pfx:ScanCode>
                            <xsl:value-of select="pfx:ScanCode"/>
                        </pfx:ScanCode>
                        <xsl:if test="current()/pfx:PercentOff">
                            <pfx:PercentOff>
                                <xsl:value-of select="current()/pfx:PercentOff"/>
                            </pfx:PercentOff>
                        </xsl:if>
                    </pfx:MammothPrice>
                </xsl:for-each>
            </pfx:MammothPrices>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>Mammoth Topic Publisher</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Mammoth Topic Publisher</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:xpathDescription>IfAnyEmergencyPrices</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($Map-Data/pfx:MammothPrices/pfx:MammothPrice)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>End</pd:to>
        <pd:xpathDescription>NoEmergencyPrices</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>