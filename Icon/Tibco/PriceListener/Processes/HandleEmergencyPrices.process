<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns02="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:WFM.DateTimeCompare="java://com.tibco.wfm.DateTimeComparison" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/pe/EngineTypes">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" schemaLocation="/SharedResources/Schemas/Mammoth/MammothPrices.xsd"/>
    <pd:name>Processes/HandleEmergencyPrices.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element ref="pfx:MammothPrices"/>
                    <xsd:element name="TransactionID" type="xsd:string"/>
                    <xsd:element name="CorrelationID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="SequenceID" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="ResetFlag" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="TransactionType" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Source" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="nonReceivingSystems" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>96</pd:startX>
    <pd:startY>104</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>666</pd:endX>
    <pd:endY>114</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1503353308380</pd:targetNamespace>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>281</pd:x>
        <pd:y>104</pd:y>
        <config>
            <element ref="pfx:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <pfx:MammothPrices>
                <xsl:for-each select="$Start/root/pfx:MammothPrices/pfx:MammothPrice[tib:compare-date(tib:parse-date(&quot;yyyy-MM-dd&quot;, pfx:StartDate), tib:parse-date(&quot;yyyy-MM-dd&quot;, current-date())) &lt;1]">
                    <pfx:MammothPrice>
                        <pfx:Region>
                            <xsl:value-of select="pfx:Region"/>
                        </pfx:Region>
                        <pfx:BusinessUnit>
                            <xsl:value-of select="pfx:BusinessUnit"/>
                        </pfx:BusinessUnit>
                        <pfx:ItemId>
                            <xsl:value-of select="pfx:ItemId"/>
                        </pfx:ItemId>
                        <pfx:GpmId>
                            <xsl:value-of select="pfx:GpmId"/>
                        </pfx:GpmId>
                        <pfx:Multiple>
                            <xsl:value-of select="pfx:Multiple"/>
                        </pfx:Multiple>
                        <pfx:Price>
                            <xsl:value-of select="pfx:Price"/>
                        </pfx:Price>
                        <pfx:StartDate>
                            <xsl:value-of select="pfx:StartDate"/>
                        </pfx:StartDate>
                        <xsl:if test="pfx:EndDate">
                            <pfx:EndDate>
                                <xsl:value-of select="pfx:EndDate"/>
                            </pfx:EndDate>
                        </xsl:if>
                        <pfx:PriceType>
                            <xsl:value-of select="pfx:PriceType"/>
                        </pfx:PriceType>
                        <pfx:PriceTypeAttribute>
                            <xsl:value-of select="pfx:PriceTypeAttribute"/>
                        </pfx:PriceTypeAttribute>
                        <pfx:SellableUom>
                            <xsl:value-of select="pfx:SellableUom"/>
                        </pfx:SellableUom>
                        <pfx:CurrencyCode>
                            <xsl:value-of select="pfx:CurrencyCode"/>
                        </pfx:CurrencyCode>
                        <xsl:if test="pfx:TagExpirationDate">
                            <pfx:TagExpirationDate>
                                <xsl:value-of select="pfx:TagExpirationDate"/>
                            </pfx:TagExpirationDate>
                        </xsl:if>
                        <pfx:Action>
                            <xsl:value-of select="pfx:Action"/>
                        </pfx:Action>
                        <pfx:ItemTypeCode>
                            <xsl:value-of select="pfx:ItemTypeCode"/>
                        </pfx:ItemTypeCode>
                        <pfx:StoreName>
                            <xsl:value-of select="pfx:StoreName"/>
                        </pfx:StoreName>
                        <pfx:ScanCode>
                            <xsl:value-of select="pfx:ScanCode"/>
                        </pfx:ScanCode>
                    </pfx:MammothPrice>
                </xsl:for-each>
            </pfx:MammothPrices>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>94</pd:x>
        <pd:y>275</pd:y>
        <pd:width>363</pd:width>
        <pd:height>244</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:errorCondition>$i &lt; $_globalVariables/pfx2:GlobalVariables/Application/MaxDbRetryCount</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Insert Emergency Prices</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Insert Emergency Prices</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Insert Emergency Prices</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65485</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Insert Emergency Prices">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>291</pd:x>
            <pd:y>355</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>INSERT INTO gpm.MessageQueueEmergencyPrice(ItemId, BusinessUnitId, PriceType, MammothPriceXml)
SELECT ?, ?, ?, ?</statement>
                <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
                <batchUpdate>true</batchUpdate>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>ItemId</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>BusinessUnitId</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>PriceType</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>MammothPriceXml</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:for-each select="$Map-Data/pfx:MammothPrices/pfx:MammothPrice">
                        <Record>
                            <ItemId>
                                <xsl:value-of select="pfx:ItemId"/>
                            </ItemId>
                            <BusinessUnitId>
                                <xsl:value-of select="pfx:BusinessUnit"/>
                            </BusinessUnitId>
                            <PriceType>
                                <xsl:value-of select="pfx:PriceType"/>
                            </PriceType>
                            <MammothPriceXml>
                                <xsl:value-of select="tib:render-xml(current())"/>
                            </MammothPriceXml>
                        </Record>
                    </xsl:for-each>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>203</pd:x>
            <pd:y>451</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx2:GlobalVariables/Application/DbRetrySleepInMilliseconds"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>364</pd:x>
            <pd:y>453</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns02:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </message>
                    <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns02:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>End</pd:to>
        <pd:xpathDescription>NoEmergencyPrices</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Group</pd:to>
        <pd:xpathDescription>IfAnyEmergencyPrices</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($Map-Data/pfx:MammothPrices/pfx:MammothPrice)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>