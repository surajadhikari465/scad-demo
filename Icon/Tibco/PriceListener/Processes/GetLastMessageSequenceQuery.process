<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://schemas.wfm.com/Enterprise/PriceMgmt/Price/V2" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx3="http://www.tibco.com/CommonLE2/namespace/Public/Exception/ExceptionRequest.xsd" xmlns:pfx4="http://schemas.wfm.com/Mammoth/Error.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://schemas.wfm.com/Enterprise/PriceMgmt/Price/V2"/>
    <xsd:import namespace="http://www.tibco.com/CommonLE2/namespace/Public/Exception/ExceptionRequest.xsd" schemaLocation="/SharedResources/Schemas/Public/Exception/ExceptionRequest.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/Error.xsd" schemaLocation="/SharedResources/Schemas/Mammoth/Error.xsd"/>
    <pd:name>Processes/GetLastMessageSequenceQuery.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="MessageID" type="xsd:string"/>
                    <xsd:element name="PatchFamilyID" type="xsd:string"/>
                    <xsd:element name="SequenceID" type="xsd:int"/>
                    <xsd:element name="ResetFlag" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>89</pd:startX>
    <pd:startY>66</pd:startY>
    <pd:returnBindings>
        <root>
            <PatchFamilyID>
                <xsl:value-of select="$Map-Data/root/PatchFamilyId"/>
            </PatchFamilyID>
            <PatchFamilySequenceID>
                <xsl:value-of select="$Map-Data/root/SequenceID"/>
            </PatchFamilySequenceID>
            <LastProcessedGpmSequenceID>
                <xsl:value-of select="$Map-Data/root/LastProcessedGpmSequenceID"/>
            </LastProcessedGpmSequenceID>
            <IsInSequence>
                <xsl:value-of select="$Map-Data/root/IsInSequence"/>
            </IsInSequence>
            <IsAlreadyProcessed>
                <xsl:value-of select="$Map-Data/root/IsAlreadyProcessed"/>
            </IsAlreadyProcessed>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="PatchFamilyID" type="xsd:string"/>
                    <xsd:element name="PatchFamilySequenceID" type="xsd:string"/>
                    <xsd:element name="LastProcessedGpmSequenceID" type="xsd:int"/>
                    <xsd:element name="IsInSequence" type="xsd:boolean"/>
                    <xsd:element name="IsAlreadyProcessed" type="xsd:boolean"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>690</pd:endX>
    <pd:endY>70</pd:endY>
    <pd:errorSchemas>
        <ErrorSchemas ref="pfx4:Error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1495058550155</pd:targetNamespace>
    <pd:activity name="GetLastSequenceQuery">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>317</pd:x>
        <pd:y>67</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>SELECT MessageSequenceID, PatchFamilyID, PatchFamilySequenceID AS LastProcessedGpmSequenceID FROM [gpm].[MessageSequence] WHERE PatchFamilyID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>PatchFamilyID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>MessageSequenceID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>PatchFamilyID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>LastProcessedGpmSequenceID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <PatchFamilyID>
                    <xsl:value-of select="$Start/root/PatchFamilyID"/>
                </PatchFamilyID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>514</pd:x>
        <pd:y>66</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="PatchFamilyId" type="xsd:string"/>
                            <xsd:element name="SequenceID" type="xsd:int"/>
                            <xsd:element name="LastProcessedGpmSequenceID" type="xsd:int" minOccurs="0"/>
                            <xsd:element name="IsInSequence" type="xsd:boolean"/>
                            <xsd:element name="IsAlreadyProcessed" type="xsd:boolean"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <PatchFamilyId>
                    <xsl:value-of select="$Start/root/PatchFamilyID"/>
                </PatchFamilyId>
                <SequenceID>
                    <xsl:value-of select="$Start/root/SequenceID"/>
                </SequenceID>
                <LastProcessedGpmSequenceID>
                    <xsl:value-of select="if (empty($GetLastSequenceQuery/resultSet/Record)) then 0 else $GetLastSequenceQuery/resultSet/Record[1]/LastProcessedGpmSequenceID"/>
                </LastProcessedGpmSequenceID>
                <IsInSequence>
                    <xsl:value-of select="($Start/root/SequenceID = ($GetLastSequenceQuery/resultSet/Record[1]/LastProcessedGpmSequenceID + 1))&#xA;or ($Start/root/SequenceID = 1 and count($GetLastSequenceQuery/resultSet/Record) = 0)&#xA;or ($Start/root/ResetFlag = $_globalVariables/ns:GlobalVariables/Application/ResetFlagTrueValue)"/>
                </IsInSequence>
                <IsAlreadyProcessed>
                    <xsl:value-of select="$Start/root/SequenceID&lt;= $GetLastSequenceQuery/resultSet/Record[1]/LastProcessedGpmSequenceID"/>
                </IsAlreadyProcessed>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>313</pd:x>
        <pd:y>223</pd:y>
        <config>
            <faultName>ErrorSchemas</faultName>
        </config>
        <pd:inputBindings>
            <pfx2:ActivityInput>
                <message>
                    <xsl:value-of select="&quot;Message SequenceID is less than 1 or CorrelationID (PatchFamilyID) is empty&quot;"/>
                </message>
                <messageCode>
                    <xsl:value-of select="&quot;InvalidMessageHeader&quot;"/>
                </messageCode>
                <data>
                    <pfx4:Error>
                        <pfx4:StackTrace>
                            <xsl:value-of select="&quot;Called From GetLastMessageSequenceQuery Process&quot;"/>
                        </pfx4:StackTrace>
                        <pfx4:Code>
                            <xsl:value-of select="&quot;InvalidMessageHeader&quot;"/>
                        </pfx4:Code>
                        <pfx4:Message>
                            <xsl:value-of select="concat(&quot;Message Sequence ID or CorrelationID is invalid.  SequenceID must be greater than 0 or CorrelationID must not be empty. MessageID: &quot;,$Start/root/MessageID,&quot;, CorrelationID (PatchFamilyID): &quot;, $Start/root/PatchFamilyID, &quot;, SequenceID:  &quot;, $Start/root/SequenceID, &quot;.&quot;)"/>
                        </pfx4:Message>
                    </pfx4:Error>
                </data>
            </pfx2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetLastSequenceQuery</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetLastSequenceQuery</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>End</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:xpathDescription>Invalid Message Header</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65485</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/root/SequenceID &lt; 1
or
$Start/root/PatchFamilyID = ""</pd:xpath>
    </pd:transition>
</pd:ProcessDefinition>