<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="www.tibco.com/plugin/Sleep" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd" xmlns:pfx2="http://schemas.wfm.com/Mammoth/Error.xsd" xmlns:ns12="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx8="http://services.wfm.com/Enterprise/DeadLetterException/V1" xmlns:pfx="http://www.tibco.com/pe/WriteToLogActivitySchema">
    <xsd:import namespace="http://services.wfm.com/Enterprise/DeadLetterException/V1" schemaLocation="/SharedResources/Schemas/Public/Exception/DeadLetterException.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/Error.xsd" schemaLocation="/SharedResources/Schemas/Application/Error.xsd"/>
    <pd:name>Processors/ProcessBODRequestION.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="GpmSequenceId" type="xsd:string"/>
                    <xsd:element name="Body" type="xsd:string"/>
                    <xsd:element name="PatchFamilyID" type="xsd:string"/>
                    <xsd:element name="MessageId" type="xsd:string"/>
                    <xsd:element ref="pfx2:Error"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>62</pd:startX>
    <pd:startY>151</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>657</pd:endX>
    <pd:endY>151</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1499349598177</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>170</pd:x>
        <pd:y>9</pd:y>
        <pd:width>379</pd:width>
        <pd:height>269</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>retryCountIndex</pd:indexSlot>
            <pd:errorCondition>$retryCountIndex = $_globalVariables/ns:GlobalVariables/BODRequest/retryCount</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Send to Error Queue</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send to Error Queue</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send to Error Queue</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:xpathDescription>Error</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-52429</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>460</pd:x>
            <pd:y>201</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns12:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                    </message>
                    <xsl:if test="$_error/ns2:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns12:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>320</pd:x>
            <pd:y>200</pd:y>
            <config/>
            <pd:inputBindings>
                <ns1:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/BODRequest/RetrySleepInMilliseconds"/>
                    </IntervalInMillisec>
                </ns1:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Send to Error Queue">
            <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
            <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
            <pd:x>318</pd:x>
            <pd:y>86</pd:y>
            <config>
                <PermittedMessageType>XML Text</PermittedMessageType>
                <SessionAttributes>
                    <transacted>false</transacted>
                    <acknowledgeMode>1</acknowledgeMode>
                    <maxSessions>1</maxSessions>
                    <destination>%%Destinations/Queues/ErrorBODQueue%%</destination>
                </SessionAttributes>
                <ConfigurableHeaders>
                    <JMSDeliveryMode>%%Environment/BOD/DeliveryMode%%</JMSDeliveryMode>
                    <JMSExpiration>%%Environment/BOD/JMSExpiration%%</JMSExpiration>
                    <JMSPriority>%%Environment/BOD/JMSPriority%%</JMSPriority>
                </ConfigurableHeaders>
                <ConnectionReference>/Connections/ESB Connection.sharedjmscon</ConnectionReference>
                <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
                <InDataxsdString>
                    <xsd:element name="Input">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="MessageID" type="xsd:string"/>
                                <xsd:element ref="pfx2:Error"/>
                                <xsd:element name="Body" type="xsd:string"/>
                                <xsd:element name="GpmSequenceID" type="xsd:string"/>
                                <xsd:element name="PatchFamilyID" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </InDataxsdString>
            </config>
            <pd:inputBindings>
                <ns0:ActivityInput>
                    <JMSPriority>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Environment/BOD/JMSPriority"/>
                    </JMSPriority>
                    <JMSDeliveryMode>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Environment/BOD/DeliveryMode"/>
                    </JMSDeliveryMode>
                    <OtherProperties>
                        <SequenceID>
                            <xsl:value-of select="$Start/Input/GpmSequenceId"/>
                        </SequenceID>
                        <PatchFamilyID>
                            <xsl:value-of select="$Start/Input/PatchFamilyID"/>
                        </PatchFamilyID>
                    </OtherProperties>
                    <Body>
                        <Input>
                            <MessageID>
                                <xsl:value-of select="$Start/Input/MessageId"/>
                            </MessageID>
                            <xsl:copy-of select="$Start/Input/pfx2:Error"/>
                            <Body>
                                <xsl:value-of select="$Start/Input/Body"/>
                            </Body>
                            <GpmSequenceID>
                                <xsl:value-of select="$Start/Input/GpmSequenceId"/>
                            </GpmSequenceID>
                            <PatchFamilyID>
                                <xsl:value-of select="$Start/Input/PatchFamilyID"/>
                            </PatchFamilyID>
                        </Input>
                    </Body>
                </ns0:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>389</pd:x>
        <pd:y>350</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>512</pd:x>
        <pd:y>348</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <pfx:ActivityInput>
                <message>
                    <xsl:value-of select="concat( &quot;Error trying to send BOD Request to JMS Queue: &#xD;&#xA; &quot; &#xA;,tib:render-xml($_error_Send-to-Error-Queue/ActivityErrors, false() , true()) )"/>
                </message>
                <msgCode>
                    <xsl:value-of select="&quot;BOD-EXCEPTION-QSEND-ERROR&quot;"/>
                </msgCode>
            </pfx:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>