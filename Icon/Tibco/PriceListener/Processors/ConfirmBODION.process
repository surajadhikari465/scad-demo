<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="www.tibco.com/plugin/Sleep" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns14="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx2="http://schemas.wfm.com/Mammoth/Error.xsd" xmlns:ns12="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns13="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://schema.infor.com/InforOAGIS/2">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/Error.xsd" schemaLocation="/Shared Resources/Schemas/Application/Error.xsd"/>
    <xsd:import namespace="http://schema.infor.com/InforOAGIS/2" schemaLocation="/SharedResources/Schemas/Application/ConfirmBOD.xsd"/>
    <pd:name>Processors/ConfirmBODION.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="InputData">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="SequenceID" type="xsd:string"/>
                    <xsd:element name="PatchFamilyID" type="xsd:string"/>
                    <xsd:element ref="pfx:ConfirmBOD"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>79</pd:startX>
    <pd:startY>223</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>457</pd:endX>
    <pd:endY>77</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1500654676533</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>475</pd:x>
        <pd:y>213</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:group name="RepeatOnError">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>187</pd:x>
        <pd:y>17</pd:y>
        <pd:width>219</pd:width>
        <pd:height>399</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>retryCountIndex</pd:indexSlot>
            <pd:errorCondition>$retryCountIndex =$_globalVariables/pfx3:GlobalVariables/ConfirmBOD/retryCount</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Send to Confirm BOD Queue</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send to Confirm BOD Queue</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send to Confirm BOD Queue</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-52429</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>308</pd:x>
            <pd:y>349</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns13:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </message>
                    <messageCode>
                        <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                    </messageCode>
                </ns13:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>305</pd:x>
            <pd:y>221</pd:y>
            <config/>
            <pd:inputBindings>
                <ns1:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/ConfirmBOD/RetrySleepInMilliseconds"/>
                    </IntervalInMillisec>
                </ns1:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Send to Confirm BOD Queue">
            <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
            <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
            <pd:x>303</pd:x>
            <pd:y>81</pd:y>
            <config>
                <PermittedMessageType>XML Text</PermittedMessageType>
                <SessionAttributes>
                    <transacted>false</transacted>
                    <acknowledgeMode>1</acknowledgeMode>
                    <maxSessions>1</maxSessions>
                    <destination>%%Destinations/Queues/ConfirmBODQueue%%</destination>
                </SessionAttributes>
                <ConfigurableHeaders>
                    <JMSDeliveryMode>%%Environment/ConfirmBOD/DeliveryMode%%</JMSDeliveryMode>
                    <JMSExpiration>%%Environment/ConfirmBOD/JMSExpiration%%</JMSExpiration>
                    <JMSPriority>%%Environment/ConfirmBOD/JMSPriority%%</JMSPriority>
                </ConfigurableHeaders>
                <ConnectionReference>/Connections/ESB Connection.sharedjmscon</ConnectionReference>
                <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
                <InDataxsdString ref="pfx:ConfirmBOD"/>
            </config>
            <pd:inputBindings>
                <ns12:ActivityInput>
                    <JMSPriority>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/Environment/ConfirmBOD/JMSPriority"/>
                    </JMSPriority>
                    <JMSDeliveryMode>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/Environment/ConfirmBOD/DeliveryMode"/>
                    </JMSDeliveryMode>
                    <OtherProperties>
                        <SequenceID>
                            <xsl:value-of select="$Start/InputData/SequenceID"/>
                        </SequenceID>
                        <PatchFamilyID>
                            <xsl:value-of select="$Start/InputData/PatchFamilyID"/>
                        </PatchFamilyID>
                    </OtherProperties>
                    <Body>
                        <xsl:copy-of select="$Start/InputData/pfx:ConfirmBOD"/>
                    </Body>
                </ns12:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>607</pd:x>
        <pd:y>120</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns14:ActivityInput>
                <message>
                    <xsl:value-of select="concat( &quot;Error trying to send BOD Request to JMS Queue: &#xD;&#xA; &quot;&#xA;,tib:render-xml($_error_Send-to-Confirm-BOD-Queue/ActivityErrors, false(),true()))"/>
                </message>
                <msgCode>
                    <xsl:value-of select="&quot;CONFIRM-BOD-EXCEPTION-QSEND-ERROR&quot;"/>
                </msgCode>
            </ns14:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>RepeatOnError</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RepeatOnError</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>