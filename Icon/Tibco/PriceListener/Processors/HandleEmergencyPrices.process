<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:WFM.DateTimeCompare="java://com.tibco.wfm.DateTimeComparison" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://schemas.wfm.com/Mammoth/MammothPrices.xsd">
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" schemaLocation="/Schemas/Application/MammothPrices.xsd"/>
    <pd:name>Processors/HandleEmergencyPrices.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:MammothPrices"/>
    <pd:startX>96</pd:startX>
    <pd:startY>104</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>666</pd:endX>
    <pd:endY>114</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1503353308380</pd:targetNamespace>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>299</pd:x>
        <pd:y>284</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <textEncoding>UTF-8</textEncoding>
            <term ref="pfx:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <pfx:MammothPrices>
                <xsl:copy-of select="$Map-Data/pfx:MammothPrices/pfx:MammothPrice"/>
            </pfx:MammothPrices>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Mammoth Topic Publisher">
        <pd:type>com.tibco.plugin.jms.JMSTopicPublishActivity</pd:type>
        <pd:resourceType>ae.activities.JMSTopicPublishActivity</pd:resourceType>
        <pd:x>530</pd:x>
        <pd:y>282</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/Topics/Mammoth/JustInTimeTopic%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Connections/SB1 Connection.sharedjmscon</ConnectionReference>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <Body>
                    <xsl:value-of select="$Render-XML/xmlString"/>
                </Body>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>281</pd:x>
        <pd:y>104</pd:y>
        <config>
            <element ref="pfx:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <pfx:MammothPrices>
                <xsl:for-each select="$Start/pfx:MammothPrices/pfx:MammothPrice[&#xA;tib:compare-date(tib:parse-date(&quot;yyyy-MM-dd&quot;, pfx:StartDate), tib:parse-date(&quot;yyyy-MM-dd&quot;, current-date())) = 0&#xA;or (upper-case(pfx:Action) = &quot;UPDATE&quot;&#xA;&#x9;and upper-case(pfx:PriceType) = &quot;TPR&quot;&#xA; &#x9;and (tib:compare-date(tib:parse-date(&quot;yyyy-MM-dd&quot;, pfx:StartDate), tib:parse-date(&quot;yyyy-MM-dd&quot;, current-date())) = -1 ))]">
                    <pfx:MammothPrice>
                        <pfx:Region>
                            <xsl:value-of select="pfx:Region"/>
                        </pfx:Region>
                        <pfx:BusinessUnit>
                            <xsl:value-of select="pfx:BusinessUnit"/>
                        </pfx:BusinessUnit>
                        <pfx:ItemId>
                            <xsl:value-of select="pfx:ItemId"/>
                        </pfx:ItemId>
                        <pfx:GpmId>
                            <xsl:value-of select="pfx:GpmId"/>
                        </pfx:GpmId>
                        <pfx:Multiple>
                            <xsl:value-of select="pfx:Multiple"/>
                        </pfx:Multiple>
                        <pfx:Price>
                            <xsl:value-of select="pfx:Price"/>
                        </pfx:Price>
                        <pfx:StartDate>
                            <xsl:value-of select="pfx:StartDate"/>
                        </pfx:StartDate>
                        <xsl:if test="pfx:EndDate">
                            <pfx:EndDate>
                                <xsl:value-of select="pfx:EndDate"/>
                            </pfx:EndDate>
                        </xsl:if>
                        <pfx:PriceType>
                            <xsl:value-of select="pfx:PriceType"/>
                        </pfx:PriceType>
                        <pfx:PriceTypeAttribute>
                            <xsl:value-of select="pfx:PriceTypeAttribute"/>
                        </pfx:PriceTypeAttribute>
                        <pfx:SellableUom>
                            <xsl:value-of select="pfx:SellableUom"/>
                        </pfx:SellableUom>
                        <pfx:CurrencyCode>
                            <xsl:value-of select="pfx:CurrencyCode"/>
                        </pfx:CurrencyCode>
                        <xsl:if test="pfx:NewTagExpiration">
                            <pfx:NewTagExpiration>
                                <xsl:value-of select="pfx:NewTagExpiration"/>
                            </pfx:NewTagExpiration>
                        </xsl:if>
                        <pfx:Action>
                            <xsl:value-of select="pfx:Action"/>
                        </pfx:Action>
                    </pfx:MammothPrice>
                </xsl:for-each>
            </pfx:MammothPrices>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>Mammoth Topic Publisher</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Mammoth Topic Publisher</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:xpathDescription>IfAnyEmergencyPrices</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>exists($Map-Data/pfx:MammothPrices/pfx:MammothPrice)</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>End</pd:to>
        <pd:xpathDescription>NoEmergencyPrices</pd:xpathDescription>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>