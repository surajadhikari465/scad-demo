<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns0="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns02="www.tibco.com/plugin/Sleep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <pd:name>DataAccess/Commands/AddOrUpdateMessageSequenceCommand.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="ItemId" type="xsd:int"/>
                    <xsd:element name="BusinessUnitId" type="xsd:int"/>
                    <xsd:element name="MessageId" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="PatchFamilyId" type="xsd:string"/>
                    <xsd:element name="GpmSequenceId" type="xsd:int"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>75</pd:startX>
    <pd:startY>241</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>862</pd:endX>
    <pd:endY>240</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1498835459517</pd:targetNamespace>
    <pd:group name="RepeatOnError">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>220</pd:x>
        <pd:y>22</pd:y>
        <pd:width>459</pd:width>
        <pd:height>428</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>retryCountIndex</pd:indexSlot>
            <pd:errorCondition>$_globalVariables/pfx:GlobalVariables/MammothDb/CommandRetryCount &lt; $retryCountIndex</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>UpdateMessageSequence</pd:to>
            <pd:xpathDescription>PatchNumber &gt; 1</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-13382656</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Start/root/GpmSequenceId !=1</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>InsertMessageSequence</pd:to>
            <pd:xpathDescription>PatchNumber = 1</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16750849</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Start/root/GpmSequenceId = 1</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>InsertMessageSequence</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>UpdateMessageSequence</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>UpdateMessageSequence</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:xpathDescription>Error</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-3407872</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>InsertMessageSequence</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-3407872</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="UpdateMessageSequence">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>423</pd:x>
            <pd:y>94</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>true</emptyStrAsNil>
                <statement>UPDATE [gpm].[MessageSequence]
SET 
	GpmSequenceID = ?,
	GpmMessageID = ?,
	ModifiedDateUtc = SYSUTCDATETIME()
WHERE PatchFamilyID = ?</statement>
                <jdbcSharedConfig>/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>GpmSequenceID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>GpmMessageID</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>PatchFamilyID</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <GpmSequenceID>
                        <xsl:value-of select="$Start/root/GpmSequenceId"/>
                    </GpmSequenceID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/root/MessageId)">
                            <GpmMessageID>
                                <xsl:value-of select="$Start/root/MessageId"/>
                            </GpmMessageID>
                        </xsl:when>
                        <xsl:otherwise>
                            <GpmMessageID>
                                <xsl:value-of select="&quot;&quot;"/>
                            </GpmMessageID>
                        </xsl:otherwise>
                    </xsl:choose>
                    <PatchFamilyID>
                        <xsl:value-of select="$Start/root/PatchFamilyId"/>
                    </PatchFamilyID>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="InsertMessageSequence">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>439</pd:x>
            <pd:y>254</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>true</emptyStrAsNil>
                <jdbcSharedConfig>/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
                <statement>INSERT INTO [gpm].[MessageSequence] (ItemID, BusinessUnitID, PatchFamilyID, GpmSequenceID, GpmMessageID)
VALUES (?, ?, ?, ?, ?)</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>ItemID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>BusinessUnitID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>PatchFamilyID</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>GpmSequenceID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>GpmMessageID</parameterName>
                        <dataType>CHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <ItemID>
                        <xsl:value-of select="$Start/root/ItemId"/>
                    </ItemID>
                    <BusinessUnitID>
                        <xsl:value-of select="$Start/root/BusinessUnitId"/>
                    </BusinessUnitID>
                    <PatchFamilyID>
                        <xsl:value-of select="$Start/root/PatchFamilyId"/>
                    </PatchFamilyID>
                    <GpmSequenceID>
                        <xsl:value-of select="$Start/root/GpmSequenceId"/>
                    </GpmSequenceID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/root/MessageId)">
                            <GpmMessageID>
                                <xsl:choose>
                                    <xsl:when test="exists($Start/root/MessageId)">
                                        <xsl:value-of select="$Start/root/MessageId"/>
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </GpmMessageID>
                        </xsl:when>
                        <xsl:otherwise>
                            <GpmMessageID>
                                <xsl:value-of select="&quot;&quot;"/>
                            </GpmMessageID>
                        </xsl:otherwise>
                    </xsl:choose>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>296</pd:x>
            <pd:y>344</pd:y>
            <config/>
            <pd:inputBindings>
                <ns02:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/MammothDb/RetrySleepInMilliseconds"/>
                    </IntervalInMillisec>
                </ns02:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>439</pd:x>
            <pd:y>349</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns0:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </message>
                    <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns0:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>RepeatOnError</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>RepeatOnError</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>