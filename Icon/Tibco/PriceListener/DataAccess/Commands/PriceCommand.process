                <fullsource>package DataAccess.Commands.PriceCommand;
import java.util.*;
import java.io.*;
public class PriceCommandExponentialRetry{
/****** START SET/GET METHOD, DO NOT MODIFY *****/
	protected double retryCount = 0.0d;
	protected double result = 0.0d;
	public double getretryCount() {
		return retryCount;
	}
	public void setretryCount(double val) {
		retryCount = val;
	}
	public double getresult() {
		return result;
	}
	public void setresult(double val) {
		result = val;
	}
/****** END SET/GET METHOD, DO NOT MODIFY *****/
	public PriceCommandExponentialRetry() {
	}
	public void invoke() throws Exception {
/* Available Variables: DO NOT MODIFY
	In  : double retryCount
	Out : double result
* Available Variables: DO NOT MODIFY *****/
result=Math.pow(2,retryCount)*100L;
}
}
</fullsource>
