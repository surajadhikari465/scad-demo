<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>ESB Connection</name>
    <resourceType>ae.shared.JMSConnectionKey</resourceType>
    <config>
        <NamingEnvironment>
            <UseJNDI>true</UseJNDI>
            <ProviderURL>%%Connections/JMS/EsbJndiContextUrl%%</ProviderURL>
            <NamingURL>%%Connections/TCP/JMS/EsbEMS/EsbJndiContextUrl%%</NamingURL>
            <NamingInitialContextFactory>%%Connections/TCP/JMS/EsbEMS/EsbJNDIContextFactory%%</NamingInitialContextFactory>
            <TopicFactoryName>%%Connections/TCP/JMS/EsbEMS/EsbTopicConnectionFactory%%</TopicFactoryName>
            <QueueFactoryName>%%Connections/TCP/JMS/EsbEMS/EsbQueueConnectionFactory%%</QueueFactoryName>
            <NamingPrincipal>%%Connections/TCP/JMS/EsbEMS/EsbJndiUserName%%</NamingPrincipal>
            <NamingCredential>%%Connections/TCP/JMS/EsbEMS/EsbJndiPassword%%</NamingCredential>
        </NamingEnvironment>
        <ConnectionAttributes>
            <username>%%Connections/TCP/JMS/EsbEMS/EsbJmsUserName%%</username>
            <password>%%Connections/TCP/JMS/EsbEMS/EsbJmsPassword%%</password>
            <clientID/>
            <autoGenClientID>true</autoGenClientID>
        </ConnectionAttributes>
        <UseXACF>false</UseXACF>
        <useSsl>true</useSsl>
        <AdmFactorySslPassword>%%Connections/TCP/JMS/EsbEMS/EsbSslPassword%%</AdmFactorySslPassword>
        <ns0:ssl xmlns:ns0="http://www.tibco.com/xmlns/aemeta/services/2002">
            <ns0:trace>true</ns0:trace>
            <ns0:debugTrace>true</ns0:debugTrace>
            <ns0:verifyHostName>true</ns0:verifyHostName>
            <ns0:strongCipherSuitesOnly>true</ns0:strongCipherSuitesOnly>
            <ns0:expectedHostName>%%BW_GLOBAL_HOSTNAME%%</ns0:expectedHostName>
            <ns0:cert isRef="true">/SharedResources/Certificates/.folder</ns0:cert>
        </ns0:ssl>
    </config>
</BWSharedResource>