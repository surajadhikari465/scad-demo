<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:ns="http://schemas.wfm.com/Enterprise/ItemMgmt/BaseItem/V1" xmlns:pfx14="http://schemas.wfm.com/Mammoth/ErrorMessage.xsd" xmlns:pfx13="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pfx12="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:pfx11="http://schemas.wfm.com/Enterprise/AmountMgmt/Amount/V1" xmlns:pfx10="http://schemas.wfm.com/Enterprise/UnitOfMeasureMgmt/UnitOfMeasure/V2" xmlns:pfx5="http://schemas.wfm.com/Enterprise/LocaleMgmt/LocaleType/V2" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx4="http://schemas.wfm.com/Enterprise/LocaleMgmt/Locale/V2" xmlns:pfx3="http://schemas.wfm.com/Enterprise/ItemMgmt/ItemType/V1" xmlns:pfx2="http://schemas.wfm.com/Mammoth/MammothPrices.xsd" xmlns:pfx9="http://schemas.wfm.com/Enterprise/PriceMgmt/PriceType/V1" xmlns:pfx8="http://schemas.wfm.com/Enterprise/PriceMgmt/Price/V2" xmlns:pfx7="http://schemas.wfm.com/Enterprise/ItemMgmt/ScanCode/V2" xmlns:pfx6="http://schemas.wfm.com/Enterprise/ItemMgmt/ItemAttributesStore/V2" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns5="http://schemas.wfm.com/Mammoth/MessageHeaders.xsd" xmlns:pfx="http://schemas.wfm.com/Enterprise/ItemMgmt/Item/V1" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://schemas.wfm.com/Enterprise/RetailMgmt/CommonRefTypes/V1" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://schemas.wfm.com/Enterprise/ItemMgmt/Item/V1"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/MammothPrices.xsd"/>
    <pd:name>Processes/MammothPriceR10QueueReceiver.process</pd:name>
    <pd:startName>JMS Queue Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="JMS Queue Receiver">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>110</pd:x>
        <pd:y>103</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>23</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/PriceR10Queue%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/SharedResources/Connections/SB1.sharedjmscon</ConnectionReference>
            <ApplicationProperties>/SharedResources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>976</pd:endX>
    <pd:endY>112</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1506966744525</pd:targetNamespace>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>271</pd:x>
        <pd:y>105</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx2:MammothPrices"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SendToESB">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>419</pd:x>
        <pd:y>103</pd:y>
        <config>
            <processName>/Processes/SendToESB.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:copy-of select="$Parse-XML/pfx2:MammothPrices"/>
                <ns5:MessageHeaders>
                    <xsl:if test="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/TransactionID">
                        <ns5:TransactionID>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/TransactionID"/>
                        </ns5:TransactionID>
                    </xsl:if>
                    <xsl:if test="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/CorrelationID">
                        <ns5:CorrelationID>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/CorrelationID"/>
                        </ns5:CorrelationID>
                    </xsl:if>
                    <xsl:if test="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/SequenceID">
                        <ns5:SequenceID>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/SequenceID"/>
                        </ns5:SequenceID>
                    </xsl:if>
                    <xsl:if test="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/TransactionType">
                        <ns5:TransactionType>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/TransactionType"/>
                        </ns5:TransactionType>
                    </xsl:if>
                    <xsl:if test="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/Source">
                        <ns5:Source>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/Source"/>
                        </ns5:Source>
                    </xsl:if>
                    <ns5:nonReceivingSysName>
                        <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Application/NonReceivingSystems"/>
                    </ns5:nonReceivingSysName>
                    <xsl:if test="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/ResetFlag">
                        <ns5:ResetFlag>
                            <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/ResetFlag"/>
                        </ns5:ResetFlag>
                    </xsl:if>
                </ns5:MessageHeaders>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="LogSuccess">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>581</pd:x>
        <pd:y>106</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <pfx12:AppLog>
                <pfx12:AppId>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Application/AppID"/>
                </pfx12:AppId>
                <pfx12:Level>
                    <xsl:value-of select="&quot;Info&quot;"/>
                </pfx12:Level>
                <pfx12:Logger>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                </pfx12:Logger>
                <pfx12:UserName>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                </pfx12:UserName>
                <pfx12:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </pfx12:MachineName>
                <pfx12:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </pfx12:LogDate>
                <pfx12:Thread>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProcessId"/>
                </pfx12:Thread>
                <pfx12:Message>
                    <xsl:value-of select="concat(&quot;Successfully processed Mammoth Price  MessageID &quot;,&#xA;$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/TransactionID,&#xA;&quot;, mapped it to the ESB price canonical, and sent a message to ESB Queue ,&quot;,&#xA;$_globalVariables/ns3:GlobalVariables/Destinations/PriceMammothJustInTimeQueue)"/>
                </pfx12:Message>
                <pfx12:CallSite>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns4:ProcessContext/ProcessId)"/>
                </pfx12:CallSite>
            </pfx12:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Acknowledge Success">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>768</pd:x>
        <pd:y>118</pd:y>
        <config>
            <ConfirmEvent>JMS Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>238</pd:x>
        <pd:y>250</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="LogError">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>366</pd:x>
        <pd:y>251</pd:y>
        <config>
            <spawn>true</spawn>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
        </config>
        <pd:inputBindings>
            <pfx12:AppLog>
                <pfx12:AppId>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Application/AppID"/>
                </pfx12:AppId>
                <pfx12:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </pfx12:Level>
                <pfx12:Logger>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                </pfx12:Logger>
                <pfx12:UserName>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                </pfx12:UserName>
                <pfx12:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </pfx12:MachineName>
                <pfx12:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </pfx12:LogDate>
                <pfx12:Thread>
                    <xsl:value-of select="$_processContext/ns4:ProcessContext/ProcessId"/>
                </pfx12:Thread>
                <xsl:if test="$_error/ns4:ErrorReport/Msg">
                    <pfx12:Message>
                        <xsl:value-of select="$_error/ns4:ErrorReport/Msg"/>
                    </pfx12:Message>
                </xsl:if>
                <xsl:if test="$_error/ns4:ErrorReport/ProcessStack">
                    <pfx12:CallSite>
                        <xsl:value-of select="$_error/ns4:ErrorReport/ProcessStack"/>
                    </pfx12:CallSite>
                </xsl:if>
                <xsl:if test="$_error/ns4:ErrorReport/FullClass">
                    <pfx12:Exception>
                        <xsl:value-of select="$_error/ns4:ErrorReport/FullClass"/>
                    </pfx12:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns4:ErrorReport/StackTrace">
                    <pfx12:StackTrace>
                        <xsl:value-of select="$_error/ns4:ErrorReport/StackTrace"/>
                    </pfx12:StackTrace>
                </xsl:if>
            </pfx12:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Acknowledge Failure">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>736</pd:x>
        <pd:y>260</pd:y>
        <config>
            <ConfirmEvent>JMS Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>605</pd:x>
        <pd:y>254</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <pfx13:ActivityInput>
                <message>
                    <xsl:value-of select="$_error/ns4:ErrorReport/StackTrace"/>
                </message>
                <xsl:if test="$_error/ns4:ErrorReport/MsgCode">
                    <msgCode>
                        <xsl:value-of select="$_error/ns4:ErrorReport/MsgCode"/>
                    </msgCode>
                </xsl:if>
            </pfx13:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Publish Error Message">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>486</pd:x>
        <pd:y>260</pd:y>
        <config>
            <processName>/SharedResources/Processes/PublishErrorMessage.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <pfx14:ErrorMessage>
                <pfx14:Application>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Application/AppID"/>
                </pfx14:Application>
                <pfx14:MessageID>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/TransactionID"/>
                </pfx14:MessageID>
                <pfx14:MessageProperties>
                    <xsl:for-each select="$JMS-Queue-Receiver/ns1:ActivityOutput/OtherProperties/node()">
                        <pfx14:NameValuePair>
                            <xsl:attribute name="name">
                                <xsl:value-of select="local-name(current())"/>
                            </xsl:attribute>
                            <xsl:attribute name="value">
                                <xsl:value-of select="current()"/>
                            </xsl:attribute>
                        </pfx14:NameValuePair>
                    </xsl:for-each>
                </pfx14:MessageProperties>
                <pfx14:Message>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body"/>
                </pfx14:Message>
                <pfx14:ErrorCode>
                    <xsl:value-of select="$_error/ns4:ErrorReport/MsgCode"/>
                </pfx14:ErrorCode>
                <pfx14:ErrorDetails>
                    <xsl:value-of select="$_error/ns4:ErrorReport/Msg"/>
                </pfx14:ErrorDetails>
                <xsl:choose>
                    <xsl:when test="contains($_error/ns4:ErrorReport/Msg, &quot;Connection is closed&quot;)">
                        <pfx14:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </pfx14:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns4:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Invalid object name&quot;)">
                        <pfx14:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </pfx14:ErrorSeverity>
                    </xsl:when>
                    <xsl:when test="contains($_error/ns4:ErrorReport/Msg, &quot;com.microsoft.sqlserver.jdbc.SQLServerException: Cannot open database&quot;)">
                        <pfx14:ErrorSeverity>
                            <xsl:value-of select="&quot;Fatal&quot;"/>
                        </pfx14:ErrorSeverity>
                    </xsl:when>
                    <xsl:otherwise>
                        <pfx14:ErrorSeverity>
                            <xsl:value-of select="&quot;Error&quot;"/>
                        </pfx14:ErrorSeverity>
                    </xsl:otherwise>
                </xsl:choose>
            </pfx14:ErrorMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>JMS Queue Receiver</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>SendToESB</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendToESB</pd:from>
        <pd:to>LogSuccess</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogSuccess</pd:from>
        <pd:to>Acknowledge Success</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Acknowledge Success</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>LogError</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogError</pd:from>
        <pd:to>Publish Error Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Acknowledge Failure</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>Acknowledge Failure</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Publish Error Message</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>