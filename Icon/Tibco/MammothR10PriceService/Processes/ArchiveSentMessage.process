    <pd:activity name="LogError">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>474</pd:x>
        <pd:y>274</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns2:AppLog>
                <ns2:AppId>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Application/AppID"/>
                </ns2:AppId>
                <ns2:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </ns2:Level>
                <ns2:Logger>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns2:Logger>
                <ns2:UserName>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns2:UserName>
                <ns2:MachineName>
                    <xsl:value-of select="BW:getHostName()"/>
                </ns2:MachineName>
                <ns2:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns2:LogDate>
                <ns2:Thread>
                    <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns1:ProcessContext/ProcessId)"/>
                </ns2:Thread>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <ns2:Message>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns2:Message>
                </xsl:if>
                <ns2:CallSite>
                    <xsl:value-of select="$_error/ns1:ErrorReport/ProcessStack"/>
                </ns2:CallSite>
                <xsl:if test="$_error/ns1:ErrorReport/FullClass">
                    <ns2:Exception>
                        <xsl:value-of select="$_error/ns1:ErrorReport/FullClass"/>
                    </ns2:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/StackTrace">
                    <ns2:StackTrace>
                        <xsl:value-of select="$_error/ns1:ErrorReport/StackTrace"/>
                    </ns2:StackTrace>
                </xsl:if>
            </ns2:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>717</pd:x>
        <pd:y>273</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns3:ActivityInput>
