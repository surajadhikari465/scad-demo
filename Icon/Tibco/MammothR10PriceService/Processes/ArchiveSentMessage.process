    <pd:activity name="ArchiveMessageDetails">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>525</pd:x>
        <pd:y>136</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
            <batchUpdate>true</batchUpdate>
            <statement>INSERT INTO [gpm].[MessageArchivePriceR10] (ItemID, BusinessUnitID, PriceType, StartDate, MessageID, MessageDetailJson)
VALUES (?, ?, ?, ?, ?, ?)</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ItemID</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>BusinessUnitID</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>PriceType</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>StartDate</parameterName>
                    <dataType>TIMESTAMP</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageDetailJson</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <xsl:for-each select="$MessageArchiveList/AccumulatedOutput/ArchiveMessage">
                    <Record>
                        <ItemID>
                            <xsl:value-of select="ItemID"/>
                        </ItemID>
                        <BusinessUnitID>
                            <xsl:value-of select="BusinessUnitID"/>
                        </BusinessUnitID>
                        <PriceType>
                            <xsl:value-of select="PriceType"/>
                        </PriceType>
                        <StartDate>
                            <xsl:value-of select="StartDate"/>
                        </StartDate>
                        <MessageID>
                            <xsl:value-of select="MessageID"/>
                        </MessageID>
                        <MessageDetailJson>
                            <xsl:value-of select="MessageDetailJson"/>
                        </MessageDetailJson>
                    </Record>
                </xsl:for-each>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ArchiveXml">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>792</pd:x>
        <pd:y>134</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
            <statement>INSERT INTO [esb].[MessageArchive] (MessageID, MessageTypeID, MessageStatusID, MessageHeadersJson, MessageBody)
SELECT 
?,
(SELECT MessageTypeId FROM esb.MessageType WHERE MessageTypeName = 'Price') as MessageTypeID,
(SELECT MessageStatusId FROM esb.MessageStatus WHERE MessageStatusName = 'Sent') as MessageStatusID,
?,
?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>MessageID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageHeadersJson</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>MessageBody</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <MessageID>
                    <xsl:value-of select="$Start/root/MessageID"/>
                </MessageID>
                <MessageHeadersJson>
                    <xsl:value-of select="$JsonSerializeHeaders/ns0:ActivityOutputClass/jsonString"/>
                </MessageHeadersJson>
                <MessageBody>
                    <xsl:value-of select="tib:render-xml($Start/root/pfx3:items,1)"/>
                </MessageBody>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
