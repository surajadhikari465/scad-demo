<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns4="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns3="http://schemas.wfm.com/Mammoth/AppLog.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:ns12="http://xmlns.tibco.com/body" xmlns:pfx="http://www.tibco.com/schemas/ErrorMessagesMonitor/Schemas/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/ErrorMessagesMonitor/Schemas/Schema.xsd" schemaLocation="/Schemas/ErrorMessageMail.xsd"/>
    <pd:name>Processes/Send Error Notifications.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Application" type="xsd:string"/>
                    <xsd:element name="InstanceID" type="xsd:string"/>
                    <xsd:element name="ErrorCode" type="xsd:string"/>
                    <xsd:element name="NumberOfErrors" type="xsd:int"/>
                    <xsd:element name="ErrorSeverity" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>54</pd:startX>
    <pd:startY>56</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>866</pd:endX>
    <pd:endY>60</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1509974982351</pd:targetNamespace>
    <pd:activity name="Send Pager Duty Alert for Fatal Error">
        <pd:type>com.tibco.plugin.json.activities.RestActivity</pd:type>
        <pd:resourceType>ae.activities.RestActivity</pd:resourceType>
        <pd:x>640</pd:x>
        <pd:y>119</pd:y>
        <config>
            <enableProtocolUI>None</enableProtocolUI>
            <restMethodUI>POST</restMethodUI>
            <restResponseType>JSON</restResponseType>
            <authChoiceUI>No Authentication</authChoiceUI>
            <WADLPaths/>
            <WADLBaseURL/>
            <restMethodIDUI/>
            <restURI>https://events.pagerduty.com/generic/2010-04-15/create_event.json</restURI>
            <restInputReferNode>
                <xsd:element name="Parameters">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Query" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Header" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="Body" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Form" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Text" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Binary" minOccurs="0">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" type="xsd:base64Binary" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="Multipart" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="content-disposition" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="name" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="filename" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content-type" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content-id" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="content" minOccurs="0">
                                                        <xsd:complexType>
                                                            <xsd:choice>
                                                                <xsd:element name="binary" type="xsd:base64Binary"/>
                                                                <xsd:element name="text" type="xsd:string"/>
                                                                <xsd:element name="fileName" type="xsd:string"/>
                                                            </xsd:choice>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </restInputReferNode>
            <restOutputReferNode>
                <xsd:element name="header">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Server" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Allow" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Date" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Length" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Encoding" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Range" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Last-Modifie" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </restOutputReferNode>
        </config>
        <pd:inputBindings>
            <pfx2:ActivityInput>
                <pfx2:Parameters>
                    <Header>
                        <param>
                            <xsl:value-of select="concat(&quot;Authorization: 'Token token=[&quot;, $_globalVariables/ns4:GlobalVariables/Application/PagerDutyIntegrationKey, &quot;]&quot;)"/>
                        </param>
                    </Header>
                    <Body>
                        <Text>
                            <type>
                                <xsl:value-of select="&quot;application/json&quot;"/>
                            </type>
                            <content>
                                <xsl:value-of select="concat(&#xA;&#x9;&quot;{&quot;,&#xA;&#x9;concat(&quot;service_key:&quot;, '&quot;',$_globalVariables/ns4:GlobalVariables/Application/PagerDutyIntegrationKey, '&quot;'),&#xA;&#x9;concat(&quot;event_type:&quot;, '&quot;trigger&quot;'),&#xA;&#x9;concat(&quot;description:&quot;, '&quot;', $Start/Input/Application, &quot; &quot;, $Start/Input/NumberOfErrors, &quot; &quot;, $Start/Input/ErrorSeverity, &quot; &quot;, $Start/Input/ErrorCode, '&quot;'),&#xA;&#x9;&quot;}&quot;)"/>
                            </content>
                        </Text>
                    </Body>
                </pfx2:Parameters>
            </pfx2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Error Details">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>180</pd:x>
        <pd:y>56</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>select MessageID,
    ErrorDetails
from gpm.ErrorMessages
where InstanceID = ?
and Application = ?
and ErrorCode = ?
and ErrorSeverity = ?</statement>
            <wizardData/>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>InstanceID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Application</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>ErrorCode</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>ErrorSeverity</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>MessageID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>ErrorDetails</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <InstanceID>
                    <xsl:value-of select="$Start/Input/InstanceID"/>
                </InstanceID>
                <Application>
                    <xsl:value-of select="$Start/Input/Application"/>
                </Application>
                <ErrorCode>
                    <xsl:value-of select="$Start/Input/ErrorCode"/>
                </ErrorCode>
                <ErrorSeverity>
                    <xsl:value-of select="$Start/Input/ErrorSeverity"/>
                </ErrorSeverity>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>305</pd:x>
        <pd:y>56</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <term ref="pfx:ErrorMessages"/>
        </config>
        <pd:inputBindings>
            <pfx:ErrorMessages>
                <xsl:for-each select="$Get-Error-Details/resultSet/Record">
                    <pfx:ErrorMessage>
                        <pfx:MessageID>
                            <xsl:value-of select="MessageID"/>
                        </pfx:MessageID>
                        <pfx:ErrorDetails>
                            <xsl:value-of select="ErrorDetails"/>
                        </pfx:ErrorDetails>
                    </pfx:ErrorMessage>
                </xsl:for-each>
                <pfx:Application>
                    <xsl:value-of select="$Start/Input/Application"/>
                </pfx:Application>
                <pfx:ErrorCode>
                    <xsl:value-of select="$Start/Input/ErrorCode"/>
                </pfx:ErrorCode>
                <pfx:ErrorSeverity>
                    <xsl:value-of select="$Start/Input/ErrorSeverity"/>
                </pfx:ErrorSeverity>
            </pfx:ErrorMessages>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Send Mail">
        <pd:type>com.tibco.plugin.mail.MailPubActivity</pd:type>
        <pd:resourceType>ae.activities.MailActivityResource</pd:resourceType>
        <pd:x>647</pd:x>
        <pd:y>207</pd:y>
        <config>
            <newMimeSupport>true</newMimeSupport>
            <inputOutputVersion>5.2.0</inputOutputVersion>
            <host>%%Application/EmailHost%%</host>
            <InputHeaders>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </InputHeaders>
        </config>
        <pd:inputBindings>
            <ns1:mailActivityInput>
                <from>
                    <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/Application/EmailFrom"/>
                </from>
                <to>
                    <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/Application/EmailTo"/>
                </to>
                <subject>
                    <xsl:value-of select="concat('Application: ', $Start/Input/Application, ', Error Code: ', $Start/Input/ErrorCode, ' Error Severity: ', $Start/Input/ErrorSeverity)"/>
                </subject>
                <ns12:bodyElement>
                    <bodyText>
                        <xsl:value-of select="$Transform-Error-Details-to-HTML-for-Email/xmlString"/>
                    </bodyText>
                </ns12:bodyElement>
                <Headers>
                    <Content-Type>
                        <xsl:value-of select="&quot;text/html&quot;"/>
                    </Content-Type>
                </Headers>
            </ns1:mailActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Transform Error Details to HTML for Email">
        <pd:type>com.tibco.plugin.xml.XMLTransformActivity</pd:type>
        <pd:resourceType>ae.activities.XMLTransformActivity</pd:resourceType>
        <pd:x>430</pd:x>
        <pd:y>71</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <engine>Saxon</engine>
            <stylesheet>/Stylesheets/ErrorMessageMailStylesheet.xslt</stylesheet>
        </config>
        <pd:inputBindings>
            <textInput>
                <xmlString>
                    <xsl:value-of select="$Render-XML/xmlString"/>
                </xmlString>
            </textInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>54</pd:x>
        <pd:y>278</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>644</pd:x>
        <pd:y>279</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
            <spawn>true</spawn>
        </config>
        <pd:inputBindings>
            <ns3:AppLog>
                <ns3:AppId>
                    <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/Application/ApplicationID"/>
                </ns3:AppId>
                <ns3:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </ns3:Level>
                <ns3:Logger>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ns3:Logger>
                <ns3:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns3:LogDate>
                <ns3:Message>
                    <xsl:value-of select="&quot;Error occurred in ErrorMessagesMonitor&quot;"/>
                </ns3:Message>
                <xsl:if test="$_error/ns:ErrorReport/Msg">
                    <ns3:Exception>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </ns3:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns:ErrorReport/StackTrace">
                    <ns3:StackTrace>
                        <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                    </ns3:StackTrace>
                </xsl:if>
            </ns3:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Get Error Details</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>Transform Error Details to HTML for Email</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Transform Error Details to HTML for Email</pd:from>
        <pd:to>Send Mail</pd:to>
        <pd:xpathDescription>If Send Emails</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$_globalVariables/ns4:GlobalVariables/Application/SendEmails = "true"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Send Mail</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Send Pager Duty Alert for Fatal Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Transform Error Details to HTML for Email</pd:from>
        <pd:to>Send Pager Duty Alert for Fatal Error</pd:to>
        <pd:xpathDescription>If Fatal Error</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/Input/ErrorSeverity = "Fatal" and $_globalVariables/ns4:GlobalVariables/Application/SendPagerDutyAlerts = "true"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Get Error Details</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Transform Error Details to HTML for Email</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>