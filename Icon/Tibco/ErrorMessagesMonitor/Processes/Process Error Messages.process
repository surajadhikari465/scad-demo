<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/ErrorMessagesMonitor/Schemas/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:WFM.UID="java://UID" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:ns2="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns3="http://schemas.wfm.com/Mammoth/AppLog.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/ErrorMessagesMonitor/Schemas/Schema.xsd" schemaLocation="/Schemas/ErrorMessageMail.xsd"/>
    <pd:name>Processes/Process Error Messages.process</pd:name>
    <pd:startName>Timer</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Timer">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>84</pd:x>
        <pd:y>85</pd:y>
        <config>
            <StartTime>%%Application/TimerStartTime%%</StartTime>
            <Frequency>false</Frequency>
            <TimeInterval>%%Application/TimerTimeIntervalInMilliseconds%%</TimeInterval>
            <FrequencyIndex>Millisecond</FrequencyIndex>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>1043</pd:endX>
    <pd:endY>84</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <InstanceID>
            <xsd:element name="InstanceID" type="xsd:string"/>
        </InstanceID>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1509941665001</pd:targetNamespace>
    <pd:group name="Iterate Error Messages">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>554</pd:x>
        <pd:y>51</pd:y>
        <pd:width>163</pd:width>
        <pd:height>153</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Get-Error-Messages/resultSet/Record</pd:over>
            <pd:iterationElementSlot>errorRecord</pd:iterationElementSlot>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Send Error Notification</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Send Error Notification</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Send Error Notification">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>631</pd:x>
            <pd:y>136</pd:y>
            <config>
                <processName>/Processes/Send Error Notifications.process</processName>
            </config>
            <pd:inputBindings>
                <Input>
                    <Application>
                        <xsl:value-of select="$errorRecord/Record/Application"/>
                    </Application>
                    <InstanceID>
                        <xsl:value-of select="$InstanceID/InstanceID"/>
                    </InstanceID>
                    <ErrorCode>
                        <xsl:value-of select="$errorRecord/Record/ErrorCode"/>
                    </ErrorCode>
                    <NumberOfErrors>
                        <xsl:value-of select="$errorRecord/Record/NumberOfErrors"/>
                    </NumberOfErrors>
                    <ErrorSeverity>
                        <xsl:value-of select="$errorRecord/Record/ErrorSeverity"/>
                    </ErrorSeverity>
                </Input>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Get Error Messages">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>433</pd:x>
        <pd:y>84</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>select 
	Application,
	ErrorCode,
	ErrorSeverity,
	COUNT(*) NumberOfErrors
from gpm.ErrorMessages
where InstanceID = ?
group by Application, ErrorCode, ErrorSeverity</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>InstanceID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>Application</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>ErrorCode</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>ErrorSeverity</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-9</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>NumberOfErrors</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <InstanceID>
                    <xsl:value-of select="$InstanceID/InstanceID"/>
                </InstanceID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Mark Error Message Records As In Process">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>308</pd:x>
        <pd:y>99</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>update gpm.ErrorMessages
set InstanceID = ?
where ProcessedDateUtc is null
	and InstanceID is null	</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>InstanceID</parameterName>
                    <dataType>LONGNVARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <InstanceID>
                    <xsl:value-of select="$InstanceID/InstanceID"/>
                </InstanceID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Mark Error Messages As Processed">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>853</pd:x>
        <pd:y>101</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>update gpm.ErrorMessages
set ProcessedDateUtc = SYSUTCDATETIME()
where InstanceID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>InstanceID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <jdbcSharedConfig>/SharedResources/Connections/Mammoth.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <InstanceID>
                    <xsl:value-of select="$InstanceID/InstanceID"/>
                </InstanceID>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>97</pd:x>
        <pd:y>252</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Assign">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>191</pd:x>
        <pd:y>86</pd:y>
        <config>
            <variableName>InstanceID</variableName>
        </config>
        <pd:inputBindings>
            <InstanceID>
                <xsl:value-of select="WFM.UID:UID()"/>
            </InstanceID>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log Error">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>857</pd:x>
        <pd:y>250</pd:y>
        <config>
            <processName>/SharedResources/Processes/LogPublisher.process</processName>
        </config>
        <pd:inputBindings>
            <ns3:AppLog>
                <ns3:AppId>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Application/ApplicationID"/>
                </ns3:AppId>
                <ns3:Level>
                    <xsl:value-of select="&quot;Error&quot;"/>
                </ns3:Level>
                <ns3:Logger>
                    <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                </ns3:Logger>
                <ns3:LogDate>
                    <xsl:value-of select="current-dateTime()"/>
                </ns3:LogDate>
                <ns3:Message>
                    <xsl:value-of select="&quot;Error occurred&quot;"/>
                </ns3:Message>
                <xsl:if test="$_error/ns:ErrorReport/Msg">
                    <ns3:Exception>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </ns3:Exception>
                </xsl:if>
                <xsl:if test="$_error/ns:ErrorReport/StackTrace">
                    <ns3:StackTrace>
                        <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                    </ns3:StackTrace>
                </xsl:if>
            </ns3:AppLog>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Mark Error Message Records As In Process</pd:from>
        <pd:to>Get Error Messages</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Error Messages</pd:from>
        <pd:to>Iterate Error Messages</pd:to>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Iterate Error Messages</pd:from>
        <pd:to>Mark Error Messages As Processed</pd:to>
        <pd:lineType>S-Shaped</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Mark Error Messages As Processed</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Log Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Timer</pd:from>
        <pd:to>Assign</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Assign</pd:from>
        <pd:to>Mark Error Message Records As In Process</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>