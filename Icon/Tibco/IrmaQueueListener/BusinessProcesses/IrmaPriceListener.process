<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:pfx="http://schemas.wfm.com/Enterprise/PriceMgmt/Price/V2" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://www.tibco.com/CommonLE/Client/Schema/ClientInterface.xsd" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns4="http://services.wfm.com/Enterprise/DeadLetterException/V1" xmlns:ns3="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx5="http://www.tibco.com/schemas/PriceListener/Shared Resources/Schemas/Application/Schema.xsd" xmlns:pfx4="http://www.tibco.com/pe/EngineTypes" xmlns:pfx3="http://schemas.wfm.com/Mammoth/Error.xsd" xmlns:pfx2="http://schemas.wfm.com/Enterprise/ItemMgmt/Item/V1" xmlns:pfx8="http://schemas.wfm.com/Mammoth/IrmaDbPrice.xsd" xmlns:pfx7="http://www.tibco.com/schemas/IrmaQueueListener/Shared Resources/Schemas/Application/Schema.xsd" xmlns:pfx6="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd">
    <xsd:import namespace="http://schemas.wfm.com/Enterprise/PriceMgmt/Price/V2"/>
    <xsd:import namespace="http://schemas.wfm.com/Enterprise/PriceMgmt/Price/V2" schemaLocation="/Shared Resources/Schemas/wfm-2.3.0.1800/WholeFoodsMarket/Price/Price.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Enterprise/ItemMgmt/Item/V1" schemaLocation="/Shared Resources/Schemas/wfm-2.3.0.1800/WholeFoodsMarket/Item/Item.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/Error.xsd" schemaLocation="/Shared Resources/Schemas/Application/Error.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/mammothDbPrice.xsd" schemaLocation="/Shared Resources/Schemas/Application/MammothDbPrice.xsd"/>
    <xsd:import namespace="http://schemas.wfm.com/Mammoth/IrmaDbPrice.xsd" schemaLocation="/Shared Resources/Schemas/Application/IrmaPrice.xsd"/>
    <pd:name>BusinessProcesses/IrmaPriceListener.process</pd:name>
    <pd:startName>IrmaPriceMessageReceiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="IrmaPriceMessageReceiver">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>102</pd:x>
        <pd:y>41</pd:y>
        <config>
            <PermittedMessageType>Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>23</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/Queues/Price%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Shared Resources/Connections/ESB Price Connection.sharedjmscon</ConnectionReference>
            <ApplicationProperties>/Shared Resources/Properties/JMS Application Properties.sharedjmsapp</ApplicationProperties>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>593</pd:endX>
    <pd:endY>265</pd:endY>
    <pd:errorSchemas>
        <OutOfSequence ref="pfx3:Error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1495053049862</pd:targetNamespace>
    <pd:activity name="SendToIrmaQueue">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>441</pd:x>
        <pd:y>158</pd:y>
        <config>
            <processName>/BusinessProcesses/SendToIrmaQueue.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Map-Data-For-Irma/pfx8:IrmaPrice"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data For Irma">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>292</pd:x>
        <pd:y>156</pd:y>
        <config>
            <element ref="pfx8:IrmaPrice"/>
        </config>
        <pd:inputBindings>
            <pfx8:IrmaPrice>
                <pfx8:Item_Key>
                    <xsl:value-of select="$GetDataFromIrma/resultSet/Record[1]/Item_Key"/>
                </pfx8:Item_Key>
                <pfx8:Store_No>
                    <xsl:value-of select="$GetDataFromIrma/resultSet/Record[1]/Store_No"/>
                </pfx8:Store_No>
                <pfx8:Action>
                    <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:Action"/>
                </pfx8:Action>
                <xsl:if test="$GetDataFromIrma/resultSet/Record/PriceChgTypeID=1">
                    <pfx8:Multiple>
                        <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:Multiple"/>
                    </pfx8:Multiple>
                </xsl:if>
                <pfx8:Price>
                    <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:Price"/>
                </pfx8:Price>
                <pfx8:PricingChgTypeId>
                    <xsl:value-of select="$GetDataFromIrma/resultSet/Record[1]/PriceChgTypeID"/>
                </pfx8:PricingChgTypeId>
                <xsl:if test="$GetDataFromIrma/resultSet/Record/PriceChgTypeID !=1">
                    <pfx8:Sale_Multiple>
                        <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:Multiple"/>
                    </pfx8:Sale_Multiple>
                </xsl:if>
                <xsl:if test="$GetDataFromIrma/resultSet/Record/PriceChgTypeID !=11">
                    <pfx8:Sale_Price>
                        <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:Price"/>
                    </pfx8:Sale_Price>
                </xsl:if>
                <xsl:if test="$GetDataFromIrma/resultSet/Record/PriceChgTypeID !=11">
                    <pfx8:Sale_StartDate>
                        <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:StartDate"/>
                    </pfx8:Sale_StartDate>
                </xsl:if>
                <xsl:if test="$GetDataFromIrma/resultSet/Record/PriceChgTypeID !=1">
                    <xsl:if test="$Parse-Price-Message/pfx6:price/pfx6:EndDate">
                        <pfx8:Sale_EndDate>
                            <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:EndDate"/>
                        </pfx8:Sale_EndDate>
                    </xsl:if>
                </xsl:if>
                <pfx8:Unit_Id>
                    <xsl:value-of select="$GetDataFromIrma/resultSet/Record[&lt;&lt; Filter >>]/unit_ID"/>
                </pfx8:Unit_Id>
            </pfx8:IrmaPrice>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse Price Message">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>241</pd:x>
        <pd:y>48</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term ref="pfx6:price"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$IrmaPriceMessageReceiver/ns:ActivityOutput/Body"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Price Change Type">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>526</pd:x>
        <pd:y>54</pd:y>
        <config>
            <element>
                <xsd:element name="PriceChangeType" type="xsd:string"/>
            </element>
        </config>
        <pd:inputBindings>
            <PriceChangeType>
                <xsl:value-of select="if (upper-case($Parse-Price-Message/pfx6:price/pfx6:PriceType) =&quot;REG&quot;) then  &quot;REG&quot;&#xA; else &quot;SAL&quot;"/>
            </PriceChangeType>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GetDataFromIrma">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>95</pd:x>
        <pd:y>152</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Shared Resources/Connections/IRMA.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT TOP 1 Store_No, Item_Key ,PriceChgTypeID, itemunit.unit_ID
 FROM store, dbo.ItemIdentifier ,  dbo.pricechgtype, dbo.itemunit
WHERE businessUnit_ID =? AND identifier= ?
AND priceChgTypeDesc = ?
AND Unit_Abbreviation = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>businessUnitId</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>identifier</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>priceChangeType</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Abbreviation</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <wizardData/>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>Store_No</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>Item_Key</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>PriceChgTypeID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>-6</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>tinyint</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>unit_ID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <businessUnitId>
                    <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:BusinessUnit"/>
                </businessUnitId>
                <identifier>
                    <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:ItemId"/>
                </identifier>
                <priceChangeType>
                    <xsl:value-of select="$Map-Price-Change-Type/PriceChangeType"/>
                </priceChangeType>
                <Abbreviation>
                    <xsl:value-of select="$Parse-Price-Message/pfx6:price/pfx6:SellableUom"/>
                </Abbreviation>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Confirm On Success">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>587</pd:x>
        <pd:y>156</pd:y>
        <config>
            <ConfirmEvent>IrmaPriceMessageReceiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Catch All">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>170</pd:x>
        <pd:y>269</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Confirm On Error">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>443</pd:x>
        <pd:y>264</pd:y>
        <config>
            <ConfirmEvent>IrmaPriceMessageReceiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Map Data For Irma</pd:from>
        <pd:to>SendToIrmaQueue</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>IrmaPriceMessageReceiver</pd:from>
        <pd:to>Parse Price Message</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse Price Message</pd:from>
        <pd:to>Map Price Change Type</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Price Change Type</pd:from>
        <pd:to>GetDataFromIrma</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetDataFromIrma</pd:from>
        <pd:to>Map Data For Irma</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SendToIrmaQueue</pd:from>
        <pd:to>Confirm On Success</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm On Success</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm On Error</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>