<?xml version="1.0"?>
<configuration>
	<appSettings>
		<!-- Flag set to true if the database connection strings are encrypted -->
		<add key="encryptedConnectionStrings" value="false"/>
		<!-- specifies the current region-->
		<add key="Region" value="SW"/>
		<!--smtp server-->
		<add key="smtpServer" value="smtp.wholefoods.com"/>
		<!--Email FROM address-->
		<add key="EmailFrom" value="SW.irma.support@wholefoods.com"/>
		<!-- where are the custom config files for this region-->
		<add key="CustomConfigurationFilesPath" value="C:\Inetpub\wwwroot\IRMAWebServices\SW\"/>
		<!-- the relative path to the temp directory when files for FTP upload: NETWORK SERVICE must have MODIFY permissions-->
		<add key="TempDIRPath" value="C:\Inetpub\wwwroot\IRMAWebServices\temp\"/>
		<!-- where are the config files common to all regions (DO NOT CHANGE)-->
		<add key="CommonConfigurationFilesPath" value="C:\Inetpub\wwwroot\IRMAWebServices\Common\"/>
		<!-- a default delimiter, in case one isn't specified -->
		<add key="ExportDelimiter" value="|"/>
		<!-- what the cost file should be named -->
		<add key="COSTFilePrefix" value="COST"/>
		<!-- what the item file should be named -->
		<add key="ITEMFilePrefix" value="ITEM"/>
		<!-- what the plano file should be named-->
		<add key="PLANOFilePrefix" value="PLANO"/>
		<!-- what the plano file should be named-->
		<add key="InventoryFilePrefix" value="SW"/>
		<!-- the file extension for all exported WIMP files -->
		<add key="FileExt" value=".dat"/>
		<!-- the file extension for all exported Inventory files -->
		<add key="InvFileExt" value=".txt"/>
		<!--Automated POS Push file archive share-->
		<add key="POSPushArchivePath" value="\\iap-SW\POSPushArchive$"/>
		<!--Automated POS Push Archive filter expression - takes a parameter (optional) for partial file name matches -->
		<add key="POSPushArchiveFilter" value="{0}"/>
		<!--Automated POS Push Logs-->
		<add key="POSPushLogPath" value="\\iad-SW\POSPushLogs$"/>
		<!--Automated POS Push log filter expression-->
		<add key="POSPushLogFilter" value=".*\.log$"/>
		<!--Day to allow UpdateSalesSubByWeekly to run (DayOfWeek Int Value - 1(Monday) thru 7 (Sunday))-->
		<add key="TLOGRunAllowedOn" value="1"/>
		<!--TLOG parser log share-->
		<add key="TLOGEmailSubject" value="Daily TLOG Error Check"/>
		<!--tlog log file share -->
		<add key="TLOGParserLogPath" value="\\iad-SW\TLOGLogs$\"/>
		<!--tlog file filter expression -->
		<add key="TLOGParserLogFilter" value=".*\.log$"/>
		<!--takes a date parameter in ddmmyyyy format-->
		<add key="TLOGParserLogFileName" value="TlogParser-{0}.log"/>
		<!-- the VIM extract filename prefix -->
		<add key="VIMExtractPrefix" value="VIM"/>
		<!-- the VIM extract filename extension -->
		<add key="VIMFileExt" value=".dat"/>
	</appSettings>
	<connectionStrings>
		<!-- DATABASE CONNECTION -->
		<add name="ItemCatalog" providerName="System.Data.SqlClient" connectionString="Data Source=idd-sw;Initial Catalog=ItemCatalog_Test;User Id=IRSUser;Password=C0ffeeCup;"/>
	</connectionStrings>
	<system.web>
		<httpRuntime executionTimeout="3600"/>
		<!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.

            Visual Basic options:
            Set strict="true" to disallow all data type conversions 
            where data loss can occur. 
            Set explicit="true" to force declaration of all variables.
        -->
		<compilation debug="true" strict="false" explicit="true" targetFramework="4.0"/>
		<customErrors mode="Off"/>
		<pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID">
			<namespaces>
				<clear/>
				<add namespace="System"/>
				<add namespace="System.Collections"/>
				<add namespace="System.Collections.Specialized"/>
				<add namespace="System.Configuration"/>
				<add namespace="System.Text"/>
				<add namespace="System.Text.RegularExpressions"/>
				<add namespace="System.Web"/>
				<add namespace="System.Web.Caching"/>
				<add namespace="System.Web.SessionState"/>
				<add namespace="System.Web.Security"/>
				<add namespace="System.Web.Profile"/>
				<add namespace="System.Web.UI"/>
				<add namespace="System.Web.UI.WebControls"/>
				<add namespace="System.Web.UI.WebControls.WebParts"/>
				<add namespace="System.Web.UI.HtmlControls"/>
			</namespaces>
		</pages>
		<!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
        -->
		<authentication mode="Windows"/>
		<!--
            The <customErrors> section enables configuration 
            of what to do if/when an unhandled error occurs 
            during the execution of a request. Specifically, 
            it enables developers to configure html error pages 
            to be displayed in place of a error stack trace.

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
	</system.web>
</configuration>
