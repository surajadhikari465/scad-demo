<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="ApplicationGUID" value="987B3258-C3D0-4CE6-BEBF-D3A5B5652AC9" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EnvironmentGUID" value="20C5DDAC-659C-4B81-84F6-5F79CC390D10" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>

  <log4net>
    <root>
      <level value="DEBUG"  xdt:Transform="Replace"/>
    </root>
    <appender name="AdoNetAppender"
              type="log4net.Appender.AdoNetAppender"
              xdt:Locator="Match(name)">
      <commandText
        value=" exec dbo.AppLogInsertEntry @logDate, '987B3258-C3D0-4CE6-BEBF-D3A5B5652AC9', @thread, @logLevel, @logger, @message, @exception "
        xdt:Transform="Replace" />
    </appender>
  </log4net>

  <system.net>
    <mailSettings>
      <smtp>
        <network host="smtp.wholefoods.com" xdt:Transform="Replace"/>
      </smtp>
    </mailSettings>
  </system.net>
</configuration>