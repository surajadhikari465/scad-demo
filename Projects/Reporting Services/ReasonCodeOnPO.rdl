  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OrderHeader_ID">
            <Value>=Parameters!OrderHeader_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT rcd.reasoncode,
                rcd.reasoncodedesc
FROM   OrderItemRefused oir
INNER JOIN reasoncodedetail rcd
		ON oir.DiscrepancyCodeID = rcd.reasoncodedetailid
WHERE  oir.orderheader_id = @OrderHeader_ID
UNION   
SELECT DISTINCT rcd.reasoncode,
                rcd.reasoncodedesc
FROM   OrderItem oi
INNER JOIN reasoncodedetail rcd
		ON oi.ReasonCodeDetailID = rcd.reasoncodedetailid
WHERE  oi.orderheader_id = @OrderHeader_ID
ORDER  BY reasoncode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ReasonCode">
          <DataField>reasoncode</DataField>
