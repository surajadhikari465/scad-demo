  <DataSets>
    <DataSet Name="AuditDetails">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@subteam">
            <Value>=Parameters!subteam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@store">
            <Value>=Parameters!store.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@auditCategory">
            <Value>=Parameters!auditCategory.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@priceType">
            <Value>=Parameters!priceType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@showFilteredItems">
            <Value>=Parameters!showFilteredItems.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
-------------------------------------------------------------------
Update History
-------------------------------------------------------------------
-------------------------------------------------------------------
Tom Lux, 8/14/12, Performance Updates
1) Changed main AuditItems table to table-var instead of temp table, due to "cannot convert value 'Y' to int" errors in NE region.
2) Created local, indexed memory tables to replace/house data from the POSAuditException and Temp_PriceAudit tables.
3) Added IF/conditional checks around some data extraction (so they are skipped if not applicable, based on report parameters).
4) Moved 'Price Precision' data collection out of main select into AuditItems table and this data is now inserted into our new @POSAuditException_Indexed table.
5) Removed cast of identifiers to bigint during joins.  We need some data analysis to see where varchar identifier fields have insignificant leading characters (values that begin with '0').
6) Replaced other references to POSAuditException and Temp_PriceAudit tables with new local vars.
-------------------------------------------------------------------
-------------------------------------------------------------------
*/
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

print 'Starting: ' + convert(nvarchar, getdate(), 121);

-- Filters Store, PriceType, Subteam, Audit Category, Show-Filtered
/*
declare @store int; select @store = 221
declare @priceType int; select @priceType = null
declare @subteam int; select @subteam = 4300 -- 4300 NE groc
declare @auditCategory int; select @auditCategory = null
declare @showFilteredItems bit; select @showFilteredItems = 0
*/

declare @auditCategories table (
	auditId int not null
	,auditName varchar(128) not null
);

declare @PricePrecisionAuditCategory int; select @PricePrecisionAuditCategory = 100
declare @PricePrecisionAuditCategoryName varchar(64); select @PricePrecisionAuditCategoryName = 'Price Precision'

declare @UnreportedAuditCategoryOffset int; select @UnreportedAuditCategoryOffset = 100
declare @UnreportedAuditCategoryName varchar(64); select @UnreportedAuditCategoryName = 'Unreported/Filtered-Out'

declare @PriceCategoryID int
select top 1 @PriceCategoryID = POSAuditExceptionTypeID from POSAuditExceptionType where POSAuditExceptionTypeDesc like 'Price'

insert into @auditCategories
	select * from POSAuditExceptionType where POSAuditExceptionTypeid not in (2)
	union
	select @PricePrecisionAuditCategory, @PricePrecisionAuditCategoryName

/* Add unreported categories for each audit category. */
insert into @auditCategories
	select auditid + @UnreportedAuditCategoryOffset, @UnreportedAuditCategoryName + ' - ' + auditname from @auditCategories


declare @bizUnitID int;
select @bizUnitID = BusinessUnit_ID from store (nolock) where store_no = @store


/*
We're building an local, indexed table of audit exceptions and do not need the POSAuditExceptionKey (PK) or the SessionID fields
because they do not participate in the unique key; the remaining fields are the key/index we're building.
We build, then insert the 'Price Precision' exceptions into this table, which helps for join in main query that pulls data into local AuditItems table.
*/
declare @POSAuditException_Indexed table (
	[Item_Key] [int] NOT NULL,
	[Store_No] [int] NOT NULL,
	[POSAuditExceptionTypeID] [int] NOT NULL,
	[Identifier] [varchar](13) NOT NULL
	primary key nonclustered ([Store_No], [Item_Key], [POSAuditExceptionTypeID], [Identifier])
);

-- Pull core exceptions.
insert into @POSAuditException_Indexed
	select
		item_key
		,store_no
		,POSAuditExceptionTypeID
		,identifier
	from
		POSAuditException (nolock)
	where
		-- We only want to pull the data we're actually going to use, so we apply the report parameters/filters here.
		store_no = isnull(@store, store_no)
		and POSAuditExceptionTypeID = isnull(@auditCategory, POSAuditExceptionTypeID)

-- Add Price-Precision exceptions, if that audit category or *ALL* is selected for report.
if @auditCategory = @PricePrecisionAuditCategory or @auditCategory is null
begin
	insert into @POSAuditException_Indexed
		select
			Item_Key = i.item_key
			,Store_No = s.store_no
			,POSAuditExceptionTypeID = @PricePrecisionAuditCategory
			,Identifier = ii.identifier
		from price p (nolock) 
			left join store s (nolock) on s.store_no = p.store_no
			left join storeitem si (nolock) on p.item_key = si.item_key and p.store_no = si.store_no
			left join item i (nolock) on i.item_key = p.item_key -- For subteam filtering.
			left join itemidentifier ii (nolock) on ii.item_key = i.item_key -- For identifier.
			left join pricechgtype pct (nolock) on pct.pricechgtypeid = p.pricechgtypeid -- For on-sale flag.
		where
			-- Apply report parameters/filters.
			i.subteam_no = isnull(@subteam, i.subteam_no)
			and s.store_no = isnull(@store, s.store_no)
			and pct.pricechgtypeid = isnull(@priceType, pct.pricechgtypeid)
			-- ** End of report parameters/filters.
			and si.authorized = 1
			and ii.default_identifier = 1
			and (
				(	pct.on_sale = 0
					and (round(price*100.0, 0)/100.0) &lt;&gt; price
				) -- Not on sale and regular price invalid.
				or
				(	pct.on_sale = 1
					and (round(sale_price*100.0, 0)/100.0) &lt;&gt; sale_price
				) -- On sale and sale price invalid.
			) -- Price is invalid based on sale status.
end


/*
Design note: No records were found in Test (all regions) in Temp_PriceAudit table where biz-unit or identifier were NULL,
but subteam_no will be NULL for items that are in the POS but not in IRMA.
*/
declare @PriceAudit table (
	[Store_No] [int] NOT NULL,
	[BusinessUnit_ID] [int] NULL,
	[CompanyName] [varchar](50) NULL,
	[SubTeam_No] [int] NOT NULL,
	[Identifier] [varchar](13) NOT NULL,
	[Item_Description] [varchar](60) NULL,
	[CostUnitAmount] [decimal](9, 4) NULL,
	[CostUnit] [varchar](25) NULL,
	[RetailUnit] [varchar](25) NULL,
	[Cost] [smallmoney] NULL,
	[Price] [smallmoney] NULL,
	[Margin] [decimal](9, 4) NULL,
	[Cycle_Cnt_Dist] [decimal](18, 4) NULL,
	[Cycle_Cnt_Store] [decimal](18, 4) NULL,
	[POS_Unit_Price] [smallmoney] NULL,
	[POS_PricingMethod_ID] [int] NULL,
	[Sale_Price] [smallmoney] NULL,
	[On_Sale] [bit] NULL,
	[Multiple] [int] NULL,
	[Sale_Multiple] [int] NULL
	primary key nonclustered ([Store_No], [SubTeam_No], [Identifier])
);

-- Add 'Price' data, if that audit category or *ALL* is selected for report.
if @auditCategory = @PriceCategoryID or @auditCategory is null
begin
insert into @PriceAudit
	select 
		s.store_no
		,pa.BusinessUnit_ID
		,pa.CompanyName
		,isnull(pa.SubTeam_No, -1) -- This is part of our key in our local, indexed PriceAudit table, so we need non-null value to pickup the entries from the table for items in POS but not in IRMA.
		,pa.Identifier
		,pa.Item_Description
		,pa.CostUnitAmount
		,pa.CostUnit
		,pa.RetailUnit
		,pa.Cost
		,pa.Price
		,pa.Margin
		,pa.Cycle_Cnt_Dist
		,pa.Cycle_Cnt_Store
		,pa.POS_Unit_Price
		,pa.POS_PricingMethod_ID
		,pa.Sale_Price
		,pa.On_Sale
		,pa.Multiple
		,pa.Sale_Multiple
	from
		store s (nolock)
		join Temp_PriceAudit pa (nolock)
			on s.businessunit_id = pa.businessunit_id
	where
		s.store_no = isnull(@store, s.store_no)
		and pa.SubTeam_No = isnull(@subteam, pa.SubTeam_No)
		-- These extras are just to ensure no PK violations bringing data into local PriceAudit table.
		-- If any of these fields were blank, they wouldn't make sense, so okay to ignore anyway.
		and pa.businessunit_id is not null
		-- We're allowing nulls for subteam_no and we force dummy subteam_no above.
		and pa.Identifier is not null
end

/*
This table holds the pieces of the items to be displayed for the report.
*/
declare @auditItems table (
	id int identity(1,1)
	,auditId int not null
	,auditName varchar(128) not null
	,store_no int not null
	,store_name varchar(50) not null
	,pricechgtypeid int null
	,pricechgtypedesc varchar(20) null
	,IRMASubteam_no int null
	,POSSubteam_no int null
	,subteam_name varchar(100) null
	,item_key int not null
	,IRMAIdentifier varchar(13) null
	,POSIdentifier varchar(13) null
	,item_description varchar(60) null
	,pos_description varchar(26) null
	,brand_name varchar(25) null
	,IRMAMultiple tinyint null
	,IRMAPrice smallmoney null
	,POSMultiple tinyint null
	,POSPrice smallmoney null
	,pricebatchheaderid int null
	,batchDescription varchar(30) null
	,applyDate datetime null
	,processedDate datetime null
	,authorized bit null
	,discontinue_item bit null
	,deleted_item bit null
	,InIRMA bit not null
	,InPOS bit not null
	,IRMAFoodStamps bit null
	,POSFoodStamps bit null
	,IRMAScaleItem bit null
	,POSScaleItem bit null
	,IRMATaxFlags varchar(7) null
	,POSTaxFlags varchar(7) null
	,IRMAPricingMethodId int null
	,IRMAPricingMethod varchar(50) null
	,POSPricingMethodId int null
	,POSPricingMethod varchar(50) null
	primary key nonclustered (id, auditId, store_no, item_key)
);


if object_id('tempdb..#batchInfo') is not null
begin
	drop table #batchInfo
end

/* Table will hold list of price discrepancy items and all the pushed unique 'batch name+apply date' pairs. */
create table #batchInfo (
	id int identity(1,1)
	,store_no int not null
	,item_key int not null
	,processedDate datetime null
	primary key nonclustered (id, store_no, item_key)
);


/* Add all audit categories. */
insert into @auditItems
	select
		AuditId = ac.auditid
		,AuditName = ac.auditname
		,StoreNumber = isnull(s.store_no, -1)
		,Store = isnull(s.store_name, -1)
		,PriceTypeId = pct.pricechgtypeid
		,PriceType = pct.pricechgtypedesc
		,IRMASubteamNumber = st.subteam_no
		,POSSubteamNumber = pi.subteam_no
		,Subteam = st.subteam_name
		,ItemKey = isnull(i.item_key, -1)
		,IRMAIdentifier = ii.identifier--isnull(ii.identifier, -1)
		,POSIdentifier = pa.identifier--isnull(pa.identifier, -1)
		,IRMAItemDesc = i.item_description
		,POSItemDesc = pi.pos_description
		,Brand = ib.brand_name
		,IRMAMultiple = case when isnull(pa.on_sale, pct.on_sale) = 1 then isnull(pa.sale_multiple, p.sale_multiple) else isnull(pa.multiple, p.multiple) end
		,IRMAPrice = case when isnull(pa.on_sale, pct.on_sale) = 1 then isnull(pa.sale_price, p.sale_price) else isnull(pa.price, p.price) end
		,POSMultiple = pi.deal_quantity
		,POSPrice = case when pa.pos_unit_price = 0.01 then pi.deal_price else pa.pos_unit_price end
		,BatchId = null
		,BatchName = null
		,ApplyDate = null
		,processedDate = null
		,IsAuthed = si.authorized
		,IsDisco = dbo.fn_GetDiscontinueStatus(i.item_Key, NULL, NULL)
		,IsDeleted = i.deleted_item
		,InIRMA = case when i.item_key is not null then 1 else 0 end
		,InPOS = case when pi.item_key is not null then 1 else 0 end
		,IRMAFoodStamps = i.food_stamps
		,POSFoodStamps = pi.food_stamps
		,IRMAScaleItem = case when isnull(iu.weight_unit, 0) = 1 and dbo.fn_isscaleitem(ii.identifier) = 1 then 1 else (case when i.item_key is null then null else 0 end) end
		,POSScaleItem = pi.soldbyweight
		,IRMATaxFlags = convert(varchar(1), tf1.taxflagvalue) + '.' + convert(varchar(1), tf2.taxflagvalue) + '.' + convert(varchar(1), tf3.taxflagvalue) + '.' + convert(varchar(1), tf4.taxflagvalue)
		,POSTaxFlags = convert(varchar(1), pi.tax_table_a) + '.' + convert(varchar(1), pi.tax_table_b) + '.' + convert(varchar(1), pi.tax_table_c) + '.' + convert(varchar(1), pi.tax_table_d)
		,IRMAPricingMethod_ID = p.pricingmethod_id
		,IRMAPricingMethod = irmapm.pricingmethod_name
		,POSPricingMethod_ID = pi.pricingmethod_id
		,POSPricingMethod = pospm.pricingmethod_name
	from 
		@POSAuditException_Indexed pae
		join @auditCategories ac on pae.POSAuditExceptionTypeID = ac.auditId -- Match to updated/restricted audit list.
		left join itemidentifier ii (nolock) on ii.item_key = pae.item_key and ii.identifier = pae.identifier -- For identifier and is-scale-item.
		left join item i (nolock) on i.item_key = pae.item_key -- For subteam filtering.
		left join store s (nolock) on pae.store_no = s.store_no -- For store info and join to temp_PriceAudit.
		left join @priceaudit pa on s.store_no = isnull(pa.store_no, s.store_no) and i.subteam_no = isnull(pa.subteam_no, i.subteam_no) and ii.identifier = isnull(pa.identifier, ii.identifier) -- To get price values so we can identify true price discrepancies.
		left join price p (nolock) on p.store_no = s.store_no and p.item_key = i.item_key -- For price change type.
		left join pricechgtype pct (nolock) on pct.pricechgtypeid = p.pricechgtypeid -- For price change type desc.
		left join positem pi (nolock) on pi.store_no = s.store_no and pi.item_key = isnull(i.item_key, pi.item_key) and pi.identifier = pae.identifier -- For POS item info.
		left join subteam st (nolock) on i.subteam_no = st.subteam_no -- For subteam name.
		left join storeitem si (nolock) on si.item_key = i.item_key and si.store_no = s.store_no -- For authed flag.
		left join itembrand ib (nolock) on ib.brand_id = i.brand_id -- For brand name.
        left join itemunit iu (nolock) on iu.unit_id = i.retail_unit_id -- For is-scale-item.
		left join taxflag tf1 (nolock) on tf1.taxjurisdictionid = s.taxjurisdictionid and tf1.taxclassid = i.taxclassid and tf1.taxflagkey = 1 -- For tax flags.
		left join taxflag tf2 (nolock) on tf2.taxjurisdictionid = s.taxjurisdictionid and tf2.taxclassid = i.taxclassid and tf2.taxflagkey = 2  -- For tax flags.
		left join taxflag tf3 (nolock) on tf3.taxjurisdictionid = s.taxjurisdictionid and tf3.taxclassid = i.taxclassid and tf3.taxflagkey = 3 -- For tax flags.
		left join taxflag tf4 (nolock) on tf4.taxjurisdictionid = s.taxjurisdictionid and tf4.taxclassid = i.taxclassid and tf4.taxflagkey = 4  -- For tax flags.
		left join pricingmethod irmapm (nolock) on p.pricingmethod_id = irmapm.pricingmethod_id
		left join pricingmethod pospm (nolock) on pi.pricingmethod_id = pospm.pricingmethod_id
	where
		pae.POSAuditExceptionTypeID = isnull(@auditCategory, pae.POSAuditExceptionTypeID) --(select POSAuditExceptionTypeID from POSAuditExceptionType where POSAuditExceptionTypeDesc = 'Price')
		and pae.store_no = isnull(@store, pae.store_no)
		and isnull(i.subteam_no, -1) = isnull(@subteam, isnull(i.subteam_no, -1)) -- This will be null when item doesn't exist in IRMA (of course).
		and isnull(pct.pricechgtypeid, 0) = isnull(@priceType, isnull(pct.pricechgtypeid, 0)) -- We have to be careful when there is no IRMA price record.  This goes for items not in IRMA as well (of course).
		and
			(	lower(ac.auditName) &lt;&gt; 'price'
				or (
						lower(ac.auditName) = 'price'
						and
						(
							case when pa.on_sale = 1 then pa.sale_price else pa.price end &lt;&gt; pos_unit_price/pa.multiple -- For price audit, ensures prices do not match.
							or
							(	isnull(pi.pricingmethod_id, 0) &lt;&gt; 3
								or (
										isnull(pi.pricingmethod_id, 0) = 3
										and case when isnull(pa.on_sale, pct.on_sale) = 1 then isnull(pa.sale_price, p.sale_price) else isnull(pa.price, p.price) end &lt;&gt; pi.deal_price
									)
							) -- For RBX/IBM stores, pricing method 3 means POS price is in the deal price field, so we only include items where this is the case and where IRMA and POS prices don't match.
						) -- End AND condition for 'price' audit.
					)
			) 


print 'Done building audit items list: ' + convert(nvarchar, getdate(), 121);


/*
Remove 'price precision' items from 'Price' audit category, if that audit category or *ALL* is selected for report.
*/
if @auditCategory = @PriceCategoryID or @auditCategory is null
begin
	delete from @auditItems
	where
		auditId = @PriceCategoryID
		and round(irmaprice*100.0, 0)/100.0 = posprice -- We're deleting items where the rounded IRMA price matches the POS price.
end

print 'Done deleting from audit item list where rounded IRMA price matches POS: ' + convert(nvarchar, getdate(), 121);

/* Get items that have been pushed.  Get most recent apply date. */
insert into #batchInfo
	select
		ai.store_no
		,ai.item_key
		,max(h.processeddate)
	from @auditItems ai
		left join pricebatchdetail d (nolock) on d.store_no = ai.store_no and d.item_key = ai.item_key -- Match audit store and item to PBD.
		left join pricebatchheader h (nolock) on h.pricebatchheaderid = d.pricebatchheaderid -- For batch apply date.
	where
		h.pricebatchstatusid = 6 -- We want only completely processed (pushed) batches.
		and h.applydate is not null -- We want an apply date value.
		and auditId = @PriceCategoryID
	group by
		ai.store_no
		,ai.item_key


print 'Done with initial batch info retrieval: ' + convert(nvarchar, getdate(), 121);

/*
Get batch description and sent date for each item.
 */
update @auditItems
	set
		pricebatchheaderid = h.pricebatchheaderid
		,processeddate = bi.processeddate
		,applydate = h.applydate
		,batchdescription = h.batchdescription
from 
	@auditItems ai
	join #batchInfo bi on ai.store_no = bi.store_no and ai.item_key = bi.item_key
	join pricebatchdetail d (nolock) on bi.item_key = d.item_key and bi.store_no = d.store_no
	join pricebatchheader h (nolock) on h.pricebatchheaderid = d.pricebatchheaderid and h.processeddate = bi.processeddate
where
	h.pricebatchstatusid = 6 -- We want only completely processed (pushed) batches.
	and h.applydate is not null -- We want an apply date value.

print 'Done with 2nd batch info retrieval: ' + convert(nvarchar, getdate(), 121);


/*
Add filtered-out items here, if option is selected.
*/
if @showFilteredItems = 1
begin
insert into @auditItems (
	auditId
	,auditName
	,store_no
	,store_name
--	,pricechgtypeid
--	,pricechgtypedesc
	,IRMASubteam_no
	,POSSubteam_no
--	,subteam_name
	,item_key
	,IRMAIdentifier
	,POSIdentifier
	,item_description
	,pos_description
--	,brand_name
	,IRMAMultiple
	,IRMAPrice
	,POSMultiple
	,POSPrice
--	,pricebatchheaderid
	,batchDescription
--	,applyDate
--	,processedDate
--	,authorized
--	,discontinue_item
--	,deleted_item
	,InIRMA
	,InPOS
--	,IRMAFoodStamps
	,POSFoodStamps
--	,IRMAScaleItem
	,POSScaleItem
--	,IRMATaxFlags
--	,POSTaxFlags
--	,IRMAPricingMethodId
--	,IRMAPricingMethod
	,POSPricingMethodId
--	,POSPricingMethod
	)
	select
		auditId = pae.posauditexceptiontypeid + @UnreportedAuditCategoryOffset
		,auditName = @UnreportedAuditCategoryName + ' - ' + ac.auditname
		,store_no = pae.store_no
		,store_name = s.store_name
--		,pricechgtypeid
--		,pricechgtypedesc
		,IRMASubteam_no = pa.subteam_no
		,POSSubteam_no = pi.subteam_no
--		,subteam_name
		,item_key = pae.item_key
		,IRMAIdentifier = pae.identifier
		,POSIdentifier = pa.identifier
		,item_description = pa.item_description
		,pos_description = pi.pos_description
--		,brand_name
		,IRMAMultiple = case when pa.on_sale = 1 then pa.sale_multiple else pa.multiple end
		,IRMAPrice = pa.price
		,POSMultiple = pi.deal_quantity
		,POSPrice = pa.pos_unit_price
--		,pricebatchheaderid
		,batchDescription = cast(pi.deal_price as varchar) + ' (POS deal price)' -- **We're sticking the POS deal price in here.
--		,applyDate
--		,processedDate
--		,authorized
--		,discontinue_item
--		,deleted_item
		,InIRMA = case when pae.item_key &gt; 0 then 1 else 0 end
		,InPOS = case when ac.auditid &lt;&gt; 9 then 1 else 0 end
--		,IRMAFoodStamps
		,POSFoodStamps = pi.food_stamps
--		,IRMAScaleItem
		,POSScaleItem = pi.soldbyweight
--		,IRMATaxFlags
--		,POSTaxFlags
--		,IRMAPricingMethodId
--		,IRMAPricingMethod
		,POSPricingMethodId = pi.pricingmethod_id
--		,POSPricingMethod
	from
		--(select * from POSAuditException (nolock) where store_no = isnull(@store, store_no)) pae
		@POSAuditException_Indexed pae
		join @auditCategories ac on pae.POSAuditExceptionTypeID = ac.auditId -- Match to updated/restricted audit list.
		join store s (nolock) on pae.store_no = s.store_no
		left join positem pi (nolock) on pae.store_no = pi.store_no and pae.item_key = pi.item_key and pae.identifier = pi.identifier
		--left join temp_priceaudit pa (nolock) on pa.businessunit_id = s.businessunit_id and cast(pa.identifier as bigint) = cast(pae.identifier as bigint)
		left join @priceaudit pa on s.store_no = isnull(pa.store_no, s.store_no) and pae.identifier = isnull(pa.identifier, pae.identifier)
		left join @auditItems ai on pae.posauditexceptiontypeid = ai.auditId and pae.store_no = ai.store_no and pae.item_key = ai.item_key and pae.identifier = ai.irmaidentifier
	where
		pae.POSAuditExceptionTypeID = isnull(@auditCategory, pae.POSAuditExceptionTypeID)
		and pae.store_no = isnull(@store, pae.store_no)
		and isnull(pa.subteam_no, -1) = isnull(@subteam, isnull(pa.subteam_no, -1)) -- This will be null when item doesn't exist in IRMA (of course).
		and pae.item_key &gt; 0 -- We are assuming the 'Not in IRMA' category will NOT be filtered out because our join above fails on the identifier match and will report the items as filtered, when they truly aren't.
		and ai.item_key is null

print 'Done adding filtered items: ' + convert(nvarchar, getdate(), 121);
end


