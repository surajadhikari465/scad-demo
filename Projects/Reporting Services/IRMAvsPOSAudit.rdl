          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

select s.store_name 'Store Name', pae.store_no 'Store Number', et.POSAuditExceptionTypeId 'POS Audit Id', POSAuditExceptionTypeDesc 'POS Audit Category', count(*) 'Count'
from POSAuditException pae
join POSAuditExceptiontype et on pae.POSAuditExceptionTypeID = et.POSAuditExceptionTypeID
join store s on s.store_no = pae.store_no
where
	pae.POSAuditExceptionTypeID &lt;&gt; 2
	and pae.store_no = isnull(@store, pae.store_no)
group by store_name, pae.store_no, et.POSAuditExceptionTypeId, POSAuditExceptionTypeDesc
union
select store_name, isnull(@store, store_no), '-1', 'Total Unique Items With Exceptions', count(*) from
	(select distinct store_name, pae.store_no, item_key
	from POSAuditException pae join store s on s.store_no = pae.store_no 
	where pae.POSAuditExceptionTypeID &lt;&gt; 2
		and pae.store_no = isnull(@store, pae.store_no)
	) pae
group by store_name, store_no
order by 'Store Name', 'POS Audit Id'</CommandText>
