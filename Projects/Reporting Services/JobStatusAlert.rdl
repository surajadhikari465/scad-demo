<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">
  <DataSources>
    <DataSource Name="idt_ce">
      <DataSourceReference>idt-ce</DataSourceReference>
      <rd:DataSourceID>bfc8f646-3701-4301-93f7-996d2360c790</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Alerts">
          <DataField>Alerts</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>idt_ce</DataSourceName>
        <CommandText>SELECT 
STUFF(
(
SELECT
	',' + (Region + ': ' + ClassName)
FROM
(

	SELECT Region = 'FL', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-FL\FLP].[ItemCatalog].[dbo].[JobStatus]

	UNION

	SELECT Region = 'MA', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-MA\MAP].[ItemCatalog].[dbo].[JobStatus]

	UNION

	SELECT Region = 'MW', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-MW\MWP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'NA', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-NA\NAP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'NC', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-NC\NCP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'NE', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-NE\NEP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'PN', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-PN\PNP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'RM', ClassName, Status = UPPER(Status), LastRun, StatusDescription FROM [IDP-RM\RMP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'SO', ClassName, Status = UPPER(Status), LastRun, StatusDescription  FROM [IDP-SO\SOP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'SP', ClassName, Status = UPPER(Status), LastRun, StatusDescription  FROM [IDP-SP\SPP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'SW', ClassName, Status = UPPER(Status), LastRun, StatusDescription  FROM [IDP-SW\SWP].[ItemCatalog].[dbo].[JobStatus]

	UNION 

	SELECT Region = 'UK', ClassName, Status = UPPER(Status), LastRun, StatusDescription  FROM [IDP-UK\UKP].[ItemCatalog].[dbo].[JobStatus]
	) inner_result
WHERE
	LastRun IS NOT NULL
	AND DATEDIFF(YEAR, LastRun, GETDATE()) &lt; 1
	AND DATEDIFF(HOUR, LastRun, GETDATE()) &gt; 4 
	AND Status &lt;&gt; 'COMPLETE' 
FOR XML PATH('')),1,1,'') AS Alerts</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
