      <Textbox Name="txtFilter">
        <CanGrow>true</CanGrow>
        <KeepTogether>true</KeepTogether>
        <Paragraphs>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>="Filter: " &amp; Mid(IIf(IsNothing(Parameters!Vendor.Value), "", "; Vendor LIKE '*" &amp; Parameters!Vendor.Value &amp; "*'") 
	&amp; IIf(IsNothing(Parameters!ItemID.Value), "", "; Vendor Item ID = " &amp; Parameters!ItemID.Value)
	&amp; IIf(IsNothing(Parameters!SubTeamNo.Value), "", "; SubTeam = '" &amp; First(Fields!SubTeam_Name.Value, "GetSubTeamName")) &amp; "'"
	&amp; IIf(IsNothing(Parameters!ItemDesc.Value), "", "; Item Description LIKE '*" &amp; Parameters!ItemDesc.Value &amp; "*'")
	&amp; IIf(IsNothing(Parameters!Identifier.Value), "", "; Identifier LIKE '" &amp; Parameters!Identifier.Value &amp; "*'")
	&amp; IIf(IsNothing(Parameters!IncludeDiscontinuedItems.Value), "", "; Include Discontinued Items = " &amp; Parameters!IncludeDiscontinuedItems.Value)
	&amp; IIf(IsNothing(Parameters!WFMItemsOnly.Value), "", "; WFM Items only = " &amp; Parameters!WFMItemsOnly.Value), 3)</Value>
                <Style>
                  <FontSize>8pt</FontSize>
                  <Color>SlateGray</Color>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
        </Paragraphs>
