  <Code>Public Shared Function CalcGrossMargin(ByVal SalesAmount as Double, _
 ByVal AvgCost as Double) as Double
	If IsDBNull(SalesAmount)
		Return 0
	Else
		Return ((SalesAmount - AvgCost) _
		/ SalesAmount)
	End If
End Function

Public Shared Function CalcMix(ByVal SalesAmount as Double, _
 ByVal TotalSalesAmount as Double) as Double
	If IsDBNull(TotalSalesAmount) Or TotalSalesAmount = 0
		Return 0
	Else
		Return SalesAmount / TotalSalesAmount 
	End If
End Function

Public Shared Function CategoryGroupName(ByVal GroupName as String) _
as String
	If GroupName.Equals("ZZZZZZZZZZZZZZZZZZZZZZZZZ")
		Return "Total"
	Else
		Return GroupName
	End If
End Function</Code>
