      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>/*

	Return count of PO's with Invoice Dates prior to the Order Creation Date, grouped by Store and Subteam into 3 categories
		2-5 days
		6-10 days
		Greater than 10 days
*/
--DECLARE
--    @BeginDate varchar(10),
--    @EndDate varchar(18),
--    @Facility int,
--    @SubTeam_No int

--	SELECT @BeginDate = '5/4/2010',  @EndDate = '5/10/2010', @Facility = NULL
--	SET @EndDate = @EndDate + ' 23:59:59'


SELECT CompanyName, Subteam_Name
--	,  	OrderHeader_ID , OrderDate, InvoiceDate, DATEDIFF(d,InvoiceDate, OrderDate)
	, SUM(CASE WHEN DATEDIFF(d,InvoiceDate, OrderDate) BETWEEN 2 and 5 THEN 1 ELSE 0 END) as Two_To_Five_days
	, SUM(CASE WHEN DATEDIFF(d,InvoiceDate, OrderDate) BETWEEN 6 and 10 THEN 1 ELSE 0 END)  as Six_to_Ten_days
	, SUM(CASE WHEN DATEDIFF(d,InvoiceDate, OrderDate) &gt; 10 THEN 1 ELSE 0 END)as Over_ten_days
	
	
FROM OrderHeader oh
INNEr JOIN Subteam st
ON st.Subteam_No = oh.Transfer_To_Subteam
INNER JOIN Vendor vP 
ON vP.Vendor_ID = oh.PurchaseLocation_ID
WHERE OrderDate &gt; InvoiceDate
AND CloseDate IS NOT NULL
--And InvoiceDate BETWEEN @BeginDate and @EndDate
And OrderDate BETWEEN @BeginDate and @EndDate
GROUP BY CompanyName, Subteam_Name
Order by 1 ASC, 2 ASC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@BeginDate">
            <Value>=Parameters!BeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
