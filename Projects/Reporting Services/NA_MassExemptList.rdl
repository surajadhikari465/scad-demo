  <Code>'---------------------------------------------------------------------------------------------------------------------------
Public Shared Function FormatPackageSize(ByVal package_desc2 As Double) As String

    Dim sPackageSize As String
    
    'only display non-zero digits after decimal 
    If package_desc2 = Int(package_desc2) Then
        'remove any trailing decimal digits
        sPackageSize = String.Format("{0}", FormatNumber(package_desc2, 0))
    Else
        sPackageSize = String.Format("{0}", package_desc2)
    End If

    Return package_desc2

End Function
</Code>
