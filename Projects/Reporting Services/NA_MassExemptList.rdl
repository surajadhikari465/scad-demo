  <Description>Items exempt from price stickers by store and statute</Description>
  <Author>tim.pope</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ItemCatalog">
      <DataSourceReference>ItemCatalog</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>fa08f80e-233f-4930-9ae3-14cf25633d21</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsStores">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SELECT     S.Store_No, S.Store_Name
FROM         Store AS S WITH (nolock) INNER JOIN
                      Vendor AS V WITH (nolock) ON S.Store_No = V.Store_no
WHERE     (V.State = 'MA')
ORDER BY S.Store_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Store_No">
          <DataField>Store_No</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Store_Name">
          <DataField>Store_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsExempt">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Store_No">
            <Value>=Parameters!Store_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeExemptByStore">
            <Value>=Parameters!IncludeExemptByStore.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeExemptByStatute">
            <Value>=Parameters!IncludeExemptByStatute.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*----------------------------------------------------
 Exempt Items by Store
----------------------------------------------------
----------------------------------------------------
 Exempt Items by Store*/
SELECT DISTINCT 
                         S.Store_No, RTRIM(S.Store_Name) AS Store_Name, RTRIM(ST.SubTeam_Name) AS SubTeam_Name, II.Identifier, RTRIM(B.Brand_Name) AS Brand_Name, 
                         II.Item_Key, RTRIM(I.Item_Description) AS Item_Description, I.Package_Desc2, RTRIM(IU.Unit_Name) AS UOM, 
                         CASE WHEN PCT.On_Sale = 1 THEN P.POSSale_Price ELSE P.POSPrice END AS CurrentRetail, 
                         CASE WHEN PCT.On_Sale = 1 THEN P.Sale_Multiple ELSE P.Multiple END AS CurrentMultiple, PCT.On_Sale, dbo.fn_GetAlternateUPC(SIA.Item_Key) 
                         AS AlternateUPC
FROM            Store AS S WITH (NOLOCK) INNER JOIN
                         StoreItemAttribute AS SIA WITH (NOLOCK) ON S.Store_No = SIA.Store_No INNER JOIN
                         ItemIdentifier AS II WITH (NOLOCK) ON SIA.Item_Key = II.Item_Key INNER JOIN
                         Item AS I WITH (NOLOCK) ON SIA.Item_Key = I.Item_Key INNER JOIN
                         Vendor AS V WITH (NOLOCK) ON S.Store_No = V.Store_no INNER JOIN
                         Price AS P WITH (NOLOCK) ON S.Store_No = P.Store_No AND P.Item_Key = I.Item_Key INNER JOIN
                         StoreItem AS SI WITH (NOLOCK) ON SI.Store_No = P.Store_No AND SI.Item_Key = P.Item_Key INNER JOIN
                         PriceChgType AS PCT WITH (NOLOCK) ON PCT.PriceChgTypeID = P.PriceChgTypeId LEFT OUTER JOIN
                         SubTeam AS ST WITH (NOLOCK) ON I.SubTeam_No = ST.SubTeam_No LEFT OUTER JOIN
                         ItemBrand AS B WITH (NOLOCK) ON B.Brand_ID = I.Brand_ID LEFT OUTER JOIN
                         ItemUnit AS IU WITH (NOLOCK) ON IU.Unit_ID = I.Package_Unit_ID
WHERE        (SIA.Exempt = 1) AND (@IncludeExemptByStore = 1) AND (S.Store_No = @Store_No) AND (SI.Authorized = 1) AND (II.Default_Identifier = 1) AND (I.Deleted_Item = 0) 
                         AND (dbo.fn_GetDiscontinueStatus(I.Item_Key, NULL, NULL) = 0)
UNION
SELECT        0 AS Store_No, '&lt; STATUTE &gt;' AS Store_Name, RTRIM(ST.SubTeam_Name) AS SubTeam_Name, II.Identifier, RTRIM(B.Brand_Name) AS Brand_Name, II.Item_Key, 
                         RTRIM(I.Item_Description) AS Item_Description, I.Package_Desc2, RTRIM(IU.Unit_Name) AS UOM, 
                         CASE WHEN PCT.On_Sale = 1 THEN P.POSSale_Price ELSE P.POSPrice END AS CurrentRetail, 
                         CASE WHEN PCT.On_Sale = 1 THEN P.Sale_Multiple ELSE P.Multiple END AS CurrentMultiple, PCT.On_Sale, dbo.fn_GetAlternateUPC(II.Item_Key) 
                         AS AlternateUPC
FROM            ItemIdentifier AS II WITH (NOLOCK) INNER JOIN
                         Item AS I WITH (NOLOCK) ON I.Item_Key = II.Item_Key INNER JOIN
                         ItemAttribute AS IA WITH (NOLOCK) ON IA.Item_Key = II.Item_Key INNER JOIN
                         Price AS P WITH (NOLOCK) ON P.Item_Key = I.Item_Key INNER JOIN
                         StoreItem AS SI WITH (NOLOCK) ON SI.Store_No = P.Store_No AND SI.Item_Key = P.Item_Key INNER JOIN
                         PriceChgType AS PCT WITH (NOLOCK) ON PCT.PriceChgTypeID = P.PriceChgTypeId LEFT OUTER JOIN
                         SubTeam AS ST WITH (NOLOCK) ON I.SubTeam_No = ST.SubTeam_No LEFT OUTER JOIN
                         ItemBrand AS B WITH (NOLOCK) ON B.Brand_ID = I.Brand_ID LEFT OUTER JOIN
                         ItemUnit AS IU WITH (NOLOCK) ON IU.Unit_ID = I.Package_Unit_ID
WHERE        (IA.Check_Box_3 = 1) AND (@IncludeExemptByStatute = 1) AND (P.Store_No = @Store_No) AND (SI.Authorized = 1) AND (II.Default_Identifier = 1) AND 
                         (I.Deleted_Item = 0) AND (dbo.fn_GetDiscontinueStatus(I.Item_Key, NULL, NULL) = 0)
ORDER BY S.Store_No DESC, SubTeam_Name, II.Identifier</CommandText>
      </Query>
      <Fields>
        <Field Name="Store_No">
          <DataField>Store_No</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Store_Name">
          <DataField>Store_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SubTeam_Name">
          <DataField>SubTeam_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Brand_Name">
          <DataField>Brand_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_Key">
          <DataField>Item_Key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Item_Description">
          <DataField>Item_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Package_Desc2">
          <DataField>Package_Desc2</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="UOM">
          <DataField>UOM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CurrentRetail">
          <DataField>CurrentRetail</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CurrentMultiple">
          <DataField>CurrentMultiple</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="On_Sale">
          <DataField>On_Sale</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="AlternateUPC">
          <DataField>AlternateUPC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsExemptByStore">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Store_No">
            <Value>=Parameters!Store_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeExemptByStore">
            <Value>=Parameters!IncludeExemptByStore.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*----------------------------------------------------
 Exempt Items by Store
----------------------------------------------------
----------------------------------------------------
 Exempt Items by Store*/
SELECT     COUNT(*) AS ExemptCount
FROM         Store AS S WITH (NOLOCK) INNER JOIN
                      StoreItemAttribute AS SIA WITH (NOLOCK) ON S.Store_No = SIA.Store_No INNER JOIN
                      ItemIdentifier AS II WITH (NOLOCK) ON SIA.Item_Key = II.Item_Key INNER JOIN
                      Item AS I WITH (NOLOCK) ON SIA.Item_Key = I.Item_Key INNER JOIN
                      Vendor AS V WITH (NOLOCK) ON S.Store_No = V.Store_no INNER JOIN
                      Price AS P WITH (NOLOCK) ON S.Store_No = P.Store_No AND P.Item_Key = I.Item_Key INNER JOIN
                      StoreItem AS SI WITH (NOLOCK) ON SI.Store_No = P.Store_No AND SI.Item_Key = P.Item_Key INNER JOIN
                      PriceChgType AS PCT WITH (NOLOCK) ON PCT.PriceChgTypeID = P.PriceChgTypeId LEFT OUTER JOIN
                      SubTeam AS ST WITH (NOLOCK) ON I.SubTeam_No = ST.SubTeam_No LEFT OUTER JOIN
                      ItemBrand AS B WITH (NOLOCK) ON B.Brand_ID = I.Brand_ID LEFT OUTER JOIN
                      ItemUnit AS IU WITH (NOLOCK) ON IU.Unit_ID = I.Package_Unit_ID
WHERE     (SIA.Exempt = 1) AND (S.Store_No = @Store_No) AND (SI.Authorized = 1) AND (@IncludeExemptByStore = 1) AND (II.Default_Identifier = 1) AND 
                      (I.Deleted_Item = 0) AND (dbo.fn_GetDiscontinueStatus(I.Item_Key, NULL, NULL) = 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="ExemptCount">
          <DataField>ExemptCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
