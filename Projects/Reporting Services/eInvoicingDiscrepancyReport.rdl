  <DataSets>
    <DataSet Name="Query">
      <Fields>
        <Field Name="PONumber">
          <DataField>PONumber</DataField>
        </Field>
        <Field Name="InvoiceNumber">
          <DataField>InvoiceNumber</DataField>
        </Field>
        <Field Name="OrderItemID">
          <DataField>OrderItemID</DataField>
        </Field>
        <Field Name="Item">
          <DataField>Item</DataField>
        </Field>
        <Field Name="Order_UOM">
          <DataField>Order UOM</DataField>
        </Field>
        <Field Name="EInvoice_UOM">
          <DataField>EInvoice UOM</DataField>
        </Field>
        <Field Name="Order_Pack">
          <DataField>Order Pack</DataField>
        </Field>
        <Field Name="EInvoice_Pack">
          <DataField>EInvoice Pack</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SELECT 
	[PONumber]	= oh.OrderHeader_ID, 
	[InvoiceNumber]	= oh.InvoiceNumber,
	[OrderItemID]	= oi.OrderItem_ID,
	[Item]		= i.Item_Description,
	[Order UOM]	= oq.Unit_Name,
	[EInvoice UOM]	= eq.Unit_Name,
                [Order Pack]            = oi.Package_Desc1,
                [EInvoice Pack]        = ei.Case_Pack
FROM 
	OrderHeader					(nolock) oh
	INNER JOIN Vendor			(nolock) v	ON	v.Vendor_ID				= oh.ReceiveLocation_ID
	INNER JOIN OrderItem		(nolock) oi	ON	oi.OrderHeader_ID		= oh.OrderHeader_ID 
											AND oi.InvoiceCost			IS NOT NULL			-- There is eInvoice info loaded
	INNER JOIN eInvoicing_Item  (nolock) ei ON	oh.eInvoice_ID			= ei.eInvoice_ID 
											AND ei.item_key				= oi.Item_Key
	INNER JOIN Item				(nolock) i	ON	oi.Item_Key				= i.Item_Key
	INNER JOIN ItemUnit			(nolock) oq	ON	oi.QuantityUnit			= oq.Unit_ID
	INNER JOIN ItemUnit			(nolock) eq	ON	oi.InvoiceQuantityUnit	= eq.Unit_ID
WHERE 
	oh.InvoiceDate			&gt;= DATEADD(d, -1, GETDATE())
	AND (
		(oi.Package_Desc1	&lt;&gt; ei.case_pack)
		OR 
		(oi.QuantityUnit	&lt;&gt; oi.InvoiceQuantityUnit)
		)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
