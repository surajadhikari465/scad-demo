  <DataSets>
    <DataSet Name="ItemAdjustments">
      <Fields>
        <Field Name="Store_No">
          <DataField>Store_No</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StoreName">
          <DataField>StoreName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_Key">
          <DataField>Item_Key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Weight">
          <DataField>Weight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AvgCost">
          <DataField>AvgCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Item_Description">
          <DataField>Item_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SubTeam_Name">
          <DataField>SubTeam_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Adjustment_ID">
          <DataField>Adjustment_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AdjustmentType">
          <DataField>AdjustmentType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AdjustmentReason">
          <DataField>AdjustmentReason</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalCost">
          <Value>=(Fields!Quantity.Value + Fields!Weight.Value) * Fields!AvgCost.Value</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>/*


Note: 'TotalCost' field comes from calculated-value dataset parameter.


##############################
Mod History
##############################

---------------------------------------------------------------------------------------------------------------------------------
TFS 11516
Tom Lux
12/31/09
Changed report dataset from stored proc call to this query in the report itself.
Converted sub-selects to joins.
Added 'AdjustmentType' column (and required join to ItemAdjustment) to link the adjustment ID to a readable description.
Added adj type field to report.
---------------------------------------------------------------------------------------------------------------------------------

*/

select
	Store_No = ih.store_no, 
	StoreName = v.companyname,
	--replaced with join and line above (SELECT ISNULL(Vendor.CompanyName, '') FROM Vendor (nolock) WHERE Store_No = @Store_No) as StoreName, 
	ih.Item_Key,
	Quantity = ih.quantity,
	Weight = ih.weight,
	AvgCost = ISNULL(dbo.fn_AvgCostHistory(ih.Item_Key, ih.Store_No, ih.SubTeam_No, ih.DateStamp), 0),
	Item_Description = i.item_description,
	Identifier = ii.identifier, 
	SubTeam_Name = st.subteam_name,
	-- replaced with join and line above (SELECT SubTeam_Name FROM SubTeam (nolock) WHERE SubTeam_No = ISNULL(@SubTeam_No, ItemHistory.SubTeam_No)) As SubTeam_Name, 
	Adjustment_ID = ih.Adjustment_ID,
	AdjustmentType = ia.adjustment_name,
	AdjustmentReason = ih.AdjustmentReason
FROM ItemHistory ih (nolock) 
	join
		Item i (nolock)
		ON ih.Item_Key = i.Item_Key
	join
		ItemIdentifier ii (nolock) 
		ON ii.Item_Key = i.Item_Key AND ii.Default_Identifier = 1
	join
		subteam st (nolock)
		on ih.subteam_no = st.subteam_no
	join
		vendor v (nolock)
		on ih.store_no = v.store_no
	join
		itemadjustment ia (nolock)
		on ih.adjustment_id = ia.adjustment_id
WHERE
	DateStamp &gt;= @BeginDate AND DateStamp &lt; DATEADD(d,1,@EndDate) AND
	ih.SubTeam_No = ISNULL(@SubTeam_No, ih.SubTeam_No) AND 
	ih.Store_No = @Store_No AND
	ih.Adjustment_ID IN (1, 8, 14)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@BeginDate">
            <Value>=Parameters!BeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SubTeam_No">
            <Value>=Parameters!SubTeam_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Store_No">
            <Value>=Parameters!Store_No.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
