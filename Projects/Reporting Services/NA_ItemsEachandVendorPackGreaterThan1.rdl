  <rd:ReportID>01f231fb-5133-4368-8fba-e491e162b2f2</rd:ReportID>
  <DataSets>
    <DataSet Name="dsIRMA">
      <Fields>
        <Field Name="identifier">
          <DataField>identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subteam_name">
          <DataField>subteam_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CaseSize">
          <DataField>CaseSize</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="companyname">
          <DataField>companyname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CaseCost">
          <DataField>CaseCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsIRMA</DataSourceName>
        <CommandText>select ii.identifier, st.subteam_name, SIVC.CaseSize, v.companyname, SIVC.CaseCost
from item i 
inner join itemidentifier ii on ii.item_key = i.item_key 
inner join storeitem si on si.item_key = i.item_key 
inner join storeitemvendor siv on siv.store_no = si.store_no
		and siv.item_key = si.item_key 
inner join subteam st on st.subteam_no = i.subteam_no
inner join vendor v on v.vendor_id = siv.vendor_id 
inner join itemunit iu on iu.unit_id = i.cost_unit_id
			and iu.unit_name = 'EACH'
inner join 
	(SELECT MVCH.Store_No, MVCH.Item_Key, MVCH.Vendor_ID, VCH.CostUnit_ID, VCH.FreightUnit_ID,
		[CaseSize] = VCH.Package_Desc1,
		[CaseCost] = (CASE VendorDealTypeID
						WHEN 1 THEN VCH.UnitCost - VDH.CaseAmt					-- allowance ($)
						WHEN 2 THEN (1 - VDH.CaseAmt / 100) * VCH.UnitCost		-- discount (%)
						ELSE VCH.UnitCost
					END),
		FromVendor, StartDate, UnitCost
	FROM (SELECT SIV.Store_No, SIV.Item_Key,  SIV.Vendor_ID, 
			[MaxVCHID] = 
				(SELECT TOP 1 VCH0.VendorCostHistoryID
				FROM dbo.VendorCostHistory VCH0 (NOLOCK)
					INNER JOIN dbo.StoreItemVendor SIV0 (NOLOCK) ON SIV0.StoreItemVendorID = VCH0.StoreItemVendorID
				WHERE SIV0.Store_No = SIV.Store_No 
					AND SIV0.Item_Key = SIV.Item_Key 
					AND SIV0.Vendor_ID = SIV.Vendor_ID
					AND getdate() BETWEEN VCH0.StartDate AND VCH0.EndDate
					AND (SIV0.DeleteDate IS NULL OR getdate() &lt; SIV0.DeleteDate)
				ORDER BY VCH0.Promotional DESC, VCH0.VendorCostHistoryID DESC)
			FROM dbo.StoreItemVendor SIV (NOLOCK)
			WHERE SIV.PrimaryVendor = 1
			AND SIV.Store_No NOT IN (925,998,279,280)
			GROUP BY SIV.Store_No, SIV.Item_Key, SIV.Vendor_ID
			) MVCH
		LEFT JOIN dbo.VendorCostHistory VCH (NOLOCK) ON VCH.VendorCostHistoryID = MVCH.MaxVCHID
		LEFT JOIN 
			(SELECT StoreItemVendorID, VendorDealTypeID, [CaseAmt] = SUM(ISNULL(CaseAmt, 0))
			FROM dbo.VendorDealHistory (NOLOCK) 
			WHERE CostPromoCodeTypeID IN (2, 4, 5, 6)
				AND getdate() BETWEEN StartDate AND EndDate
			GROUP BY StoreItemVendorID, VendorDealTypeID
			) VDH ON VDH.StoreItemVendorID = VCH.StoreItemVendorID
		) SIVC ON SIVC.Store_No = si.Store_No AND SIVC.Item_Key = si.Item_Key
where	si.store_no = @StoreNo
and		SIVC.CaseSize &gt; 1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StoreNo">
            <Value>=Parameters!StoreNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Stores">
      <Fields>
        <Field Name="store_name">
          <DataField>store_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="store_no">
          <DataField>store_no</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsIRMA</DataSourceName>
        <CommandText>select store_name, store_no from store order by store_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
