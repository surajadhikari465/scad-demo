  <Code>    Public Shared Function FormatIdentifier(ByVal identifier As String, ByVal checkdigit as String) _
    As String
        Dim result As String

        If identifier.Length &gt;= 12 Then
            result = identifier.Substring(0, 12) + checkdigit
        Else
            result = identifier.PadLeft(12, "0") + checkdigit
        End If

        Return result

    End Function

    Public Shared Function FormatItemKey(ByVal item_key As Integer) _
    As String
        Dim strItemKey as String
        Dim result As String
        
        strItemKey = CType(item_key, String)

        If strItemKey.Length &gt;= 12 Then
            result = strItemKey.Substring(0, 12)
        Else
            result = strItemKey.PadLeft(12, "0")
        End If

        Return result

    End Function

    Public Shared Function FormatVendorItemID(ByVal VendorItemID As String) _
    As String

        Dim result As String
        
        If VendorItemID.Trim.Length &gt;= 12 Then
            result = VendorItemID.Trim.Substring(0, 12)
        Else
            result = VendorItemID.Trim.PadLeft(12, "0")
        End If

        Return result

    End Function

    Public Shared Function FormatPricePerUOM( _
        ByVal price As Double, _
        ByVal package_desc2 As Double, _
        ByVal package_unit As String) As String

        Dim result As String
        Dim multiplier As Double
        Dim new_price As Double
        Dim new_package_desc2 As String
        multiplier = 1.0

        If price &lt;&gt; Nothing And price &gt; 0.0 Then
            If package_desc2 &lt;&gt; Nothing And package_desc2 &gt; 0.0 Then
                Select Case Trim(package_unit)
                    Case "g"
                        multiplier = 100.0 / package_desc2
                        new_package_desc2 = "100 "
                    Case "ml"
                        multiplier = 100.0 / package_desc2
                        new_package_desc2 = "100 "
                    Case Else
                        multiplier = 1.0
                        new_package_desc2 = FormatNumber(package_desc2, 0).ToString + " "
                End Select

                new_price = multiplier * price
            Else
                new_price = price
                new_package_desc2 = "unknown "
            End If

            'result = FormatNumber(new_price, 2).ToString + " per " + new_package_desc2 + package_unit
            result = FormatPriceDisplay(new_price) + " per " + new_package_desc2 + package_unit
        Else
            If package_desc2 &lt;&gt; Nothing And package_desc2 &gt; 0.0 Then
                new_package_desc2 = FormatNumber(package_desc2, 0).ToString + " "
                result = "Unknown " + " per " + new_package_desc2 + package_unit
            Else
                result = "Unknown " + " per " + "unknown " + package_unit
            End If
        End If

        Return result

    End Function


    Public Shared Function FormatPriceDisplay(ByVal Price As Double) _
    As String

        Dim sDisplayPrice as String
        
        'compare to 0.995 since that will be rounded up to 1.00
        If Price &lt; 0.995 Then
            'multiply by 100 and display as Pence rather than Pound
            sDisplayPrice = FormatNumber(Price * 100, 0) &amp; "p"
        Else
            'display price to 2 decimal places without the Pound symbol
            sDisplayPrice = FormatNumber(Price, 2)
        End If

        Return sDisplayPrice

    End Function</Code>
