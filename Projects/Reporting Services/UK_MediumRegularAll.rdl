  <DataSets>
    <DataSet Name="dsPrintSigns">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Store_No">
            <Value>=Parameters!StoreNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OutputLabelSizeID">
            <Value>=Parameters!LabelType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Team_No">
            <Value>=Parameters!TeamNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SubTeam_No">
            <Value>=Parameters!SubTeamNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category_ID">
            <Value>=Parameters!CategoryID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-------------------------------------------------------
-- LOCAL VARIABLES
-------------------------------------------------------
DECLARE 
	    @TeamNo_WholeBody int,
	    @LabelTypeID_Small int

SELECT @TeamNo_WholeBody = Team_No 
FROM Team (NOLOCK) 
WHERE Team_Name = 'Whole Body'

SELECT @LabelTypeID_Small = LabelType_ID 
FROM LabelType (NOLOCK) 
WHERE LabelTypeDesc = 'SMLL'

-------------------------------------------------------
-- OUTPUT QUERY
-------------------------------------------------------
SELECT
	-------------------------------------------------
--	T.Team_Name, 
--	ST.SubTeam_Name, 
--	IC.Category_Name, 
--	V.CompanyName,
	-------------------------------------------------
	I.Category_ID,
	I.LabelType_ID,
	II.Identifier, 
	II.CheckDigit,
	I.Sign_Description, 
	I.Package_Desc1, 
	I.Package_Desc2, 
	[Package_Unit] = IU.Unit_Abbreviation, 
	V.Vendor_Key,
	IV.Item_ID AS VendorItemID,
	[CurrentPrice] = 
		dbo.fn_ShelfTags_GetCurrentPrice(CASE WHEN P.On_Sale = 1 THEN 2 ELSE NULL END, P.PricingMethod_ID, P.POSPrice, P.POSSale_Price),
	[Origin_Name] = NULL, 
	[Brand_Name] = NULL, 
	[Ingredients] = NULL,
	[Multiple] = NULL, 
	[Price] = NULL, 
	[MSRPPrice] = NULL, 
	[Sale_Multiple] = NULL, 
	[Sale_Price] = NULL, 
	[Sale_Start_Date] = NULL, 
	[Sale_End_Date] = NULL, 
	[Retail_Unit_Abbr] = NULL, 
	[Retail_Unit_Full] = NULL, 
	[Sold_By_weight] = NULL, 
	[Organic] = NULL, 
	[On_Sale] = NULL,
	[On_Offer] = 0,
	[Offer_Start_Date] = NULL,
	[Offer_End_Date] = NULL,
	[Offer_Quantity] = NULL, 
	[Offer_RewardAmount] = NULL,
	[Offer_PricingMethod_ID] = NULL, 
	[PricingMethod_ID] = NULL, 
	[Case_Price] = NULL, 
	[Vendor_ID] = NULL,
	[Item_Key] = NULL,
	[PLU] = NULL,
	[POSPrice] = NULL,
	[POSSale_Price] = NULL 
FROM Price P (NOLOCK)
	INNER JOIN Item I (NOLOCK) ON I.Item_Key = P.Item_Key
	INNER JOIN ItemIdentifier II (NOLOCK) ON II.Item_Key = I.Item_Key
	INNER JOIN StoreSubTeam SST (NOLOCK) ON SST.Store_No = P.Store_No AND SST.SubTeam_No = I.SubTeam_No
	INNER JOIN StoreItemVendor SIV (NOLOCK) ON SIV.Store_No = P.Store_No AND SIV.Item_Key = P.Item_Key
	LEFT JOIN SubTeam ST (NOLOCK) ON ST.SubTeam_No = SST.SubTeam_No
	LEFT JOIN Team T (NOLOCK) ON T.Team_No = SST.Team_No
	LEFT JOIN LabelType LT (NOLOCK) ON LT.LabelType_ID = I.LabelType_ID
	LEFT JOIN ItemCategory IC (NOLOCK) ON IC.Category_ID = I.Category_ID
	LEFT JOIN Vendor V (NOLOCK) ON V.Vendor_ID = SIV.Vendor_ID
	LEFT JOIN ItemVendor IV (NOLOCK) ON IV.Item_Key = I.Item_Key AND IV.Vendor_ID = SIV.Vendor_ID
	LEFT JOIN ItemUnit IU (NOLOCK) ON IU.Unit_ID = I.Package_Unit_ID
WHERE I.Deleted_Item = 0 
	AND dbo.fn_GetDiscontinueStatus(I.Item_Key, @Store_No, NULL) = 0 
	AND II.Default_Identifier = 1 
	AND II.Deleted_Identifier = 0
	AND P.Store_No = @Store_No
	AND ((I.LabelType_ID = @OutputLabelSizeID AND SST.Team_No &lt;&gt; @TeamNo_WholeBody)
		OR (@LabelTypeID_Small = @OutputLabelSizeID AND SST.Team_No = @TeamNo_WholeBody))
	AND SST.Team_No = ISNULL(@Team_No, SST.Team_No)
	AND SST.SubTeam_No = ISNULL(@SubTeam_No, SST.SubTeam_No)
	AND I.Category_ID = ISNULL(@Category_ID, I.Category_ID)
ORDER BY T.Team_Name, ST.SubTeam_Name, IC.Category_Name, V.CompanyName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
