  <DataSets>
    <DataSet Name="ItemCatalog">
      <Fields>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Subteam_Name">
          <DataField>Subteam Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reg_Multiple">
          <DataField>Reg Multiple</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Reg_Retail">
          <DataField>Reg Retail</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Store_Count">
          <DataField>Store Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>select distinct i.Identifier,
			    s.Subteam_Name as 'Subteam Name',
				t.Item_Description as 'Description',
				p.Multiple as 'Reg Multiple',
				p.Price as 'Reg Retail',
				count(distinct p.Store_No) as 'Store Count'
from Item t
INNER JOIN ItemIdentifier i on i.Item_Key = t.Item_Key
INNER JOIN Subteam s on t.Subteam_No = s.Subteam_No
INNER JOIN Price p on i.Item_Key = p.Item_Key and p.Store_No not in ('407','20014','50003')
INNER JOIN Price p2 on p.Item_Key = p2.Item_Key and p2.Store_No not in ('407','20014','50003')
INNER JOIN StoreItem si on t.Item_Key = si.Item_Key and p.Store_No = si.Store_No and si.Authorized = 'TRUE'
INNER JOIN StoreItem si2 on t.Item_Key = si2.Item_Key and p2.Store_No = si2.Store_No and si2.Authorized = 'TRUE'
where t.Subteam_No in (@Subteam) and p.PriceChgTypeID = '1' and p2.PriceChgTypeID = '1' and dbo.fn_GetDiscontinueStatus(t.Item_Key, NULL, NULL) = 'FALSE'
group by i.Identifier, s.Subteam_Name, t.Item_Description, p.Multiple, p.Price
having count(distinct p2.Price) &gt; '1'
order by s.Subteam_Name, i.Identifier</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Subteam">
            <Value>=Parameters!Subteam.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="listSubteam">
      <Fields>
        <Field Name="subteam_no">
          <DataField>subteam_no</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="subteam_name">
          <DataField>subteam_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>select subteam_no, subteam_name from subteam</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
