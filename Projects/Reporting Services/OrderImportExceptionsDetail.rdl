    <DataSet Name="LastestImportErrorDate">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SELECT        DATEADD(dd, 0, DATEDIFF(dd, 0, MAX(Timestamp))) AS LastImportErrorDate
FROM            OrderImportExceptionLog (nolock)</CommandText>
      </Query>
      <Fields>
        <Field Name="LastImportErrorDate">
          <DataField>LastImportErrorDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
