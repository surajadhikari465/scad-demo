  <DataSets>
    <DataSet Name="ItemCatalog">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OrderHeaderID">
            <Value>=Parameters!OrderHeaderID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ImportDate">
            <Value>=Parameters!ImportDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT distinct 
        olel.msg,
        olel.OrderHeader_Id,
        olel.Timestamp,
        oes.Description,
        s.Store_Name,
        st.SubTeam_Name,
        olel.Identifier,
        olel.Item_ID,
        olel.ItemDescription,
        olel.PackSize,
        olel.OrderUnit,
        olel.Cost
FROM    dbo.OrderImportExceptionLog olel
        INNER JOIN orderheader oh ON oh.OrderHeader_ID = olel.OrderHeader_Id
        INNER JOIN OrderExternalSource oes ON oh.OrderExternalSourceID = oes.ID
        LEFT JOIN Store s on s.Store_No = olel.Store_No
        LEFT JOIN SubTeam st on st.SubTeam_No = olel.SubTeam_No
WHERE  OH.CloseDate IS NULL
        AND olel.Timestamp &gt;= @ImportDate
        AND olel.msg like (CASE WHEN Exists (select OrderHeader_Id from OrderImportExceptionLog a where a.OrderHeader_Id = olel.OrderHeader_Id and a.Timestamp = olel.Timestamp and a.msg like '%ERROR: Could not find UPC %')
         THEN '%ERROR: Could not find UPC %'
        ELSE (SELECT TOP 1 Msg from OrderImportExceptionLog b where b.OrderHeader_Id = olel.OrderHeader_Id and b.Timestamp = olel.Timestamp ) END)
        AND OH.Orderheader_id = (CASE WHEN ISNULL(@OrderHeaderID, 0) = 0
	THEN oh.OrderHeader_ID
								ELSE
								@OrderHeaderID END)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Msg">
          <DataField>msg</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderHeader_Id">
          <DataField>OrderHeader_Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Timestamp">
          <DataField>Timestamp</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Store_Name">
          <DataField>Store_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SubTeam_Name">
          <DataField>SubTeam_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Item_ID">
          <DataField>Item_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemDescription">
          <DataField>ItemDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PackSize">
          <DataField>PackSize</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderUnit">
          <DataField>OrderUnit</DataField>
