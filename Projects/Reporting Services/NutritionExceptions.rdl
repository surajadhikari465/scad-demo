        </Field>
        <Field Name="Subteam">
          <DataField>Subteam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NutritionRequired">
          <DataField>NutritionRequired</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="Ingredients">
          <DataField>Ingredients</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="Allergens">
          <DataField>Allergens</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="NutrifactRecord">
          <DataField>NutrifactRecord</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
