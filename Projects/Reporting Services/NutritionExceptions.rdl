  <DataSets>
    <DataSet Name="Subteams">
      <Query>
        <DataSourceName>Icon</DataSourceName>
        <CommandText>
          -- SELECT '&lt;Any&gt;' as traitValue UNION ALL
          SELECT DISTINCT HCT.traitValue
          FROM      HierarchyClassTrait AS HCT INNER JOIN
          ItemHierarchyClass AS IHC ON HCT.hierarchyClassID = IHC.hierarchyClassID INNER JOIN
          Trait AS T ON HCT.traitID = T.traitID
          WHERE   (T.traitDesc = 'Merch Fin Mapping')
          ORDER BY traitvalue
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="traitValue">
          <DataField>traitValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SubteamsAny">
      <Query>
        <DataSourceName>Icon</DataSourceName>
        <CommandText>SELECT   '&lt;Any&gt;' AS traitValue</CommandText>
      </Query>
      <Fields>
        <Field Name="traitValue">
          <DataField>traitValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="NutritionExceptions">
      <Query>
        <DataSourceName>Icon</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Subteams">
            <Value>=Join(Parameters!Subteams.Value, ",")</Value>
          </QueryParameter>
          <QueryParameter Name="@ShowOnlyExceptions">
            <Value>=Parameters!ShowOnlyExceptions.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

          declare @wholeFoodsLocale as int = (SELECT l.localeID FROM Locale l WHERE l.localeName = 'Whole Foods');
          declare @productDescriptionTraitID as int = (SELECT t.traitID FROM Trait t WHERE t.traitCode = 'PRD');
          declare @posTraitID	as int = (SELECT t.traitID FROM Trait t WHERE t.traitCode = 'POS');
          declare @brandHierarchyID as int = (SELECT h.hierarchyID FROM Hierarchy h WHERE h.hierarchyName = 'Brands');
          declare @merchFinMappingTraitID as int = (SELECT t.traitID FROM Trait t WHERE t.traitCode = 'MFM');
          declare @nutritionReqId as int = (SELECT t.traitId from trait t where t.traitCode = 'NR');

          if OBJECT_ID('tempdb..#SubteamsNutrifactReq', N'U') is not null
          begin
          drop table #SubteamsNutrifactReq
          end

          create table #SubteamsNutrifactReq
          (
          name nvarchar(4000)
          );

          with
          subteams_CTE
          as
          (
          select 1 as start, charindex(',', @Subteams) as stop
          union all
          select stop + 1, charindex(',', @Subteams, stop + 1)
          from subteams_CTE
          where stop &gt; 0
          )
          insert into #SubteamsNutrifactReq
          select substring(@Subteams, start, case when stop &gt; 0 then stop - start else len(@Subteams) end)
          from subteams_CTE

          declare
          @ctesql nvarchar(max) = '',
          @selectsql nvarchar(max) = '',
          @fromsql nvarchar(max) = '',
          @wheresql nvarchar(max) = ''

          set @ctesql = N'
          with
          ItemTrait_CTE (traitID, traitCode, traitValue, itemID)
          as
          (
          select it.traitID, t.traitCode, it.traitValue, it.itemID
          from itemtrait it inner join trait t on it.traitID = t.traitID
          where it.localeID = @wholeFoodsLocale
          ),
          Brand_CTE (itemID, hierarchyClassName)
          as
          (
          select ihc.itemID, hc.hierarchyClassName
          from ItemHierarchyClass ihc
          join HierarchyClass hc on ihc.hierarchyClassID = hc.hierarchyClassID
          join Hierarchy h on hc.hierarchyID = h.hierarchyID and h.hierarchyID = @brandHierarchyID
          ),
          MFM_CTE (itemID, traitValue)
          as
          (
          select  ihc.itemID, hct.traitValue
          from ItemHierarchyClass ihc
          join HierarchyClass hc on ihc.hierarchyClassID = hc.hierarchyClassID
          join HierarchyClassTrait hct on hc.hierarchyClassID = hct.hierarchyClassID
          join Trait t on hct.traitID = t.traitID and t.traitID = @merchFinMappingTraitID
          )'

          if OBJECT_ID('tempdb..#nutritionReqOutput', N'U') is not null
          begin
          drop table #nutritionReqOutput
          end

          create table #nutritionReqOutput
          (
          ItemId int
          ,Identifier nvarchar(255)
          ,POSDescription nvarchar(255)
          ,ItemDescription nvarchar(255)
          ,BrandName nvarchar(255)
          ,Subteam nvarchar(255)
          ,NutritionRequired bit
          ,Ingredients bit
          ,Allergens bit
          ,NutrifactRecord bit
          );

          set @selectsql = N'
          insert into #nutritionReqOutput
          (
          ItemId
          ,Identifier
          ,POSDescription
          ,ItemDescription
          ,BrandName
          ,Subteam
          ,NutritionRequired
          ,Ingredients
          ,Allergens
          ,NutrifactRecord
          )
          select
          i.ItemId as [ItemId]
          ,sc.scancode as [Identifier]
          ,pos.traitvalue as [POSDescription]
          ,prd.traitValue as [ItemDescription]
          ,brd.hierarchyClassName as [BrandName]
          ,mfm.traitValue as [Subteam]
          ,nrq.traitValue as [NutritionRequired]
          ,case when itn.Ingredients is null then 0 else 1 end as [Ingredients]
          ,case when itn.Allergens is null then 0 else 1 end as [Allergens]
          ,case when (isnull(itn.calories, 0) = 0 and isnull(itn.totalcarbohydrateweight, 0) = 0) then 0 else 1 end as [NutrifactRecord]'

          set @fromsql = N'
          from item i
          join scancode sc on i.itemid = sc.itemid
          join ItemTrait_CTE pos on i.itemID=pos.itemID and pos.traitID=@posTraitID
          join ItemTrait_CTE prd on i.itemID=prd.itemID and prd.traitID=@productDescriptionTraitID
          join Brand_CTE brd on i.itemID=brd.itemID
          join MFM_CTE mfm on i.itemID=mfm.itemID
          left join ItemTrait_CTE nrq on i.itemId=nrq.itemId and nrq.traitID=@nutritionReqId
          left join nutrition.ItemNutrition itn on sc.scancode = itn.plu'

          if @ShowOnlyExceptions = 0
          set @wheresql = @wheresql + ' where isnull(itn.calories, 0) &lt;&gt; 0 and isnull(itn.totalcarbohydrateweight, 0) &lt;&gt; 0'
          else
          set @wheresql = @wheresql + ' where ((nrq.traitValue = ''1'' and isnull(itn.calories, 0) = 0 and isnull(itn.totalcarbohydrateweight, 0) = 0)
          or	(isnull(nrq.traitValue, ''0'') = ''0'' and isnull(itn.calories, 0) &lt;&gt; 0 and isnull(itn.totalcarbohydrateweight, 0) &lt;&gt; 0))'

          set @wheresql = @wheresql + N' and mfm.traitvalue in (select name from #SubteamsNutrifactReq)';

          print @ctesql;
          print @selectsql;
          print @fromsql;
          print @wheresql;

          declare @sql nvarchar(max) = concat(@ctesql, @selectsql, @fromsql, @wheresql);
          declare @paramDef nvarchar(max) = N'
          @Subteams as nvarchar(255)
          ,@ShowOnlyExceptions as bit
          ,@wholeFoodsLocale as int
          ,@merchFinMappingTraitID as int
          ,@brandHierarchyID as int
          ,@posTraitID as int
          ,@productDescriptionTraitID as int
          ,@nutritionReqId as int'

          -- ====================================================
          -- Execute SQL for Items
          -- ====================================================

          execute sp_executesql
          @sql
          ,@paramDef
          ,@Subteams
          ,@ShowOnlyExceptions
          ,@wholeFoodsLocale
          ,@merchFinMappingTraitID
          ,@brandHierarchyID
          ,@posTraitID
          ,@productDescriptionTraitID
          ,@nutritionReqId
          select
          ItemId
          ,Identifier
          ,POSDescription
          ,ItemDescription
          ,BrandName
          ,Subteam
          ,NutritionRequired
          ,Ingredients
          ,Allergens
          ,NutrifactRecord
          from #nutritionReqOutput

          drop table #SubteamsNutrifactReq
          drop table #nutritionReqOutput
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ItemId">
          <DataField>ItemId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="POSDescription">
          <DataField>POSDescription</DataField>
