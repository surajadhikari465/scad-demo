      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>DECLARE @VendorItemStatusList TABLE (StatusID int)
--if an item vendor record has a null status, we will treat it as active
DECLARE @ActiveStatusID int
SELECT @ActiveStatusID = StatusID from VendorItemStatuses where StatusName = 'Active'


INSERT INTO @VendorItemStatusList 
	SELECT StatusID 
	FROM VendorItemStatuses 
	WHERE StatusID IN (SELECT * FROM dbo.fn_ParseStringList(@VendorItemStatuses,','))
   
    
SELECT DISTINCT
    II.Identifier
    , ST.SubTeam_Name AS Subteam
    , IB.Brand_Name AS Brand
    , I.Item_Description AS [IRMA Item Description]
    , I.Package_Desc2 AS [IRMA Item Size]
    , IU.Unit_Abbreviation AS [IRMA Item UOM]
    , ISNULL(IV.RetailCasePack,0) AS [Retail Casepack]
    , ISNULL(VCA.Package_Desc1,0) AS [Vendor Casepack]  
    , VCA.UnitCost  AS [Regular Cost]
    , CU.Unit_Abbreviation AS [Cost Unit]
    , IV.Item_ID AS [VIN]
    , V.CompanyName AS [Vendor Name]
FROM
    Item I (NoLock)
    INNER JOIN ItemIdentifier II (NoLock) ON I.Item_Key = II.Item_Key and II.Default_Identifier in (1, @DefaultIdentifier)
    INNER JOIN ItemVendor IV (NoLock) ON I.Item_Key = IV.Item_Key
    INNER JOIN Vendor V (NoLock) ON IV.Vendor_ID = V.Vendor_ID
    INNER JOIN SubTeam ST (NoLock) ON I.SubTeam_No = ST.SubTeam_No
    INNER JOIN ItemBrand IB (NoLock) ON I.Brand_ID = IB.Brand_ID
    INNER JOIN ItemUnit IU (NoLock) ON I.Package_Unit_ID = IU.Unit_ID
    INNER JOIN StoreItemVendor SIV 
		ON I.Item_Key = SIV.Item_Key 
		and IV.Vendor_ID = SIV.Vendor_ID 
		and ISNULL(SIV.DeleteDate,GETDATE()+1) &gt;= GETDATE()
	INNER JOIN dbo.fn_VendorCostAll(getdate()) VCA 
		ON SIV.Item_Key = VCA.Item_Key
		and SIV.Store_No = VCA.Store_No
		and SIV.Vendor_ID = VCA.Vendor_ID
	INNER JOIN ItemUnit CU (NoLock) on VCA.CostUnit_ID = CU.Unit_ID
	-- INNER JOIN @VendorItemStatusList VIS ON ISNULL(IV.VendorItemStatus,@ActiveStatusID) = VIS.StatusID
	-- If IV.VendorItemstatus is null or 0, treat this value as Active.
	INNER JOIN @VendorItemStatusList VIS ON CASE 
						WHEN IV.VendorItemStatus IS NULL THEN @ActiveStatusID 
						WHEN IV.VendorItemStatus = 0 THEN @ActiveStatusID 
						ELSE IV.VendorItemStatus END = VIS.StatusID
WHERE I.Subteam_No = ISNULL(@Subteam_No,I.SubTeam_No)
    AND ISNULL(@Vendor_ID,V.Vendor_ID) = V.Vendor_ID
    AND IV.IgnoreCasePack = 1</CommandText>
        <QueryParameters>
          <QueryParameter Name="@VendorItemStatuses">
            <Value>=Parameters!VendorItemStatuses.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DefaultIdentifier">
            <Value>=Parameters!DefaultIdentifier.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Subteam_No">
            <Value>=Parameters!Subteam_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Vendor_ID">
            <Value>=Parameters!Vendor_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
