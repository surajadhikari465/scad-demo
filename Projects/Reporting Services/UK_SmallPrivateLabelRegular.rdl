  <Code>Public Shared Function FormatIdentifier(ByVal Identifier As String, ByVal CheckDigit As String) As String
    Dim result As String

    If Identifier.Length &gt;= 12 Then
        result = String.Format("{0}{1}", Identifier.Substring(0, 12), CheckDigit)
    Else
        result = String.Format("{0}{1}", Identifier.PadLeft(12, "0"), CheckDigit)
    End If

    Return result

End Function

'---------------------------------------------------------------------------------------------------------------------------
Public Shared Function FormatItemKey(ByVal Item_Key As Integer) As String
    Dim sItemKey As String
    Dim result As String

    sItemKey = CType(Item_Key, String)

    If sItemKey.Length &gt;= 12 Then
        result = sItemKey.Substring(0, 12)
    Else
        result = sItemKey.PadLeft(12, "0")
    End If

    Return result

End Function

'---------------------------------------------------------------------------------------------------------------------------
Public Shared Function FormatVendorItemID(ByVal VendorItemID As String) _
As String

    Dim result As String

    If VendorItemID.Trim.Length &gt;= 12 Then
        result = VendorItemID.Trim.Substring(0, 12)
    Else
        result = VendorItemID.Trim.PadLeft(12, "0")
    End If

    Return result

End Function

'---------------------------------------------------------------------------------------------------------------------------
Public Shared Function FormatPricePerUOM( _
    ByVal price As Double, _
    ByVal package_desc2 As Double, _
    ByVal package_unit As String) As String

    Dim result As String
    Dim new_price As Double
    Dim new_package_desc2 As String
    Dim multiplier As Double = 1.0

    If price &lt;&gt; Nothing AndAlso price &gt; 0.0 Then
        If package_desc2 &lt;&gt; Nothing AndAlso package_desc2 &gt; 0.0 Then
            Select Case Trim(package_unit)
                Case "g"
                    multiplier = 100.0 / package_desc2
                    new_package_desc2 = "100"
                Case "ml"
                    multiplier = 100.0 / package_desc2
                    new_package_desc2 = "100"
                Case Else
                    multiplier = 1.0
                    new_package_desc2 = FormatNumber(package_desc2, 0)
            End Select

            new_price = multiplier * price
        Else
            new_price = price
            new_package_desc2 = "unknown"
        End If

        result = String.Format("{0} {1} {2} {3}", FormatPriceDisplay(new_price), "per", new_package_desc2, package_unit)
    Else
        If package_desc2 &lt;&gt; Nothing AndAlso package_desc2 &gt; 0.0 Then
            new_package_desc2 = FormatNumber(package_desc2, 0)
            result = String.Format("{0} {1} {2} {3}", "Unknown", "per", new_package_desc2, package_unit)
        Else
            result = String.Format("{0} {1} {2} {3}", "Unknown", "per", "unknown", package_unit)
        End If
    End If

    Return result

End Function

'---------------------------------------------------------------------------------------------------------------------------
Public Shared Function FormatPriceDisplay(ByVal Price As Double) As String

    Dim sDisplayPrice As String

    'compare to 0.995 since that will be rounded up to 1.00
    If Price &lt; 0.995 Then
        'multiply by 100 and display as Pence rather than Pound
        sDisplayPrice = String.Format("{0}{1}", FormatNumber(Price * 100, 0), "p")
    Else
        'display price to 2 decimal places without the Pound symbol
        sDisplayPrice = String.Format("{0}", FormatNumber(Price, 2))
    End If

    Return sDisplayPrice

End Function</Code>
