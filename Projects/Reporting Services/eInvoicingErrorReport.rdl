  <DataSets>
    <DataSet Name="Query">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

-- Create temp table
DECLARE @ErrorRecords table 
	(
		InvoiceNumber	varchar(255),
		ImportDate	datetime,
		eInvoiceID	int,
		StoreNo		int,
		Store_Name	varchar(50),
		VendorID	varchar(255),
		CompanyName	varchar(50),
		PONumber	varchar(255),
		ErrorCode	int,
		ErrorMessage	varchar(2048)
	)

-- Populate temp table
INSERT INTO @ErrorRecords
	SELECT
		[InvoiceNumber]	= ei.Invoice_Num,
		[ImportDate]	= ei.ImportDate,
		[eInvoiceID]	= ei.eInvoice_id,
		[StoreNo]	= ei.Store_No,
		[Store_Name]	= s.Store_Name,
		[VendorID]	= RIGHT('0000000000' + ei.psvendor_id, 10),
		[CompanyName]	= v.CompanyName,
		[PONumber]	= ei.po_num,
		[ErrorCode]	= ISNULL(ec.ErrorCode_ID, 0),
		[ErrorMessage]	= ISNULL(ec.ErrorMessage, 'Error code not found in lookup table')
	FROM
		EInvoicing_Invoices		(nolock) ei
		LEFT JOIN EInvoicing_errorCodes	(nolock) ec ON ei.ErrorCode_id = ec.ErrorCode_Id
		INNER JOIN Store			(nolock) s ON ei.Store_No = s.BusinessUnit_ID
		INNER JOIN Vendor		(nolock) v ON ei.psvendor_id = v.PS_Export_Vendor_ID
							AND v.EInvoicing = 1
	WHERE
		ei.Archived		IS NULL
		AND ei.ErrorEmailSentDate	IS NULL
		AND ei.ErrorCode_ID	IS NOT NULL

-- Select records
SELECT 
	InvoiceNumber,
	ImportDate,
	eInvoiceID,
	StoreNo	,
	Store_Name,
	VendorID,
	CompanyName,
	PONumber,
	ErrorCode,
	ErrorMessage
FROM 
	@ErrorRecords

-- Toggle records
UPDATE 
	EInvoicing_Invoices
SET 
	ErrorEmailSentDate = GETDATE()
FROM 
	@ErrorRecords er
WHERE
	er.eInvoiceID = EInvoicing_Invoices.EInvoice_Id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
