  <DataSets>
    <DataSet Name="ItemCatalog">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SubTeam_List">
            <Value>=Parameters!SubTeam_List.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT     Store.BusinessUnit_ID AS Store, Store.Store_Name AS 'Store Name', SubTeam.SubTeam_Name AS SubTeam, 
                      ItemHistory.AdjustmentReason AS 'Type of Shrink', CAST(SUM((1 * (ItemHistory.Quantity + ItemHistory.Weight) 
                      * dbo.fn_GetUnitCostForSpoilage(ItemHistory.Item_Key, ItemHistory.Store_No, ItemHistory.SubTeam_No, ItemHistory.DateStamp))) AS DECIMAL(8, 2)) 
                      AS Cost
FROM         ItemHistory WITH (NoLock) INNER JOIN
                      SubTeam WITH (NoLock) ON SubTeam.SubTeam_No = ItemHistory.SubTeam_No INNER JOIN
                      Store WITH (NoLock) ON Store.Store_No = ItemHistory.Store_No AND (Store.Mega_Store = 1 OR
                      Store.WFM_Store = 1) INNER JOIN
                          (SELECT     Key_Value
                            FROM          dbo.fn_Parse_List(@SubTeam_List, ',') AS FN
                            WHERE      EXISTS
                                                       (SELECT     SubTeam_No
                                                         FROM          SubTeam AS SubTeam_1
                                                         WHERE      (SubTeam_No = FN.Key_Value))) AS SL ON ItemHistory.SubTeam_No = SL.Key_Value
WHERE     (ItemHistory.Adjustment_ID = 1) AND (ItemHistory.DateStamp BETWEEN @StartDate AND DATEADD(D, 1, @EndDate))
GROUP BY SubTeam.SubTeam_Name, Store.BusinessUnit_ID, Store.Store_Name, ItemHistory.AdjustmentReason
ORDER BY SubTeam, Store</CommandText>
      </Query>
      <Fields>
        <Field Name="Store">
          <DataField>Store</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Store_Name">
          <DataField>Store Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SubTeam">
          <DataField>SubTeam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Type_of_Shrink">
          <DataField>Type of Shrink</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
