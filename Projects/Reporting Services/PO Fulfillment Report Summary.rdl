    <DataSet Name="DS_Time_Zone_Offset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT        
      CentralTimeZoneOffset AS Time_Zone_Offset, 
      DATEADD(Hour, CentralTimeZoneOffset, GETDATE()) AS LocalRunTime, 
      CASE  WHEN DATEPART(hh, DATEADD(Hour, CentralTimeZoneOffset, GetDate())) &gt;= 12
            THEN DATEADD(Day, 0, DATEDIFF(Day, 0, GetDate())) 
            ELSE DATEADD(Hour, 12, DATEDIFF(Day, 0, DATEADD(Day, - 1, GETDATE()))) 
            END AS StartDate, 
      CASE  WHEN DATEPART(hh, DATEADD(Hour, CentralTimeZoneOffset, GetDate())) &lt; 12 
            THEN DATEADD(Day, 0, DATEDIFF(Day, 0, GetDate())) 
            ELSE DATEADD(Hour, 12, DATEDIFF(Day, 0, GetDate())) 
            END AS EndDate
FROM
      Region</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
