        </QueryParameters>
        <CommandText>-- SUBSCRIPTION QUERY:
/*
DECLARE 
      @Time_Zone_Offset AS INT,
		@StartDate AS DATETIME,
		@EndDate AS DATETIME,
		@offsetStartDate AS DATETIME,
		@offsetEndDate AS DATETIME

SELECT 
	   @Time_Zone_Offset = CentralTimeZoneOffset, 
	   @StartDate  =  CASE  WHEN DATEPART(hh, DATEADD(Hour, CentralTimeZoneOffset, GetDate())) &gt;= 12 
                           THEN DATEADD(Day, 0, DATEDIFF(Day, 0, GetDate())) 
                           ELSE DATEADD(Hour, 12, DATEDIFF(Day, 0, DATEADD(Day, - 1, GETDATE()))) 
                           END, 
      @EndDate    =  CASE  WHEN DATEPART(hh, DATEADD(Hour, CentralTimeZoneOffset, GetDate())) &lt; 12
                           THEN DATEADD(Day, 0, DATEDIFF(Day, 0, GetDate()))
                           ELSE DATEADD(Hour, 12, DATEDIFF(Day, 0, GetDate()))
                           END
FROM
      Region

SELECT
      @offsetStartDate = dateadd(hh, @Time_Zone_Offset, @StartDate),
      @offsetEndDate = dateadd(hh, @Time_Zone_Offset, @EndDate)

DECLARE @temp TABLE (createdby int, email nvarchar(100), offsetStartDate datetime, offsetEndDate datetime)
INSERT INTO @temp
SELECT DISTINCT
      oh.createdby,
      u.email,
      @offsetStartDate as [offsetStartDate],
      @offsetEndDate as [offsetEndDate]
FROM 
      dbo.orderheader oh (nolock)
      JOIN dbo.orderitem oi (nolock)            ON oi.orderheader_id = oh.orderheader_id
      JOIN dbo.einvoicing_invoices ein (nolock) ON ein.EInvoice_Id = oh.EInvoice_Id
      JOIN dbo.users u (nolock)                 ON u.user_id = oh.createdby
WHERE    ( oh.return_order = 0 )
   AND   ( Isnull(oi.quantityordered, 0) &lt;&gt; Isnull(oi.QuantityReceived, 0) )
   AND   ( oh.closedate between @offsetStartDate and @offsetEndDate )

SELECT * FROM @temp

*/
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
--BEGIN REPORT QUERY:
SELECT DISTINCT
      [Store Name]                                    =	vstore.companyname,
      [SubTeam]                                       =  ttsubteam.SubTeam_Name,
      [Vendor Name]                                   =	v.companyname,
      [PO Number]                                     =  oh.orderheader_id, 
      [DVO/POET #]                                    =  oh.OrderExternalSourceOrderID,
      [Order Date]                                    =  oh.orderdate,
      [Item Identifier]                               =  ii.identifier,
      [Brand]                                         =  ib.Brand_Name,
      [Item Description]                              =  i.item_description,
      [Ordered Quantity]                              =  oi.QuantityOrdered,
      [Unit of Measure]                               =  iu.unit_name,
      [eInvoiced Quantity]                            =  ei.qty_shipped,
      [Received Quantity]                             =  oi.QuantityReceived,
      [Cost]                                          =	isnull(oi.cost, 0),
      [Pkg/Desc]                                      =  oi.Package_Desc1,
      [Receiving Discrepancy Exception Reason Code]   =	rec.reasoncodedesc
                   
FROM  
      dbo.orderheader oh (nolock)
      JOIN dbo.vendor v (nolock)                      ON v.vendor_id = oh.vendor_id
      JOIN dbo.vendor vstore (nolock)                 ON vstore.vendor_id = oh.receivelocation_id
      JOIN dbo.subteam ttsubteam (nolock)             ON ttsubteam.subteam_no = oh.transfer_to_subteam
      JOIN dbo.orderitem oi (nolock)                  ON oi.orderheader_id = oh.orderheader_id
      JOIN dbo.itemidentifier ii (nolock)             ON ii.item_key = oi.item_key
      JOIN dbo.item i (nolock)                        ON i.item_key = oi.item_key
      JOIN dbo.itembrand ib (nolock)                  ON ib.brand_id = i.brand_id
      JOIN dbo.itemunit iu (nolock)                   ON iu.unit_id = i.package_unit_id
      JOIN dbo.einvoicing_invoices ein (nolock)       ON ein.EInvoice_Id = oh.EInvoice_Id
      JOIN dbo.einvoicing_item ei (nolock)            ON ei.einvoice_id = oh.einvoice_id and ei.item_key = oi.item_key
      LEFT JOIN dbo.reasoncodedetail rec (nolock)     ON rec.reasoncodedetailid = oi.receivingdiscrepancyreasoncodeid -- receiving exception code orderitem level

WHERE    ( oh.return_order = 0 )
   AND   ( Isnull(oi.quantityordered, 0) &lt;&gt; Isnull(oi.QuantityReceived, 0) )
   AND   ( oh.createdby = @CreatedBy )
   AND   ( oh.closedate between @offsetStartDate and @offsetEndDate )</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
