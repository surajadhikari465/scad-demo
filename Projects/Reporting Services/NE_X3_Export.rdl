  <DataSets>
    <DataSet Name="ItemCatalog">
      <Fields>
        <Field Name="Header">
          <DataField>Header</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Line">
          <DataField>Line</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SELECT
	'E,STO,' 
		+	
			CASE OH.Transfer_To_SubTeam
				WHEN 4110 THEN CAST(S.BusinessUnit_ID as varchar(10)) + '-B' -- Bakery
				WHEN 4320 THEN CAST(S.BusinessUnit_ID as varchar(10)) + '-F' -- Frozen
				WHEN 4120 THEN CAST(S.BusinessUnit_ID as varchar(10)) + '-D' -- Prep Foods
				ELSE CAST(S.BusinessUnit_ID as varchar(10)) + '-M' -- All Others
			END 
		+ 
			',' 
		+ 
			CONVERT(nvarchar(30), OH.OrderDate, 112)
		+
			',' 
		+
			CONVERT(nvarchar(30), OH.Expected_Date, 112)
		+	
			','
		+
			CAST(OH.OrderHeader_ID as varchar(10))									AS Header,
	'L,' 
		+ 
			REPLACE(LTRIM(REPLACE(IV.Item_ID , '0', ' ')), ' ', '0')
		+ 
			','
		+
			CAST(CAST(OI.QuantityOrdered AS int) as varchar(10))	AS Line
FROM         
	OrderItem OI					(NOLOCK)
	INNER JOIN OrderHeader OH		(NOLOCK) ON OI.OrderHeader_ID		= OH.OrderHeader_ID
	INNER JOIN Vendor V				(NOLOCK) ON OH.ReceiveLocation_ID	= V.Vendor_ID
	INNER JOIN ItemVendor IV		(NOLOCK) ON OH.Vendor_ID			= IV.Vendor_ID
											 AND OI.Item_Key			= IV.Item_Key
	INNER JOIN Store S				(NOLOCK) ON V.Store_No				= S.Store_No
WHERE
	OH.Vendor_ID			=	473
	AND OH.Expected_Date	=	@ExpectedDate
                AND OH.Return_Order = 0
GROUP BY
	OH.Transfer_To_SubTeam,
	S.BusinessUnit_ID,
	OH.OrderHeader_ID,
	OH.Expected_Date,
	OH.OrderDate,
	OI.OrderItem_ID,
	OI.QuantityOrdered,
	IV.Item_ID
ORDER BY
	S.BusinessUnit_ID,
	OH.OrderHeader_ID,
	--OI.OrderItem_ID
                IV.Item_ID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ExpectedDate">
            <Value>=Parameters!ExpectedDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
