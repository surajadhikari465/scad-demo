  <DataSets>
    <DataSet Name="ItemAttributes">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Attributes">
            <Value>=Parameters!Attributes.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

DECLARE @SQL AS varchar(MAX)

DECLARE @TABLE table(SubTeam varchar(100), Identifier varchar(13), Brand  varchar(25), Description  varchar(60), ItemSize  varchar(10), ItemUOM  varchar(25), AttributeValue  varchar(50))

DECLARE @Operator AS varchar(11)

SELECT @Operator =	
		CASE LEFT(@Attributes,4)
			WHEN 'Chec' THEN ' = 1'
                                                WHEN 'Text' THEN 'IS NOT NULL'
			WHEN 'Date' THEN 'IS NOT NULL'			
		END

SELECT @SQL = 
	'SELECT
		[SubTeam]	 = s.subteam_name,
		[Identifier]	 = ii.identifier,	
		[Brand]		 = ib.brand_name,
		[Description]	 = i.item_description,
		[ItemSize]	 = i.package_desc1,
		[ItemUOM]	 = iu.unit_name,'

IF  LEFT(@Attributes,4)  = 'Date' 
                SELECT @SQL = @SQL + '[AttributeValue] = CONVERT(VARCHAR(10),ia.' + RTRIM(@Attributes)+ ',101)' + ' ' 
IF  LEFT(@Attributes,4)  = 'Text' 
                SELECT @SQL = @SQL + '[AttributeValue] = RTRIM(ia.' + RTRIM(@Attributes) + ') '
IF  LEFT(@Attributes,4)  = 'Chec'                    
                SELECT @SQL = @SQL + '[AttributeValue] = ' + 	               
		'CASE  ia.' + RTRIM(@Attributes) + ' ' +
		          'WHEN 1 THEN ''True''
                                           WHEN 0 THEN ''False''			
		END '
SELECT @SQL = @SQL + 	
		'FROM item i (NOLOCK) 
			INNER JOIN itemidentifier ii (NOLOCK) ON ii.item_key = i.item_key
			INNER JOIN subteam s (NOLOCK) ON s.subteam_no = i.subteam_no
			INNER JOIN itembrand ib (NOLOCK) ON ib.brand_id = i.brand_id
			INNER JOIN itemunit iu (NOLOCK) ON iu.unit_id = i.package_unit_id
			INNER JOIN itemattribute ia (NOLOCK) ON i.item_key = ia.item_key 
		WHERE (i.deleted_item &lt;&gt; 1) AND' + ' '
	SELECT @SQL = @SQL + 	
		'(ia.' + RTRIM(@Attributes) + ' ' + @Operator + ') '
	SELECT @SQL = @SQL +
		'ORDER BY s.subteam_name, ii.identifier'
		
INSERT INTO @TABLE(SubTeam, Identifier, Brand, Description, ItemSize, ItemUOM, AttributeValue)

EXECUTE (@SQL)

SELECT * FROM @TABLE</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
