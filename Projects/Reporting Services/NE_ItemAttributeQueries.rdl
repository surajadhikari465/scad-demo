    <DataSet Name="AttributeList">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT 
	   FieldType = 
		CASE LEFT(field_type,4)
			WHEN 'Chec' THEN 'Check_Box_' + RTRIM(SUBSTRING(field_type, 9,2))
			WHEN 'Text' THEN 'Text_' + RTRIM(SUBSTRING(field_type, 5,2))
			WHEN 'Date' THEN 'Date_Time_' + RTRIM(SUBSTRING(field_type, 9,2))	
		END

                 ,Screen_Text 
FROM AttributeIdentifier (NOLOCK) 
WHERE Screen_Text &lt;&gt; ''
ORDER BY Screen_Text</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
