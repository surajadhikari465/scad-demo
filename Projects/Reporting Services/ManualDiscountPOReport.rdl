    <DataSet Name="GetCostAdjustmentReasonCodes">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
	rcd.ReasonCodeDetailID,
	rcd.ReasonCode,
	rcd.ReasonCodeDesc
FROM
	ReasonCodeDetail			rcd (nolock)
	INNER JOIN ReasonCodeMappings	rcm (nolock) ON rcd.ReasonCodeDetailID = rcm.ReasonCodeDetailID
	INNER JOIN ReasonCodeType	rct (nolock) ON rcm.ReasonCodeTypeID = rct.ReasonCodeTypeID
WHERE
	rct.ReasonCodeTypeAbbr = 'CA'
ORDER BY
	rcd.ReasonCodeDesc ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="ReasonCodeDetailID">
          <DataField>ReasonCodeDetailID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
