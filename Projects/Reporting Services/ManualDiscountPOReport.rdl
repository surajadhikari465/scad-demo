    <DataSet Name="Main">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Store">
            <Value>=JOIN(Parameters!StoreNo.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Vendor">
            <Value>=JOIN(Parameters!Vendor.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@SubTeam">
            <Value>=JOIN(Parameters!SubTeam.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@LowestDiscount">
            <Value>=Parameters!LowestDiscount.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@HighestDiscount">
            <Value>=Parameters!HighestDiscount.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReasonCode">
            <Value>=JOIN(Parameters!ReasonCode.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@CreatedBy">
            <Value>=Parameters!CreatedBy.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
	[Store]			= vs.companyname,
	[PONum]			= oh.orderheader_id,
	[AltPONum]		= oh.orderexternalsourceorderid,
	[Subteam]			= st.subteam_name,
	[Vendor]			= v.companyname,
	[POOrigRecCost]		= oh.OriginalReceivedCost,
	[PODiscountQty]		= oh.quantitydiscount,
	[PODiscountType]		= oh_rcd.ReasonCodeDesc,
	[PODiscountCode]		= oh_rcd.ReasonCode,
	[ItemDiscountQty]		= oi.quantitydiscount,
	[ItemDiscountType]		= oi_rcd.ReasonCodeDesc,
	[ItemDiscountCode]		= oi_rcd.ReasonCode,
	[UPC]			= ii.identifier,
	[ItemDesc]		= i.item_description,
	[DatePOSent]		= oh.sentdate,
	[DateItemReceived]		= oi.datereceived,
	[ItemCost]		= oi.cost,
	[OrigReceivedItemCost]	= oi.origreceiveditemcost,
	[ItemQtyReceived]		= oi.quantityreceived,
	[WeightReceived]		= oi.Total_Weight,
	[AdjustedReceivedCost]	= oi.ReceivedItemCost,
	[PONotes]			= replace(replace(oh.orderheaderdesc, char(10), ''), char(13), ''),
	[OHResolutionCode]		= oh.ResolutionCodeID,
	[OIResolutionCode]		= oi.ResolutionCodeID,
	[RegCaseCost]		= vca.UnitCost,
	[HostedAllowance]		= vca.NetDiscount,
	[NetCaseCost]		= vca.NetCost,
	[VendorPack]		= vca.Package_Desc1,
	[CostedByWeight]		= i.CostedByWeight,
	oh.ApprovedDate,
	oh.CloseDate,
	oh.UploadedDate,
	oh.AccountingUploadDate,
	u.UserName,
	
	-- If an item is costed by weight, we need the per weight cost to get the true net received cost (cost prior to a manual discount); if the item is not costed by weight we need the ordered unit cost.
	-- Dollars Saved is essentially the difference between what we would have ordered prior to a discount and what we did order after the discount
	-- The following logic is needed to account for catch weight and wine scenarios as well as all other normal ordering scenarios.  The fn_CostConversion handles these scenarios where the cost and order units differ.
	[NetReceivedCost]			=	CASE
							WHEN i.CostedByWeight = 1 THEN
								(dbo.fn_CostConversion(oi.Cost,oi.CostUnit,i.Package_Unit_ID,vca.Package_Desc1,i.Package_Desc2,i.Package_Unit_ID) * oi.Total_Weight)
							ELSE
								(dbo.fn_CostConversion(oi.Cost,oi.CostUnit,oi.QuantityUnit,vca.Package_Desc1,i.Package_Desc2,i.Package_Unit_ID) * oi.QuantityReceived)
						END,
	[DollarsSaved]			=	CASE
							WHEN i.CostedByWeight = 1 THEN
								(dbo.fn_CostConversion(oi.Cost,oi.CostUnit,i.Package_Unit_ID,vca.Package_Desc1,i.Package_Desc2,i.Package_Unit_ID) * oi.Total_Weight) - oi.ReceivedItemCost
							ELSE
								(dbo.fn_CostConversion(oi.Cost,oi.CostUnit,oi.QuantityUnit,vca.Package_Desc1,i.Package_Desc2,i.Package_Unit_ID) * oi.QuantityReceived) - oi.ReceivedItemCost
						END

	
from 
	orderheader				oh	(nolock) 
	INNER JOIN orderitem	oi	(nolock) on oh.orderheader_id		= oi.orderheader_id
	INNER JOIN vendor		v	(nolock) on oh.vendor_id		= v.vendor_id
	INNER JOIN vendor		vs	(nolock) on oh.receivelocation_id	= vs.vendor_id
	INNER JOIN subteam		st	(nolock) on oh.transfer_to_subteam	= st.subteam_no
	INNER JOIN item		i	(nolock) on oi.item_key		= i.item_key
	INNER JOIN itemidentifier	ii	(nolock) on i.item_key		= ii.item_key
	INNER JOIN Users		u	(nolock) on oh.CreatedBy		= u.User_ID
	LEFT JOIN ReasonCodeMappings	oh_rcm	(nolock) on oh.ReasonCodeDetailId		= oh_rcm.ReasonCodeDetailId
	LEFT JOIN ReasonCodeDetail		oh_rcd	(nolock) on oh_rcm.ReasonCodeDetailId	= oh_rcd.ReasonCodeDetailId
	LEFT JOIN ReasonCodeMappings	oi_rcm	(nolock) on oi.ReasonCodeDetailId		= oi_rcm.ReasonCodeDetailId
	LEFT JOIN ReasonCodeDetail		oi_rcd	(nolock) on oi_rcm.ReasonCodeDetailId	= oi_rcd.ReasonCodeDetailId
	OUTER APPLY	(
				SELECT * 
				FROM
					dbo.fn_VendorCostAll((CASE WHEN oh.OrderExternalSourceID = 5 THEN oh.Expected_Date ELSE oh.SentDate END) + dbo.fn_GetLeadTimeDays(oh.Vendor_ID)) vca
				WHERE 
					vca.Item_Key				= i.Item_Key 
					AND vca.Vendor_ID			= oh.Vendor_ID 
					AND vca.Store_No			= vs.Store_No
				)	AS vca

where
	CONVERT(SMALLDATETIME, CONVERT(VARCHAR(255), oh.CloseDate, 101))	&gt;= @StartDate
	AND CONVERT(SMALLDATETIME, CONVERT(VARCHAR(255), oh.CloseDate, 101))	&lt; Dateadd(d, 1, @EndDate)
	AND vs.Store_No			IN (SELECT key_value FROM dbo.fn_Parse_List(@Store, ','))
	AND oh.Transfer_To_SubTeam		IN (SELECT key_value FROM dbo.fn_Parse_List(@SubTeam, ','))
	AND oh.vendor_id			IN (SELECT key_value FROM dbo.fn_Parse_List(@Vendor, ','))
	AND oh.closedate			IS NOT NULL
	-- If user puts '0' as the @LowestDiscount param, we need to not include POs with a 0 QuantityDiscount
	AND ((oh.QuantityDiscount &gt; @LowestDiscount AND oh.QuantityDiscount &lt;= @HighestDiscount AND @LowestDiscount = 0)
		OR
		(oh.QuantityDiscount &gt;= @LowestDiscount AND oh.QuantityDiscount &lt;= @HighestDiscount AND @LowestDiscount &lt;&gt; 0)
		OR
		(oi.QuantityDiscount &gt; @LowestDiscount AND oi.QuantityDiscount &lt;= @HighestDiscount AND @LowestDiscount = 0 AND oi.DiscountType = 2)
		OR
		(oi.QuantityDiscount &gt;= @LowestDiscount AND oi.QuantityDiscount &lt;= @HighestDiscount AND @LowestDiscount &lt;&gt; 0 AND oi.DiscountType = 2))
	AND ((oh.ReasonCodeDetailId			IN (SELECT key_value FROM dbo.fn_Parse_List(@ReasonCode, ','))
		OR 
		oi.ReasonCodeDetailId			IN (SELECT key_value FROM dbo.fn_Parse_List(@ReasonCode, ','))))
	AND ((u.UserName LIKE '%'+@CreatedBy+'%' AND @CreatedBy IS NOT NULL AND @CreatedBy &lt;&gt; '')
		OR
		(@CreatedBy IS NULL OR @CreatedBy = ''))

order by
	vs.companyname, 
	v.companyname, 
	oh.orderheader_id desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
