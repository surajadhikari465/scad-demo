    <DataSet Name="Main">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Store">
            <Value>=JOIN(Parameters!StoreNo.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Vendor">
            <Value>=JOIN(Parameters!Vendor.Value,",")</Value>
          </QueryParameter>
          <QueryParameter Name="@SubTeam">
            <Value>=JOIN(Parameters!SubTeam.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	[Store Name]		= vr.CompanyName,
	[Sub Team]		= st.SubTeam_Name,
	[Vendor Name]		= v.CompanyName,
	[PO#]			= oh.OrderHeader_ID,
	[Original Close Date]	= oh.OriginalCloseDate,
	[Count of Refused Items] = COUNT(oir.OrderItemRefusedID),
	[Refused Total]		= oh.TotalRefused
					
FROM
	OrderHeader			oh	(nolock)
	INNER JOIN Vendor		v	(nolock) ON oh.Vendor_ID		= v.Vendor_ID
	INNER JOIN Vendor		vr	(nolock) ON oh.ReceiveLocation_ID	= vr.Vendor_ID
	INNER JOIN SubTeam		st	(nolock) ON oh.Transfer_To_SubTeam	= st.SubTeam_No
	LEFT JOIN OrderItemRefused	oir 	(nolock) ON oh.OrderHeader_ID		= oir.OrderHeader_ID
	
WHERE
	vr.Store_no		IN (SELECT key_value FROM dbo.fn_Parse_List(@Store, ','))
	AND st.SubTeam_No	IN (SELECT key_value FROM dbo.fn_Parse_List(@SubTeam, ','))
	AND v.Vendor_ID 	IN (SELECT key_value FROM dbo.fn_Parse_List(@Vendor, ','))
	AND CONVERT(SMALLDATETIME, CONVERT(VARCHAR(255), oh.OriginalCloseDate, 101))	&gt;= @StartDate
    	AND CONVERT(SMALLDATETIME, CONVERT(VARCHAR(255), oh.OriginalCloseDate, 101))	&lt; Dateadd(d, 1, @EndDate)

GROUP BY
	vr.CompanyName,
	st.SubTeam_Name,
	v.CompanyName,
	oh.OrderHeader_ID,
	oh.OriginalCloseDate,
	oh.TotalRefused

