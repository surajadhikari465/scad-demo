      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>DECLARE @ePLUM_PLUMaster           TABLE (UPC varchar(30), DeptNo int, Currup int, UTare int, Desc1 varchar(MAX), Desc2 varchar(MAX), Desc3 varchar(MAX), 
										  Desc4 varchar(MAX), ShelfLife int, UOM varchar(5), IngrNo int, StoreNo int, StoreName varchar(50))
DECLARE @ePLUM_Ingredient          TABLE (IngrNo varchar(10), IngrText varchar(MAX), StoreNo int, StoreName varchar(50))
DECLARE @PriceDiscrepancies        TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMAPrice money, ePLUMPrice money)
DECLARE @TareDiscrepancies         TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMATare decimal(3,3), ePLUMTare decimal(3,3))
DECLARE @Description1Discrepancies TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMADescription varchar(MAX), ePLUMDescription varchar(MAX))
DECLARE @Description2Discrepancies TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMADescription varchar(MAX), ePLUMDescription varchar(MAX))
DECLARE @Description3Discrepancies TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMADescription varchar(MAX), ePLUMDescription varchar(MAX))
DECLARE @Description4Discrepancies TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMADescription varchar(MAX), ePLUMDescription varchar(MAX))
DECLARE @IngredientDiscrepancies   TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMAIngredient varchar(MAX), ePLUMIngredient varchar(MAX))
DECLARE @ShelfLifeDiscrepancies    TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMAShelfLife int, ePLUMShelfLife int)
DECLARE @UOMDiscrepancies          TABLE (Item_Key int, DiscrepancyType varchar(2), StoreNo int, StoreName varchar(50), IRMAUOM varchar(5), ePLUMUOM varchar(5))
DECLARE @IRMAScaleItemMaster	   TABLE (Item_Key int)

INSERT INTO @ePLUM_PLUMaster
	SELECT
		SUBSTRING(pm.UPC,3,LEN(pm.UPC)), pm.DeptNo, pm.Currup, CAST(pm.UTare AS int), pm.Desc1, pm.Desc2, pm.Desc3, pm.Desc4, pm.ShelfLife, 
		pm.UOM, pm.IngrNo, es.StoreNo, es.StoreName
	FROM
		eplumsql.eplum.plumaster pm (nolock)
		join eplumsql.eplum.stores es (nolock) ON es.Store_Key = pm.StoreKey
		join dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = es.StoreNo

INSERT INTO @ePLUM_Ingredient
	SELECT
		im.IngrNo, im.IngrText, es.StoreNo, es.StoreName
	FROM
		eplumsql.eplum.ingrmaster im (nolock)
		join eplumsql.eplum.stores es (nolock) ON es.Store_Key = im.StoreKey
		join dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = es.StoreNo

INSERT INTO @IRMAScaleItemMaster
	SELECT
		ii.Item_Key
	FROM
		ItemIdentifier ii (nolock)
		JOIN Item i ON i.Item_Key = ii.Item_Key
	WHERE
		ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND i.Deleted_Item = 0

INSERT INTO @PriceDiscrepancies
	SELECT 
		ii.Item_Key, 'P', ep.StoreNo, ep.StoreName, 
		CASE WHEN p.Sale_End_Date &gt; GETDATE() THEN
			p.Sale_Price
		ELSE 
			p.POSPrice    
		END,
		(CAST(ep.Currup AS FLOAT)/100)
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC										  
		JOIN Price p (nolock) ON p.Item_Key = ii.Item_Key AND p.Store_No = ep.StoreNo
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = p.Store_No
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND
		(CAST(ep.Currup AS FLOAT)/100) &lt;&gt; CASE WHEN p.Sale_End_Date &gt; GETDATE() THEN
											 p.Sale_Price
										  ELSE 
							 				 p.POSPrice    
										  END

INSERT INTO @TareDiscrepancies
	SELECT 
		ii.Item_Key, 'T', ep.StoreNo, ep.StoreName, CAST(st.Zone1 AS decimal(3,3)), CAST(CAST(ep.UTare as decimal)/1000 AS DECIMAL(3,3))
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN Scale_Tare st (nolock) ON st.Scale_Tare_ID = isc.Scale_Tare_ID
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND st.Zone1 &lt;&gt; CAST(CAST(ep.UTare as decimal)/1000 AS DECIMAL(3,3))

INSERT INTO @Description1Discrepancies
	SELECT 
		ii.Item_Key, 'D1', ep.StoreNo, ep.StoreName, isc.Scale_Description1, ep.Desc1
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND RTRIM(ep.Desc1) &lt;&gt; RTRIM(isc.Scale_Description1)

INSERT INTO @Description2Discrepancies
	SELECT 
		ii.Item_Key, 'D2', ep.StoreNo, ep.StoreName, isc.Scale_Description2, ep.Desc2
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND RTRIM(ep.Desc2) &lt;&gt; RTRIM(isc.Scale_Description2)

INSERT INTO @Description3Discrepancies
	SELECT 
		ii.Item_Key, 'D3', ep.StoreNo, ep.StoreName, isc.Scale_Description3, ep.Desc3
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND RTRIM(ep.Desc3) &lt;&gt; RTRIM(isc.Scale_Description3)

INSERT INTO @Description4Discrepancies
	SELECT 
		ii.Item_Key, 'D4', ep.StoreNo, ep.StoreName, isc.Scale_Description4, ep.Desc4
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND RTRIM(ep.Desc4) &lt;&gt; RTRIM(isc.Scale_Description4)

INSERT INTO @ShelfLifeDiscrepancies
	SELECT 
		ii.Item_Key, 'S', ep.StoreNo, ep.StoreName, isc.ShelfLife_Length, ep.ShelfLife
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND ep.ShelfLife &lt;&gt; isc.ShelfLife_Length

INSERT INTO @UOMDiscrepancies
	SELECT 
		ii.Item_Key, 'U', ep.StoreNo, ep.StoreName, iu.PlumUnitAbbr, ep.UOM
	FROM 
		@ePLUM_PLUMaster ep
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN ItemUnit iu (nolock) ON iu.Unit_ID = isc.Scale_ScaleUOMUnit_ID
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND UPPER(ep.UOM) &lt;&gt; UPPER(iu.PlumUnitAbbr)

INSERT INTO @IngredientDiscrepancies
	SELECT 
		ii.Item_Key, 'I', ep.StoreNo, ep.StoreName, et.ExtraText, ei.ingrtext
	FROM 
		@ePLUM_PLUMaster ep
		JOIN @ePLUM_Ingredient ei ON ei.IngrNo = ep.IngrNo
		JOIN ItemIdentifier ii (nolock) ON ii.Identifier = ep.UPC
		JOIN ItemScale isc (nolock) ON isc.Item_Key = ii.Item_Key
		JOIN Scale_ExtraText et ON et.Scale_ExtraText_ID = isc.Scale_ExtraText_ID
		JOIN dbo.fn_Parse_List(@StoreList, '|') s ON s.Key_Value = ep.StoreNo
	WHERE
		ii.Deleted_Identifier = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND RTRIM(ei.ingrtext) &lt;&gt; RTRIM(et.ExtraText)
	
IF @Summary = 1
	BEGIN
		SELECT s.Store_Name AS StoreName, 
			   (SELECT COUNT(*) FROM @PriceDiscrepancies		WHERE StoreNo = s.Store_No) AS PriceDiscrepancyCount, 
			   (SELECT COUNT(*) FROM @TareDiscrepancies			WHERE StoreNo = s.Store_No) AS TareDiscrepancyCount,
			   (SELECT COUNT(*) FROM @Description1Discrepancies WHERE StoreNo = s.Store_No) AS Description1DiscrepancyCount,
			   (SELECT COUNT(*) FROM @Description2Discrepancies WHERE StoreNo = s.Store_No) AS Description2DiscrepancyCount,
			   (SELECT COUNT(*) FROM @Description3Discrepancies WHERE StoreNo = s.Store_No) AS Description3DiscrepancyCount,
			   (SELECT COUNT(*) FROM @Description4Discrepancies WHERE StoreNo = s.Store_No) AS Description4DiscrepancyCount,
			   (SELECT COUNT(*) FROM @IngredientDiscrepancies   WHERE StoreNo = s.Store_No) AS IngredientDiscrepancyCount,
			   (SELECT COUNT(*) FROM @ShelfLifeDiscrepancies    WHERE StoreNo = s.Store_No) AS ShelfLifeDiscrepancyCount,
			   (SELECT COUNT(*) FROM @UOMDiscrepancies          WHERE StoreNo = s.Store_No) AS UOMDiscrepancyCount
		FROM Store s
		JOIN dbo.fn_Parse_List(@StoreList, '|') l ON l.Key_Value = s.Store_No
	END
ELSE
	BEGIN
		SELECT
			ISNULL(pd.StoreName, ISNULL(td.StoreName, ISNULL(d1.StoreName, ISNULL(d2.StoreName, ISNULL(d3.StoreName, ISNULL(d4.StoreName, ISNULL(sd.StoreName, ISNULL(ud.StoreName, ISNULL(id.StoreName, ''))))))))) AS StoreName,
			CASE WHEN ii.NumPLUDigitsSentToScale = 4 THEN SUBSTRING(ii.Identifier, 3, 4) ELSE SUBSTRING(ii.Identifier, 2, 5) END AS PLU,
			ii.Identifier, i.Item_Description, st.SubTeam_Name, pd.IRMAPrice, pd.ePLUMPrice, td.IRMATare, td.ePLUMTare,
			d1.DiscrepancyType AS Description1Discrepancy, d2.DiscrepancyType AS Description2Discrepancy, 
			d3.DiscrepancyType AS Description3Discrepancy, d4.DiscrepancyType AS Description4Discrepancy,
			id.DiscrepancyType AS IngredientDiscrepancy, sd.IRMAShelfLife, sd.ePLUMShelfLife, ud.IRMAUOM, ud.ePLUMUOM
		FROM 
			@IRMAScaleItemMaster im
			LEFT JOIN @PriceDiscrepancies		pd ON pd.Item_Key = im.Item_Key
			LEFT JOIN @TareDiscrepancies         td ON td.Item_Key = im.Item_Key
			LEFT JOIN @Description1Discrepancies d1 ON d1.Item_Key = im.Item_Key
			LEFT JOIN @Description2Discrepancies d2 ON d2.Item_Key = im.Item_Key
			LEFT JOIN @Description3Discrepancies d3 ON d3.Item_Key = im.Item_Key
			LEFT JOIN @Description4Discrepancies d4 ON d4.Item_Key = im.Item_Key
			LEFT JOIN @ShelfLifeDiscrepancies    sd ON sd.Item_Key = im.Item_Key
			LEFT JOIN @UOMDiscrepancies          ud ON ud.Item_Key = im.Item_Key
			LEFT JOIN @IngredientDiscrepancies   id ON id.Item_Key = im.Item_Key
			LEFT JOIN Item						 i  ON i.Item_Key = im.Item_Key
			LEFT JOIN ItemIdentifier			 ii ON ii.Item_Key = im.Item_Key
			LEFT JOIN SubTeam					 st ON st.SubTeam_No = i.SubTeam_No
		WHERE
			i.Deleted_Item = 0 AND ii.Scale_Identifier = 1 AND ii.Default_Identifier = 1 AND st.Team_No = ISNULL(@TeamNo, st.Team_No)
			AND (pd.Item_Key IS NOT NULL OR td.Item_Key IS NOT NULL OR d1.Item_Key IS NOT NULL OR d2.Item_Key IS NOT NULL OR
				 d3.Item_Key IS NOT NULL OR d4.Item_Key IS NOT NULL OR sd.Item_Key IS NOT NULL OR ud.Item_Key IS NOT NULL OR
				 id.Item_Key IS NOT NULL)			
	END</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StoreList">
            <Value>=Parameters!StoreList.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Summary">
            <Value>=Parameters!Summary.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TeamNo">
            <Value>=Parameters!TeamNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
