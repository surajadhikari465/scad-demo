        </Field>
        <Field Name="Sale_Multiple">
          <DataField>Sale_Multiple</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Sale_Price">
          <DataField>Sale_Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
