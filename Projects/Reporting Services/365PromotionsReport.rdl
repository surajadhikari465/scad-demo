  <DataSets>
    <DataSet Name="BusinessUnits">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SET         TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
                    SELECT   BusinessUnit_ID, Store_Name
                    FROM      Store AS s
                    WHERE   (Mega_Store = 1)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BusinessUnit_ID">
          <DataField>BusinessUnit_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Store_Name">
          <DataField>Store_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StoreNameID">
          <Value>= CStr(Fields!BusinessUnit_ID.Value) &amp; " - " &amp; Fields!Store_Name.Value</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Promotions">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@businessUnitId">
            <Value>=Parameters!BusinessUnit.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT DISTINCT ii.Identifier,
	ii.Default_Identifier,
	i.Item_Description,
	p.Multiple,
	p.POSPrice AS Price,
	p.Sale_Multiple,
	p.POSSale_Price AS Sale_Price,
	p.Sale_Start_Date,
	p.Sale_End_Date,
	st.SubTeam_Name,
	s.Store_Name,
	ib.Brand_Name
FROM Item AS i
JOIN ItemIdentifier ii ON i.Item_Key = ii.Item_Key
JOIN StoreItem si ON i.Item_Key = si.Item_Key --and si.Authorized =1 
JOIN store s ON si.Store_No = s.Store_No
JOIN price p ON i.Item_Key = p.Item_Key
	AND s.Store_No = p.Store_No
JOIN subteam st ON i.SubTeam_No = st.SubTeam_No
JOIN ItemBrand AS ib ON i.Brand_ID = ib.Brand_ID
WHERE (s.BusinessUnit_ID = @businessUnitId)
	AND (p.Sale_End_Date &gt;= CONVERT(DATE, GETDATE()))
	AND si.Authorized = 1
	AND i.Deleted_Item = 0
	AND i.Remove_Item = 0
	AND ii.Deleted_Identifier = 0
	AND ii.Remove_Identifier = 0
ORDER BY ii.Identifier,
	st.SubTeam_Name,
	ii.Default_Identifier,
	i.Item_Description,
	p.Multiple,
	Price,
	p.Sale_Multiple,
	Sale_Price,
	p.Sale_Start_Date,
	p.Sale_End_Date,
	s.Store_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Default_Identifier">
          <DataField>Default_Identifier</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Item_Description">
          <DataField>Item_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Multiple">
          <DataField>Multiple</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Price">
