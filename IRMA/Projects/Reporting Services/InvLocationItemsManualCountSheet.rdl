    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="LocationItems">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InvLocID">
            <Value>=Parameters!InvLocID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT        s.Store_Name AS store_name, ii.Identifier AS identifier, il.InvLoc_Name AS invloc_name, i.Item_Description AS item_description,
                         case sj.StoreJurisdictionDesc when 'US' then  CONVERT(VARCHAR, CAST(i.Package_Desc1 AS DECIMAL(20, 2))) else  (case when iod.Package_Desc1 is null then  CONVERT(VARCHAR, CAST(i.Package_Desc1 AS DECIMAL(20, 2)))  else CONVERT(VARCHAR, CAST(iod.Package_Desc1 AS DECIMAL(20, 2))) end) end 
						 + '/' 
						 + case sj.StoreJurisdictionDesc when 'US' then CONVERT(VARCHAR, CAST(i.Package_Desc2 AS DECIMAL(20, 2))) else (case when iod.Package_Desc2 is null then  CONVERT(VARCHAR, CAST(i.Package_Desc2 AS DECIMAL(20, 2))) else CONVERT(VARCHAR, CAST(iod.Package_Desc2 AS DECIMAL(20, 2))) end ) end
						 + '/' 						 
						 +case sj.StoreJurisdictionDesc when 'US' then  CAST(iu.Unit_Name AS VARCHAR(10))
						  else (case when apu.Unit_Name is null then  CAST(iu.Unit_Name AS VARCHAR(10)) else CAST(apu.Unit_Name AS VARCHAR(10)) end )end   
                         AS PackSizeUnit, RIGHT('0000000000000' + CAST(ii.Identifier AS VARCHAR(20)), 12) AS PaddedIdentifier,
						  case sj.StoreJurisdictionDesc when 'US' then  ru.Unit_Name
						  else  (case when au.Unit_Name is null then ru.Unit_Name else au.Unit_Name end) end as  retail_uom
FROM            InventoryLocation AS il WITH (nolock) INNER JOIN
                         Store AS s WITH (nolock) ON s.Store_No = il.Store_No INNER JOIN
                         InventoryLocationItems AS ili WITH (nolock) ON il.InvLoc_ID = ili.InvLocID INNER JOIN
                         Item AS i WITH (nolock) ON ili.Item_Key = i.Item_Key INNER JOIN
                         ItemIdentifier AS ii WITH (nolock) ON i.Item_Key = ii.Item_Key AND ii.Default_Identifier = 1 INNER JOIN
                         ItemUnit AS iu WITH (nolock) ON i.Package_Unit_ID = iu.Unit_ID INNER JOIN
                         ItemUnit AS ru WITH (nolock) ON i.Retail_Unit_ID = ru.Unit_ID INNER JOIN
						 [StoreJurisdiction] AS sj on s.StoreJurisdictionID = sj.StoreJurisdictionID left join
						 ItemOverride As iod on i.Item_Key = iod.Item_Key and sj.StoreJurisdictionID = iod.StoreJurisdictionID left join
						 ItemUnit AS au WITH (nolock) ON iod.Retail_Unit_ID = au.Unit_ID left JOIN
						 ItemUnit AS apu WITH (nolock) ON iod.Package_Unit_ID = apu.Unit_ID
WHERE        (il.InvLoc_ID = @InvLocID)
ORDER BY invloc_name</CommandText>
        <Timeout>600</Timeout>
      </Query>
      <Fields>
        <Field Name="Store_Name">
          <DataField>store_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Identifier">
          <DataField>identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InvLoc_Name">
