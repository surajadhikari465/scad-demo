      <Fields>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Subteam">
          <DataField>Subteam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Brand">
          <DataField>Brand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Store">
          <DataField>Store</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Vendor">
          <DataField>Vendor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsIRMA</DataSourceName>
        <CommandText>select 
	[Identifier]		= ii.identifier,  
	[Subteam]			= s.subteam_name,
	[Brand]				= ib.brand_name,
	[Description]		= i.item_description,
	[Store]				= st.store_name,
	[Vendor]			= v.companyname
from 
(select 
	item_key, 
	store_no, 
	count(primaryvendor) as amt
	from storeitemvendor (nolock)
	where primaryvendor = 1
                and deletedate is null
	group by item_key, store_no
	having count(primaryvendor) &gt; 1) sivs
inner join storeitemvendor (nolock) siv on siv.item_key = sivs.item_key
							and siv.store_no = sivs.store_no
							and siv.primaryvendor = 1
inner join vendor (nolock) v on v.vendor_id = siv.vendor_id
inner join itemidentifier (nolock) ii on ii.item_key = siv.item_key
inner join item (nolock) i on i.item_key = siv.item_key
inner join itembrand (nolock) ib on ib.brand_id = i.brand_id
inner join subteam (nolock) s on s.subteam_no = i.subteam_no
inner join store (nolock) st on st.store_no = siv.store_no
order by ii.identifier, st.store_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
