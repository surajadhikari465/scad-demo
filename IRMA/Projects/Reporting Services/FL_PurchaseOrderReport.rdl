          <DataField>Lot_No</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="eInvoiceQuantity">
          <DataField>eInvoiceQuantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
