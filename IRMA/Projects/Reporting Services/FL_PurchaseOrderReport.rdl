          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemCasePack">
          <DataField>ItemCasePack</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="VendorOrderUOMName">
          <DataField>VendorOrderUOMName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="VendorOrderUOMCost">
          <DataField>VendorOrderUOMCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SET NOCOUNT ON
    
DECLARE @StoreNo As int, @ReceiveLoc As int    
    
 SELECT @ReceiveLoc = (SELECT ReceiveLocation_ID FROM OrderHeader (NOLOCK) WHERE OrderHeader_ID = @OrderHeader_ID)    
    
 SELECT @StoreNo = (SELECT Store_No FROM Vendor WHERE Vendor_ID = @ReceiveLoc)    
    
    SELECT OrderItem.OrderItem_ID,     
		CASE WHEN ISNULL(ItemVendor.Item_ID,'') &gt; '' THEN rtrim(ItemVendor.Item_ID) ELSE rtrim(Identifier) END AS VIN,     
		rtrim(ISNULL(ItemOverride.Item_Description, Item.Item_Description)) as Item_Description,
		OrderItem.QuantityOrdered,     
		OrderItem.QuantityReceived,    
		OrderItem.Total_Weight,     
		OrderItem.QuantityDiscount,     
		OrderItem.DiscountType,    
		OrderItem.UnitCost,    
		OrderItem.UnitExtCost,    
		OrderItem.LineItemCost,    
		OrderItem.LineItemFreight,     
		OrderItem.LineItemHandling,     
		Unit_Name = rtrim(ItemUnit.Unit_Name),
		OrderItem.Package_Desc1,    
		OrderItem.Package_Desc2,    
		ISNULL(I.Unit_Name, 'Unit') AS Package_Unit,    
		SubTeam_Name = rtrim(SubTeam.SubTeam_Name),    
		Item.SubTeam_No,    
		Category_Name = rtrim(Category_Name),     
		Brand_Name = rtrim(Brand_Name),     
		Origin_Name = rtrim(ItemOrigin.Origin_Name),    
		Proc_Name = rtrim(ItemProc.Origin_Name),    
		CAST(CASE WHEN Item.SubTeam_No = 2800 AND ((OrderItem.Origin_ID IS NULL) OR (OrderItem.CountryProc_ID IS NULL)) THEN 1 ELSE 0 END As bit) As SeafoodMissingCountryInfo,    
		Pre_Order,    
		Item.EXEDistributed,                
		OrderItem.Lot_No,    
		OrderItem.eInvoiceQuantity,  
		CASE WHEN OrderItem.AdjustedCost &gt; 0 THEN OrderItem.AdjustedCost ELSE OrderItem.MarkUpCost END AS ActualCost,
		Identifier								= rtrim(ItemIdentifier.identifier),
		ItemCasePack						= Item.Package_Desc1, -- This is # units in an item-pack.
		VendorOrderUOMName	= rtrim(OrderUOMRef.Unit_Name), -- For Item.Vendor_Unit_ID.
		VendorOrderUOMCost		= OrderItem.Cost -- This is usually case cost, unless vendor supplies in eaches or other UOM.

    FROM ItemCategory (NOLOCK) RIGHT JOIN (    
            ItemVendor (NOLOCK) RIGHT JOIN (    
               ItemUnit I (NOLOCK) RIGHT JOIN (     
                 SubTeam (NOLOCK) INNER JOIN (    
                  ItemIdentifier (NOLOCK) INNER JOIN (    
                   ItemOrigin ItemProc (NOLOCK) RIGHT JOIN (    
                    ItemOrigin (NOLOCK) RIGHT JOIN (    
                     ItemBrand (NOLOCK) RIGHT JOIN (    
                      Item (NOLOCK) INNER JOIN (    
                       ItemUnit (NOLOCK) INNER JOIN (    
                         OrderHeader INNER JOIN OrderItem ON (OrderHeader.OrderHeader_ID = OrderItem.OrderHeader_ID)    
                           ) ON (OrderItem.QuantityUnit = ItemUnit.Unit_ID)    
                         ) ON (OrderItem.Item_Key = Item.Item_Key)    
                       ) ON (Item.Brand_ID = ItemBrand.Brand_ID)    
                     ) ON (ItemOrigin.Origin_ID = ISNULL(OrderItem.Origin_ID, Item.Origin_ID))    
                    ) ON (ItemProc.Origin_ID = ISNULL(OrderItem.CountryProc_ID, Item.Origin_ID))    
                   ) ON (ItemIdentifier.Item_Key = Item.Item_Key AND ItemIdentifier.Default_Identifier = 1)    
                 ) ON (SubTeam.SubTeam_No = Transfer_To_SubTeam)    
               ) ON (OrderItem.Package_Unit_ID = I.Unit_ID)    
             ) ON (@Item_ID = 1 AND ItemVendor.Item_Key = OrderItem.Item_Key AND ItemVendor.Vendor_ID = OrderHeader.Vendor_ID)    
           ) ON (Item.Category_ID = ItemCategory.Category_ID)    
          LEFT JOIN ItemOverride (nolock) ON ItemOverride.Item_Key = Item.Item_Key AND ItemOverride.StoreJurisdictionID = (SELECT SN.StoreJurisdictionID FROM Store SN WHERE SN.Store_No = @StoreNo)    
	LEFT JOIN
		ItemUnit (nolock) OrderUOMRef
		ON OrderUOMRef.Unit_ID = Item.Vendor_Unit_ID
    WHERE OrderHeader.OrderHeader_ID = @OrderHeader_ID    
ORDER BY     
  CASE @SortType    
   -- Line Item Cost    
   WHEN 2 THEN ABS(OrderItem.LineItemCost + OrderItem.LineItemFreight)    
   WHEN 3 THEN CASE WHEN ISNULL(ItemVendor.Item_ID,'') &gt; '' THEN ItemVendor.Item_ID ELSE Identifier END    
   ELSE OrderItem.OrderItem_ID END    
        
    SET NOCOUNT OFF</CommandText>
        <QueryParameters>
          <QueryParameter Name="@OrderHeader_ID">
            <Value>=Parameters!OrderHeader_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Item_ID">
            <Value>=Parameters!Item_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SortType">
            <Value>=Parameters!SortType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
