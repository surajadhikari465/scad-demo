        <Field Name="stocking_uom">
          <DataField>stocking uom</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>FSPRD</DataSourceName>
        <CommandText>select
	b.inv_item_id as "item code"
	, b.dt_timestamp "last avg cost update"
	, b.CM_UNIT_COST as "avg cost"
	, a.ITEM_FIELD_C30_A as "irma identifier"
	, a.replenish_point as "pack size"
	, c.inv_item_size "retail size"
	, a.related_item_id as "retail uom"
	, d.UNIT_OF_MEASURE as "stocking uom"
from
	ps_cm_perpavg_cost b
	, ps_bu_items_inv a
	, ps_inv_items c
	, ps_inv_item_uom d
where
	a.business_unit=b.business_unit
	and c.inv_item_id=a.inv_item_id
	and c.inv_item_id=b.inv_item_id
	and a.inv_item_id=b.inv_item_id
	and d.inv_item_id=a.inv_item_id
	and d.inv_item_id=b.inv_item_id
	and d.inv_item_id=c.inv_item_id
	and d.dflt_uom_stock='Y'
	and b.business_unit='20028'
	and b.dt_timestamp = (SELECT MAX (b1.dt_timestamp)
				FROM ps_cm_perpavg_cost b1
				WHERE b1.business_unit = b.business_unit
					AND b1.inv_item_id = b.inv_item_id)
	and b.inv_item_id = ?
group by
	b.inv_item_id
	, cm_unit_cost
	, b.dt_timestamp
	,a.ITEM_FIELD_C30_A
	, a.replenish_point
	, c.inv_item_size
	, d.UNIT_OF_MEASURE
	,a.related_item_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Item_Code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
