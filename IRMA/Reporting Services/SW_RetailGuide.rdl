  </DataSources>
  <DataSets>
    <DataSet Name="OrderGuide">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Store_No">
            <Value>=Parameters!Store_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Vendor_ID">
            <Value>=Parameters!Vendor_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SubTeam_No">
            <Value>=Parameters!SubTeam_No.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category_ID">
            <Value>=Parameters!Category_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

SELECT        Item.Item_Key, Item.Item_Description, ItemIdentifier.Identifier, retailunit.Unit_Name, ItemBrand.Brand_Name, 
                         (CASE WHEN retail.pricechgtypeid &gt; 1 THEN CAST(retail.sale_price AS DECIMAL(9, 2)) ELSE CAST(retail.price AS DECIMAL(9, 2)) END) AS price, 
                         (CASE WHEN retail.pricechgtypeid &gt; 1 THEN retail.sale_multiple ELSE retail.multiple END) AS multiple, CAST(retail.POSPrice AS DECIMAL(9, 2)) AS posprice, 
                         LTRIM(SUBSTRING(ItemCategory.Category_Name, CHARINDEX(' ', ItemCategory.Category_Name) + 1, LEN(ItemCategory.Category_Name) - CHARINDEX(' ', 
                         ItemCategory.Category_Name) + 1)) AS catname
FROM            Item WITH (nolock) INNER JOIN
                         StoreItemVendor AS siv WITH (nolock) ON Item.Item_Key = siv.Item_Key AND siv.Store_No = @Store_No AND siv.Vendor_ID = @Vendor_ID AND 
                         Item.SubTeam_No = @SubTeam_No INNER JOIN
                         ItemCategory WITH (nolock) ON ItemCategory.Category_ID = Item.Category_ID AND (@Category_ID IS NULL OR
                         Item.Category_ID = @Category_ID) INNER JOIN
                         ItemUnit AS retailunit WITH (nolock) ON retailunit.Unit_ID = Item.Retail_Unit_ID INNER JOIN
                         ItemBrand WITH (nolock) ON Item.Brand_ID = ItemBrand.Brand_ID INNER JOIN
                         Price AS retail WITH (nolock) ON Item.Item_Key = retail.Item_Key AND retail.Store_No = @Store_No INNER JOIN
                         StoreSubTeam AS sst WITH (nolock) ON sst.Store_No = retail.Store_No AND sst.SubTeam_No = Item.SubTeam_No INNER JOIN
                         ItemIdentifier WITH (nolock) ON ItemIdentifier.Item_Key = Item.Item_Key AND ItemIdentifier.Default_Identifier = 1
WHERE        (siv.DiscontinueItem = 0) AND (Item.Deleted_Item = 0) AND (Item.Not_Available = 0)
ORDER BY catname, Item.Item_Description, ItemBrand.Brand_Name</CommandText>
      </Query>
      <Fields>
        <Field Name="Item_Key">
          <DataField>Item_Key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Item_Description">
          <DataField>Item_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Unit_Name">
          <DataField>Unit_Name</DataField>
