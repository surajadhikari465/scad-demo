  <Code>Public Shared Function UnitRetail(ByVal SalesQuantity as Double, _
 ByVal SalesAmount as Double, ByVal Price as Double) as Double
	If IsDBNull(SalesAmount) Or SalesAmount = 0
		Return Price
	Else
		Return SalesAmount / SalesQuantity 
	End If
End Function

Public Shared Function UnitCost(ByVal SalesQuantity as Double, _
 ByVal AvgCost as Double, ByVal Cost as Double) as Double
	If IsDBNull(SalesQuantity) Or SalesQuantity = 0
		Return Cost
	Else
		Return AvgCost / SalesQuantity 
	End If
End Function

Public Shared Function CalcGrossMargin(ByVal SalesAmount as Double, _
 ByVal AvgCost as Double) as Double
	If IsDBNull(SalesAmount) Or SalesAmount = 0
		Return 0
	Else
		Return ((SalesAmount - AvgCost) _
		/ SalesAmount) 
	End If
End Function

Public Shared Function CalcMix(ByVal SalesAmount as Double, _
 ByVal StoreSalesAmount as Double) as Double
	If IsDBNull(StoreSalesAmount) Or StoreSalesAmount = 0
		Return 0
	Else
		Return SalesAmount / StoreSalesAmount
	End If
End Function

Public Shared Function CategoryGroupName(ByVal GroupName as String) _
as String
	If GroupName.Equals("ZZZZZZZZZZZZZZZZZZZZZZZZZ")
		Return "Total"
	Else
		Return GroupName
	End If
End Function</Code>
