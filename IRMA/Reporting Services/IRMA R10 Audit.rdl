  <DataSources>
    <DataSource Name="DataSource2">
      <DataSourceReference>ItemCatalog</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>5d0c58d6-4149-4e86-8a74-46bfe4dccc43</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>DECLARE @today DATETIME = getdate();

SELECT [r].[code],
	[r].[R10_Price],
	[r].[R10_PM],
	[r].[EffectiveDate],
	[r].[ExpirationDate],
	[r].[BusinessUnit_Id],
	[r].[UOM],
	[p2].[IRMA_price],
	([r].[R10_Price] - [p2].[IRMA_price]) AS [Difference],
	[sub].[SubTeam_Name],
	[ib].[brand_name],
	[i].Item_Description,
	[p2].[IRMA Start Date],
	[p2].[IRMA End Date],
	[p2].[IRMA REG Price],
	[p2].[IRMA_REGPriceMultiple],
	[pct].[PriceChgTypeDesc],
	[p2].[IRMA_SALEPriceMultiple],
	[s].[Store_Name]
FROM storeitem si(NOLOCK)
JOIN item i(NOLOCK) ON si.item_key = i.item_key
	AND si.Authorized = 1
JOIN store s(NOLOCK) ON s.Store_No = si.Store_No
JOIN StorePOSConfig SPC(NOLOCK) ON s.Store_No = SPC.Store_No
JOIN POSWriter POS(NOLOCK) ON SPC.POSFileWriterKey = POS.POSFileWriterKey
JOIN itemidentifier ii(NOLOCK) ON i.item_key = ii.item_key
	AND ii.deleted_identifier = 0
	AND ii.remove_identifier = 0
JOIN subteam st(NOLOCK) ON st.subteam_no = i.subteam_no
JOIN ItemBrand ib(NOLOCK) ON i.Brand_ID = ib.Brand_ID
JOIN SubTeam sub(NOLOCK) ON i.SubTeam_No = sub.SubTeam_No
JOIN (
	SELECT (
			CASE 
				WHEN (p.pricechgtypeid &gt; 1)
					AND (p.Sale_End_Date &gt; @today)
					THEN p.sale_price / (
							CASE 
								WHEN (p.Sale_Multiple = 0)
									OR p.Sale_Multiple IS NULL
									THEN 1
								ELSE p.Sale_Multiple
								END
							)
				ELSE p.price / (
						CASE 
							WHEN p.Multiple = 0
								OR p.Multiple IS NULL
								THEN 1
							ELSE p.Multiple
							END
						)
				END
			) AS [IRMA_Price],
		p.Sale_Start_Date AS [IRMA Start Date],
		p.Sale_End_Date AS [IRMA End Date],
		p.Price AS [IRMA REG Price],
		p.Multiple AS [IRMA_REGPriceMultiple],
		p.Sale_Multiple AS [IRMA_SALEPriceMultiple],
		p.PriceChgTypeId,
		p.item_key,
		p.store_no
	FROM price p(NOLOCK)
	) AS p2 ON p2.item_key = i.item_key
	AND p2.store_no = s.store_no
JOIN PriceChgType pct(NOLOCK) ON p2.PriceChgTypeId = pct.PriceChgTypeID
JOIN R10PriceAudit r(NOLOCK) ON s.BusinessUnit_ID = r.BusinessUnit_Id
	AND r.code = ii.Identifier
WHERE i.Deleted_Item = 0
	AND i.Remove_Item = 0
	AND ii.deleted_identifier = 0
	AND ii.remove_identifier = 0
	AND i.Retail_Sale = 1
	AND pos.POSFileWriterCode = 'R10'
	AND r.rn = 1
	AND ABS(([r].[R10_Price] / [r].[R10_PM]) - [p2].[IRMA_price]) &gt; .01 -- filter out '3-fers'
	AND [p2].[IRMA_price] &lt;&gt; 0 -- filter out zero price crap in hillcrest
	--AND datediff(day, [p2].[IRMA End Date], [r].[ExpirationDate]) &gt; 0 -- filter out sales ending today
	AND ISNULL(datediff(day, [p2].[IRMA End Date], [r].[ExpirationDate]), 1) &gt; 0 -- filter out sales ending today
GROUP BY [r].[code],
	[r].[R10_Price],
	[r].[R10_PM],
	[r].[EffectiveDate],
