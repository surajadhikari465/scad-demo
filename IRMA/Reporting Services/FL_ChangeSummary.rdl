          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemChanges">
          <DataField>ItemChanges</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PriceChanges">
          <DataField>PriceChanges</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>--table vars
DECLARE @tblItemChanges TABLE (SessionID INT,UserID INT,SubTeamNo INT,SubTeam VARCHAR(120),FullName VARCHAR(120),ItemChanges INT)
DECLARE @tblPriceChanges TABLE (SessionID INT,UserID INT,SubTeamNo INT,SubTeam VARCHAR(120),FullName VARCHAR(120),PriceChanges INT)
DECLARE @tblItemPriceChanges TABLE (SessionID INT,UserID INT,SubTeamNo INT,SubTeam VARCHAR(120),FullName VARCHAR(120),ItemChanges INT,PriceChanges INT)


-- get item changes
INSERT INTO @tblItemChanges
	SELECT DISTINCT
		us.UploadSession_ID,us.CreatedByUserID,st.SubTeam_No,st.SubTeam_Name,
		(SELECT u.FullName FROM dbo.Users u WHERE us.CreatedByUserID = u.[User_ID]) AS 'FullName',
		--us.ItemsLoadedCount
		us.ItemsLoadedCount AS 'TotalItemChanges'
	FROM dbo.UploadSession us
		INNER JOIN dbo.UploadSessionUploadType usut ON us.UploadSession_ID = usut.UploadSession_ID
		INNER JOIN dbo.UploadRow ur ON us.UploadSession_ID = ur.UploadSession_ID
		INNER JOIN dbo.ItemIdentifier ii ON ur.Identifier = ii.Identifier
		INNER JOIN dbo.Item i ON ii.Item_Key = i.Item_Key
		INNER JOIN dbo.SubTeam st ON i.SubTeam_No = st.SubTeam_No
	WHERE us.CreatedDateTime BETWEEN ISNULL(@StartDate,us.CreatedDateTime) AND ISNULL(DATEADD(DAY,1,@EndDate),us.CreatedDateTime)
		AND usut.UploadType_Code &lt;&gt; 'PRICE_UPLOAD'
		AND st.SubTeam_No = ISNULL(@SubTeamNo,st.SubTeam_No)
		AND us.CreatedByUserID = ISNULL(@userID,us.CreatedByUserID)


-- get price changes
INSERT INTO @tblPriceChanges
	SELECT DISTINCT
		us.UploadSession_ID,us.CreatedByUserID,st.SubTeam_No,st.SubTeam_Name,
		(SELECT u.FullName FROM dbo.Users u WHERE us.CreatedByUserID = u.[User_ID]) AS 'FullName',		
		us.ItemsLoadedCount AS 'TotalPriceChanges'
	FROM dbo.UploadSession us
		INNER JOIN dbo.UploadSessionUploadType usut ON us.UploadSession_ID = usut.UploadSession_ID
		INNER JOIN dbo.UploadRow ur ON us.UploadSession_ID = ur.UploadSession_ID
		INNER JOIN dbo.ItemIdentifier ii ON ur.Identifier = ii.Identifier
		INNER JOIN dbo.Item i ON ii.Item_Key = i.Item_Key
		INNER JOIN dbo.SubTeam st ON i.SubTeam_No = st.SubTeam_No
	WHERE us.CreatedDateTime BETWEEN ISNULL(@StartDate,us.CreatedDateTime) AND ISNULL(DATEADD(DAY,1,@EndDate),us.CreatedDateTime)
		AND usut.UploadType_Code = 'PRICE_UPLOAD'
		AND st.SubTeam_No = ISNULL(@SubTeamNo,st.SubTeam_No)
		AND us.CreatedByUserID = ISNULL(@userID,us.CreatedByUserID)


INSERT INTO @tblItemPriceChanges
-- get records that have both item and price changes
	SELECT ic.SessionID,ic.UserID,ic.SubTeamNo,ic.SubTeam,ic.FullName,ic.ItemChanges,pc.PriceChanges
	FROM @tblItemChanges ic
		INNER JOIN @tblPriceChanges pc ON ic.SessionID = pc.SessionID


-- put it all together
SELECT DISTINCT * 
FROM
(
	-- get item and price changes
	SELECT 
		ipc.SessionID,ipc.UserID,ipc.SubTeamNo,ipc.SubTeam,
		ipc.FullName,ipc.ItemChanges,ipc.PriceChanges
	FROM @tblItemPriceChanges ipc	
) AS dtPriceChanges
UNION
(
	-- get item changes only
	SELECT 
		ic.SessionID,ic.UserID,ic.SubTeamNo,ic.SubTeam,ic.FullName,
		ic.ItemChanges,0 AS 'PriceChanges'
	FROM @tblItemChanges ic 
	WHERE ic.SessionID NOT IN (SELECT ipc.SessionID FROM @tblItemPriceChanges ipc)	
)
UNION
(
	-- get price changes only
	SELECT 
		pc.SessionID,pc.UserID,pc.SubTeamNo,pc.SubTeam,pc.FullName,
		0 AS 'ItemChanges',pc.PriceChanges
	FROM @tblPriceChanges pc WHERE pc.SessionID NOT IN (SELECT ipc.SessionID FROM @tblItemPriceChanges ipc)
)
ORDER BY dtPriceChanges.SubTeamNo</CommandText>
