        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>SELECT NULL AS 'User_ID','All Team Members' AS 'FullName'
UNION
(
	SELECT u.[User_ID],u.FullName FROM dbo.Users u WHERE u.Accountenabled = 1
)
ORDER BY FullName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
