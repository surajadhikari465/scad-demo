      <Fields>
        <Field Name="Identifier">
          <DataField>Identifier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Store_No">
          <DataField>Store_No</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Subteam_No">
          <DataField>Subteam_No</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="REG_Price">
          <DataField>REG_Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SAL_Price">
          <DataField>SAL_Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <CommandText>select i.Identifier, p.Store_No, t.Subteam_No, r.Price/r.Multiple as REG_Price, p.Sale_Price/p.Sale_Multiple as SAL_Price
from ItemIdentifier i, PriceBatchDetail p, Price r, StoreItem si, Item t
where i.Item_Key = p.Item_Key and i.Item_Key = r.Item_Key and p.Store_No = r.Store_No
and i.Item_Key = si.Item_Key and p.Store_No = si.Store_No and i.Item_Key = t.Item_Key
and r.Price/r.Multiple &lt;= p.Sale_Price/p.Sale_Multiple and si.Authorized = 'TRUE'
and p.StartDate = @StartDate and p.PriceChgTypeID = '2' and p.Expired = 'FALSE'


order by t.Subteam_No, i.Identifier, p.Store_No</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
