  <DataSets>
    <DataSet Name="UPCCostAudit">
      <Query>
        <DataSourceName>ItemCatalog</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TeamNo">
            <Value>=Parameters!TeamNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SubTeamNo">
            <Value>=Parameters!SubTeamNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@VendorId">
            <Value>=Parameters!VendorId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdentifierList">
            <Value>=Parameters!IdentifierList.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

IF ISNULL(@IdentifierList,'') = ''
	BEGIN
		SELECT 
			oh.OrderDate, oh.UploadedDate, ii.Identifier, i.Item_Description, oh.OrderHeader_ID, oh.InvoiceNumber,
			CONVERT(VARCHAR(10), oh.InvoiceDate, 101) AS InvoiceDate, recVend.CompanyName AS ReceivingStore, CONVERT(VARCHAR(10), oh.RecvLogDate, 101) AS ReceivedDate, v.Vendor_Id, v.CompanyName AS VendorName
		FROM
			OrderHeader oh (nolock)
			JOIN OrderItem oi (nolock) ON oi.OrderHeader_ID = oh.OrderHeader_ID
			JOIN Item i (nolock) ON i.Item_Key = oi.Item_Key
			JOIN ItemIdentifier ii (nolock) ON ii.Item_Key = oi.Item_Key
			JOIN SubTeam st (nolock) ON st.SubTeam_No = i.SubTeam_No
			JOIN Vendor recVend (nolock) ON recVend.Vendor_Id = oh.ReceiveLocation_Id
			JOIN Vendor v (nolock) ON v.Vendor_Id = oh.Vendor_Id
		WHERE
			oh.CloseDate IS NOT NULL AND 
			(oh.RecvLogDate IS NOT NULL OR (SELECT s.Distribution_Center
											FROM Vendor v
											JOIN Store s ON s.Store_No = v.Store_No
											WHERE Vendor_Id = oh.Vendor_ID) = 1) AND 
			(oh.UploadedDate BETWEEN @StartDate AND @EndDate OR (SELECT s.Distribution_Center
																 FROM Vendor v
																 JOIN Store s ON s.Store_No = v.Store_No
																 WHERE Vendor_Id = oh.Vendor_ID) = 1 AND
																	   oh.CloseDate BETWEEN @StartDate AND @EndDate)    AND
			st.Team_No   = ISNULL(@TeamNo, st.Team_No)      AND
			i.SubTeam_No = ISNULL(@SubTeamNo, i.SubTeam_No) AND
			oh.Vendor_Id = ISNULL(@VendorId, oh.Vendor_Id)  AND
			oh.Return_Order = 0  AND
			ii.Default_Identifier = 1 AND
			oi.QuantityReceived &gt; 0
		ORDER BY 
			InvoiceDate, oh.OrderHeader_ID
	END
ELSE
	BEGIN
		DECLARE @tblItemKeys TABLE (ItemKey int)
		DECLARE @DelimPos int

        --NOTE: There must be a pipe '|' at the end of @IdentifierList or else this will result in an infinite loop
		WHILE (@IdentifierList &lt;&gt; '')
			BEGIN
				SELECT @DelimPos = CHARINDEX('|',@IdentifierList)

				INSERT INTO @tblItemKeys
					SELECT Item_Key FROM ItemIdentifier WHERE Identifier = (SELECT SUBSTRING(@IdentifierList,1,@DelimPos - 1))

				SELECT @IdentifierList = SUBSTRING(@IdentifierList,@DelimPos + 1,LEN(@IdentifierList))
			END

		SELECT 
			oh.OrderDate, oh.UploadedDate, oh.OrderHeader_Id, ii.Identifier, i.Item_Description, oh.OrderHeader_ID, oh.InvoiceNumber,
			CONVERT(VARCHAR(10), oh.InvoiceDate, 101) AS InvoiceDate, recVend.CompanyName AS ReceivingStore, CONVERT(VARCHAR(10), oh.RecvLogDate, 101) AS ReceivedDate, v.Vendor_Id, v.CompanyName AS VendorName
		FROM
			OrderHeader oh (nolock)
			JOIN OrderItem oi (nolock) ON oi.OrderHeader_ID = oh.OrderHeader_ID
			JOIN Item i (nolock) ON i.Item_Key = oi.Item_Key
			JOIN ItemIdentifier ii (nolock) ON ii.Item_Key = oi.Item_Key
			JOIN SubTeam st (nolock) ON st.SubTeam_No = i.SubTeam_No
			JOIN Vendor recVend (nolock) ON recVend.Vendor_Id = oh.ReceiveLocation_Id
			JOIN Vendor v (nolock) ON v.Vendor_Id = oh.Vendor_Id
			JOIN @tblItemKeys ik ON ik.ItemKey = oi.Item_Key
		WHERE
			oh.CloseDate IS NOT NULL AND 
			(oh.RecvLogDate IS NOT NULL OR (SELECT s.Distribution_Center
											FROM Vendor v
											JOIN Store s ON s.Store_No = v.Store_No
											WHERE Vendor_Id = oh.Vendor_ID) = 1) AND 
			(oh.UploadedDate BETWEEN @StartDate AND @EndDate OR (SELECT s.Distribution_Center
																 FROM Vendor v
																 JOIN Store s ON s.Store_No = v.Store_No
																 WHERE Vendor_Id = oh.Vendor_ID) = 1 AND
																	   oh.CloseDate BETWEEN @StartDate AND @EndDate)    AND
			st.Team_No   = ISNULL(@TeamNo, st.Team_No)      AND
			i.SubTeam_No = ISNULL(@SubTeamNo, i.SubTeam_No) AND
			oh.Vendor_Id = ISNULL(@VendorId, oh.Vendor_Id)  AND
			oh.Return_Order = 0  AND
			ii.Default_Identifier = 1 AND
			oi.QuantityReceived &gt; 0
		ORDER BY 
			InvoiceDate, oh.OrderHeader_ID
	END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
