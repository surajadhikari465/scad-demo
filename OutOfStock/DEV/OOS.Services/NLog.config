<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <!-- Note that a stacktrace at this point would be equvalent to the stack trace provided in an exception -->
    
    <target name="console" xsi:type="Console" />

    <target name="file" xsi:type="File" fileName="${basedir}/Logs/StoreSync.log"
            layout="${message}"
            archiveFileName="${basedir}/Logs/StoreSync.{####}.log"
            ArchiveEvery="Month"
            archiveNumbering="Sequence" />



  </targets>
  <rules>
   <logger name="*" minlevel="Debug" writeTo="console,file" />
  </rules>
</nlog>