<?xml version="1.0"?>

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings>
    <add name="OOSConnectionString"
         connectionString="Data Source=OOSdbprd;Initial Catalog=OOS;Integrated Security=True"
         providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
    <add name="OOSEntities"
         connectionString="metadata=res://*/DataContext.OOS.csdl|res://*/DataContext.OOS.ssdl|res://*/DataContext.OOS.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=OOSdbprd;initial catalog=OOS;integrated security=True;multipleactiveresultsets=True;App=EntityFramework&quot;"
         providerName="System.Data.EntityClient" xdt:Transform="Replace" xdt:Locator="Match(name)" />
  </connectionStrings>
  <appSettings>
    <add key="SecurityGroupsRegional" value="WFM\OOS Regional User" />
    <add key="SecurityStoresRegional" value="NAO,NRO,SWC" /> 
    <add key="SecurityGroupsCentral" value="WFM\OOS Central User,WFM\WFM NEXuS Support,WFM\WFM NEXuS Contractors" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="SecurityStoresCentral" value="CEN" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="CEDefaultRegion" value="NC" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="ForceRegionalUsersList" value="" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="Suffix" value="_Production" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <nlog>
    <rules xdt:Transform="Replace" >
      <logger name="*" levels="Fatal,Error,Warn,Info,Debug" writeTo="file" />
      <logger name="*" levels="Fatal,Error,Warn,Info,Debug" writeTo="json-file" />
    </rules>
  </nlog>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>